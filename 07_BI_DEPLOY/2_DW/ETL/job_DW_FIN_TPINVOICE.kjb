    <entry>
      <name>TO_DW</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;--Settings&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DW&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Udf&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;INSERT OVERWRITE TABLE DW.DW_FIN_TPINVOICE PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;T.START_DATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID --  &#x65e5;&#x671f;&#x7ef4;ID&#xd;&#xa;      ,T.INVOICE_GID AS INVOICE_GID --  &#x53d1;&#x7968;GID&#xd;&#xa;      ,T.INVOICE_XID AS INVOICE_XID --  &#x53d1;&#x7968;XID&#xd;&#xa;      ,T.INVOICE_TYPE AS INVOICE_TYPE --  &#x8868;&#x793a;&#x8fd9;&#x662f;&#x4e00;&#x5f20;&#x7968;&#x636e;&#x8fd8;&#x662f;&#x4e00;&#x5f20;&#x53d1;&#x7968;&#xff08;&#x6709;&#x6548;&#x503c;&#x662f;&#x2018;i&#x2019;&#x6216;&#x2018;b&#x2019;&#xff09;&#xd;&#xa;      ,T.INVOICE_SOURCE AS INVOICE_SOURCE --  &#x53d1;&#x7968;&#x6e90;&#x6709;&#x6548;&#x503c;&#x662f;G&#xff0c;I&#xff0c;M&#x3002;&#xd;&#xa;      ,&#x27;CMAL&#x2f;ZC&#x27; AS OC_ID --  &#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#xd;&#xa;      ,COALESCE&#x28;O.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --  &#x5927;&#x533a;&#xd;&#xa;      ,COALESCE&#x28;O.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --  &#x57fa;&#x5730;&#xd;&#xa;      ,T.ATTRIBUTE1 AS BASE_NAME --  &#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;      ,NULL AS PROJ_ID --  &#x9879;&#x76ee;ID&#xd;&#xa;      ,NULL AS PROJ_NAME --  &#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;      ,T.ATTRIBUTE9 AS VIN --  VIN&#x7801;&#xd;&#xa;      ,T.ATTRIBUTE3 AS BRAND_ID --  &#x54c1;&#x724c;ID&#xd;&#xa;      ,T.ATTRIBUTE6 AS MODEL_ID --  &#x8f66;&#x578b;ID&#xd;&#xa;      ,T.ATTRIBUTE2 AS MDLGRP_ID --  &#x8f66;&#x578b;&#x7ec4;&#xd;&#xa;      ,NULL AS SOURCE_PRVN_ID --  &#x59cb;&#x53d1;&#x5730;&#x7701;&#x4efd;&#xd;&#xa;      ,NULL AS SOURCE_CTY_ID --  &#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;&#xd;&#xa;      ,NULL AS DEST_PRVN_ID --  &#x76ee;&#x7684;&#x5730;&#x7701;&#x4efd;&#xd;&#xa;      ,NULL AS DEST_CTY_ID --  &#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;&#xd;&#xa;      ,T.ATTRIBUTE5 AS TRANSPORTMODE --  &#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x28;&#x4e00;&#x6bb5;&#x29;&#xd;&#xa;      ,T.SERVPROV_GID AS SERVPROV_GID --  &#x627f;&#x8fd0;&#x5546;&#xd;&#xa;      ,T.SERVPROV_ALIAS_VALUE AS SERVPROV_ALIAS_VALUE --  &#x627f;&#x8fd0;&#x5546;&#x522b;&#x540d;&#xd;&#xa;      ,T.ENTERED_BY_GL_USER_GID AS ENTERED_BY_GL_USER_GID --  entered_by_gl_user_gid identifies which gl user entered the invoice into the system.&#xd;&#xa;      ,T.INVOICE_NUMBER AS INVOICE_NUMBER --  invoice_number indicates the number that was assigned to the invoice.&#xd;&#xa;      ,T.SERVPROV_ALIAS_QUAL_GID AS SERV_ALIAS_QUAL_GID --  servprov_alias_qual_gid identifies the service provider qualifier &#x28;such as scec&#x29;, which  is then associated with the value in servprov_alias_value.&#xd;&#xa;      ,T.CORRECTION_CODE_ID AS CORRECTION_CODE_ID --  correction  code identifier.&#xd;&#xa;      ,T.INVOICE_DATE AS INVOICE_DATE --  &#x53d1;&#x7968;&#x65e5;&#x671f;&#xd;&#xa;      ,T.CURRENCY_GID AS CURRENCY_GID --  currency_gid is the unique identifier for currency.&#xd;&#xa;      ,T.NET_AMOUNT_DUE AS NET_AMOUNT_DUE --  &#x53d1;&#x7968;&#x6240;&#x5e94;&#x6709;&#x7684;&#x51c0;&#x91d1;&#x989d;&#xd;&#xa;      ,T.NET_AMOUNT_DUE_GID AS NET_AMOUNT_DUE_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.NET_AMOUNT_BASE AS NET_AMOUNT_BASE --  net_amount_base indicates the amount that is due in the base currency.&#xd;&#xa;      ,T.NET_DUE_DATE AS NET_DUE_DATE --  net_due_date contains the date on which the net amount for the invoice is due.&#xd;&#xa;      ,T.PAYMENT_METHOD_CODE_GID AS PAYMENT_METHOD_CODE_GID --  payment_method_code_gid contains the method of payment that is expected for the invoice.examples include&#x3a; check, cash on deliver, credit card and electronic funds transfer.&#xd;&#xa;      ,T.START_DATE AS START_DATE --  &#x5f00;&#x59cb;&#x65f6;&#x95f4;&#xd;&#xa;      ,T.END_DATE AS END_DATE --  &#x7ed3;&#x675f;&#x65f6;&#x95f4;&#xd;&#xa;      ,T.INVOICE_SERVICE_CODE_GID AS INVOICE_SERV_CODE_GID --  invoice_service_code_gid indicates the type of service that was performed. foreign key to invoice_service_code.&#xd;&#xa;      ,T.INCO_TERM_GID AS INCO_TERM_GID --  inco_term_gid qualifies the terms of sale for this particular invoice.&#xd;&#xa;      ,T.DISCOUNT_AMOUNT AS DISCOUNT_AMOUNT --  discount_amount contains the total dollar amount of any discounts applied to the invoice.&#xd;&#xa;      ,T.DISCOUNT_AMOUNT_GID AS DISCOUNT_AMOUNT_GID --  gc3 global indentifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.DISCOUNT_AMOUNT_BASE AS DISCOUNT_AMOUNT_BASE --  discount_amount_base contains the amount of the discount in the base currency.&#xd;&#xa;      ,T.DISCOUNT_PERCENTAGE AS DISCOUNT_PERCENTAGE --  discount_percentage contains either a specific amount for the discount or a percentage to be used for the discount.&#xd;&#xa;      ,T.DISCOUNT_DAYS_DUE AS DISCOUNT_DAYS_DUE --  discount_days_due contains the number of days in the terms discount period by which payment is due if the terms of the discount are to be earned.&#xd;&#xa;      ,T.DISCOUNT_DUE_DATE AS DISCOUNT_DUE_DATE --  discount_due_date contains the date that the payment must be made by in order for the discount to be earned.&#xd;&#xa;      ,T.ORIGIN_STATION_CITY AS ORIGIN_STATION_CITY --  origin_station_city identifies the city in which the related shipment began.&#xd;&#xa;      ,T.ORIGIN_STATION_FSAC AS ORIGIN_STATION_FSAC --  origin_station_fsac identifies the fsac code for the station in which the related shipment began.&#xd;&#xa;      ,T.ORIGIN_STATION_SPLC AS ORIGIN_STATION_SPLC --  origin_station_splc identifies the splc code for the station in which the related shipment began.&#xd;&#xa;      ,T.ORIGIN_STATION_PROVINCE_CODE AS ORG_STATION_PROV_CODE --  origin_station_province_code identifies the province code in which the related shipment began.&#xd;&#xa;      ,T.ORIGIN_STATION_POSTAL_CODE AS ORG_STATION_POSTAL_CODE --  origin_station_postal_code identifies the postal code in which the related shipment began.&#xd;&#xa;      ,T.ORIGIN_COUNTRY_CODE3_GID AS ORG_COUNTRY_CODE3_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.DEST_STATION_CITY AS DEST_STATION_CITY --  dest_station_city identifies the city in which the related shipment ends.&#xd;&#xa;      ,T.DEST_STATION_FSAC AS DEST_STATION_FSAC --  origin_station_fsac identifies the fsac code for the station in which the related shipment ended.&#xd;&#xa;      ,T.DEST_STATION_SPLC AS DEST_STATION_SPLC --  the splc code for the destination of the shipment which relates to this invoice.&#xd;&#xa;      ,T.DEST_STATION_PROVINCE_CODE AS DEST_STATION_PRVN_CODE --  dest_station_province_code identifies the province code in which the related shipment ended.&#xd;&#xa;      ,T.DEST_STATION_POSTAL_CODE AS DEST_STATION_POSTAL_CODE --  dest_station_postal_code identifies the postal code in which the related shipment ended.&#xd;&#xa;      ,T.DEST_COUNTRY_CODE3_GID AS DEST_COUNTRY_CODE3_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.LETTER_OF_CREDIT_EXP_DATE AS LT_OF_CREDIT_EXP_DATE --  letter_of_credit_exp_date contains the date until which the letter of credit is in effect.after this date, the letter of credit expires.&#xd;&#xa;      ,T.LETTER_OF_CREDIT_ISSUE_DATE AS LT_OF_CREDIT_ISSUE_DATE --  letter_of_credit_issue_date contains the date that the lette