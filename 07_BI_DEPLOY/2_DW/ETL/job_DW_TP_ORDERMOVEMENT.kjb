        WHEN T.USER_DEFINED1_ICON_GID &#x3d; &#x27;CMAL&#x2f;ZC.LSPCONFIRMED&#x27; THEN&#xd;&#xa;                  &#x27;&#x5206;&#x914d;&#x5df2;&#x786e;&#x8ba4;&#x27;&#xd;&#xa;                 WHEN T.USER_DEFINED1_ICON_GID &#x3d; &#x27;CMAL&#x2f;ZC.DISPATCHED&#x27; THEN&#xd;&#xa;                  &#x27;&#x5df2;&#x53d1;&#x8fd0;&#x7533;&#x8bf7;&#x27;&#xd;&#xa;                 ELSE&#xd;&#xa;                  T.USER_DEFINED1_ICON_GID&#xd;&#xa;               END AS ORDER_STATUS -- &#x8ba2;&#x5355;&#x72b6;&#x6001;&#xd;&#xa;              ,T.USER_DEFINED2_ICON_GID AS ALLOT_STAUS -- &#x5206;&#x914d;&#x72b6;&#x6001;&#x28;NEW&#x2f;PROCESS&#x2f;SUCCESS&#x2f;FAIL&#x29;&#xd;&#xa;               -- ,NULL AS PROJ_ID -- &#x9879;&#x76ee;&#xd;&#xa;               -- ,T.ATTRIBUTE2 AS PROJ_NAME -- &#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.EARLY_PICKUP_DATE,&#xd;&#xa;                                            &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                             &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; AS EARLY_PICKUP_DATE -- &#x8ba1;&#x5212;&#x65e5;&#xd;&#xa;              ,T.USER_DEFINED4_ICON_GID AS ICON4 -- &#x672a;&#x4f7f;&#x7528;&#xd;&#xa;              ,T.USER_DEFINED5_ICON_GID AS ICON5 -- &#x672a;&#x4f7f;&#x7528;&#xd;&#xa;              ,T.PERSPECTIVE AS PERSPECTIVE -- B&#x3a;&#x8fd0;&#x8f93;&#x3b;S&#x3a;&#x7ed3;&#x7b97;&#xd;&#xa;              ,T.SERVPROV_GID AS SERVPROV_GID -- &#x865a;&#x62df;&#x627f;&#x8fd0;&#x5546;&#xd;&#xa;              ,T.ATTRIBUTE3 AS ORDER_RELEASE_NAME -- FT&#x677f;&#x53f7; order release name&#xd;&#xa;              ,T.ATTRIBUTE5 AS DRIVER_PHONE -- &#x9a7e;&#x9a76;&#x5458;&#x7535;&#x8bdd;&#xd;&#xa;              ,T.ATTRIBUTE7 AS LIC_NO -- &#x8f66;&#x724c;&#x53f7;&#xd;&#xa;              ,T.ATTRIBUTE8 AS ALLOT_NO -- &#x5206;&#x62e8;&#x5355;&#x53f7;&#xd;&#xa;              ,T.ATTRIBUTE10 AS TICKET_UNIT -- &#x542f;&#x7968;&#x5355;&#x4f4d;&#xd;&#xa;              ,T.ATTRIBUTE14 AS PLATE_NO -- &#x5206;&#x677f;&#x5e8f;&#x53f7;&#xd;&#xa;              ,T.ATTRIBUTE16 AS DRIVER_VOYAGE_RAIL -- &#x9a7e;&#x9a76;&#x5458;&#x59d3;&#x540d;&#x2f;&#x8239;&#x540d;&#x822a;&#x6b21;&#x2f;&#x94c1;&#x8def;&#x5927;&#x7ec4;&#xd;&#xa;              ,T.ATTRIBUTE17 AS REST_REASON -- &#x534f;&#x529b;&#x539f;&#x56e0;&#xd;&#xa;              ,T.ATTRIBUTE18 AS LSP -- &#x5b9e;&#x9645;&#x627f;&#x8fd0;&#x5546;&#xd;&#xa;              ,T.ATTRIBUTE19 AS SSP -- &#x4efd;&#x989d;&#x627f;&#x8fd0;&#x5546;&#xd;&#xa;              ,T.ATTRIBUTE20 AS ORDER_COMMENT -- &#x8ba2;&#x5355;&#x5907;&#x6ce8;&#xd;&#xa;              ,T.ATTRIBUTE_NUMBER4 AS ON_PASSAGE_DAYS -- &#x5728;&#x9014;&#x8003;&#x6838;&#x5929;&#x6570;&#xd;&#xa;              ,T.ATTRIBUTE_NUMBER10 AS IS_STORAGE -- &#x5728;&#x9014;&#x542f;&#x7968;&#x8f66;&#x8f86;&#x662f;&#x5426;&#x5df2;&#x5165;&#x5e93;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ATTRIBUTE_DATE1, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;  AS FACTORY_DAY -- &#x51fa;&#x573a;&#x65e5;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ATTRIBUTE_DATE2, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;  AS ARRANGE_DAY -- &#x5b89;&#x6392;&#x65e5;&#x28;&#x6d3e;&#x8f66;&#x786e;&#x8ba4;&#x65e5;&#x29;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ATTRIBUTE_DATE3, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;  AS OUT_STORE_DAY -- &#x51fa;&#x5e93;&#x65e5;&#x2f;&#x4e2d;&#x8f6c;&#x51fa;&#x5e93;&#x65e5;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ATTRIBUTE_DATE4, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;  AS ALLOT_PLN_DAY -- &#x8c03;&#x5ea6;&#x5206;&#x914d;&#x8ba1;&#x5212;&#x65e5;&#x671f;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ATTRIBUTE_DATE5, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;  AS DLVR_DAY -- &#x4e2d;&#x8f6c;&#x5165;&#x5e93;&#x65e5;&#x2f;&#x4ea4;&#x8f66;&#x65e5;&#x2f;&#x5230;&#x8fbe;&#x65e5;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ATTRIBUTE_DATE6, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;  AS ARRIVAL_DAY -- &#x5230;&#x6e2f;&#x65e5;&#x671f;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ATTRIBUTE_DATE7, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;  AS SHIPMENT_DAY -- &#x88c5;&#x8239;&#x65e5;&#x671f;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ATTRIBUTE_DATE8, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;  AS UNLOAD_DAY -- &#x5378;&#x8239;&#x65e5;&#x671f;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ATTRIBUTE_DATE9, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;  AS LOAD_DAY -- &#x88c5;&#x8f66;&#x65e5;&#x671f;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ATTRIBUTE_DATE10, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b; 28800,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;  AS LEAVE_BASE_DAY -- &#x79bb;&#x5f00;&#x57fa;&#x5730;&#x548c;&#x4e2d;&#x8f6c;&#x7ad9;&#x65e5;&#x671f;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN  T.ATTRIBUTE2 NOT IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  B1.MILEGE &#xd;&#xa;                 WHEN T.SEQ_NO &#x3d; &#x27;1&#x27; AND T.ATTRIBUTE2 IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  B3.MILEGE&#xd;&#xa;                 WHEN T.SEQ_NO &#x3d; &#x27;2&#x27; AND T.ATTRIBUTE2 IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  B3.MILEGE_TRANSFER&#xd;&#xa;                 ELSE&#xd;&#xa;                  NULL&#xd;&#xa;               END AS MILEGE&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN T.ATTRIBUTE2 IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  NVL&#x28;B3.AMOUNT_VALUE, 0&#x29;&#xd;&#xa;                 WHEN  T.ATTRIBUTE2 NOT IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  B1.AMOUNT_VALUE &#xd;&#xa;                 ELSE&#xd;&#xa;                  0&#xd;&#xa;               END AS AMOUNT_VALUE&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN T.ATTRIBUTE2 IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  NVL&#x28;B3.AMOUNT_COST, 0&#x29;&#x2b;NVL&#x28;B3.AMOUNT_COST_TRANSFER, 0&#x29;&#xd;&#xa;                 WHEN T.ATTRIBUTE2 NOT IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  B1.AMOUNT_COST &#xd;&#xa;                 ELSE&#xd;&#xa;                  0&#xd;&#xa;               END AS AMOUNT_COST&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN T.SEQ_NO &#x3d; &#x27;1&#x27; AND T.ATTRIBUTE2 IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  NVL&#x28;B3.AMOUNT_VALUE, 0&#x29; - NVL&#x28;B3.AMOUNT_VALUE_TRANSFER, 0&#x29;&#xd;&#xa;                 WHEN T.SEQ_NO &#x3d; &#x27;2&#x27; AND T.ATTRIBUTE2 IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  NVL&#x28;B3.AMOUNT_VALUE_TRANSFER, 0&#x29;&#xd;&#xa;                 WHEN  T.ATTRIBUTE2 NOT IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  B1.AMOUNT_VALUE &#xd;&#xa;                 ELSE&#xd;&#xa;                  0&#xd;&#xa;               END AS AMOUNT_SEQ_VALUE&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN T.SEQ_NO &#x3d; &#x27;1&#x27; AND T.ATTRIBUTE2 IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  NVL&#x28;B3.AMOUNT_COST, 0&#x29; &#xd;&#xa;                 WHEN T.SEQ_NO &#x3d; &#x27;2&#x27; AND T.ATTRIBUTE2 IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  NVL&#x28;B3.AMOUNT_COST_TRANSFER, 0&#x29;&#xd;&#xa;                 WHEN T.ATTRIBUTE2 NOT IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29; THEN&#xd;&#xa;                  B1.AMOUNT_COST &#xd;&#xa;                 ELSE&#xd;&#xa;                  0&#xd;&#xa;               END AS AMOUNT_SEQ_COST&#xd;&#xa;          FROM ODS_ZCOTM.ORDER_MOVEMENT T&#xd;&#xa;          JOIN TEMP.TEMP_DW_TP_ORDERMOVEMENT_PART P&#xd;&#xa;            ON REGEXP_REPLACE&#x28;SUBSTRING&#x28;T.EARLY_PICKUP_DATE, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;  &#x3d; P.PART_DT &#xd;&#xa;          LEFT JOIN ODS_ZCOTM.ORDER_MOVEMENT_REFNUM R1&#xd;&#xa;            ON T.ORDER_MOVEMENT_GID &#x3d; R1.ORDER_MOVEMENT_GID&#xd;&#xa;           AND R1.OM_REFNUM_QUAL_GID &#x3d; &#x27;CMAL&#x2f;ZC.OM_VDRNO&#x27;&#xd;&#xa;          LEFT JOIN ODS_ZCOTM.XX_T_ZC_BUDGET B1&#xd;&#xa;            ON T.ORDER_MOVEMENT_GID &#x3d; B1.ORDER_RELEASE_GID&#xd;&#xa;           AND B1.BRAND_NAME NOT IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29;&#xd;&#xa;           --AND T.SEQ_NO &#x3d; &#x27;1&#x27;&#xd;&#xa;          --LEFT JOIN ODS_ZCOTM.XX_T_ZC_BUDGET B2&#xd;&#xa;            --ON T.ORDER_MOVEMENT_GID &#x3d; B2.ORDER_RELEASE_GID&#xd;&#xa;           --AND B2.BRAND_NAME NOT IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29;&#xd;&#xa;           --AND T.SEQ_NO &#x3d; &#x27;2&#x27; &#xd;&#xa;          LEFT JOIN ODS_ZCOTM.XX_T_ZC_BUDGET B3&#xd;&#xa;            ON T.ORDER_RELEASE_GID &#x3d; B3.ORDER_RELEASE_GID&#xd;&#xa;           AND B3.BRAND_NAME IN &#x28;&#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#x29;&#xd;&#xa;          LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;            ON T.SOURCE_LOCATION_GID &#x3d; O.ORG_ID&#xd;&#xa;          LEFT JOIN DIM.DIM_ORG DO&#xd;&#xa;            ON T.DEST_LOCATION_GID &#x3d; DO.ORG_ID&#xd;&#xa;          LEFT JOIN DIM.DIM_DEALER D&#xd;&#xa;            ON T.DEST_LOCATION_GID &#x3d; D.DLR_ID&#xd;&#xa;         WHERE T.DOMAIN_NAME &#x3d; &#x27;CMAL&#x2f;ZC&#x27;  &#x29; T&#xd;&#xa;  LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;    ON T.BASE_ID &#x3d; P.BASE_ID&#xd;&#xa;   AND T.BRAND_ID &#x3d; P.BRAND_ID  &#x3b;</sql>
