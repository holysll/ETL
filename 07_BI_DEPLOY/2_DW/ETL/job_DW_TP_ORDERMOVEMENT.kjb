   ,T.DLR_ID&#xd;&#xa;      ,T.SUPPLIER_ID&#xd;&#xa;      ,T.VIN&#xd;&#xa;      ,T.VIN_SIMPLE&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,T.PRODUCT_NAME&#xd;&#xa;      ,T.MODEL_ID&#xd;&#xa;      ,T.MDLGRP_ID&#xd;&#xa;      ,T.ALL_TRANSPORTMODE&#xd;&#xa;      ,T.SEGMETN_TRANSPORTMODE&#xd;&#xa;      ,T.TRANSPORTSTATUS&#xd;&#xa;      ,T.ORDER_STATUS&#xd;&#xa;      ,T.ALLOT_STAUS&#xd;&#xa;      ,T.EARLY_PICKUP_DATE&#xd;&#xa;      ,T.ICON4&#xd;&#xa;      ,T.ICON5&#xd;&#xa;      ,T.PERSPECTIVE&#xd;&#xa;      ,T.SERVPROV_GID&#xd;&#xa;      ,T.ORDER_RELEASE_NAME&#xd;&#xa;      ,T.DRIVER_PHONE&#xd;&#xa;      ,T.LIC_NO&#xd;&#xa;      ,T.ALLOT_NO&#xd;&#xa;      ,T.TICKET_UNIT&#xd;&#xa;      ,T.PLATE_NO&#xd;&#xa;      ,T.DRIVER_VOYAGE_RAIL&#xd;&#xa;      ,T.REST_REASON&#xd;&#xa;      ,T.LSP&#xd;&#xa;      ,T.SSP&#xd;&#xa;      ,T.ORDER_COMMENT&#xd;&#xa;      ,T.ON_PASSAGE_DAYS&#xd;&#xa;      ,T.IS_STORAGE&#xd;&#xa;      ,T.FACTORY_DAY&#xd;&#xa;      ,T.ARRANGE_DAY&#xd;&#xa;      ,T.OUT_STORE_DAY&#xd;&#xa;      ,T.ALLOT_PLN_DAY&#xd;&#xa;      ,NVL&#x28;T.DLVR_DAY,T.ARRIVAL_DAY&#x29; AS DLVR_DAY      --&#x5982;&#x679c;&#x6ca1;&#x6709;&#x5230;&#x8fbe;&#x65f6;&#x95f4;&#xff0c;&#x4f46;&#x662f;&#x6709;&#x4ea4;&#x8f66;&#x65f6;&#x95f4;&#xff0c;&#x9ed8;&#x8ba4;&#x4e3a;&#x5df2;&#x5230;&#x8fbe;&#xd;&#xa;      ,CASE WHEN T.BRAND_ID &#x3d; &#x27;FT&#x27; THEN T.DLVR_DAY ELSE T.ARRIVAL_DAY END AS ARRIVAL_DAY   --&#x798f;&#x7279;&#x7684;&#x5230;&#x8fbe;&#x4e0e;&#x4ea4;&#x8f66;&#x4e3a;&#x540c;&#x4e00;&#x4e2a;&#x5b57;&#x6bb5;&#xd;&#xa;      ,T.SHIPMENT_DAY&#xd;&#xa;      ,T.UNLOAD_DAY&#xd;&#xa;      ,T.LOAD_DAY&#xd;&#xa;      ,T.LEAVE_BASE_DAY&#xd;&#xa;      ,T.MILEGE&#xd;&#xa;      ,T.AMOUNT_VALUE&#xd;&#xa;      ,T.AMOUNT_COST&#xd;&#xa;      ,T.AMOUNT_SEQ_VALUE&#xd;&#xa;      ,T.AMOUNT_SEQ_COST&#xd;&#xa;      ,T.SOURCE_CODE --&#x6e90;&#x7cfb;&#x7edf;&#x6807;&#x8bc6;&#xd;&#xa;      ,T.ORDER_MOVEMENT_GID AS INTEGRATION_ID --&#x8bb0;&#x5f55;&#x552f;&#x4e00;&#x6807;&#x8bc6;&#xd;&#xa;      ,T.AUX1_CHANGED_ON_DT --&#x539f;&#x7cfb;&#x7edf;&#x8bb0;&#x5f55;&#x521b;&#x5efa;&#x65f6;&#x95f4;&#xd;&#xa;      ,T.W_INSERT_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,T.W_UPDATE_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x4fee;&#x6539;&#x65f6;&#x95f4;&#xd;&#xa;      ,T.PART_SC --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;      ,T.PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;      ,ROW_NUMBER&#x28;&#x29;OVER&#x28;PARTITION BY T.ORDER_MOVEMENT_GID ORDER BY PERIOD_WID DESC&#x29; AS RN&#xd;&#xa;  FROM TEMP.TEMP_DW_TP_ORDERMOVEMENT T &#x29; AS T&#xd;&#xa;WHERE T.RN &#x3d; 1&#xd;&#xa;  AND PART_DT &#x3e;&#x3d; &#x27;201708&#x27;&#x3b;</sql>
