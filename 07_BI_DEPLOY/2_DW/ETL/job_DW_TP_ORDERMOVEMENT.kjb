    <entry>
      <name>TO_TEMP</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DW&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DW_TP_ORDERMOVEMENT &#xd;&#xa;SELECT CASE&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;FT&#x27; THEN&#xd;&#xa;          SUBSTR&#x28;REGEXP_REPLACE&#x28;T.FACTORY_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;   &#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;CA&#x27; THEN&#xd;&#xa;          SUBSTR&#x28;REGEXP_REPLACE&#x28;T.UNLOAD_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;  &#xd;&#xa;         ELSE&#xd;&#xa;          SUBSTR&#x28;REGEXP_REPLACE&#x28;T.EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#xd;&#xa;       END AS PERIOD_WID&#xd;&#xa;      ,T.ORDER_MOVEMENT_GID&#xd;&#xa;      ,T.ORDER_RELEASE_GID&#xd;&#xa;      ,T.BUDGET_SHIPMENT_GID&#xd;&#xa;      ,T.SEQ_NO&#xd;&#xa;      ,T.YD_NO&#xd;&#xa;      ,T.OC_ID&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,T.BASE_NAME&#xd;&#xa;      ,P.PROJ_ID AS PROJ_ID -- &#x9879;&#x76ee;&#xd;&#xa;      ,P.PROJ_NAME PROJ_NAME -- &#x9879;&#x76ee;&#x540d;&#x79f0;       &#xd;&#xa;      ,T.SOURCE_LOCATION_TYPE&#xd;&#xa;      ,T.SOURCE_LOCATION_ID&#xd;&#xa;      ,T.DEST_LOCATION_TYPE&#xd;&#xa;      ,T.DEST_LOCATION_ID&#xd;&#xa;      ,T.SOURCE_PRVN_ID&#xd;&#xa;      ,T.SOURCE_CTY_ID&#xd;&#xa;      ,T.DEST_PRVN_ID&#xd;&#xa;      ,T.DEST_CTY_ID&#xd;&#xa;      ,T.DLR_ID&#xd;&#xa;      ,T.SUPPLIER_ID&#xd;&#xa;      ,T.VIN&#xd;&#xa;      ,T.VIN_SIMPLE&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,T.PRODUCT_NAME&#xd;&#xa;      ,T.MODEL_ID&#xd;&#xa;      ,T.MDLGRP_ID&#xd;&#xa;      ,T.ALL_T