      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_INV_TRANSACTION_ORG&#xd;&#xa;SELECT T.ATTRIBUTE3&#xd;&#xa;      ,O.ORG_ID&#xd;&#xa;      ,O.ORG_TYPE&#xd;&#xa;      ,O.CO_ID&#xd;&#xa;      ,O.CO_NAME&#xd;&#xa;      ,O.OC_ID&#xd;&#xa;      ,O.OC_NAME&#xd;&#xa;      ,O.RGN_ID&#xd;&#xa;      ,O.RGN_NAME&#xd;&#xa;      ,O.BASE_TYPE&#xd;&#xa;      ,O.BASE_ID&#xd;&#xa;      ,O.BASE_NAME&#xd;&#xa;      ,O.STR_TYPE&#xd;&#xa;      ,O.STR_ID&#xd;&#xa;      ,O.STR_NAME&#xd;&#xa;      ,O.PRIOR_ID&#xd;&#xa;      ,O.PRVN_ID&#xd;&#xa;      ,O.PRVN_NAME&#xd;&#xa;      ,O.CITY_ID&#xd;&#xa;      ,O.CITY_NAME&#xd;&#xa;      ,O.POSTAL_ID&#xd;&#xa;      ,O.POSTAL_NAME&#xd;&#xa;      ,O.LAT&#xd;&#xa;      ,O.LON&#xd;&#xa;  FROM DIM.DIM_ORG O&#xd;&#xa;  JOIN ODS_ZCOTM.LOCATION T&#xd;&#xa;    ON O.ORG_ID &#x3d; T.LOCATION_GID&#xd;&#xa; WHERE T.ATTRIBUTE3 IS NOT NULL &#xd;&#xa;    AND T.ATTRIBUTE3 NOT IN &#x28;&#x27;&#x865a;&#x62df;&#x5e93;&#x623f;&#x27;&#x29;&#xd;&#xa;     AND O.STR_ID NOT IN &#x28;&#x27;CMAL&#x2f;ZC.CQ2VDC2&#x27;&#x29;&#xd;&#xa; GROUP BY T.ATTRIBUTE3&#xd;&#xa;         ,O.ORG_ID&#xd;&#xa;         ,O.ORG_TYPE&#xd;&#xa;         ,O.CO_ID&#xd;&#xa;         ,O.CO_NAME&#xd;&#xa;         ,O.OC_ID&#xd;&#xa;         ,O.OC_NAME&#xd;&#xa;         ,O.RGN_ID&#xd;&#xa;         ,O.RGN_NAME&#xd;&#xa;         ,O.BASE_TYPE&#xd;&#xa;         ,O.BASE_ID&#xd;&#xa;         ,O.BASE_NAME&#xd;&#xa;         ,O.STR_TYPE&#xd;&#xa;         ,O.STR_ID&#xd;&#xa;         ,O.STR_NAME&#xd;&#xa;         ,O.PRIOR_ID&#xd;&#xa;         ,O.PRVN_ID&#xd;&#xa;         ,O.PRVN_NAME&#xd;&#xa;         ,O.CITY_ID&#xd;&#xa;         ,O.CITY_NAME&#xd;&#xa;         ,O.POSTAL_ID&#xd;&#xa;         ,O.POSTAL_NAME&#xd;&#xa;         ,O.LAT&#xd;&#xa;         ,O.LON&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
