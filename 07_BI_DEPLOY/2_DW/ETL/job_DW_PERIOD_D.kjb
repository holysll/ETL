      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DW_SRM_PRO_VENDOR_INFO&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;-- &#x6ee1;&#x610f;&#x5ea6;&#x8ba4;&#x53ef;&#x8868;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DW.DW_PERIOD_D&#xd;&#xa;SELECT &#xd;&#xa;    PERIOD_WID&#xd;&#xa;    ,PERIOD_CODE&#xd;&#xa;    ,PERIOD_DATE&#xd;&#xa;    ,PERIOD_TYPE&#xd;&#xa;    ,DAY_NAME&#xd;&#xa;    ,WEEK_NAME&#xd;&#xa;    ,MONTH_NAME&#xd;&#xa;    ,QUARTER_NAME&#xd;&#xa;    ,HALF_YEAR_NAME&#xd;&#xa;    ,YEAR_NAME&#xd;&#xa;    ,DAY_NBR_IN_WEEK&#xd;&#xa;    ,DAY_NBR_IN_MONTH&#xd;&#xa;    ,DAY_NBR_IN_YEAR&#xd;&#xa;    ,WEEK_NBR_IN_MONTH&#xd;&#xa;    ,WEEK_NBR_IN_YEAR&#xd;&#xa;    ,MONTH_NBR_IN_YEAR&#xd;&#xa;    ,QTR_NBR_IN_YEAR&#xd;&#xa;    ,HALF_NBR_IN_YEAR&#xd;&#xa;    ,FIRST_WEEK_IN_MONTH_FLAG&#xd;&#xa;    ,LAST_WEEK_IN_MONTH_FLAG&#xd;&#xa;    ,FIRST_MONTH_IN_YEAR_FLAG&#xd;&#xa;    ,LAST_MONTH_IN_YEAR_FLAG&#xd;&#xa;    ,PERIOD_STATS&#xd;&#xa;    ,PERIOD_START_DT&#xd;&#xa;    ,PERIOD_START_DT_WID&#xd;&#xa;    ,PERIOD_END_DT&#xd;&#xa;    ,PERIOD_END_DT_WID&#xd;&#xa;    ,CREATED_ON_DT&#xd;&#xa;    ,CHANGED_ON_DT&#xd;&#xa;    ,YEAR&#xd;&#xa;    ,MONTH&#xd;&#xa;    ,DAY&#xd;&#xa;    ,MONTH_START_DT_WID&#xd;&#xa;    ,MONTH_END_DT_WID&#xd;&#xa;    ,AGO_PERIOD_WID&#xd;&#xa;    ,AGO_MONTH_START_DT_WID&#xd;&#xa;    ,AGO_MONTH_END_DT_WID&#xd;&#xa;    ,AGO11_MONTH_START_DT_WID&#xd;&#xa;    ,AGO11_MONTH_END_DT_WID&#xd;&#xa;    ,YEAR_START_DT_WID&#xd;&#xa;    ,YEAR_END_DT_WID&#xd;&#xa;    ,AGO_YEAR_START_DT_WID&#xd;&#xa;    ,AGO_YEAR_END_DT_WID&#xd;&#xa;    ,YEARMONTH&#xd;&#xa;    ,AGO_YEARMONTH&#xd;&#xa;    ,AGO11_YEARMONTH&#xd;&#xa;    ,LAST_MONTH_START_DT_WID&#xd;&#xa;    ,LAST_MONTH_END_DT_WID&#xd;&#xa;    ,PRE_45_DAY_WID&#xd;&#xa;    ,WEEK_NBR_IN_MONTH_IW&#xd;&#xa;    ,WEEK_NBR_IN_YEAR_IW&#xd;&#xa;    ,YEAR_IW&#xd;&#xa;    ,HOLIDAY_NAME&#xd;&#xa;    ,HOLIDAY_FLAG&#xd;&#xa;    ,&#x27;DIM&#x27;               AS SOURCE_CODE          -- &#x6e90;&#x7cfb;&#x7edf;&#xd;&#xa;    ,NULL                 AS INTEGRATION_ID       -- &#x539f;&#x7cfb;&#x7edf;&#x552f;&#x4e00;&#x6807;&#x8bc6;&#xd;&#xa;    ,NULL                 AS AUX_CHANGED_ON_DT    -- &#x539f;&#x7cfb;&#x7edf;&#x5173;&#x8054;&#x8868;&#x4fee;&#x6539;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;YYYY-MM-dd&#x27;&#x29;             AS W_INSERT_DT             -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;YYYY-MM-dd&#x27;&#x29;             AS W_UPDATE_DT             -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    --,CONCAT&#x28;A.YEAR,&#x27;-&#x27;,LPAD&#x28;A.MONTH, 2, &#x27;0&#x27;&#x29;&#x29;       AS PART_DT                 -- &#x5206;&#x533a;&#x5b57;&#x6bb5;&#x2f;&#x4e1a;&#x52a1;&#x53d1;&#x751f;&#x6708;&#x4efd;&#xd;&#xa;&#xd;&#xa;FROM &#xd;&#xa;    DIM.DIM_PERIOD_D AS A&#x3b;</sql>
