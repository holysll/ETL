      <type>SQL</type>
      <sql>&#xd;&#xa;--Settings&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DW_2&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Udf&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;INSERT OVERWRITE TABLE DW.DW_TP_SHIPMENT PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;T.START_TIME, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID --  &#x65e5;&#x671f;&#x7ef4;ID&#x28;&#x5230;&#x8fbe;&#x65e5;&#x29;&#xd;&#xa;      ,T.SHIPMENT_GID AS SHIPMENT_GID --  &#x8fd0;&#x5355;GID&#xd;&#xa;      ,B.BILL_GID AS INVOICE_GID --  &#x53d1;&#x7968;GID&#xd;&#xa;      ,T.SHIPMENT_XID AS SHIPMENT_XID --  &#x8fd0;&#x5355;XID&#xd;&#xa;      ,T.SHIPMENT_NAME AS SHIPMENT_NAME --  &#x8fd0;&#x8f93;&#x7ebf;&#x8def;&#xd;&#xa;      ,O.OC_ID AS OC_ID --  &#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#xd;&#xa;      ,O.RGN_ID AS RGN_ID --  &#x5927;&#x533a;&#xd;&#xa;      ,O.BASE_ID AS BASE_ID --  &#x57fa;&#x5730;&#xd;&#xa;      ,O.BASE_NAME AS BASE_NAME --  &#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;      ,NULL AS PROJ_ID --  &#x9879;&#x76ee;ID&#xff0c;&#x54c1;&#x724c;&#x76ee;&#x524d;&#x6ca1;&#x6709;&#x4ed4;&#x7ec6;&#x6e05;&#x6d17;&#xff0c;&#x6807;&#x7b7e;&#x4e0d;&#x660e;&#x663e;&#xd;&#xa;      ,NULL AS PROJ_NAME --  &#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;      ,O.BASE_TYPE AS SOURCE_LOCATION_TYPE --  &#x59cb;&#x53d1;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;      ,T.SOURCE_LOCATION_GID AS SOURCE_LOCATION_ID --  &#x59cb;&#x53d1;&#x5730;ID&#xd;&#xa;      ,COALESCE&#x28;R.BASE_TYPE,&#xd;&#xa;                CASE&#xd;&#xa;                  WHEN D.DLR_ID IS NOT NULL THEN&#xd;&#xa;                   &#x27;DLR&#x27;&#xd;&#xa;                  ELSE&#xd;&#xa;                   NULL&#xd;&#xa;                END&#x29; AS DEST_LOCATION_TYPE --  &#x76ee;&#x7684;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;      ,T.DEST_LOCATION_GID AS DEST_LOCATION_ID --  &#x76ee;&#x7684;&#x5730;ID&#xd;&#xa;      ,T.TRANSPORT_MODE_GID AS TRANSPORT_MODE_ID --  &#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;      ,T.SERVPROV_GID AS SERVPROV_GID --  &#x627f;&#x8fd0;&#x5546;&#x4ee3;&#x7801;,&#x627f;&#x8fd0;&#x5546;&#x53d8;&#x66f4;&#x65f6;&#xff0c;&#x63d2;&#x5165;&#x8fd0;&#x8f93;&#x4e8b;&#x4ef6;&#xd;&#xa;      ,T.IS_TEMPLATE AS IS_TEMPLATE&#xd;&#xa;      ,T.IS_PRIMARY AS IS_PRIMARY&#xd;&#xa;      ,T.IS_SPOT_COSTED AS IS_SPOT_COSTED&#xd;&#xa;      ,T.IS_CREDIT_NOTE AS IS_CREDIT_NOTE&#xd;&#xa;      ,T.CM_PREV_DEST_LOCATION_GID AS CM_PREV_DEST_LOCATION_GID --  destination location_gid of the previous shipment in the cm tour.&#xd;&#xa;      ,T.CM_NEXT_SOURCE_LOCATION_GID AS CM_NEXT_SOURCE_LOCATION_GID --  source location_gid of the next shipment in the cm tour&#xd;&#xa;      ,T.TOTAL_ACTUAL_COST AS TOTAL_ACTUAL_COST --  this field stores the total actual cost of the shipment.&#xd;&#xa;      ,T.TOTAL_ACTUAL_COST_BASE AS TOTAL_ACTUAL_COST_BASE --  total_actual_cost in base units.&#xd;&#xa;      ,T.TOTAL_WEIGHTED_COST AS TOTAL_WEIGHTED_COST --  &#x3f;&#xd;&#xa;      ,T.TOTAL_WEIGHTED_COST_BASE AS TOTAL_WEIGHTED_COST_BASE --  total_weighted_cost in base units.&#xd;&#xa;      ,T.T_ACTUAL_COST_CURRENCY_GID AS T_ACTUAL_COST_CRY_GID --  a unique identifier to the currency that the actual cost is specified in.&#xd;&#xa;      ,T.T_WEIGHTED_COST_CURRENCY_GID AS T_WEIGHTED_COST_CRY_GID --  a unique identifier to the currency that the weighted actual cost is specified in.&#xd;&#xa;      ,T.TOTAL_TRANS_COST AS TOTAL_TRANS_COST --  this column will be the total cost of the shipment.it represents the total transportation cost after applying the rebate.&#xd;&#xa;      ,T.TOTAL_TRANS_COST_CURRENCY_GID AS TOTAL_TRANS_COST_CRY_GID --  the currency type for the total_trans_cost column.&#xd;&#xa;      ,T.TOTAL_TRANS_COST_BASE AS TOTAL_TRANS_COST_BASE --  the total_trans_cost &#x28;total cost of the shipment&#x29;, in base units.&#xd;&#xa;      ,T.LOADED_DISTANCE AS LOADED_DISTANCE --  the total distance that the truck or the transportation mode will travel when teh shipment is loaded.&#xd;&#xa;      ,T.LOADED_DISTANCE_UOM_CODE AS LOADED_DISTANCE_UOM_CODE --  unit of measure for loaded_distance column.&#xd;&#xa;      ,T.LOADED_DISTANCE_BASE AS LOADED_DISTANCE_BASE --  loaded_distance in base units.&#xd;&#xa;      ,T.UNLOADED_DISTANCE AS UNLOADED_DISTANCE --  the total distance that the truck or the transportation mode will travel when empty.&#xd;&#xa;      ,T.UNLOADED_DISTANCE_UOM_CODE AS UNLOADED_DISTANCE_UOM_CODE --  unit of measure for unloaded_distance column.&#xd;&#xa;      ,T.UNLOADED_DISTANCE_BASE AS UNLOADED_DISTANCE_BASE --  unloaded_distance in base units.&#xd;&#xa;      ,T.START_TIME AS START_TIME --  the actual shipment start time.initially it is the system expected start time.it is updated once the carrier actually starts the shipment.&#xd;&#xa;      ,T.END_TIME AS END_TIME --  this is the time the shipment is delievered.initially this is the system expected delivery time.it is updated with the actual delivery time when the shipment is delivered.&#xd;&#xa;      ,T.FREIGHT_TERMS AS FREIGHT_TERMS&#xd;&#xa;      ,T.PARENT_LEG_GID AS PARENT_LEG_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.VOYAGE_GID AS VOYAGE_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.RATE_OFFERING_GID AS RATE_OFFERING_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.RATE_GEO_GID AS RATE_GEO_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.