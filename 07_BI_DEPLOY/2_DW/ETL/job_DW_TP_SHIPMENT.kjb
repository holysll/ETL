      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_PART&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x5f97;&#x5230;&#x672c;&#x6b21;&#x7ffb;&#x724c;&#x65f6;&#x95f4;&#x5e94;&#x5904;&#x7406;&#x7684;&#x5206;&#x533a;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DW_TP_SHIPMENT_PART&#xd;&#xa;&#xd;&#xa;SELECT  A.PART_DT&#xd;&#xa;       ,ROW_NUMBER&#x28;&#x29;OVER&#x28;ORDER BY A.PART_DT&#x29; AS RN&#xd;&#xa;  FROM &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;T.INSERT_DATE, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;          FROM ODS_ZCOTM.SHIPMENT T&#xd;&#xa;         WHERE 1 &#x3d; 1&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTR&#x28;T.INSERT_DATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3e;&#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTR&#x28;T.INSERT_DATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3c;&#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;END_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;&#xd;&#xa;        --&#x521b;&#x5efa;&#x6708;&#x4efd;&#x4f5c;&#x4e3a;&#x5206;&#x533a;&#xd;&#xa;        UNION ALL&#xd;&#xa;        --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#x53d1;&#x751f;&#x5728;&#x589e;&#x91cf;&#x8303;&#x56f4;&#x5185;&#xff0c;&#x53d6;&#x51fa;&#x88ab;&#x66f4;&#x65b0;&#x7684;&#x521b;&#x5efa;&#x6708;&#x4efd;&#xd;&#xa;        SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;T.INSERT_DATE, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;          FROM ODS_ZCOTM.SHIPMENT T&#xd;&#xa;         WHERE 1 &#x3d; 1&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTR&#x28;T.UPDATE_DATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3e;&#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTR&#x28;T.UPDATE_DATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3c;&#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;END_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;&#x29; AS A&#xd;&#xa;WHERE A.PART_DT &#x3e;&#x3d; &#x27;201708&#x27;&#xd;&#xa; GROUP BY A.PART_DT  &#x3b;</sql>
