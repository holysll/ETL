SSED AS DROP_INVENTORY_PROCESSED --  indicates fleet equipment inventory processing for dropping an empty equipment after freight.&#xd;&#xa;      ,T.DIM_VOLUME AS DIM_VOLUME --  dimensional volume - mathematical weight equivalence according to specified dim_rate_factor&#xd;&#xa;      ,T.DIM_VOLUME_UOM_CODE AS DIM_VOLUME_UOM_CODE&#xd;&#xa;      ,T.DIM_VOLUME_BASE AS DIM_VOLUME_BASE&#xd;&#xa;      ,T.CHARGEABLE_VOLUME AS CHARGEABLE_VOLUME --  actual volume used in calculating the cost of the shipment&#xd;&#xa;      ,T.CHARGEABLE_VOLUME_UOM_CODE AS CHARGEABLE_VOLUME_UOM_CODE&#xd;&#xa;      ,T.CHARGEABLE_VOLUME_BASE AS CHARGEABLE_VOLUME_BASE&#xd;&#xa;      ,T.PLANNING_PARAMETER_SET_GID AS PLANNING_PARAMETER_SET_GID --  this column holds the planning parameter set gid which is used in building this shipment&#xd;&#xa;      ,T.NUM_EQUIPMENT_ORDERED AS NUM_EQUIPMENT_ORDERED --  number of equipment ordered&#x3b; this is manually set by the user and can be rated on&#xd;&#xa;      ,T.EM_PHONE_NUMBER AS EM_PHONE_NUMBER --  emergency phone number when shipping hazardous materials.the 24 hours contact for the shipment.&#xd;&#xa;      ,T.PARCEL_ZONE AS PARCEL_ZONE --  ship to mail zone, price key used for parcel rate calculations&#xd;&#xa;      ,T.ATTRIBUTE1 AS ATTRIBUTE1 --  &#x57fa;&#x5730;&#x4ee3;&#x7801;&#xd;&#xa;      ,T.ATTRIBUTE2 AS ATTRIBUTE2 --  &#x9879;&#x76ee;&#x4ee3;&#x7801;,&#x6df7;&#x88c5;&#x65f6;&#x65e0;&#x6cd5;&#x552f;&#x4e00;&#xd;&#xa;      ,T.ATTRIBUTE3 AS ATTRIBUTE3 --  &#x9500;&#x552e;&#x7ec4;&#x7ec7;&#x4ee3;&#x7801;,&#x6df7;&#x88c5;&#x65f6;&#x65e0;&#x6cd5;&#x552f;&#x4e00;&#xd;&#xa;      ,T.ATTRIBUTE4 AS ATTRIBUTE4&#xd;&#xa;      ,T.ATTRIBUTE5 AS ATTRIBUTE5 --  &#x9a7e;&#x9a76;&#x5458;&#x624b;&#x673a;&#x53f7;&#xd;&#xa;      ,T.ATTRIBUTE6 AS ATTRIBUTE6 --  &#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#xd;&#xa;      ,T.ATTRIBUTE7 AS ATTRIBUTE7&#xd;&#xa;      ,T.ATTRIBUTE8 AS ATTRIBUTE8&#xd;&#xa;      ,T.ATTRIBUTE9 AS ATTRIBUTE9&#xd;&#xa;      ,T.ATTRIBUTE10 AS ATTRIBUTE10&#xd;&#xa;      ,T.ATTRIBUTE11 AS ATTRIBUTE11&#xd;&#xa;      ,T.ATTRIBUTE12 AS ATTRIBUTE12&#xd;&#xa;      ,T.ATTRIBUTE13 AS ATTRIBUTE13&#xd;&#xa;      ,T.ATTRIBUTE14 AS ATTRIBUTE14&#xd;&#xa;      ,T.ATTRIBUTE15 AS ATTRIBUTE15&#xd;&#xa;      ,T.ATTRIBUTE16 AS ATTRIBUTE16 --  &#x9a7e;&#x9a76;&#x5458;&#x59d3;&#x540d;&#xd;&#xa;      ,T.ATTRIBUTE17 AS ATTRIBUTE17&#xd;&#xa;      ,T.ATTRIBUTE18 AS ATTRIBUTE18 --  &#x5b9e;&#x9645;&#x627f;&#x8fd0;&#x5546;&#xd;&#xa;      ,T.ATTRIBUTE19 AS ATTRIBUTE19&#xd;&#xa;      ,T.ATTRIBUTE20 AS ATTRIBUTE20&#xd;&#xa;      ,T.ATTRIBUTE_NUMBER1 AS ATTRIBUTE_NUMBER1 --  &#x4ea4;&#x63a5;&#x5355;&#x6570;&#xd;&#xa;      ,T.ATTRIBUTE_NUMBER2 AS ATTRIBUTE_NUMBER2&#xd;&#xa;      ,T.ATTRIBUTE_NUMBER3 AS ATTRIBUTE_NUMBER3&#xd;&#xa;      ,T.ATTRIBUTE_NUMBER4 AS ATTRIBUTE_NUMBER4&#xd;&#xa;      ,T.ATTRIBUTE_NUMBER5 AS ATTRIBUTE_NUMBER5&#xd;&#xa;      ,T.ATTRIBUTE_NUMBER6 AS ATTRIBUTE_NUMBER6&#xd;&#xa;      ,T.ATTRIBUTE_NUMBER7 AS ATTRIBUTE_NUMBER7&#xd;&#xa;      ,T.ATTRIBUTE_NUMBER8 AS ATTRIBUTE_NUMBER8&#xd;&#xa;      ,T.ATTRIBUTE_NUMBER9 AS ATTRIBUTE_NUMBER9&#xd;&#xa;      ,T.ATTRIBUTE_NUMBER10 AS ATTRIBUTE_NUMBER10&#xd;&#xa;      ,T.ATTRIBUTE_DATE1 AS ATTRIBUTE_DATE1&#xd;&#xa;      ,T.ATTRIBUTE_DATE2 AS ATTRIBUTE_DATE2&#xd;&#xa;      ,T.ATTRIBUTE_DATE3 AS ATTRIBUTE_DATE3&#xd;&#xa;      ,T.ATTRIBUTE_DATE4 AS ATTRIBUTE_DATE4&#xd;&#xa;      ,T.ATTRIBUTE_DATE5 AS ATTRIBUTE_DATE5&#xd;&#xa;      ,T.ATTRIBUTE_DATE6 AS ATTRIBUTE_DATE6&#xd;&#xa;      ,T.ATTRIBUTE_DATE7 AS ATTRIBUTE_DATE7&#xd;&#xa;      ,T.ATTRIBUTE_DATE8 AS ATTRIBUTE_DATE8&#xd;&#xa;      ,T.ATTRIBUTE_DATE9 AS ATTRIBUTE_DATE9&#xd;&#xa;      ,T.ATTRIBUTE_DATE10 AS ATTRIBUTE_DATE10&#xd;&#xa;      ,T.ATTRIBUTE_CURRENCY1 AS ATTRIBUTE_CRY1&#xd;&#xa;      ,T.ATTRIBUTE_CURRENCY1_GID AS ATTRIBUTE_CRY1_GID&#xd;&#xa;      ,T.ATTRIBUTE_CURRENCY1_BASE AS ATTRIBUTE_CRY1_BASE&#xd;&#xa;      ,T.ATTRIBUTE_CURRENCY2 AS ATTRIBUTE_CRY2&#xd;&#xa;      ,T.ATTRIBUTE_CURRENCY2_GID AS ATTRIBUTE_CRY2_GID&#xd;&#xa;      ,T.ATTRIBUTE_CURRENCY2_BASE AS ATTRIBUTE_CRY2_BASE&#xd;&#xa;      ,T.ATTRIBUTE_CURRENCY3 AS ATTRIBUTE_CRY3&#xd;&#xa;      ,T.ATTRIBUTE_CURRENCY3_GID AS ATTRIBUTE_CRY3_GID&#xd;&#xa;      ,T.ATTRIBUTE_CURRENCY3_BASE AS ATTRIBUTE_CRY3_BASE&#xd;&#xa;      ,T.COMBINATION_EQUIP_GRP_GID AS COMBINATION_EQUIP_GRP_GID&#xd;&#xa;      ,T.SI_CUTOFF_DATE AS SI_CUTOFF_DATE&#xd;&#xa;      ,T.CFS_CUTOFF_DATE AS CFS_CUTOFF_DATE&#xd;&#xa;      ,T.SRC_ARB_LEVEL_OF_SERVICE_GID AS SRC_ARB_LEVEL_OF_SERV_GID&#xd;&#xa;      ,T.DEST_ARB_LEVEL_OF_SERVICE_GID AS DEST_ARB_LEVEL_OF_SERV_GID&#xd;&#xa;      ,T.CFS_LOCATION_GID AS CFS_LOCATION_GID&#xd;&#xa;      ,T.DOMAIN_NAME AS DOMAIN_NAME&#xd;&#xa;      ,T.INSERT_USER AS INSERT_USER&#xd;&#xa;      ,T.INSERT_DATE AS INSERT_DATE&#xd;&#xa;      ,T.UPDATE_USER AS UPDATE_USER&#xd;&#xa;      ,T.UPDATE_DATE AS UPDATE_DATE&#xd;&#xa;      ,LO.INSERT_DATE AS RECEIPT_DATE&#xd;&#xa;      ,&#x27;OTM&#x27; AS SOURCE_CODE --&#x6e90;&#x7cfb;&#x7edf;&#x6807;&#x8bc6;&#xd;&#xa;      ,T.SHIPMENT_GID AS INTEGRATION_ID --&#x8bb0;&#x5f55;&#x552f;&#x4e00;&#x6807;&#x8bc6;&#xd;&#xa;      ,NULL AS AUX1_CHANGED_ON_DT --&#x539f;&#x7cfb;&#x7edf;&#x8bb0;&#x5f55;&#x521b;&#x5efa;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_INSERT_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_UPDATE_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x4fee;&#x6539;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;OTM&#x27; AS PART_SC --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;      ,PART.PART_DT AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM ODS_ZCOTM.SHIPMENT T&#xd;&#xa;  JOIN TEMP.TEMP_DW_TP_SHIPMENT_PART PART&#xd;&#xa;    ON REGEXP_REPLACE&#x28;SUBSTRING&#x28;T.INSERT_DATE, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;  &#x3d; PART.PART_DT &#xd;&#xa;  LEFT JOIN ODS_ZCOTM.SHIPMENT_BILL B&#xd;&#xa;    ON T.SHIPMENT_GID &#x3d; B.SHIPMENT_GID&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.SOURCE_LOCATION_GID &#x3d; O.ORG_ID&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG R&#xd;&#xa;    ON T.DEST_LOCATION_GID &#x3d; R.ORG_ID&#xd;&#xa;  LEFT JOIN DIM.DIM_DEALER D&#xd;&#xa;    ON T.DEST_LOCATION_GID &#x3d; D.DLR_ID&#xd;&#xa;  LEFT JOIN TEMP.TEMP_DW_TP_SHIPMENT_LOG  AS LO&#xd;&#xa;    ON T.SHIPMENT_GID &#x3d; LO.SHIPMENT_GID&#xd;&#xa; WHERE T.DOMAIN_NAME &#x3d; &#x27;CMAL&#x2f;ZC&#x27; &#xd;&#xa;   AND PART.RN BETWEEN 1 AND 4&#x3b;&#xd;&#xa;</sql>
