      <type>SQL</type>
      <sql>&#xd;&#xa;--Settings&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DW_3&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Udf&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;INSERT OVERWRITE TABLE DW.DW_TP_SHIPMENT PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;T.START_TIME, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID --  &#x65e5;&#x671f;&#x7ef4;ID&#x28;&#x5230;&#x8fbe;&#x65e5;&#x29;&#xd;&#xa;      ,T.SHIPMENT_GID AS SHIPMENT_GID --  &#x8fd0;&#x5355;GID&#xd;&#xa;      ,B.BILL_GID AS INVOICE_GID --  &#x53d1;&#x7968;GID&#xd;&#xa;      ,T.SHIPMENT_XID AS SHIPMENT_XID --  &#x8fd0;&#x5355;XID&#xd;&#xa;      ,T.SHIPMENT_NAME AS SHIPMENT_NAME --  &#x8fd0;&#x8f93;&#x7ebf;&#x8def;&#xd;&#xa;      ,O.OC_ID AS OC_ID --  &#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#xd;&#xa;      ,O.RGN_ID AS RGN_ID --  &#x5927;&#x533a;&#xd;&#xa;      ,O.BASE_ID AS BASE_ID --  &#x57fa;&#x5730;&#xd;&#xa;      ,O.BASE_NAME AS BASE_NAME --  &#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;      ,NULL AS PROJ_ID --  &#x9879;&#x76ee;ID&#xff0c;&#x54c1;&#x724c;&#x76ee;&#x524d;&#x6ca1;&#x6709;&#x4ed4;&#x7ec6;&#x6e05;&#x6d17;&#xff0c;&#x6807;&#x7b7e;&#x4e0d;&#x660e;&#x663e;&#xd;&#xa;      ,NULL AS PROJ_NAME --  &#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;      ,O.BASE_TYPE AS SOURCE_LOCATION_TYPE --  &#x59cb;&#x53d1;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;      ,T.SOURCE_LOCATION_GID AS SOURCE_LOCATION_ID --  &#x59cb;&#x53d1;&#x5730;ID&#xd;&#xa;      ,COALESCE&#x28;R.BASE_TYPE,&#xd;&#xa;                CASE&#xd;&#xa;                  WHEN D.DLR_ID IS NOT NULL THEN&#xd;&#xa;                   &#x27;DLR&#x27;&#xd;&#xa;                  ELSE&#xd;&#xa;                   NULL&#xd;&#xa;                END&#x29; AS DEST_LOCATION_TYPE --  &#x76ee;&#x7684;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;      ,T.DEST_LOCATION_GID AS DEST_LOCATION_ID --  &#x76ee;&#x7684;&#x5730;ID&#xd;&#xa;      ,T.TRANSPORT_MODE_GID AS TRANSPORT_MODE_ID --  &#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;      ,T.SERVPROV_GID AS SERVPROV_GID --  &#x627f;&#x8fd0;&#x5546;&#x4ee3;&#x7801;,&#x627f;&#x8fd0;&#x5546;&#x53d8;&#x66f4;&#x65f6;&#xff0c;&#x63d2;&#x5165;&#x8fd0;&#x8f93;&#x4e8b;&#x4ef6;&#xd;&#xa;      ,T.IS_TEMPLATE AS IS_TEMPLATE&#xd;&#xa;      ,T.IS_PRIMARY AS IS_PRIMARY&#xd;&#xa;      ,T.IS_SPOT_COSTED AS IS_SPOT_COSTED&#xd;&#xa;      ,T.IS_CREDIT_NOTE AS IS_CREDIT_NOTE&#xd;&#xa;      ,T.CM_PREV_DEST_LOCATION_GID AS CM_PREV_DEST_LOCATION_GID --  destination location_gid of the previous shipment in the cm tour.&#xd;&#xa;      ,T.CM_NEXT_SOURCE_LOCATION_GID AS CM_NEXT_SOURCE_LOCATION_GID --  source location_gid of the next shipment in the cm tour&#xd;&#xa;      ,T.TOTAL_ACTUAL_COST AS TOTAL_ACTUAL_COST --  this field stores the total actual cost of the shipment.&#xd;&#xa;      ,T.TOTAL_ACTUAL_COST_BASE AS TOTAL_ACTUAL_COST_BASE --  total_actual_cost in base units.&#xd;&#xa;      ,T.TOTAL_WEIGHTED_COST AS TOTAL_WEIGHTED_COST --  &#x3f;&#xd;&#xa;      ,T.TOTAL_WEIGHTED_COST_BASE AS TOTAL_WEIGHTED_COST_BASE --  total_weighted_cost in base units.&#xd;&#xa;      ,T.T_ACTUAL_COST_CURRENCY_GID AS T_ACTUAL_COST_CRY_GID --  a unique identifier to the currency that the actual cost is specified in.&#xd;&#xa;      ,T.T_WEIGHTED_COST_CURRENCY_GID AS T_WEIGHTED_COST_CRY_GID --  a unique identifier to the currency that the weighted actual cost is specified in.&#xd;&#xa;      ,T.TOTAL_TRANS_COST AS TOTAL_TRANS_COST --  this column will be the total cost of the shipment.it represents the total transportation cost after applying the rebate.&#xd;&#xa;      ,T.TOTAL_TRANS_COST_CURRENCY_GID AS TOTAL_TRANS_COST_CRY_GID --  the currency type for the total_trans_cost column.&#xd;&#xa;      ,T.TOTAL_TRANS_COST_BASE AS TOTAL_TRANS_COST_BASE --  the total_trans_cost &#x28;total cost of the shipment&#x29;, in base units.&#xd;&#xa;      ,T.LOADED_DISTANCE AS LOADED_DISTANCE --  the total distance that the truck or the transportation mode will travel when teh shipment is loaded.&#xd;&#xa;      ,T.LOADED_DISTANCE_UOM_CODE AS LOADED_DISTANCE_UOM_CODE --  unit of measure for loaded_distance column.&#xd;&#xa;      ,T.LOADED_DISTANCE_BASE AS LOADED_DISTANCE_BASE --  loaded_distance in base units.&#xd;&#xa;      ,T.UNLOADED_DISTANCE AS UNLOADED_DISTANCE --  the total distance that the truck or the transportation mode will travel when empty.&#xd;&#xa;      ,T.UNLOADED_DISTANCE_UOM_CODE AS UNLOADED_DISTANCE_UOM_CODE --  unit of measure for unloaded_distance column.&#xd;&#xa;      ,T.UNLOADED_DISTANCE_BASE AS UNLOADED_DISTANCE_BASE --  unloaded_distance in base units.&#xd;&#xa;      ,T.START_TIME AS START_TIME --  the actual shipment start time.initially it is the system expected start time.it is updated once the carrier actually starts the shipment.&#xd;&#xa;      ,T.END_TIME AS END_TIME --  this is the time the shipment is delievered.initially this is the system expected delivery time.it is updated with the actual delivery time when the shipment is delivered.&#xd;&#xa;      ,T.FREIGHT_TERMS AS FREIGHT_TERMS&#xd;&#xa;      ,T.PARENT_LEG_GID AS PARENT_LEG_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.VOYAGE_GID AS VOYAGE_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.RATE_OFFERING_GID AS RATE_OFFERING_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.RATE_GEO_GID AS RATE_GEO_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.ORIGINAL_SHIPMENT_GID AS ORIGINAL_SHIPMENT_GID --  if the shipment represents a secondary charge, the original_shipment_gid points back to the &#x22;parent&#x22; shipment for which the secondary shipment is a &#x22;child&#x22;.&#xd;&#xa;      ,T.SHIPMENT_GROUP_GID AS SHIPMENT_GROUP_GID&#xd;&#xa;      ,T.IS_AUTO_MERGE_CONSOLIDATE AS IS_AUTO_MERGE_CONSOLIDATE&#xd;&#xa;      ,T.PERSPECTIVE AS PERSPECTIVE&#xd;&#xa;      ,T.FLIGHT_INSTANCE_ID AS FLIGHT_INSTANCE_ID&#xd;&#xa;      ,T.TOTAL_WEIGHT AS TOTAL_WEIGHT&#xd;&#xa;      ,T.TOTAL_WEIGHT_UOM_CODE AS TOTAL_WEIGHT_UOM_CODE&#xd;&#xa;      ,T.TOTAL_WEIGHT_BASE AS TOTAL_WEIGHT_BASE --  total_weight in base units.&#xd;&#xa;      ,T.TOTAL_VOLUME AS TOTAL_VOLUME&#xd;&#xa;      ,T.TOTAL_VOLUME_UOM_CODE AS TOTAL_VOLUME_UOM_CODE&#xd;&#xa;      ,T.TOTAL_VOLUME_BASE AS TOTAL_VOLUME_BASE --  total_volume in base units.&#xd;&#xa;      ,T.TOTAL_SHIP_UNIT_COUNT AS TOTAL_SHIP_UNIT_COUNT&#xd;&#xa;      ,T.TOTAL_PACKAGING_UNIT_COUNT AS TOTAL_PACKAGING_UNIT_COUNT&#xd;&#xa;      ,T.TOTAL_ITEM_PACKAGE_COUNT AS TOTAL_ITEM_PACKAGE_COUNT --  summary of item_package_count of all line items.&#xd;&#xa;      ,T.INTERMEDIARY_CORPORATION_GID AS INTERMEDIARY_CRRT_GID --  a unique identier that represents the corporation that the location represents.the gid contains the domain portion.&#xd;&#xa;      ,T.SHIPMENT_TYPE_GID AS SHIPMENT_TYPE_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.ROUTE_CODE_COMBINATION_GID AS ROUTE_CODE_COMBINATION_GID&#xd;&#xa;      ,T.RAIL_INTER_MODAL_PLAN_GID AS RAIL_INTER_MODAL_PLAN_GID&#xd;&#xa;      ,T.CUSTOMER_RATE_CODE AS CUSTOMER_RATE_CODE&#xd;&#xa;      ,T.COFC_TOFC AS COFC_TOFC&#xd;&#xa;      ,T.RAIL_RETURN_LOCATION_GID AS RAIL_RETURN_LOCATION_GID&#xd;&#xa;      ,T.RAIL_RETURN_ROUTE_GID AS RAIL_RETURN_ROUTE_GID&#xd;&#xa;      ,T.RULE_11_INDICATOR AS RULE_11_INDICATOR&#xd;&#xa;      ,T.CURRENT_STOP_NUM AS CURRENT_STOP_NUM --  this field defines the stop number within the shipment.stop num &#x3d; 1 is the pickup stop, and the max &#x28;stop num&#x29; for a shipment is the delivery stop.&#xd;&#xa;      ,T.SHIPMENT_AS_WORK AS SHIPMENT_AS_WORK --  flag to indicate the shipment as work&#xd;&#xa;      ,T.SECONDARY_CHARGE_RULE_GID AS SECONDARY_CHARGE_RULE_GID&#xd;&#xa;      ,T.CHECK_TIME_CONSTRAINT AS CHECK_TIME_CONSTRAINT&#xd;&#xa;      ,T.CHECK_COST_CONSTRAINT AS CHECK_COST_CONSTRAINT&#xd;&#xa;      ,T.CHECK_CAPACITY_CONSTRAINT AS CHECK_CAPACITY_CONSTRAINT&#xd;&#xa;      ,T.CM_NAME AS CM_NAME --  shipment_gid of the first shipment in the tour.if the cm tour is received as input to gc3, this is the supplied tour id&#xd;&#xa;      ,T.CM_SEQUENCE_NUM AS CM_SEQUENCE_NUM --  indicates the shipments sequence in the cm tour&#xd;&#xa;      ,T.CM_COST AS CM_COST --  the shipment cost when the shipment is part of a cm tour.&#xd;&#xa;      ,T.CM_COST_CURRENCY_GID AS CM_COST_CRY_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.CM_COST_BASE AS CM_COST_BASE --  cm_cost in base units.&#xd;&#xa;      ,T.CM_EMPTY_DISTANCE AS CM_EMPTY_DISTANCE --  the empty leg distance between previous and this shipments&#xd;&#xa;      ,T.CM_EMPTY_DISTANCE_UOM_CODE AS CM_EMPTY_DISTANCE_UOM_CODE --  the unit of cm_empty_distance&#xd;&#xa;      ,T.CM_EMPTY_DISTANCE_BASE AS CM_EMPTY_DISTANCE_BASE --  cm_empty_distance in base units.&#xd;&#xa;      ,T.HAZMAT_MODE_GID AS HAZMAT_MODE_GID --  a unique identifier to define the transportation modes.&#x28;includes the domain name as part of the key&#x29;&#xd;&#xa;      ,T.HAZMAT_REGION_GID AS HAZMAT_REGION_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.WEIGH_CODE AS WEIGH_CODE --  the type of weight.&#xd;&#xa;      ,T.RULE_7 AS RULE_7 --  y,n notification from shipper to carrier that shipper is not liable for freight charges.&#xd;&#xa;      ,T.SHIPMENT_RELEASED AS SHIPMENT_RELEASED --  y&#x2f;n, used in rail shipping to indicate that the shipment is ready.&#xd;&#xa;      ,T.PROCESSING_CODE_GID AS PROCESSING_CODE_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.AIR_RAIL_ROUTE_CODE AS AIR_RAIL_ROUTE_CODE&#xd;&#xa;      ,T.PAYMENT_METHOD_CODE_GID AS PAYMENT_METHOD_CODE_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.INCO_TERM_GID AS INCO_TERM_GID --  gc3 global identifier for the entity.is created by concatenating domain name,&#x22;.&#x22; and xid.&#xd;&#xa;      ,T.DIM_WEIGHT AS DIM_WEIGHT --  weight used to calculate cost when dim rating&#xd;&#xa;      ,T.DIM_WEIGHT_UOM_CODE AS DIM_WEIGHT_UOM_CODE --  unit of measure&#xd;&#xa;      ,T.DIM_WEIGHT_BASE AS DIM_WEIGHT_BASE --  comparison value.&#xd;&#xa;      ,T.PLANNED_COST AS PLANNED_COST --  cost when original shipment was created&#xd;&#xa;      ,T.PLANNED_COST_BASE AS PLANNED_COST_BASE --  comparison value&#xd;&#xa;      ,T.PLANNED_COST_CURRENCY_GID AS PLANNED_COST_CRY_GID --  currency.&#xd;&#xa;      ,T.TERM_LOCATION_TEXT AS TERM_LOCATION_TEXT&#xd;&#xa;      ,T.TARIFF_ORGANIZATION_NUMBER AS TARIFF_ORGANIZATION_NUMBER&#xd;&#xa;      ,T.TARIFF_ORGANIZATION_NAME AS TARIFF_ORGANIZATION_NAME&#xd;&#xa;      ,T.TARIFF_NAME AS TARIFF_NAME&#xd;&#xa;      ,T.TARIFF_NUMBER AS TARIFF_NUMBER&#xd;&#xa;      ,T.TARIFF_DATE AS TARIFF_DATE&#xd;&#xa;      ,T.IS_HAZARDOUS AS IS_HAZARDOUS&#xd;&#xa;      ,T.IS_TEMPERATURE_CONTROL AS IS_TEMPERATURE_CONTROL&#xd;&#xa;      ,T.IS_COST_FIXED AS IS_COST_FIXED --  indicates shipment cost is a fixed cost.&#xd;&#xa;      ,T.IS_SERVICE_TIME_FIXED AS IS_SERVICE_TIME_FIXED&#xd;&#xa;      ,T.IS_RATE_GEO_FIXED AS IS_RATE_GEO_FIXED --  user assigned rate geo is fixed.&#xd;&#xa;      ,T.IS_RATE_OFFERING_FIXED AS IS_RATE_OFFERING_FIXED --  user assigned rate offering is fixed.&#xd;&#xa;      ,T.IS_SERVPROV_FIXED AS IS_SERVPROV_FIXED&#xd;&#xa;      ,T.IS_PRELOAD AS IS_PRELOAD&#xd;&#xa;      ,T.IS_FIXED_TENDER_CONTACT AS IS_FIXED_TENDER_CONTACT&#xd;&#xa;      ,T.IS_TO_BE_HELD AS IS_TO_BE_HELD&#xd;&#xa;      ,T.IS_PREFERRED_CARRIER AS IS_PREFERRED_CARRIER --  flag indicating whether the carrier selected for the shipment is preferred.&#xd;&#xa;      ,T.IS_EQUIPMENT_FIXED AS IS_EQUIPMENT_FIXED --  is_equipment_fixed is used to check if a service provider can update the equipment_number and equipment_initials when accepting a tendered shipment.&#xd;&#xa;      ,T.IS_RECALC_TOTALS AS IS_RECALC_TOTALS --  turns on&#x2f;off totaling logic&#xd;&#xa;      ,T.HAS_APPOINTMENTS AS HAS_APPOINTMENTS --  indicates if any stops on the shipment have appointments&#xd;&#xa;      ,T.INDICATOR AS INDICATOR --  &#x6d3e;&#x8f66;&#x786e;&#x8ba4;&#x72b6;&#x6001;&#x28;W&#x3a;&#x672a;&#x786e;&#x8ba4; G&#x3a;&#x5df2;&#x786e;&#x8ba4;&#x29;&#xd;&#xa;      ,T.CONTACT_GID AS CONTACT_GID&#xd;&#xa;      ,T.BULK_PLAN_GID AS BULK_PLAN_GID --  if the shipment was planned via the bulk plan process, then the global identifier for the bulk plan.&#xd;&#xa;      ,T.TOTAL_NUM_REFERENCE_UNITS AS TOTAL_NUM_REF_UNITS&#xd;&#xa;      ,T.ITINERARY_GID AS ITINERARY_GID&#xd;&#xa;      ,T.FEASIBILITY_CODE_GID AS FEASIBILITY_CODE_GID&#xd;&#xa;      ,T.NUM_ORDER_RELEASES AS NUM_ORDER_RELEASES --  &#x8f66;&#x8f86;&#x53f0;&#x6570;&#xd;&#xa;      ,T.NUM_STOPS AS NUM_STOPS --  &#x7ad9;&#x70b9;&#x6570;&#xd;&#xa;      ,T.TOTAL_NET_WEIGHT AS TOTAL_NET_WEIGHT&#xd;&#xa;      ,T.TOTAL_NET_WEIGHT_UOM_CODE AS TOTAL_NET_WEIGHT_UOM_CODE&#xd;&#xa;      ,T.TOTAL_NET_WEIGHT_BASE AS TOTAL_NET_WEIGHT_BASE&#xd;&#xa;      ,T.TOTAL_NET_VOLUME AS TOTAL_NET_VOLUME&#xd;&#xa;      ,T.TOTAL_NET_VOLUME_UOM_CODE AS TOTAL_NET_VOLUME_UOM_CODE&#xd;&#xa;      ,T.TOTAL_NET_VOLUME_BASE AS TOTAL_NET_VOLUME_BASE&#xd;&#xa;      ,T.EARLIEST_START_TIME AS EARLIEST_START_TIME --  the earliest start time that will make this shipment feasible.&#xd;&#xa;      ,T.LATEST_START_TIME AS LATEST_START_TIME --  the latest start time that will make this shipment feasible.&#xd;&#xa;      ,T.LAST_EVENT_GROUP_GID AS LAST_EVENT_GROUP_GID&#xd;&#xa;      ,T.REPETITION_SCHEDULE_GID AS REPETITION_SCHEDULE_GID&#xd;&#xa;      ,T.IN_TRAILER_BUILD AS IN_TRAILER_BUILD --  flag indicating whether this shipment is in a trailer build shipment group&#xd;&#xa;      ,T.FIRST_EQUIPMENT_GROUP_GID AS FIRST_EQUIPMENT_GROUP_GID&#xd;&#xa;      ,T.BULK_CM_GID AS BULK_CM_GID --  the identifier for the bulk cm if the shipment was selected in the bulk cm process.&#xd;&#xa;      ,T.VESSEL_GID AS VESSEL_GID --  vessel that the shipment is on. if populated, this actually overrides the value that is in the voyage table.&#xd;&#xa;      ,T.USER_DEFINED1_ICON_GID AS USER_DEFINED1_ICON_GID --  &#x8fd0;&#x8f93;&#x72b6;&#x6001;&#xd;&#xa;      ,T.USER_DEFINED2_ICON_GID AS USER_DEFINED2_ICON_GID --  can store a user-defined icon that will appear within the user interface to help visually identify attributes of the shipment.&#xd;&#xa;      ,T.USER_DEFINED3_ICON_GID AS USER_DEFINED3_ICON_GID --  can store a user-defined icon that will appear within the user interface to help visually identify attributes of the shipment.&#xd;&#xa;      ,T.USER_DEFINED4_ICON_GID AS USER_DEFINED4_ICON_GID --  can store a user-defined icon that will appear within the user interface to help visually identify attributes of the shipment.&#xd;&#xa;      ,T.USER_DEFINED5_ICON_GID AS USER_DEFINED5_ICON_GID --  &#x8f66;&#x724c;&#x53d8;&#x66f4;&#x72b6;&#x6001;&#x28;PUNOTCHANGE&#x3a;&#x8f66;&#x724c;&#x672a;&#x53d8;&#x66f4; NOTAPPROVE &#x672a;&#x5ba1;&#x6838; APPROVED &#x5df2;&#x5ba1;&#x6838;&#x29;&#xd;&#xa;      ,T.SOURCE_LOCATION_OVERRIDE_GID AS SOURCE_LOCATION_OR_GID&#xd;&#xa;      ,T.DEST_LOCATION_OVERRIDE_GID AS DEST_LOCATION_OR_GID&#xd;&#xa;      ,T.EQUIPMENT_REFERENCE_UNIT_GID AS EQUIPMENT_REF_UNIT_GID&#xd;&#xa;      ,T.SCHEDULE_TYPE AS SCHEDULE_TYPE --  when the schedul