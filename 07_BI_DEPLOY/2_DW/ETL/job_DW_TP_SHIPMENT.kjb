      <sql>&#xd;&#xa;--Settings&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_LOG&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Udf&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DW_TP_SHIPMENT_LOG &#xd;&#xa;SELECT T.SHIPMENT_GID AS SHIPMENT_GID --  &#x8fd0;&#x5355;GID&#xd;&#xa;      ,LO.INSERT_DATE&#xd;&#xa;  FROM ODS_ZCOTM.SHIPMENT T&#xd;&#xa;  JOIN TEMP.TEMP_DW_TP_SHIPMENT_PART PART&#xd;&#xa;    ON REGEXP_REPLACE&#x28;SUBSTRING&#x28;T.INSERT_DATE, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;  &#x3d; PART.PART_DT &#xd;&#xa;  JOIN &#x28;SELECT LO.KEY_ID,MAX&#x28;LO.INSERT_DATE&#x29; AS INSERT_DATE FROM ODS_ZCOTM.XX_ZC_SS_OPERATION_LOG LO&#xd;&#xa;               WHERE LO.TABLE_NAME &#x3d; &#x27;SHIPMENT&#x27; AND LO.DESCRIPTION &#x3d;&#x27;&#x56de;&#x5355;&#x786e;&#x8ba4;&#x27;&#xd;&#xa;             GROUP BY LO.KEY_ID&#x29; AS LO&#xd;&#xa;    ON T.SHIPMENT_GID &#x3d; LO.KEY_ID&#xd;&#xa; WHERE T.DOMAIN_NAME &#x3d; &#x27;CMAL&#x2f;ZC&#x27; &#x3b;&#xd;&#xa;</sql>
