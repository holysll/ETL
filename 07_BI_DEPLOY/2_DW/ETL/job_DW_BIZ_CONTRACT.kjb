    <entry>
      <name>TO_DW</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DW&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;INSERT OVERWRITE TABLE DW.DW_BIZ_CONTRACT PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT PERIOD_WID&#xd;&#xa;      ,T.ID&#xd;&#xa;      ,CONTRACT_NUMBER&#xd;&#xa;      ,CONTRACT_NAME&#xd;&#xa;      ,CONTRACT_TYPE_ID&#xd;&#xa;      ,CONTRACT_TYPE_NAME&#xd;&#xa;      ,STATE&#xd;&#xa;      ,START_MEMBER_ID&#xd;&#xa;      ,START_DATE&#xd;&#xa;      ,APPROVE_MEMBER_ID&#xd;&#xa;      ,APPROVE_DATE&#xd;&#xa;      ,FINISHEDFLAG&#xd;&#xa;      ,RATIFYFLAG&#xd;&#xa;      ,RATIFY_MEMBER_ID&#xd;&#xa;      ,RATIFY_DATE&#xd;&#xa;      ,CONTRACT_AMT&#xd;&#xa;      ,O.NAME AS START_UNIT&#xd;&#xa;      ,PARTY_NUMBER&#xd;&#xa;      ,CONTACT_INFO&#xd;&#xa;      ,YOUR_UNIT&#xd;&#xa;      ,YOUR_CONTACT_INFO&#xd;&#xa;      ,IS_RALATION&#xd;&#xa;      ,RELATION_FILE&#xd;&#xa;      ,BIZ_TYPE&#xd;&#xa;      ,PERSON_NUMBER&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN LENGTH&#x28;T.CONTRACT_END_DATE&#x29; &#x3d; 17 THEN T.CONTRACT_END_DATE&#xd;&#xa;         WHEN TRIM&#x28;REGEXP_REPLACE&#x28;BL.ED_DT, &#x27;&#x5b;&#x5e;0-9&#x5d;&#x27;, &#x27;&#x27;&#x29;&#x29; IS NULL THEN&#xd;&#xa;          NULL&#xd;&#xa;         WHEN BL.ED_DT &#x3d; &#x27;&#x27; THEN&#xd;&#xa;          NULL&#xd;&#xa;         ELSE&#xd;&#xa;          CONCAT&#x28;TRIM&#x28;REGEXP_REPLACE&#x28;BL.ST_DT, &#x27;&#x5b;&#x5e;0-9&#x5d;&#x27;, &#x27;&#x27;&#x29;&#x29;,&#xd;&#xa;                 &#x27;-&#x27;,&#xd;&#xa;                 TRIM&#x28;REGEXP_REPLACE&#x28;BL.ED_DT, &#x27;&#x5b;&#x5e;0-9&#x5d;&#x27;, &#x27;&#x27;&#x29;&#x29;&#x29;&#xd;&#xa;       END AS CONTRACT_END_DATE&#xd;&#xa;      ,IS_BOND&#xd;&#xa;      ,BOND_AMT&#xd;&#xa;      ,IS_CONTRACT_AMT&#xd;&#xa;      ,IS_FIXED_AMT&#xd;&#xa;      ,FEE_SCALE&#xd;&#xa;      ,UNIT_AMT&#xd;&#xa;      ,UNIT_AMT_2&#xd;&#xa;      ,IS_TEMP_CONTRANCT&#xd;&#xa;      ,REMARK&#xd;&#xa;      ,APPR_TEAM_SIGN&#xd;&#xa;      ,LEGA_SPECIALIST_SIGN&#xd;&#xa;      ,AUDIT_RISK_LEADER_SIGN&#xd;&#xa;      ,START_UNIT_LEADER_SIGN&#xd;&#xa;      ,IS_OTHER_SIGN&#xd;&#xa;      ,SIGN_NUMBER&#xd;&#xa;      ,IS_SOME_COMPANY&#xd;&#xa;      ,OUR_COMPANY_NAME&#xd;&#xa;      ,START_REVIEW&#xd;&#xa;      ,START_NUMBER&#xd;&#xa;      ,IS_BRANCH&#xd;&#xa;      ,IS_GRANT_AUTH_CONTRACT&#xd;&#xa;      ,IS_PERMT_LEGAL_ADVISORS&#xd;&#xa;      ,SORT&#xd;&#xa;      ,MODIFY_MEMBER_ID&#xd;&#xa;      ,MODIFY_DATE&#xd;&#xa;      ,SOURCE_CODE&#xd;&#xa;      ,INTEGRATION_ID&#xd;&#xa;      ,AUX1_CHANGED_ON_DT&#xd;&#xa;      ,W_INSERT_DT&#xd;&#xa;      ,W_UPDATE_DT&#xd;&#xa;      ,&#x27;OA&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM TEMP.TEMP_DW_BIZ_CONTRACT T&#xd;&#xa;  LEFT JOIN TEMP.TEMP_DW_BIZ_CONTRACT_BLHTSJ BL  --&#x624b;&#x5de5;&#x521d;&#x59cb;&#x5316;EXCEL&#xff0c;&#x4fdd;&#x5b58;&#x5728;svn &#x5c;DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;06_DMR&#x6570;&#x636e;&#x5b57;&#x5178;&#x5c;&#x8868;&#x521d;&#x59cb;&#x5316;&#x6570;&#x636e;&#x5c;2 &#x5408;&#x540c;&#x6709;&#x6548;&#x671f;&#x7f3a;&#x5931;&#x8865;&#x5f55; &#x62a5;&#x51fa;1024.xlsx&#xd;&#xa;    ON T.ID &#x3d; BL.ID&#xd;&#xa;  LEFT JOIN ODS_OA.ORG_UNIT O&#xd;&#xa;    ON T.START_UNIT &#x3d; O.ID&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
