    <entry>
      <name>CREATE_TEMP_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--&#x624b;&#x5de5;&#x521d;&#x59cb;&#x5316;&#x90e8;&#x5206;&#x5408;&#x540c;&#x671f;&#x9650;&#x4e0d;&#x6807;&#x51c6;&#x7684;&#x6570;&#x636e;&#xd;&#xa;--CREATE TABLE TEMP.TEMP_DW_BIZ_CONTRACT_BLHTSJ&#xd;&#xa;--&#x28;&#xd;&#xa;--ID STRING,&#xd;&#xa;--NO STRING,&#xd;&#xa;--ST_DT STRING,&#xd;&#xa;--ED_DT STRING&#x29;&#xd;&#xa;--COMMENT &#x27;&#x8865;&#x5f55;&#x5408;&#x540c;&#x65f6;&#x95f4;&#x28;&#x521d;&#x59cb;&#x5316;&#x29;&#x27; &#xd;&#xa;--ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;--STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DW_BIZ_CONTRACT&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DW_BIZ_CONTRACT&#x28;&#xd;&#xa;PERIOD_WID                    BIGINT                             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;ID                            STRING                             COMMENT  &#x27;ID&#x27;,&#xd;&#xa;CONTRACT_NUMBER               STRING                             COMMENT  &#x27;&#x5408;&#x540c;&#x7f16;&#x53f7;&#x27;,&#xd;&#xa;CONTRACT_NAME                 STRING                             COMMENT  &#x27;&#x5408;&#x540c;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;CONTRACT_TYPE_ID              STRING                             COMMENT  &#x27;&#x5408;&#x540c;&#x7c7b;&#x578b;ID&#x27;,&#xd;&#xa;CONTRACT_TYPE_NAME            STRING                             COMMENT  &#x27;&#x5408;&#x540c;&#x7c7b;&#x578b;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;STATE                         STRING                  