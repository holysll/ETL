    <entry>
      <name>TO_DW</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DW&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;INSERT OVERWRITE TABLE DW.DW_TP_LSP_EQUIPMENT PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,T.ID&#xd;&#xa;      ,O.OC_ID&#xd;&#xa;      ,O.RGN_ID&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,T.LOC_BASE_ID&#xd;&#xa;      ,T.BRAND_ID --  &#x54c1;&#x724c;ID &#xd;&#xa;      ,CONCAT&#x28;&#x27;CMAL&#x2f;ZC.&#x27;, T.SERVPROV&#x29; AS SUPPLIER_ID --  &#x627f;&#x8fd0;&#x5546;ID&#xd;&#xa;      ,T.PLAN_TO_AREA AS PLAN_DEST_LOC --  &#x8ba1;&#x5212;&#x53d1;&#x8fd0;&#x76ee;&#x7684;&#x5730;&#xd;&#xa;      ,T.VALID_DATE AS VALID_DATE --  &#x6709;&#x6548;&#x65f6;&#x95f4;&#xd;&#xa;      ,T.SALES_GROUP_ID AS CAR_GROUP_TYPE --  &#x8f66;&#x578b;&#x7ec4;&#x7c7b;&#x578b;&#xd;&#xa;      ,T.EQUIPMENT_ID AS LIC_NO --  &#x8fd0;&#x8f93;&#x5de5;&#x5177;&#x8f66;&#x724c;&#x53f7;&#xd;&#xa;      ,T.EQUIPMENT_VALID AS VALID_EQUIPMENT --  &#x6709;&#x6548;&#x8fd0;&#x529b;&#xd;&#xa;      ,T.EQUIPMENT_MAX AS MAX_EQUIPMENT --  &#x6700;&#x5927;&#x8fd0;&#x529b;&#xd;&#xa;      ,T.IS_VALID AS IS_VALID --  &#x662f;&#x5426;&#x6709;&#x6548;&#xd;&#xa;      ,T.CUR_LOC_GPS AS CUR_LOC_GPS --  &#x5ba2;&#x6237;&#x5f53;&#x524d;&#x4f4d;&#x7f6e;&#x540d;&#x79f0;&#xd;&#xa;      ,T.LAT AS LAT --  &#x7ecf;&#x5ea6;&#xd;&#xa;      ,T.LON AS LON --  &#x7eac;&#x5ea6;&#xd;&#xa;      ,T.LAST_DATE AS LAST_DATE --  &#x6700;&#x540e;&#x8bb0;&#x5f55;&#x65f6;&#x95f4;&#xd;&#xa;      ,T.DOMAIN_NAME AS DOMAIN_NAME --  &#x57df;&#xd;&#xa;      ,T.STATUS_6 AS STATUS --  &#x5ba1;&#x6838;&#x72b6;&#x6001;&#xd;&#xa;      ,T.IS_NEED_APPROVE AS IS_NEED_APPROVE --  &#x662f;&#x5426;&#x9700;&#x8981;&#x5ba1;&#x6838;&#xd;&#xa;      ,T.REMARK_6 AS REMARK --  &#x5907;&#x6ce8;&#xd;&#xa;      ,&#x27;ODS&#x27; AS SOURCE_CODE --&#x6e90;&#x7cfb;&#x7edf;&#x6807;&#x8bc6;&#xd;&#xa;      ,&#x27;-1&#x27; AS INTEGRATION_ID --&#x8bb0;&#x5f55;&#x552f;&#x4e00;&#x6807;&#x8bc6;&#xd;&#xa;      ,NULL AS AUX1_CHANGED_ON_DT --&#x539f;&#x7cfb;&#x7edf;&#x8bb0;&#x5f55;&#x521b;&#x5efa;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_INSERT_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_UPDATE_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x4fee;&#x6539;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;ODS&#x27; AS PART_SC --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;      ,SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;VALID_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS PERIOD_WID --  &#x671f;&#x95f4;ID&#xd;&#xa;              ,T.ID AS ID --  id &#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.CQ&#x27; THEN&#xd;&#xa;                  &#x27;CQJD&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.CS&#x27; THEN&#xd;&#xa;                  &#x27;CSZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.GZ&#x27; THEN&#xd;&#xa;                  &#x27;GZZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.HB&#x27; THEN&#xd;&#xa;                  &#x27;HBJD&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.HEB&#x27; THEN&#xd;&#xa;                  &#x27;HEBJD&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.HF&#x27; THEN&#xd;&#xa;                  &#x27;HFJD&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.HZ&#x27; THEN&#xd;&#xa;                  &#x27;HZJD&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.JN&#x27; THEN&#xd;&#xa;                  &#x27;JNZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.KM&#x27; THEN&#xd;&#xa;                  &#x27;KMZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.NC&#x27; THEN&#xd;&#xa;                  &#x27;NCJD&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.NJ&#x27; THEN&#xd;&#xa;                  &#x27;NJJD&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.TJ&#x27; THEN&#xd;&#xa;                  &#x27;TJZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.TY&#x27; THEN&#xd;&#xa;                  &#x27;TYZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.WH&#x27; THEN&#xd;&#xa;                  &#x27;WUHZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.WU&#x27; THEN&#xd;&#xa;                  &#x27;WHZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.XA&#x27; THEN&#xd;&#xa;                  &#x27;XAZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.XJ&#x27; THEN&#xd;&#xa;                  &#x27;XJZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.XT&#x27; THEN&#xd;&#xa;                  &#x27;XAZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.ZZ&#x27; THEN&#xd;&#xa;                  &#x27;ZZZZZ&#x27;&#xd;&#xa;                 WHEN LOC_BASE_ID &#x3d; &#x27;CMAL&#x2f;ZC.&#x829c;&#x6e56;&#x7801;&#x5934;&#x27; THEN&#xd;&#xa;                  &#x27;WHG&#x27;&#xd;&#xa;                 ELSE&#xd;&#xa;                  &#x27;BASE&#x2f;-1&#x27;&#xd;&#xa;               END AS BASE_ID --  &#x57fa;&#x5730;ID&#xd;&#xa;              ,T.LOC_BASE_ID AS LOC_BASE_ID&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN T.SALES_GROUP_ID &#x3d; &#x27;CMAL&#x2f;ZC.&#x957f;&#x5b89;&#x798f;&#x7279;&#x27; THEN&#xd;&#xa;                  &#x27;FT&#x27;&#xd;&#xa;                 WHEN T.SALES_GROUP_ID &#x3d; &#x27;CMAL&#x2f;ZC.&#x8f7b;&#x578b;&#x8f66;&#x27; THEN&#xd;&#xa;                  &#x27;CA&#x27;&#xd;&#xa;                 WHEN T.SALES_GROUP_ID &#x3d; &#x27;CMAL&#x2f;ZC.&#x89c2;&#x81f4;&#x6c7d;&#x8f66;&#x27; THEN&#xd;&#xa;                  &#x27;GZ&#x27;&#xd;&#xa;                 WHEN T.SALES_GROUP_ID &#x3d; &#x27;CMAL&#x2f;ZC.&#x8f7f;&#x8f66;&#x27; THEN&#xd;&#xa;                  &#x27;CA&#x27;&#xd;&#xa;                 WHEN T.SALES_GROUP_ID &#x3d; &#x27;CMAL&#x2f;ZC.&#x5fae;&#x8f66;&#x27; THEN&#xd;&#xa;                  &#x27;CA&#x27;&#xd;&#xa;                 ELSE&#xd;&#xa;                  NULL&#xd;&#xa;               END AS BRAND_ID --  &#x54c1;&#x724c;ID&#xd;&#xa;              ,T.SERVPROV&#xd;&#xa;              ,T.PLAN_TO_AREA&#xd;&#xa;              ,T.VALID_DATE&#xd;&#xa;              ,T.SALES_GROUP_ID&#xd;&#xa;              ,T.EQUIPMENT_ID&#xd;&#xa;              ,T.EQUIPMENT_VALID&#xd;&#xa;              ,T.EQUIPMENT_MAX&#xd;&#xa;              ,T.IS_VALID&#xd;&#xa;              ,T.CUR_LOC_GPS&#xd;&#xa;              ,T.LAT&#xd;&#xa;              ,T.LON&#xd;&#xa;              ,T.LAST_DATE&#xd;&#xa;              ,T.DOMAIN_NAME&#xd;&#xa;              ,T.STATUS_6&#xd;&#xa;              ,T.IS_NEED_APPROVE&#xd;&#xa;              ,T.REMARK_6&#xd;&#xa;          FROM ODS_ZCOTM.XX_T_SER_EQUIP_CAP T&#x29; AS T&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.BASE_ID &#x3d; O.BASE_ID&#xd;&#xa;   AND O.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;UNION ALL&#xd;&#xa;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,T.ID&#xd;&#xa;      ,O.OC_ID&#xd;&#xa;      ,O.RGN_ID&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,T.LOC_BASE_ID&#xd;&#xa;      ,T.BRAND_ID --  &#x54c1;&#x724c;ID &#xd;&#xa;      ,CONCAT&#x28;&#