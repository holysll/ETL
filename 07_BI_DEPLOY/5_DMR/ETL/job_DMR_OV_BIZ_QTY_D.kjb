      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_BASE&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_BIZ_QTY_D_BASE&#xd;&#xa;--&#x57fa;&#x5730;&#xd;&#xa;SELECT M.PERIOD_WID&#xd;&#xa;      ,M.ORG_TYPE&#xd;&#xa;      ,M.ORG_ID&#xd;&#xa;      ,T1.BIZ_QTY   AS CUR_D_SHIP_QTY&#xd;&#xa;      ,S1.BIZ_QTY   AS LST_D_SHIP_QTY&#xd;&#xa;      ,M1.BIZ_QTY   AS CUR_M_SHIP_QTY&#xd;&#xa;      ,L1.BIZ_QTY   AS LST_M_SHIP_QTY&#xd;&#xa;      ,Y1.BIZ_QTY   AS CUR_Y_SHIP_QTY&#xd;&#xa;      ,T1.BIZ_CNT   AS CUR_D_SHIP_CNT&#xd;&#xa;      ,S1.BIZ_CNT   AS LST_D_SHIP_CNT&#xd;&#xa;      ,M1.BIZ_CNT   AS CUR_M_SHIP_CNT&#xd;&#xa;      ,L1.BIZ_CNT   AS LST_M_SHIP_CNT&#xd;&#xa;      ,Y1.BIZ_CNT   AS CUR_Y_SHIP_CNT&#xd;&#xa;      ,T3.BIZ_QTY   AS CUR_D_STOCK_IN_QTY&#xd;&#xa;      ,S3.BIZ_QTY   AS LST_D_STOCK_IN_QTY&#xd;&#xa;      ,M3.BIZ_QTY   AS CUR_M_STOCK_IN_QTY&#xd;&#xa;      ,L3.BIZ_QTY   AS LST_M_STOCK_IN_QTY&#xd;&#xa;      ,Y3.BIZ_QTY   AS CUR_Y_STOCK_IN_QTY&#xd;&#xa;  FROM TEMP.TEMP_DMR_OV_BIZ_QTY_D_PERIOD_ORG M&#xd;&#xa;  LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;CASE WHEN B.BASE_TYPE &#x3d; &#x27;VDC&#x27; THEN T.SHIPPING_QTY ELSE 0 END&#x29; AS BIZ_QTY&#xd;&#xa;				   ,SUM&#x28;T.SHIPPING_QTY&#x29; AS BIZ_CNT&#xd;&#xa;               FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;			   LEFT JOIN DIM.DIM_ORG B&#xd;&#xa;			   ON T.BASE_ID &#x3d; B.BASE_ID AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;				AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;				AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;              GROUP BY T.PERIOD_WID&#xd;&#xa;                      ,T.BASE_ID&#x29; AS T1&#xd;&#xa;    ON M.ORG_ID &#x3d; T1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; T1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.INSTOCK_QTY&#x29; AS BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T.PERIOD_WID&#xd;&#xa;                      ,T.BASE_ID&#x29; AS T3&#xd;&#xa;    ON M.ORG_ID &#x3d; T3.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; T3.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT CONCAT&#x28;SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;,&#xd;&#xa;                           &#x27;-&#x27;,&#xd;&#xa;                           SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;,&#xd;&#xa;                           &#x27;-&#x27;,&#xd;&#xa;                           SUBSTR&#x28;T.PERIOD_WID, 7, 2&#x29;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;CASE WHEN B.BASE_TYPE &#x3d; &#x27;VDC&#x27; THEN T.SHIPPING_QTY ELSE 0 END&#x29; AS BIZ_QTY&#xd;&#xa;				   ,SUM&#x28;T.SHIPPING_QTY&#x29; AS BIZ_CNT&#xd;&#xa;               FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;			   LEFT JOIN DIM.DIM_ORG B&#xd;&#xa;			   ON T.BASE_ID &#x3d; B.BASE_ID AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; BETWEEN&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             7&#x29;,&#xd;&#xa;                                   &#x27;-&#x27;,&#xd;&#xa;                                   &#x27;&#x27;&#x29; AND&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;				AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;				AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;              GROUP BY CONCAT&#x28;SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;,&#xd;&#xa;                              &#x27;-&#x27;,&#xd;&#xa;                              SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;,&#xd;&#xa;                              &#x27;-&#x27;,&#xd;&#xa;                              SUBSTR&#x28;T.PERIOD_WID, 7, 2&#x29;&#x29;&#xd;&#xa;                      ,T.BASE_ID&#x29; AS S1&#xd;&#xa;    ON M.ORG_ID &#x3d; S1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;DATE_SUB&#x28;S1.PERIOD_WID, -1&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; --LAST_DAY&#xd;&#xa;  LEFT JOIN &#x28;SELECT CONCAT&#x28;SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;,&#xd;&#xa;                           &#x27;-&#x27;,&#xd;&#xa;                           SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;,&#xd;&#xa;                           &#x27;-&#x27;,&#xd;&#xa;                           SUBSTR&#x28;T.PERIOD_WID, 7, 2&#x29;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.INSTOCK_QTY&#x29; AS BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; BETWEEN&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             7&#x29;,&#xd;&#xa;                                   &#x27;-&#x27;,&#xd;&#xa;                                   &#x27;&#x27;&#x29; AND&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY CONCAT&#x28;SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;,&#xd;&#xa;                              &#x27;-&#x27;,&#xd;&#xa;                              SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;,&#xd;&#xa;                              &#x27;-&#x27;,&#xd;&#xa;                              SUBSTR&#x28;T.PERIOD_WID, 7, 2&#x29;&#x29;&#xd;&#xa;                      ,T.BASE_ID&#x29; AS S3&#xd;&#xa;    ON M.ORG_ID &#x3d; S3.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;DATE_SUB&#x28;S3.PERIOD_WID, -1&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; --LAST_DAY&#xd;&#xa;  LEFT JOIN &#x28;SELECT P.PERIOD_WID AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN P.PERIOD_WID &#x3e;&#x3d; T.PERIOD_WID THEN&#xd;&#xa;                           T.BIZ_QTY&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_QTY&#xd;&#xa;					,SUM&#x28;CASE&#xd;&#xa;                          WHEN P.PERIOD_WID &#x3e;&#x3d; T.PERIOD_WID THEN&#xd;&#xa;                           T.BIZ_NUM&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_CNT&#xd;&#xa;               FROM &#x28;SELECT PERIOD_WID&#xd;&#xa;                       FROM DIM.DIM_PERIOD_D&#xd;&#xa;                      WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                        AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                            SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                   1,&#xd;&#xa;                                   6&#x29;&#xd;&#xa;                        AND PERIOD_WID &#x3c;&#x3d;&#xd;&#xa;                            FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; P&#xd;&#xa;                LEFT JOIN &#x28;&#xd;&#xa;                SELECT T.period_wid&#xd;&#xa;                    ,NVL&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;                    ,sum&#x28;CASE WHEN B.base_type &#x3d; &#x27;VDC&#x27; THEN T.shipping_qty ELSE 0 END&#x29; AS BIZ_QTY&#xd;&#xa;                    ,sum&#x28;T.shipping_qty&#x29; AS BIZ_NUM &#xd;&#xa;                FROM DMK.DMK_TP_SHIPQTY_D T   &#xd;&#xa;                LEFT JOIN DIM.dim_org B  &#xd;&#xa;                ON T.base_id &#x3d; B.base_id AND B.org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;                WHERE substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                    AND substr&#x28;T.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyMMdd&#x27;&#x29;&#xd;&#xa;                    AND T.tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                    AND T.brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;                GROUP BY T.period_wid&#xd;&#xa;                    ,NVL&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;                    &#x29; T&#xd;&#xa;                ON 1&#x3d;1&#xd;&#xa;              GROUP BY P.PERIOD_WID&#xd;&#xa;                      ,T.BASE_ID&#x29; AS M1 --MONTH&#xd;&#xa;    ON M.ORG_ID &#x3d; M1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; M1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT P.PERIOD_WID AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN P.PERIOD_WID &#x3e;&#x3d; T.PERIOD_WID THEN&#xd;&#xa;                           T.BIZ_QTY&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_QTY&#xd;&#xa;               FROM &#x28;SELECT PERIOD_WID&#xd;&#xa;                       FROM DIM.DIM_PERIOD_D&#xd;&#xa;                      WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                        AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                            SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                   1,&#xd;&#xa;                                   6&#x29;&#xd;&#xa;                        AND PERIOD_WID &#x3c;&#x3d;&#xd;&#xa;                            FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; P&#xd;&#xa;               LEFT JOIN &#x28;&#xd;&#xa;						SELECT T.period_wid&#xd;&#xa;                            ,nvl&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                ,sum&#x28;T.instock_qty&#x29; AS BIZ_QTY &#xd;&#xa;                        FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;                        WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                            AND SUBSTR&#x28;T.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;                        GROUP BY T.period_wid&#xd;&#xa;                            ,nvl&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;                            &#x29; T   &#xd;&#xa;                        ON 1&#x3d;1&#xd;&#xa;              GROUP BY P.PERIOD_WID&#xd;&#xa;                      ,T.BASE_ID&#x29; AS M3 --MONTH   &#xd;&#xa;    ON M.ORG_ID &#x3d; M3.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; M3.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;P.PERIOD_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;P.PERIOD_DATE, -1&#x29;,&#xd;&#xa;                                              &#x27;-&#x27;,&#xd;&#xa;                                              &#x27;&#x27;&#x29; &#x3e;&#x3d; T.PERIOD_WID THEN&#xd;&#xa;                           T.BIZ_QTY&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_QTY&#xd;&#xa;					,SUM&#x28;CASE&#xd;&#xa;                          WHEN REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;P.PERIOD_DATE, -1&#x29;,&#xd;&#xa;                                              &#x27;-&#x27;,&#xd;&#xa;                                              &#x27;&#x27;&#x29; &#x3e;&#x3d; T.PERIOD_WID THEN&#xd;&#xa;                           T.BIZ_NUM&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_CNT&#xd;&#xa;               FROM &#x28;SELECT PERIOD_DATE&#xd;&#xa;                       FROM DIM.DIM_PERIOD_D&#xd;&#xa;                      WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                        AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                            SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                   1,&#xd;&#xa;                                   6&#x29;&#xd;&#xa;                        AND PERIOD_WID &#x3c;&#x3d;&#xd;&#xa;                            FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; P&#xd;&#xa;               LEFT JOIN &#x28;&#xd;&#xa;               SELECT T.period_wid&#xd;&#xa;                    ,NVL&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;                    ,sum&#x28;CASE WHEN B.base_type &#x3d; &#x27;VDC&#x27; THEN T.shipping_qty ELSE 0 END&#x29; AS BIZ_QTY&#xd;&#xa;                    ,sum&#x28;T.shipping_qty&#x29; AS BIZ_NUM &#xd;&#xa;                FROM DMK.DMK_TP_SHIPQTY_D T   &#xd;&#xa;                LEFT JOIN DIM.dim_org B  &#xd;&#xa;                ON T.base_id &#x3d; B.base_id AND B.org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;                WHERE substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                    AND T.tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                    AND T.brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;                GROUP BY T.period_wid&#xd;&#xa;                    ,NVL&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;                    &#x29; T&#xd;&#xa;                ON 1&#x3d;1&#xd;&#xa;              GROUP BY REGEXP_REPLACE&#x28;P.PERIOD_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                      ,T.BASE_ID&#x29; AS L1 --LAST MONTH&#xd;&#xa;    ON M.ORG_ID &#x3d; L1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; L1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;P.PERIOD_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;P.PERIOD_DATE, -1&#x29;,&#xd;&#xa;                                              &#x27;-&#x27;,&#xd;&#xa;                                              &#x27;&#x27;&#x29; &#x3e;&#x3d; T.PERIOD_WID THEN&#xd;&#xa;                           T.BIZ_QTY&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_QTY&#xd;&#xa;               FROM &#x28;SELECT PERIOD_DATE&#xd;&#xa;                       FROM DIM.DIM_PERIOD_D&#xd;&#xa;                      WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                        AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                            SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                   1,&#xd;&#xa;                                   6&#x29;&#xd;&#xa;                        AND PERIOD_WID &#x3c;&#x3d;&#xd;&#xa;                            FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; P&#xd;&#xa;               LEFT JOIN &#x28;&#xd;&#xa;					SELECT T.period_wid&#xd;&#xa;                            ,nvl&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                ,sum&#x28;T.instock_qty&#x29; AS BIZ_QTY &#xd;&#xa;                        FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;                        WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                        GROUP BY T.period_wid&#xd;&#xa;                            ,nvl&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;					&#x29; T&#xd;&#xa;				ON 1&#x3d;1&#xd;&#xa;              GROUP BY REGEXP_REPLACE&#x28;P.PERIOD_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                      ,T.BASE_ID&#x29; AS L3 --LAST MONTH   &#xd;&#xa;    ON M.ORG_ID &#x3d; L3.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; L3.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT X.PERIOD_WID&#xd;&#xa;                   ,X.ORG_ID&#xd;&#xa;                   ,X.BIZ_QTY&#xd;&#xa;				   ,X.BIZ_CNT&#xd;&#xa;               FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                           ,D.ORG_ID&#xd;&#xa;                           ,SUM&#x28;BIZ_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;D.PERIOD_WID, 1, 4&#x29;, D.ORG_ID ORDER BY D.PERIOD_WID&#x29; AS BIZ_QTY&#xd;&#xa;						   ,SUM&#x28;BIZ_CNT&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;D.PERIOD_WID, 1, 4&#x29;, D.ORG_ID ORDER BY D.PERIOD_WID&#x29; AS BIZ_CNT&#xd;&#xa;                       FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                                   ,O.BASE_ID AS ORG_ID&#xd;&#xa;                               FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                               JOIN DIM.DIM_ORG O&#xd;&#xa;                                 ON 1 &#x3d; 1&#xd;&#xa;                              WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                                AND O.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                                AND SUBSTR&#x28;D.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                                    SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                          &#x27;-&#x27;,&#xd;&#xa;                                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                                           1,&#xd;&#xa;                                           4&#x29;&#xd;&#xa;                                AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                                    SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                          &#x27;-&#x27;,&#xd;&#xa;                                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                                           1,&#xd;&#xa;                                           6&#x29;&#xd;&#xa;                                AND SUBSTR&#x28;D.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;                                    FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; D&#xd;&#xa;                       LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                                       ,T.BASE_ID AS ORG_ID&#xd;&#xa;                                       ,SUM&#x28;CASE WHEN B.BASE_TYPE &#x3d; &#x27;VDC&#x27; THEN T.SHIPPING_QTY ELSE 0 END&#x29; AS BIZ_QTY&#xd;&#xa;									   ,SUM&#x28;T.SHIPPING_QTY&#x29; AS BIZ_CNT&#xd;&#xa;                                   FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;								   LEFT JOIN DIM.DIM_ORG B &#xd;&#xa;								   ON T.BASE_ID &#x3d; B.BASE_ID AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                                  WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                                        REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                 1,&#xd;&#xa;                                                                 4&#x29;,&#xd;&#xa;                                                       &#x27;-&#x27;,&#xd;&#xa;                                                       &#x27;&#x27;&#x29;&#xd;&#xa;                                    AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                                        REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                 1,&#xd;&#xa;                                                                 7&#x29;,&#xd;&#xa;                                                       &#x27;-&#x27;,&#xd;&#xa;                                                       &#x27;&#x27;&#x29;&#xd;&#xa;                                    AND SUBSTR&#x28;T.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;                                        FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#xd;&#xa;                                                      &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;									AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;									AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;                                  GROUP BY T.PERIOD_WID&#xd;&#xa;                                          ,T.BASE_ID&#x29; T&#xd;&#xa;                         ON D.PERIOD_WID &#x3d; T.PERIOD_WID&#xd;&#xa;                        AND D.ORG_ID &#x3d; T.ORG_ID&#x29; AS X&#xd;&#xa;              WHERE SUBSTR&#x28;X.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#x29; AS Y1 --YEAR&#xd;&#xa;    ON M.ORG_ID &#x3d; Y1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; Y1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;P.PERIOD_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN REGEXP_REPLACE&#x28;P.PERIOD_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3e;&#x3d;&#xd;&#xa;                               T.PERIOD_WID THEN&#xd;&#xa;                           T.BIZ_QTY&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_QTY&#xd;&#xa;               FROM &#x28;SELECT PERIOD_DATE&#xd;&#xa;                       FROM DIM.DIM_PERIOD_D&#xd;&#xa;                      WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                        AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                            SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                   1,&#xd;&#xa;                                   6&#x29;&#xd;&#xa;                        AND PERIOD_WID &#x3c;&#x3d;&#xd;&#xa;                            FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; P&#xd;&#xa;               LEFT JOIN &#x28;&#xd;&#xa;                SELECT T.period_wid&#xd;&#xa;                    ,nvl&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;                    ,sum&#x28;T.instock_qty&#x29; AS BIZ_QTY&#xd;&#xa;                FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;                WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                AND SUBSTR&#x28;T.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;                    FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;                GROUP BY T.period_wid&#xd;&#xa;                    ,nvl&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;                &#x29; T&#xd;&#xa;                ON 1&#x3d;1&#xd;&#xa;              GROUP BY REGEXP_REPLACE&#x28;P.PERIOD_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                      ,T.BASE_ID&#x29; AS Y3 --YEAR &#xd;&#xa;    ON M.ORG_ID &#x3d; Y3.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; Y3.PERIOD_WID&#xd;&#xa; WHERE M.ORG_TYPE &#x3d; &#x27;B&#x27;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT M.PERIOD_WID&#xd;&#xa;      ,M.ORG_TYPE&#xd;&#xa;      ,M.ORG_ID&#xd;&#xa;      ,T1.BIZ_QTY   AS CUR_D_SHIP_QTY&#xd;&#xa;      ,S1.BIZ_QTY   AS LST_D_SHIP_QTY&#xd;&#xa;      ,M1.BIZ_QTY   AS CUR_M_SHIP_QTY&#xd;&#xa;      ,L1.BIZ_QTY   AS LST_M_SHIP_QTY&#xd;&#xa;      ,Y1.BIZ_QTY   AS CUR_Y_SHIP_QTY&#xd;&#xa;      ,T2.BIZ_QTY   AS CUR_D_SHIP_CNT&#xd;&#xa;      ,S2.BIZ_QTY   AS LST_D_SHIP_CNT&#xd;&#xa;      ,M2.BIZ_QTY   AS CUR_M_SHIP_CNT&#xd;&#xa;      ,L2.BIZ_QTY   AS LST_M_SHIP_CNT&#xd;&#xa;      ,Y2.BIZ_QTY   AS CUR_Y_SHIP_CNT&#xd;&#xa;      ,T3.BIZ_QTY   AS CUR_D_STOCK_IN_QTY&#xd;&#xa;      ,S3.BIZ_QTY   AS LST_D_STOCK_IN_QTY&#xd;&#xa;      ,M3.BIZ_QTY   AS CUR_M_STOCK_IN_QTY&#xd;&#xa;      ,L3.BIZ_QTY   AS LST_M_STOCK_IN_QTY&#xd;&#xa;      ,Y3.BIZ_QTY   AS CUR_Y_STOCK_IN_QTY&#xd;&#xa;  FROM TEMP.TEMP_DMR_OV_BIZ_QTY_D_PERIOD_ORG M&#xd;&#xa;  LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_TP_ARRIVE_D T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T.PERIOD_WID&#xd;&#xa;                      ,T.BASE_ID&#x29; AS T1&#xd;&#xa;    ON M.ORG_ID &#x3d; T1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; T1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.SHIPPING_NUM&#x29; AS BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_TP_OPERATE_D T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T.PERIOD_WID&#xd;&#xa;                      ,T.BASE_ID&#x29; AS T2&#xd;&#xa;    ON M.ORG_ID &#x3d; T2.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; T2.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.INSTOCK_QTY&#x29; AS BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T.PERIOD_WID&#xd;&#xa;                      ,T.BASE_ID&#x29; AS T3&#xd;&#xa;    ON M.ORG_ID &#x3d; T3.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; T3.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT CONCAT&#x28;SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;,&#xd;&#xa;                           &#x27;-&#x27;,&#xd;&#xa;                           SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;,&#xd;&#xa;                           &#x27;-&#x27;,&#xd;&#xa;                           SUBSTR&#x28;T.PERIOD_WID, 7, 2&#x29;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_TP_ARRIVE_D T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; BETWEEN&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             7&#x29;,&#xd;&#xa;                                   &#x27;-&#x27;,&#xd;&#xa;                                   &#x27;&#x27;&#x29; AND&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY CONCAT&#x28;SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;,&#xd;&#xa;                              &#x27;-&#x27;,&#xd;&#xa;                              SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;,&#xd;&#xa;                              &#x27;-&#x27;,&#xd;&#xa;                              SUBSTR&#x28;T.PERIOD_WID, 7, 2&#x29;&#x29;&#xd;&#xa;                      ,T.BASE_ID&#x29; AS S1&#xd;&#xa;    ON M.ORG_ID &#x3d; S1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;DATE_SUB&#x28;S1.PERIOD_WID, -1&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; --LAST_DAY&#xd;&#xa;  LEFT JOIN &#x28;SELECT CONCAT&#x28;SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;,&#xd;&#xa;                           &#x27;-&#x27;,&#xd;&#xa;                           SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;,&#xd;&#xa;                           &#x27;-&#x27;,&#xd;&#xa;                           SUBSTR&#x28;T.PERIOD_WID, 7, 2&#x29;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.SHIPPING_NUM&#x29; AS BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_TP_OPERATE_D T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; BETWEEN&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             7&#x29;,&#xd;&#xa;                                   &#x27;-&#x27;,&#xd;&#xa;                                   &#x27;&#x27;&#x29; AND&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY CONCAT&#x28;SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;,&#xd;&#xa;                              &#x27;-&#x27;,&#xd;&#xa;                              SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;,&#xd;&#xa;                              &#x27;-&#x27;,&#xd;&#xa;                              SUBSTR&#x28;T.PERIOD_WID, 7, 2&#x29;&#x29;&#xd;&#xa;                      ,T.BASE_ID&#x29; AS S2&#xd;&#xa;    ON M.ORG_ID &#x3d; S2.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;DATE_SUB&#x28;S2.PERIOD_WID, -1&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; --LAST_DAY&#xd;&#xa;  LEFT JOIN &#x28;SELECT CONCAT&#x28;SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;,&#xd;&#xa;                           &#x27;-&#x27;,&#xd;&#xa;                           SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;,&#xd;&#xa;                           &#x27;-&#x27;,&#xd;&#xa;                           SUBSTR&#x28;T.PERIOD_WID, 7, 2&#x29;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.INSTOCK_QTY&#x29; AS BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; BETWEEN&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             7&#x29;,&#xd;&#xa;                                   &#x27;-&#x27;,&#xd;&#xa;                                   &#x27;&#x27;&#x29; AND&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY CONCAT&#x28;SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;,&#xd;&#xa;                              &#x27;-&#x27;,&#xd;&#xa;                              SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;,&#xd;&#xa;                              &#x27;-&#x27;,&#xd;&#xa;                              SUBSTR&#x28;T.PERIOD_WID, 7, 2&#x29;&#x29;&#xd;&#xa;                      ,T.BASE_ID&#x29; AS S3&#xd;&#xa;    ON M.ORG_ID &#x3d; S3.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;DATE_SUB&#x28;S3.PERIOD_WID, -1&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; --LAST_DAY&#xd;&#xa;  LEFT JOIN &#x28;SELECT P.PERIOD_WID AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN P.PERIOD_WID &#x3e;&#x3d; T.PERIOD_WID THEN&#xd;&#xa;                           T.BIZ_QTY&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_QTY&#xd;&#xa;               FROM &#x28;SELECT PERIOD_WID&#xd;&#xa;                       FROM DIM.DIM_PERIOD_D&#xd;&#xa;                      WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                        AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                            SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                   1,&#xd;&#xa;                                   6&#x29;&#xd;&#xa;                        AND PERIOD_WID &#x3c;&#x3d;&#xd;&#xa;                            FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; P&#xd;&#xa;               LEFT JOIN DMK.DMK_TP_ARRIVE_D T&#xd;&#xa;                 ON SUBSTR&#x28;P.PERIOD_WID, 1, 6&#x29; &#x3d; SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29;&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                AND SUBSTR&#x28;T.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;                    FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;              GROUP BY P.PERIOD_WID&#xd;&#xa;                      ,T.BASE_ID&#x29; AS M1 --MONTH&#xd;&#xa;    ON M.ORG_ID &#x3d; M1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; M1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT P.PERIOD_WID AS PERIOD_WID&#xd;&#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN P.PERIOD_WID &#x3e;&#x3d; T.PERIOD_WID THEN&#xd;&#xa;                           T.SHIPPING_NUM&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_QTY&#xd;&#xa;               FROM &#x28;SELECT PERIOD_WID&#xd;&#xa;                       FROM DIM.DIM_PERIOD_D&#xd;&#xa;                      WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                        AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                            SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                   1,&#xd;&#xa;                                   6&#x29;&#xd;&#xa;                        AND PERIOD_WID &#x3c;&#x3d;&#xd;&#xa;                            FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; P&#xd;&#xa;               LEFT JOIN DMK.