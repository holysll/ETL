    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_FIN_TPBUDGET_D	&#x8fd0;&#x8f93;&#x8d22;&#x52a1;&#x9884;&#x7b97;&#x65e5;&#x660e;&#x7ec6;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;05_DMK&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_BIZ_QTY_D PARTITION&#x28;PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,T.ORG_TYPE&#xd;&#xa;      ,T.ORG_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.ORG_TYPE &#x3d; &#x27;O&#x27; THEN&#xd;&#xa;          O.OC_NAME&#xd;&#xa;         WHEN T.ORG_TYPE &#x3d; &#x27;R&#x27; THEN&#xd;&#xa;          O.RGN_NAME&#xd;&#xa;         WHEN T.ORG_TYPE &#x3d; &#x27;B&#x27; THEN&#xd;&#xa;          O.BASE_NAME&#xd;&#xa;         WHEN T.ORG_TYPE &#x3d; &#x27;S&#x27; THEN&#xd;&#xa;          O.STR_NAME&#xd;&#xa;         ELSE&#xd;&#xa;          NULL&#xd;&#xa;       END AS ORG_NAME&#xd;&#xa;      ,T.CUR_D_SHIP_QTY&#xd;&#xa;      ,T.LST_D_SHIP_QTY&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;T.LST_D_SHIP_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;NVL&#x28;T.CUR_D_SHIP_QTY, 0&#x29; - NVL&#x28;T.LST_D_SHIP_QTY, 0&#x29;&#x29; &#x2f;&#xd;&#xa;          NVL&#x28;T.LST_D_SHIP_QTY, 0&#x29;&#xd;&#xa;       END AS CUR_D_SHIP_QTY_HB&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;T.LST_D_SHIP_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          2&#xd;&#xa;         WHEN &#x28;NVL&#x28;T.CUR_D_SHIP_QTY, 0&#x29; - NVL&#x28;T.LST_D_SHIP_QTY, 0&#x29;&#x29; &#x2f;&#xd;&#xa;              NVL&#x28;T.LST_D_SHIP_QTY, 0&#x29; &#x3e; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         WHEN &#x28;NVL&#x28;T.CUR_D_SHIP_QTY, 0&#x29; - NVL&#x28;T.LST_D_SHIP_QTY, 0&#x29;&#x29; &#x2f;&#xd;&#xa;              NVL&#x28;T.LST_D_SHIP_QTY, 0&#x29; &#x3c; 0 THEN&#xd;&#xa;          1&#xd;&#xa;         WHEN &#x28;NVL&#x28;T.CUR_D_SHIP_QTY, 0&#x29; - NVL&#x28;T.LST_D_SHIP_QTY, 0&#x29;&#x29; &#x2f;&#xd;&#xa;              NVL&#x28;T.LST_D_SHIP_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          2&#xd;&#xa;         ELSE&#xd;&#xa;          NULL&#xd;&#xa;       END AS CUR_D_SHIP_QTY_HB_FLAG&#xd;&#xa;      ,T.CUR_D_STOCK_IN_QTY&#xd;&#xa;      ,T.LST_D_STOCK_IN_QTY&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;T.LST_D_STOCK_IN_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;NVL&#x28;T.CUR_D_STOCK_IN_QTY, 0&#x29; - NVL&#x28;T.LST_D_STOCK_IN_QTY, 0&#x29;&#x29; &#x2f;&#xd;&#xa;          NVL&#x28;T.LST_D_STOCK_IN_QTY, 0&#x29;&#xd;&#xa;       END AS CUR_D_STOCK_IN_QTY_HB&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;T.LST_D_STOCK_IN_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          2&#xd;&#xa;         WHEN &#x28;NVL&#x28;T.CUR_D_STOCK_IN_QTY, 0&#x29; - NVL&#x28;T.LST_D_STOCK_IN_QTY, 0&#x29;&#x29; &#x2f;&#xd;&#xa;              NVL&#x28;T.LST_D_STOCK_IN_QTY, 0&#x29; &#x3e; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         WHEN &#x28;NVL&#x28;T.CUR_D_STOCK_IN_QTY, 0&#x29; - NVL&#x28;T.LST_D_STOCK_IN_QTY, 0&#x29;&#x29; &#x2f;&#xd;&#xa;              NVL&#x28;T.LST_D_STOCK_IN_QTY, 0&#x29; &#x3c; 0 THEN&#xd;&#xa;          1&#xd;&#xa;         WHEN &#x28;NVL&#x28;T.CUR_D_STOCK_IN_QTY, 0&#x29; - NVL&#x28;T.LST_D_STOCK_IN_QTY, 0&#x29;&#x29; &#x2f;&#xd;&#xa;              NVL&#x28;T.LST_D_STOCK_IN_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          2&#xd;&#xa;         ELSE&#xd;&#xa;          NULL&#xd;&#xa;       END AS CUR_D_STOCK_IN_QTY_HB_FLAG&#xd;&#xa;      ,T.CUR_M_SHIP_QTY&#xd;&#xa;      ,T.LST_M_SHIP_QTY&#xd;&#xa;      ,T.CUR_M_STOCK_IN_QTY&#xd;&#xa;      ,T.LST_M_STOCK_IN_QTY&#xd;&#xa;      ,T.CUR_Y_SHIP_QTY&#xd;&#xa;      ,T.CUR_Y_STOCK_IN_QTY&#xd;&#xa;      ,T.CUR_D_SHIP_CNT&#xd;&#xa;      ,T.LST_D_SHIP_CNT&#xd;&#xa;      ,T.CUR_D_STOCK_IN_QTY AS CUR_D_STOCK_IN_CNT&#xd;&#xa;      ,T.LST_D_STOCK_IN_QTY AS LST_D_STOCK_IN_CNT&#xd;&#xa;      ,T.CUR_M_SHIP_CNT&#xd;&#xa;      ,T.LST_M_SHIP_CNT&#xd;&#xa;      ,T.CUR_M_STOCK_IN_QTY AS CUR_M_STOCK_IN_CNT&#xd;&#xa;      ,T.LST_M_STOCK_IN_QTY&#xd;&#xa;      ,T.CUR_Y_SHIP_CNT&#xd;&#xa;      ,T.CUR_Y_STOCK_IN_QTY AS CUR_Y_STOCK_IN_CNT&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_OV_BIZ_QTY_D_STR T1&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_OV_BIZ_QTY_D_BASE T2&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_OV_BIZ_QTY_D_RGN T3&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_OV_BIZ_QTY_D_OC T4&#x29; AS T&#xd;&#xa;  JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.ORG_ID &#x3d; O.ORG_ID&#xd;&#xa;   AND T.ORG_TYPE &#x3d; O.ORG_TYPE&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
