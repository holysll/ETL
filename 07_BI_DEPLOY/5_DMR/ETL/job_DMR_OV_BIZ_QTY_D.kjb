D_SHIP_CNT                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x65e5;&#x53d1;&#x8fd0;&#x91cf;&#x28;&#x8f86;&#x6b21;&#xff09;&#x27;,&#xd;&#xa;  LST_D_SHIP_CNT                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x4e0a;&#x65e5;&#x53d1;&#x8fd0;&#x91cf;&#x28;&#x8f86;&#x6b21;&#xff09;&#x27;,&#xd;&#xa;  CUR_M_SHIP_CNT                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x6708;&#x53d1;&#x8fd0;&#x91cf;&#x28;&#x8f86;&#x6b21;&#xff09;&#x27;,&#xd;&#xa;  LST_M_SHIP_CNT                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x4e0a;&#x6708;&#x53d1;&#x8fd0;&#x91cf;&#x28;&#x8f86;&#x6b21;&#xff09;&#x27;,&#xd;&#xa;  CUR_Y_SHIP_CNT                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x5e74;&#x53d1;&#x8fd0;&#x91cf;&#x28;&#x8f86;&#x6b21;&#xff09;&#x27;,&#xd;&#xa;  CUR_D_STOCK_IN_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x65e5;&#x5165;&#x5e93;&#x91cf;&#x27;,&#xd;&#xa;  LST_D_STOCK_IN_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x4e0a;&#x65e5;&#x5165;&#x5e93;&#x91cf;&#x27;,&#xd;&#xa;  CUR_M_STOCK_IN_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x6708;&#x5165;&#x5e93;&#x91cf;&#x27;,&#xd;&#xa;  LST_M_STOCK_IN_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x4e0a;&#x6708;&#x5165;&#x5e93;&#x91cf;&#x27;,&#xd;&#xa;  CUR_Y_STOCK_IN_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x5e74;&#x5165;&#x5e93;&#x91cf;&#x27;&#xd;&#xa;  &#x29; &#xd;&#xa;  COMMENT &#x27;&#x5927;&#x533a;&#x91cf;&#x27;&#xd;&#xa;  ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;  STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;  CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_OV_BIZ_QTY_D_OC&#x28;&#xd;&#xa;  PERIOD_WID                    BIGINT                             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;  ORG_TYPE                      STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;&#x7c7b;&#x578b;&#x27;,&#xd;&#xa;  ORG_ID                        STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;ID&#x27;,&#xd;&#xa;  CUR_D_SHIP_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x65e5;&#x53d1;&#x8fd0;&#x91cf;&#x27;,&#xd;&#xa;  LST_D_SHIP_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x4e0a;&#x65e5;&#x53d1;&#x8fd0;&#x91cf;&#x27;,&#xd;&#xa;  CUR_M_SHIP_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x6708;&#x53d1;&#x8fd0;&#x91cf;&#x27;,&#xd;&#xa;  LST_M_SHIP_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x4e0a;&#x6708;&#x53d1;&#x8fd0;&#x91cf;&#x27;,&#xd;&#xa;  CUR_Y_SHIP_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x5e74;&#x53d1;&#x8fd0;&#x91cf;&#x27;,&#xd;&#xa;  CUR_D_SHIP_CNT                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x65e5;&#x53d1;&#x8fd0;&#x91cf;&#x28;&#x8f86;&#x6b21;&#xff09;&#x27;,&#xd;&#xa;  LST_D_SHIP_CNT                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x4e0a;&#x65e5;&#x53d1;&#x8fd0;&#x91cf;&#x28;&#x8f86;&#x6b21;&#xff09;&#x27;,&#xd;&#xa;  CUR_M_SHIP_CNT                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x6708;&#x53d1;&#x8fd0;&#x91cf;&#x28;&#x8f86;&#x6b21;&#xff09;&#x27;,&#xd;&#xa;  LST_M_SHIP_CNT                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x4e0a;&#x6708;&#x53d1;&#x8fd0;&#x91cf;&#x28;&#x8f86;&#x6b21;&#xff09;&#x27;,&#xd;&#xa;  CUR_Y_SHIP_CNT                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x5e74;&#x5