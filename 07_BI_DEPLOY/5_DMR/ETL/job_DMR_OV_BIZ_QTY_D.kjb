#xa;                   ,T.BASE_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;P.PERIOD_DATE, -1&#x29;,&#xd;&#xa;                                              &#x27;-&#x27;,&#xd;&#xa;                                              &#x27;&#x27;&#x29; &#x3e;&#x3d; T.PERIOD_WID THEN&#xd;&#xa;                           T.INSTOCK_QTY&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_QTY&#xd;&#xa;               FROM &#x28;SELECT PERIOD_DATE&#xd;&#xa;                       FROM DIM.DIM_PERIOD_D&#xd;&#xa;                      WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                        AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                            SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                   1,&#xd;&#xa;                                   6&#x29;&#xd;&#xa;                        AND PERIOD_WID &#x3c;&#x3d;&#xd;&#xa;                            FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; P&#xd;&#xa;               LEFT JOIN DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;                 ON SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;P.PERIOD_DATE, -1&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                           1,&#xd;&#xa;                           6&#x29; &#x3d; SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29;&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             7&#x29;,&#xd;&#xa;                                   &#x27;-&#x27;,&#xd;&#xa;                                   &#x27;&#x27;&#x29;&#xd;&#xa;                AND PERIOD_WID &#x3c;&#x3d;&#xd;&#xa;                    FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;              GROUP BY REGEXP_REPLACE&#x28;P.PERIOD_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                      ,T.BASE_ID&#x29; AS L3 --LAST MONTH   &#xd;&#xa;    ON M.ORG_ID &#x3d; L3.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; L3.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT X.PERIOD_WID&#xd;&#xa;                   ,X.ORG_ID&#xd;&#xa;                   ,X.BIZ_QTY&#xd;&#xa;               FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                           ,D.ORG_ID&#xd;&#xa;                           ,SUM&#x28;BIZ_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;D.PERIOD_WID, 1, 4&#x29;, D.ORG_ID ORDER BY D.PERIOD_WID&#x29; AS BIZ_QTY&#xd;&#xa;                       FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                                   ,O.BASE_ID AS ORG_ID&#xd;&#xa;                               FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                               JOIN DIM.DIM_ORG O&#xd;&#xa;                                 ON 1 &#x3d; 1&#xd;&#xa;                              WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                                AND O.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                                AND SUBSTR&#x28;D.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                                    SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                          &#x27;-&#x27;,&#xd;&#xa;                                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                                           1,&#xd;&#xa;                                           4&#x29;&#xd;&#xa;                                AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                             