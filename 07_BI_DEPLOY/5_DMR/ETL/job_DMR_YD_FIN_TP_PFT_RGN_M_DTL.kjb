    <entry>
      <name>TO_BIZ_DETAIL</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_FIN_TP_PFT_RGN_M_DTL_BIZ&#xd;&#xa;SELECT coalesce&#x28;A.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;A.BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;A.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;A.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,sum&#x28;A.CUR_TP_BIZ&#x29; AS CUR_TP_BIZ&#xd;&#xa;    ,sum&#x28;A.LST_TP_BIZ&#x29; AS LST_TP_BIZ&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,sum&#x28;CASE WHEN B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN A.shipping_qty ELSE 0 END&#x29; AS CUR_TP_BIZ  --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;        ,sum&#x28;CASE WHEN B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN A.shipping_qty ELSE 0 END&#x29; AS LST_TP_BIZ  --&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;    FROM DMK.DMK_TP_SHIPQTY_D A &#xd;&#xa;    LEFT JOIN DIM.dim_period_d B&#xd;&#xa;    ON A.period_wid &#x3d; B.period_wid AND B.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;    WHERE &#x28;B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        OR B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;        AND A.tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;        AND A.brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; A&#xd;&#xa;GROUP BY A.RGN_ID,A.BASE_ID,A.BRAND_ID,A.PROJ_ID WITH CUBE</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
