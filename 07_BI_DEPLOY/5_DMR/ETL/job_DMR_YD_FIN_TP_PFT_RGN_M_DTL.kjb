    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--target table in hive&#x3a; DMR.DMR_YD_FIN_TP_PFT_RGN_M_DTL	&#x6574;&#x8f66;&#x7269;&#x6d41;&#x603b;&#x89c8;&#x6b62;&#x635f;&#x7387;&#x8be6;&#x60c5;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_FIN_TP_PFT_RGN_M_DTL PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID &#xd;&#xa;    ,T.rgn_id&#xd;&#xa;    ,CASE WHEN T.rgn_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.rgn_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.rgn_name END AS RNG_NAME&#xd;&#xa;    ,T.base_id&#xd;&#xa;    ,CASE WHEN T.base_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.base_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.base_name END AS BASE_NAME&#xd;&#xa;    ,T.brand_id&#xd;&#xa;    ,CASE WHEN T.brand_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.brand_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.brand_name END AS BRAND_NAME&#xd;&#xa;    ,T.proj_id&#xd;&#xa;    ,CASE WHEN T.proj_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.proj_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T4.proj_name END AS PROJ_NAME&#xd;&#xa;    ,&#x27;TP&#x27; AS BIZ_TYPE_ID  --&#x4e1a;&#x52a1;&#x5c5e;&#x6027;ID&#xd;&#xa;    ,&#x27;&#x8fd0;&#x8f93;&#x27; AS BIZ_TYPE_NAME  --&#x4e1a;&#x52a1;&#x5c5e;&#x6027;&#x540d;&#x79f0;&#xd;&#xa;    ,sum&#x28;T.CUR_TP_BIZ&#x29; AS CUR_M_BIZ_QTY  --&#x5f53;&#x671f;&#x4e1a;&#x52a1;&#x91cf;&#x6b21;&#xd;&#xa;    ,sum&#x28;T.LST_TP_BIZ&#x29; AS LST_M_BIZ_QTY  --&#x4e0a;&#x671f;&#x4e1a;&#x52a1;&#x91cf;&#x6b21;&#xd;&#xa;    ,&#x28;sum&#x28;T.CUR_TP_BIZ&#x29;-sum&#x28;T.LST_TP_BIZ&#x29;&#x29;&#x2f;sum&#x28;T.LST_TP_BIZ&#x29;&#x2a;100 AS BIZ_QTY_MOM  --&#x4e1a;&#x52a1;&#x91cf;&#x73af;&#x6bd4;&#xd;&#xa;    ,sum&#x28;T.cur_tp_income&#x29; AS CUR_M_FIN_INCOME  --&#x5f53;&#x671f;&#x8d22;&#x52a1;&#x6536;&#x5165;&#xd;&#xa;    ,sum&#x28;T.lst_tp_income&#x29; AS LST_M_FIN_INCOME  --&#x4e0a;&#x671f;&#x8d22;&#x52a1;&#x6536;&#x5165;&#xd;&#xa;    ,&#x28;sum&#x28;T.cur_tp_income&#x29;-sum&#x28;T.lst_tp_income&#x29;&#x29;&#x2f;sum&#x28;T.lst_tp_income&#x29;&#x2a;100 AS FIN_INCOME_MOM  --&#x8d22;&#x52a1;&#x6536;&#x5165;&#x73af;&#x6bd4;&#xd;&#xa;    ,sum&#x28;T.cur_tp_profit&#x29; AS CUR_M_FIN_PROFIT  --&#x5f53;&#x671f;&#x8d22;&#x52a1;&#x5229;&#x6da6;&#xd;&#xa;    ,sum&#x28;T.lst_tp_profit&#x29; AS LST_M_FIN_PROFIT  --&#x4e0a;&#x671f;&#x8d22;&#x52a1;&#x5229;&#x6da6;&#xd;&#xa;    ,&#x28;sum&#x28;T.cur_tp_profit&#x29;-sum&#x28;T.lst_tp_profit&#x29;&#x29;&#x2f;sum&#x28;T.lst_tp_profit&#x29;&#x2a;100 AS FIN_PROFIT_MOM  --&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x73af;&#x6bd4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT rgn_id&#xd;&#xa;        ,base_id&#xd;&#xa;        ,brand_id&#xd;&#xa;        ,proj_id&#xd;&#xa;        ,cur_tp_income&#xd;&#xa;        ,lst_tp_income&#xd;&#xa;        ,cur_tp_profit&#xd;&#xa;        ,lst_tp_profit&#xd;&#xa;        ,0 AS CUR_TP_BIZ&#xd;&#xa;        ,0 AS LST_TP_BIZ&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_FIN_TP_PFT_RGN_M_DTL_FIN&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT rgn_id&#xd;&#xa;        ,base_id&#xd;&#xa;        ,brand_id&#xd;&#xa;        ,proj_id&#xd;&#xa;        ,0 AS CUR_TP_INCOME&#xd;&#xa;        ,0 AS LST_TP_INCOME&#xd;&#xa;        ,0 AS CUR_TP_PROFIT&#xd;&#xa;        ,0 AS LST_TP_PROFIT&#xd;&#xa;        ,cur_tp_biz&#xd;&#xa;        ,lst_tp_biz&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_FIN_TP_PFT_RGN_M_DTL_BIZ &#xd;&#xa;    &#x29; T&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT base_id,base_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY base_id,base_name&#xd;&#xa;    &#x29; T2&#xd;&#xa;ON T.base_id &#x3d; T2.base_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.dim_brand &#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29; T3&#xd;&#xa;ON T.brand_id &#x3d; T3.brand_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT proj_id,proj_name&#xd;&#xa;    FROM DIM.dim_proj &#xd;&#xa;    GROUP BY proj_id,proj_name&#xd;&#xa;    &#x29; T4&#xd;&#xa;ON T.proj_id &#x3d; T4.proj_id&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;GROUP BY T.rgn_id,T.base_id,T.brand_id,T.proj_id,T1.rgn_name,T2.base_name,T3.brand_name,T4.proj_name&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT&#xd;&#xa;     REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;  AS MONTH_ID                  --&#x6708;&#x4efd;ID&#xd;&#xa;    ,A.RGN_ID                                                AS RNG_ID                    --&#x5927;&#x533a;ID&#xd;&#xa;    ,NVL&#x28;D1.RGN_NAME,&#x27;&#x5168;&#x90e8;&#x27;&#x29;                                   AS RNG_NAME                  --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;    ,A.BASE_ID                                               AS BASE_ID                   --&#x57fa;&#x5730;ID&#xd;&#xa;    ,NVL&#x28;D2.BASE_NAME,&#x27;&#x5168;&#x90e8;&#x27;&#x29;                                  AS BASE_NAME                 --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;    ,A.BRAND_ID                                              AS BRAND_ID                  --&#x54c1;&#x724c;ID&#xd;&#xa;    ,NVL&#x28;D3.BRAND_NAME,&#x27;&#x5168;&#x90e8;&#x27;&#x29;                                 AS BRAND_NAME                --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;	,A.PROJ_ID                                               AS PROJ_ID					  --&#x9879;&#x76ee;ID&#xd;&#xa;	,NVL&#x28;D4.PROJ_NAME,&#x27;&#x5168;&#x90e8;&#x27;&#x29;                                  AS PROJ_NAME                 --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;    ,BIZTYPE.BIZTYPE_ID                                      AS BIZ_TYPE_ID               --&#x4e1a;&#x52a1;&#x5c5e;&#x6027;ID&#xd;&#xa;    ,BIZTYPE.BIZTYPE_NAME                                    AS BIZ_TYPE_NAME             --&#x4e1a;&#x52a1;&#x5c5e;&#x6027;&#x540d;&#x79f0;&#xd;&#xa;    ,A.SHIPPING_NUM                                          AS CUR_M_BIZ_QTY             --&#x5f53;&#x671f;&#x4e1a;&#x52a1;&#x91cf;&#x6b21;&#xd;&#xa;    ,B.SHIPPING_NUM                                          AS LST_M_BIZ_QTY             --&#x4e0a;&#x671f;&#x4e1a;&#x52a1;&#x91cf;&#x6b21;&#xd;&#xa;    ,CASE WHEN NVL&#x28;B.SHIPPING_NUM,0&#x29; &#x3d;0 THEN 0 ELSE &#x28;&#x28;NVL&#x28;A.SHIPPING_NUM,0&#x29;-NVL&#x28;B.SHIPPING_NUM,0&#x29;&#x29;&#x2f;B.SHIPPING_NUM&#x29;&#x2a;100 END AS BIZ_QTY_RATE_MOM            --&#x73af;&#x6bd4;&#xd;&#xa;    ,C.FIN_INCOME                                            AS CUR_M_FIN_INCOME          --&#x5f53;&#x671f;&#x8d22;&#x52a1;&#x6536;&#x5165;&#xd;&#xa;    ,D.FIN_INCOME                                            AS LST_M_FIN_INCOME          --&#x4e0a;&#x671f;&#x8d22;&#x52a1;&#x6536;&#x5165;&#xd;&#xa;    ,CASE WHEN NVL&#x28;D.FIN_INCOME,0&#x29; &#x3d;0 THEN 0 ELSE &#x28;&#x28;NVL&#x28;C.FIN_INCOME,0&#x29;-NVL&#x28;D.FIN_INCOME,0&#x29;&#x29;&#x2f;D.FIN_INCOME&#x29;&#x2a;100 END AS FIN_INCOME_RATE_MOM             --&#x73af;&#x6bd4; &#xd;&#xa;    ,C.FIN_PROFIT                                            AS CUR_M_FIN_PROFIT          --&#x5f53;&#x671f;&#x8d22;&#x52a1;&#x5229;&#x6da6;&#xd;&#xa;    ,D.FIN_PROFIT                                            AS LST_M_FIN_PROFIT          --&#x4e0a;&#x671f;&#x8d22;&#x52a1;&#x5229;&#x6da6;&#xd;&#xa;    ,CASE WHEN NVL&#x28;D.FIN_PROFIT,0&#x29; &#x3d;0 THEN 0 ELSE &#x28;&#x28;NVL&#x28;C.FIN_PROFIT,0&#x29;-NVL&#x28;D.FIN_PROFIT,0&#x29;&#x29;&#x2f;D.FIN_PROFIT&#x29;&#x2a;100 END AS FIN_PROFIT_RATE_MOM             --&#x73af;&#x6bd4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;  AS INSERT_TIME               --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;  AS UPDATE_TIME               --&#x66f4;&#x65b0;&#x65f6;&#x95f4;        &#xd;&#xa;    ,&#x27;DMK&#x27;                                                   AS PART_SC&#xd;&#xa;    ,REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;  AS PART_DT &#xd;&#xa;FROM&#xd;&#xa;&#x28;&#xd;&#xa;--source table &#x3a; DMK_FIN_INCOMECOST_M, DMK_TP_OPERATE_D&#xd;&#xa;--join  condition &#x3a; oc_id, rgn_id, base_id, brand_id, proj_id&#xd;&#xa;--&#x5229;&#x6da6;&#x603b;&#x989d; &#x3d; &#x6574;&#x8f66;&#x603b;&#x6536;&#x5165;-&#x6574;&#x8f66;&#x603b;&#x6210;&#x672c;&#xd;&#xa;--&#x6536;&#x5165;&#x603b;&#x989d; &#x3d; &#x6574;&#x8f66;&#x7684;&#x5b9e;&#x9645;&#x6536;&#x5165;&#x3d;&#x8425;&#x4e1a;&#x6536;&#x5165;&#xff08;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x6536;&#x5165;&#x2b;&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x6536;&#x5165;&#x2b;&#x9500;&#x552e;&#x6536;&#x5165;&#xff09;&#x2b;&#x8425;&#x4e1a;&#x5916;&#x6536;&#x5165;&#xff08;&#x624b;&#x5de5;&#x6570;&#x636e;&#x8425;&#x4e1a;&#x5916;&#x6536;&#x5165;&#x6570;&#x636e;&#xff09;&#xd;&#xa;--THIS PERIOD&#xd;&#xa;SELECT NVL&#x28;OC_ID,&#x27;ALL&#x27;&#x29; AS OC_ID&#xd;&#xa;     , NVL&#x28;RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;     , NVL&#x28;BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;     , NVL&#x28;BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;     , NVL&#x28;PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;     , SUM&#x28;SHIPPING_NUM&#x29; AS SHIPPING_NUM&#xd;&#xa;FROM DMK.DMK_TP_OPERATE_D&#xd;&#xa;WHERE PART_SC &#x3d; &#x27;DW&#x27; &#xd;&#xa;  AND PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;GROUP BY&#xd;&#xa;       NVL&#x28;OC_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;RGN_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;BASE_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;BRAND_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;PROJ_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;WITH CUBE&#xd;&#xa;&#x29;A&#xd;&#xa;INNER JOIN&#xd;&#xa;&#x28;--LAST PERIOD&#xd;&#xa;SELECT NVL&#x28;OC_ID,&#x27;ALL&#x27;&#x29; AS OC_ID&#xd;&#xa;     , NVL&#x28;RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;     , NVL&#x28;BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;     , NVL&#x28;BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;     , NVL&#x28;PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;     , SUM&#x28;SHIPPING_NUM&#x29; AS SHIPPING_NUM&#xd;&#xa;FROM DMK.DMK_TP_OPERATE_D&#xd;&#xa;WHERE PART_SC &#x3d; &#x27;DW&#x27; &#xd;&#xa;  AND PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;GROUP BY&#xd;&#xa;       NVL&#x28;OC_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;RGN_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;BASE_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;BRAND_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;PROJ_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;WITH CUBE&#xd;&#xa;&#x29;B&#xd;&#xa;ON A.OC_ID &#x3d; B.OC_ID&#xd;&#xa;AND A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;AND A.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;AND A.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;AND A.PROJ_ID &#x3d; B.PROJ_ID&#xd;&#xa;INNER JOIN&#xd;&#xa;&#x28;--THIS PERIOD&#xd;&#xa;SELECT NVL&#x28;OC_ID,&#x27;ALL&#x27;&#x29; AS OC_ID&#xd;&#xa;     , NVL&#x28;RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;     , NVL&#x28;BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;     , NVL&#x28;BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;     , NVL&#x28;PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;     , SUM&#x28;BIZ_INCOME_AMT&#x29; &#x2b; SUM&#x28;OUT_BIZ_INCOME_AMT&#x29; AS FIN_INCOME&#xd;&#xa;     , SUM&#x28;VCL_INCOME_AMT&#x29; - SUM&#x28;VCL_COST_AMT&#x29; AS FIN_PROFIT&#xd;&#xa;FROM DMK.DMK_FIN_INCOMECOST_M&#xd;&#xa;WHERE PART_SC &#x3d; &#x27;DW&#x27; &#xd;&#xa;  AND PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;GROUP BY&#xd;&#xa;       NVL&#x28;OC_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;RGN_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;BASE_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;BRAND_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;PROJ_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;WITH CUBE&#xd;&#xa;&#x29;C&#xd;&#xa;ON A.OC_ID &#x3d; C.OC_ID&#xd;&#xa;AND A.RGN_ID &#x3d; C.RGN_ID&#xd;&#xa;AND A.BASE_ID &#x3d; C.BASE_ID&#xd;&#xa;AND A.BRAND_ID &#x3d; C.BRAND_ID&#xd;&#xa;AND A.PROJ_ID &#x3d; C.PROJ_ID&#xd;&#xa;INNER JOIN&#xd;&#xa;&#x28;--LAST PERIOD&#xd;&#xa;SELECT NVL&#x28;OC_ID,&#x27;ALL&#x27;&#x29; AS OC_ID&#xd;&#xa;     , NVL&#x28;RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;     , NVL&#x28;BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;     , NVL&#x28;BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;     , NVL&#x28;PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;     , SUM&#x28;BIZ_INCOME_AMT&#x29; &#x2b; SUM&#x28;OUT_BIZ_INCOME_AMT&#x29; AS FIN_INCOME&#xd;&#xa;     , SUM&#x28;VCL_INCOME_AMT&#x29; - SUM&#x28;VCL_COST_AMT&#x29; AS FIN_PROFIT&#xd;&#xa;FROM DMK.DMK_FIN_INCOMECOST_M&#xd;&#xa;WHERE PART_SC &#x3d; &#x27;DW&#x27; &#xd;&#xa;  AND PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;GROUP BY&#xd;&#xa;       NVL&#x28;OC_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;RGN_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;BASE_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;BRAND_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;     , NVL&#x28;PROJ_ID,&#x27;ALL&#x27;&#x29;  &#xd;&#xa;WITH CUBE&#xd;&#xa;&#x29;D&#xd;&#xa;ON A.OC_ID &#x3d; D.OC_ID&#xd;&#xa;AND A.RGN_ID &#x3d; D.RGN_ID&#xd;&#xa;AND A.BASE_ID &#x3d; D.BASE_ID&#xd;&#xa;AND A.BRAND_ID &#x3d; D.BRAND_ID&#xd;&#xa;AND A.PROJ_ID &#x3d; D.PROJ_ID&#xd;&#xa;LEFT JOIN DIM.DIM_BIZTYPE BIZTYPE&#xd;&#xa;ON BIZTYPE.BIZTYPE_NAME &#x3d; &#x27;&#x8fd0;&#x8f93;&#x27;&#xd;&#xa;AND BIZTYPE.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;LEFT JOIN &#x28;SELECT RGN_ID, RGN_NAME FROM DIM.DIM_ORG WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27; GROUP BY RGN_ID, RGN_NAME&#x29; D1&#xd;&#xa;ON A.RGN_ID &#x3d; D1.RGN_ID&#xd;&#xa;LEFT JOIN &#x28;SELECT BASE_ID, BASE_NAME FROM DIM.DIM_ORG WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27; GROUP BY BASE_ID, BASE_NAME&#x29; D2&#xd;&#xa;ON A.BASE_ID &#x3d; D2.BASE_ID&#xd;&#xa;LEFT JOIN DIM.DIM_BRAND D3&#xd;&#xa;ON A.BRAND_ID &#x3d; D3.BRAND_ID&#xd;&#xa;AND D3.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;LEFT JOIN &#x28;SELECT PROJ_ID,PROJ_NAME FROM DIM.DIM_PROJ WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27; GROUP BY PROJ_ID,PROJ_NAME&#x29; D4&#xd;&#xa;ON A.PROJ_ID &#x3d; D4.PROJ_ID&#xd;&#xa;--BIZ_INCOME_AMT	&#x8425;&#x4e1a;&#x6536;&#x5165;&#xd;&#xa;--OUT_BIZ_INCOME_AMT	&#x8425;&#x4e1a;&#x5916;&#x6536;&#x5165;&#xd;&#xa;--BIZ_COST_AMT	&#x8425;&#x4e1a;&#x6210;&#x672c;&#xd;&#xa;--OUT_BIZ_COST_AMT	&#x8425;&#x4e1a;&#x5916;&#x652f;&#x51fa;&#xd;&#xa;--VCL_INCOME_TGT_AMT	&#x6574;&#x8f66;&#x6536;&#x5165;&#x76ee;&#x6807;&#xd;&#xa;--VCL_INCOME_AMT	&#x6574;&#x8f66;&#x6536;&#x5165;&#xd;&#xa;--VCL_COST_AMT	&#x6574;&#x8f66;&#x6210;&#x672c;&#xd;&#xa;&#xd;&#xa;--SHIPPING_NUM&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
