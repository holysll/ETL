      <name>TO_FIN_DETAIL</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_FIN_TP_PFT_RGN_M_DTL_FIN&#xd;&#xa;SELECT coalesce&#x28;A.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;A.BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;A.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;A.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,sum&#x28;A.CUR_TP_INCOME&#x29; AS CUR_TP_INCOME&#xd;&#xa;    ,sum&#x28;A.LST_TP_INCOME&#x29; AS LST_TP_INCOME&#xd;&#xa;    ,sum&#x28;A.CUR_TP_PROFIT&#x29; AS CUR_TP_PROFIT&#xd;&#xa;    ,sum&#x28;A.LST_TP_PROFIT&#x29; AS LST_TP_PROFIT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,SUM&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN tp_biz_income_amt ELSE 0 END&#x29; AS CUR_TP_INCOME  --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x6536;&#x5165;&#xd;&#xa;        ,SUM&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN tp_biz_income_amt ELSE 0 END&#x29; AS LST_TP_INCOME  --&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x6536;&#x5165;&#xd;&#xa;        ,SUM&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN tp_profit_amt ELSE 0 END&#x29; AS CUR_TP_PROFIT  --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x5229;&#x6da6;&#xd;&#xa;        ,SUM&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN tp_profit_amt ELSE 0 END&#x29; AS LST_TP_PROFIT  --&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x5229;&#x6da6;&#xd;&#xa;    FROM DMK.dmk_fin_incomecost_m&#xd;&#xa;    WHERE part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;        OR part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; A&#xd;&#xa;    GROUP BY A.RGN_ID,A.BASE_ID,A.BRAND_ID,A.PROJ_ID WITH CUBE</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
