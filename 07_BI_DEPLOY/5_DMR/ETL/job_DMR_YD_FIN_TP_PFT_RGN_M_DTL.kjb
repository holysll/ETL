      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_FIN_TP_PFT_RGN_M_DTL_FIN&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_FIN_TP_PFT_RGN_M_DTL_FIN&#x28;&#xd;&#xa;RGN_ID	STRING	COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID STRING    COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID STRING   COMMENT&#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,PROJ_ID STRING		COMMENT&#x27;&#x9879;&#x76ee;ID&#x27;&#xd;&#xa;,CUR_TP_INCOME	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x6536;&#x5165;&#x27;&#xd;&#xa;,LST_TP_INCOME	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x6536;&#x5165;&#x27;&#xd;&#xa;,CUR_TP_PROFIT	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x5229;&#x6da6;&#x27;&#xd;&#xa;,LST_TP_PROFIT	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x5229;&#x6da6;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x660e;&#x7ec6;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_FIN_TP_PFT_RGN_M_DTL_BIZ&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_FIN_TP_PFT_RGN_M_DTL_BIZ&#x28;&#xd;&#xa;RGN_ID	STRING	COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID STRING    COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID STRING   COMMENT&#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,PROJ_ID STRING		COMMENT&#x27;&#x9879;&#x76ee;ID&#x27;&#xd;&#xa;,CUR_TP_BIZ	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;,LST_TP_BIZ	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x660e;&#x7ec6;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;</sql>
