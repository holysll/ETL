    <entry>
      <name>TMP1</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_STR_ANALYSIS_MON_01&#xd;&#xa;SELECT T.RGN_ID&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,T.STR_ID&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,T.MODEL_ID&#xd;&#xa;      ,&#x27;&#x79fb;&#x5e93;&#x27; AS INDEX_NAME&#xd;&#xa;      ,SUM&#x28;MOVEOUT_QTY&#x29; AS FENZI&#xd;&#xa;      ,NULL AS FENMU&#xd;&#xa;      ,NULL AS FENZI_LM&#xd;&#xa;      ,NULL AS FENZI_LYM&#xd;&#xa;  FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;      WHERE 1 &#x3d; 1&#xd;&#xa;        AND SUBSTRING&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;          GROUP BY T.RGN_ID&#xd;&#xa;                  ,T.BASE_ID&#xd;&#xa;                  ,T.STR_ID&#xd;&#xa;                  ,T.BRAND_ID&#xd;&#xa;                  ,T.MODEL_ID&#xd;&#xa;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>96</xloc>
      <yloc>288</yloc>
      </entry>
    <entry>
      <name>CREATE_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DMR_STR_ANALYSIS_MON_01&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_STR_ANALYSIS_MON_01&#x28;&#xd;&#xa;RGN_ID             STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID            STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;STR_ID             STRING        COMMENT  &#x27;&#x4ed3;&#x5e93;ID&#x27;,&#xd;&#xa;BRAND_ID           STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;MODEL_ID           STRING        COMMENT  &#x27;&#x8f66;&#x578b;ID&#x27;,&#xd;&#xa;INDEX_NAME         STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;FENZI              DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x5206;&#x5b50;&#x27;,&#xd;&#xa;FENMU              DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x5206;&#x6bcd;&#x27;,&#xd;&#xa;FENZI_LM           DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x5206;&#x5b50;&#x4e0a;&#x671f;&#x27;,&#xd;&#xa;FENZI_LYM          DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x5206;&#x5b50;&#x540c;&#x671f;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x4ed3;&#x50a8;&#x5206;&#x6790;-&#x4ed3;&#x50a8;&#x5206;&#x6790;&#x28;&#x6708;&#x29;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DMR_STR_ANALYSIS_MON_02&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_STR_ANALYSIS_MON_02&#x28;&#xd;&#xa;RGN_ID             STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID            STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;STR_ID             STRING        COMMENT  &#x27;&#x4ed3;&#x5e93;ID&#x27;,&#xd;&#xa;BRAND_ID           S