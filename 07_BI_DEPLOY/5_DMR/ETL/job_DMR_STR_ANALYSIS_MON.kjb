      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_STR_ANALYSIS_MON_10&#xd;&#xa;SELECT NVL&#x28;T1.RGN_ID,T2.RGN_ID&#x29; AS RGN_ID&#xd;&#xa;      ,NVL&#x28;T1.BASE_ID,T2.BASE_ID&#x29; AS BASE_ID&#xd;&#xa;      ,&#x27;-1&#x27; AS STR_ID&#xd;&#xa;      ,NVL&#x28;T1.BRAND_ID,T2.BRAND_ID&#x29; AS BRAND_ID&#xd;&#xa;      ,&#x27;-1&#x27; AS MODEL_ID&#xd;&#xa;      ,&#x27;&#x5728;&#x5e93;&#x8d28;&#x635f;&#x27;&#xd;&#xa;      ,NVL&#x28;T2.FENZI,0&#x29; AS FENZI&#xd;&#xa;      ,NVL&#x28;T1.FENMU,0&#x29; AS FENMU&#xd;&#xa;      ,NULL AS FENZI_LM&#xd;&#xa;      ,NULL AS FENZI_LYM&#xd;&#xa;  FROM &#x28;SELECT RGN_ID,&#xd;&#xa;               BASE_ID,&#xd;&#xa;               BRAND_ID,&#xd;&#xa;               SUM&#x28;BIZ_QTY&#x29; AS FENMU&#xd;&#xa;          FROM DMK.DMK_STR_PROJBIZQTY_M&#xd;&#xa;            WHERE SUBSTR&#x28;PERIOD_WID,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                GROUP BY RGN_ID,&#xd;&#xa;                         BASE_ID,&#xd;&#xa;                         BRAND_ID&#xd;&#xa;       &#x29; T1&#xd;&#xa;    FULL JOIN &#x28;SELECT NVL&#x28;RGN.RGN_ID,&#x27;-1&#x27;&#x29; AS RGN_ID,&#xd;&#xa;                      NVL&#x28;BAS.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID,&#xd;&#xa;                      NVL&#x28;BRD.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID,&#xd;&#xa;                      SUM&#x28;LOSS_CNT&#x29; AS FENZI&#xd;&#xa;                 FROM ODS_BLPT.T_HAND_IN_STOCK_LOSS_LV LOS&#xd;&#xa;                   LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;                          ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                         AND LOS.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;                   LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;                          ON BAS.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                         AND LOS.BASE_NAME &#x3d; BAS.BASE_NAME&#xd;&#xa;                   LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;                          ON LOS.BRAND_NAME &#x3d; BRD.BRAND_NAME&#xd;&#xa;                     WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;CHECK_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                         GROUP BY RGN.RGN_ID,&#xd;&#xa;                                  BAS.BASE_ID,&#xd;&#xa;                                  BRD.BRAND_ID&#xd;&#xa;              &#x29; T2 ON T1.RGN_ID &#x3d; T2.RGN_ID&#xd;&#xa;                  AND T1.BASE_ID &#x3d; T2.BASE_ID&#xd;&#xa;                  AND T1.BRAND_ID &#x3d; T2.BRAND_ID&#xd;&#xa;&#x3b;</sql>
