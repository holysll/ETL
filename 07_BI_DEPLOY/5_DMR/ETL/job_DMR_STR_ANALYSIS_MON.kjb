      <name>TMP6</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_STR_ANALYSIS_MON_06&#xd;&#xa;SELECT T.RGN_ID&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,&#x27;-1&#x27; AS STR_ID&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,&#x27;-1&#x27; AS MODEL_ID&#xd;&#xa;      ,&#x27;PDI&#x27;&#xd;&#xa;      ,SUM&#x28;T.FENZI&#x29; AS FENZI&#xd;&#xa;      ,SUM&#x28;T.FENMU&#x29; AS FENMU&#xd;&#xa;      ,SUM&#x28;T.FENZI_LM&#x29; AS FENZI_LM&#xd;&#xa;      ,SUM&#x28;T.FENZI_LYM&#x29; AS FENZI_LYM&#xd;&#xa;  FROM &#x28;SELECT A.RGN_ID&#xd;&#xa;              ,A.BASE_ID&#xd;&#xa;              ,A.BRAND_ID&#xd;&#xa;              ,B.FENZI&#xd;&#xa;              ,A.FENMU&#xd;&#xa;              ,0          AS FENZI_LM&#xd;&#xa;              ,0          AS FENZI_LYM&#xd;&#xa;          FROM &#x28;SELECT RGN_ID&#xd;&#xa;                      ,BASE_ID&#xd;&#xa;                      ,BRAND_ID&#xd;&#xa;                      ,SUM&#x28;INSTOCK_QTY&#x29; AS FENMU&#xd;&#xa;                  FROM DMK.DMK_STR_OPERQTY_D&#xd;&#xa;                 WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -0&#x29;,&#xd;&#xa;                                             &#x27;-&#x27;,&#xd;&#xa;                                             &#x27;&#x27;&#x29;,&#xd;&#xa;                              1,&#xd;&#xa;                              6&#x29;&#xd;&#xa;                 GROUP BY RGN_ID&#xd;&#xa;                         ,BASE_ID&#xd;&#xa;                         ,BRAND_ID&#x29; A&#xd;&#xa;          LEFT JOIN &#x28;SELECT RGN.RGN_ID&#xd;&#xa;                          ,BAS.BASE_ID&#xd;&#xa;                          ,BRD.BRAND_ID&#xd;&#xa;                          ,SUM&#x28;TOTAL_CNT&#x29; AS FENZI&#xd;&#xa;                      FROM ODS_BLPT.T_HAND_PDI_CHECK_LV PDI&#xd;&#xa;                      LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;                        ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                       AND PDI.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;                      LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;                        ON BAS.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                       AND PDI.BASE_NAME &#x3d; BAS.BASE_NAME&#xd;&#xa;                      LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;                        ON PDI.BRAND_NAME &#x3d; BRD.BRAND_NAME&#xd;&#xa;                     WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                            -0&#x29;,&#xd;&#xa;                                                 &#x27;-&#x27;,&#xd;&#xa;                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29;&#xd;&#xa;                     GROUP BY RGN.RGN_ID&#xd;&#xa;                             ,BAS.BASE_ID&#xd;&#xa;                             ,BRD.BRAND_ID&#x29; B&#xd;&#xa;            ON A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;           AND A.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;           AND A.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT RGN.RGN_ID&#xd;&#xa;              ,BAS.BASE_ID&#xd;&#xa;              ,BRD.BRAND_ID&#xd;&#xa;              ,0 AS FENZI&#xd;&#xa;              ,0 AS FENMU&#xd;&#xa;              ,SUM&#x28;TOTAL_CNT&#x29; AS FENZI_LM&#xd;&#xa;              ,0 AS FENZI_LYM&#xd;&#xa;          FROM ODS_BLPT.T_HAND_PDI_CHECK_LV PDI&#xd;&#xa;          LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;            ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;           AND PDI.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;          LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;            ON BAS.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND PDI.BASE_NAME &#x3d; BAS.BASE_NAME&#xd;&#xa;          LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;            ON PDI.BRAND_NAME &#x3d; BRD.BRAND_NAME&#xd;&#xa;         WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                     &#x27;-&#x27;,&#xd;&#xa;                                     &#x27;&#x27;&#x29;,&#xd;&#xa;                      1,&#xd;&#xa;                      6&#x29;&#xd;&#xa;         GROUP BY RGN.RGN_ID&#xd;&#xa;                 ,BAS.BASE_ID&#xd;&#xa;                 ,BRD.BRAND_ID&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT RGN.RGN_ID&#xd;&#xa;              ,BAS.BASE_ID&#xd;&#xa;              ,BRD.BRAND_ID&#xd;&#xa;              ,0 AS FENZI&#xd;&#xa;              ,0 AS FENMU&#xd;&#xa;              ,0 AS FENZI_LM&#xd;&#xa;              ,SUM&#x28;TOTAL_CNT&#x29; AS FENZI_LYM&#xd;&#xa;          FROM ODS_BLPT.T_HAND_PDI_CHECK_LV PDI&#xd;&#xa;          LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;            ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;           AND PDI.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;          LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;            ON BAS.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND PDI.BASE_NAME &#x3d; BAS.BASE_NAME&#xd;&#xa;          LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;            ON PDI.BRAND_NAME &#x3d; BRD.BRAND_NAME&#xd;&#xa;         WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#xd;&#xa;                                     &#x27;-&#x27;,&#xd;&#xa;                                     &#x27;&#x27;&#x29;,&#xd;&#xa;                      1,&#xd;&#xa;                      6&#x29;&#xd;&#xa;         GROUP BY RGN.RGN_ID&#xd;&#xa;                 ,BAS.BASE_ID&#xd;&#xa;                 ,BRD.BRAND_ID&#x29; T&#xd;&#xa; GROUP BY T.RGN_ID&#xd;&#xa;         ,T.BASE_ID&#xd;&#xa;         ,T.BRAND_ID&#x3b;&#xd;&#xa;</sql>
