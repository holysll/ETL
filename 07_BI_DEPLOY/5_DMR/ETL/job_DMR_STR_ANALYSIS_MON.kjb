    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_STR_ANALYSIS_MON PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; AS PERIOD_WID,&#xd;&#xa;       NVL&#x28;RGN.RGN_ID,&#x27;-1&#x27;&#x29; AS RGN_ID,&#xd;&#xa;       NVL&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME,&#xd;&#xa;       NVL&#x28;BAS.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID,&#xd;&#xa;       NVL&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME,&#xd;&#xa;       NVL&#x28;STR.STR_ID,&#x27;-1&#x27;&#x29; AS STR_ID,&#xd;&#xa;       NVL&#x28;STR.STR_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS STR_NAME,&#xd;&#xa;       NVL&#x28;BRD.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID,&#xd;&#xa;       NVL&#x28;BRD.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME,&#xd;&#xa;       NVL&#x28;MDL.MODEL_ID,&#x27;-1&#x27;&#x29; AS MODEL_ID,&#xd;&#xa;       MDL.MODEL_NAME,&#xd;&#xa;       TMP.INDEX_NAME,&#xd;&#xa;       TMP.FENZI,&#xd;&#xa;       TMP.FENMU,&#xd;&#xa;       TMP.FENZI_LM,&#xd;&#xa;       TMP.FENZI_LYM,&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;&#x29; AS W_INSERT_DT, --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;       &#x27;DW&#x27;                            AS PART_SC,     --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;       REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM &#x28;SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_ANALYSIS_MON_01&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_ANALYSIS_MON_02&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_ANALYSIS_MON_03&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_ANALYSIS_MON_04&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_ANALYSIS_MON_05&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_ANALYSIS_MON_06&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_ANALYSIS_MON_07&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_ANALYSIS_MON_08&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_ANALYSIS_MON_09&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_ANALYSIS_MON_10&#xd;&#xa;       &#x29; TMP&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;		SELECT RGN_ID,RGN_NAME&#xd;&#xa;		FROM DIM.DIM_ORG&#xd;&#xa;		WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;		GROUP BY RGN_ID,RGN_NAME&#xd;&#xa;	&#x29; RGN&#xd;&#xa;           ON NVL&#x28;TMP.RGN_ID,&#x27;-1&#x27;&#x29; &#x3d; RGN.RGN_ID&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;		SELECT BASE_ID,BASE_NAME&#xd;&#xa;		FROM DIM.DIM_ORG&#xd;&#xa;		WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;		GROUP BY BASE_ID,BASE_NAME&#xd;&#xa;	&#x29; BAS&#xd;&#xa;           ON NVL&#x28;TMP.BASE_ID,&#x27;-1&#x27;&#x29; &#x3d; BAS.BASE_ID&#xd;&#xa;   LEFT JOIN &#x28;&#xd;&#xa;		SELECT STR_ID,STR_NAME&#xd;&#xa;		FROM DIM.DIM_ORG&#xd;&#xa;		WHERE ORG_TYPE &#x3d; &#x27;S&#x27;&#xd;&#xa;		GROUP BY STR_ID,STR_NAME&#xd;&#xa;	&#x29; STR&#xd;&#xa;           ON NVL&#x28;TMP.STR_ID,&#x27;-1&#x27;&#x29; &#x3d; STR.STR_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;           ON NVL&#x28;TMP.BRAND_ID,&#x27;-1&#x27;&#x29; &#x3d; BRD.BRAND_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_MODEL MDL&#xd;&#xa;           ON NVL&#x28;TMP.MODEL_ID,&#x27;-1&#x27;&#x29; &#x3d; MDL.MODEL_ID&#xd;&#xa;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
