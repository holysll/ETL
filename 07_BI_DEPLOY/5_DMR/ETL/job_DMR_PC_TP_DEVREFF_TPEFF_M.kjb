    <entry>
      <name>TO_TEMP</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_DEVREFF_TPEFF_M_1&#xd;&#xa;SELECT &#x2a; FROM &#xd;&#xa;&#x28;&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;T.UNLOAD_DAY, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,T.SEGMETN_TRANSPORTMODE AS TPMODE_ID&#xd;&#xa;      ,&#x27;-1&#x27; AS ROAD_SECTION_ID&#xd;&#xa;      ,T.SUPPLIER_ID&#xd;&#xa;      ,SUBSTRING&#x28;PU.POWER_UNIT_TYPE_GID, INSTR&#x28;PU.POWER_UNIT_TYPE_GID, &#x27;.&#x27;&#x29; &#x2b; 1, 10&#x29; AS SHIPMENT_TOOL_TYPE_ID&#xd;&#xa;      ,COUNT&#x28;T.ORDER_RELEASE_GID&#x29; AS ORDER_QTY&#xd;&#xa;  FROM DW.DW_TP_ORDERMOVEMENT T&#xd;&#xa;  LEFT JOIN DIM.DIM_POWER_UNIT PU&#xd;&#xa;    ON T.LIC_NO &#x3d; PU.POWER_UNIT_ID&#xd;&#xa;   AND PU.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa; WHERE T.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;   AND SUBSTRING&#x28;T.UNLOAD_DAY, 1, 7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;&#xd;&#xa; GROUP BY SUBSTRING&#x28;T.UNLOAD_DAY, 1, 7&#x29;&#xd;&#xa;         ,T.RGN_ID&#xd;&#xa;         ,T.SEGMETN_TRANSPORTMODE&#xd;&#xa;         ,T.SUPPLIER_ID&#xd;&#xa;         ,PU.POWER_UNIT_TYPE_GID&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;T.FACTORY_DAY, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,T.SEGMETN_TRANSPORTMODE AS TPMODE_ID&#xd;&#xa;      ,&#x27;-1&#x27; AS ROAD_SECTION_ID&#xd;&#xa;      ,T.SUPPLIER_ID&#xd;&#xa;      ,SUBSTRING&#x28;PU.POWER_UNIT_TYPE_GID, INSTR&#x28;PU.POWER_UNIT_TYPE_GID, &#x27;.&#x27;&#x29; &#x2b; 1, 10&#x29; AS SHIPMENT_TOOL_TYPE_ID&#xd;&#xa;      ,COUNT&#x28;T.ORDER_RELEASE_GID&#x29; AS ORDER_QTY&#xd;&#xa;  FROM DW.DW_TP_ORDERMOVEMENT T&#xd;&#xa;  LEFT JOIN DIM.DIM_POWER_UNIT PU&#xd;&#xa;    ON T.LIC_NO &#x3d; PU.POWER_UNIT_ID&#xd;&#xa;   AND PU.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa; WHERE T.BRAND_ID &#x3c;&#x3e; &#x27;CA&#x27;&#xd;&#xa;   AND SUBSTRING&#x28;T.FACTORY_DAY, 1, 7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;&#xd;&#xa; GROUP BY SUBSTRING&#x28;T.FACTORY_DAY, 1, 7&#x29;&#xd;&#xa;         ,T.RGN_ID&#xd;&#xa;         ,T.SEGMETN_TRANSPORTMODE&#xd;&#xa;         ,T.SUPPLIER_ID&#xd;&#xa;         ,PU.POWER_UNIT_TYPE_GID&#xd;&#xa;&#x29;A&#x3b;&#xd;&#xa;</sql>
