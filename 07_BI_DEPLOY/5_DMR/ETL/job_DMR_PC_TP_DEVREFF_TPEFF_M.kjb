      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_DEVREFF_TPEFF_M_1&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_DEVREFF_TPEFF_M_1&#xd;&#xa;&#x28;&#xd;&#xa;    PERIOD_WID             STRING             COMMENT &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;    RGN_ID                 STRING             COMMENT &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;    TPMODE_ID              STRING             COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;,&#xd;&#xa;    ROAD_SECTION_ID        STRING             COMMENT &#x27;&#x8def;&#x6bb5;ID&#x27;,&#xd;&#xa;    SUPPLIER_ID            STRING             COMMENT &#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;,&#xd;&#xa;    SHIPMENT_TOOL_TYPE_ID  STRING             COMMENT &#x27;&#x8fd0;&#x8f93;&#x5de5;&#x5177;&#x7c7b;&#x578b;ID&#x27;,&#xd;&#xa;    ORDER_QTY              BIGINT             COMMENT &#x27;&#x8ba2;&#x5355;&#x6570;&#x91cf;&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;COMMENT &#x27;temporary table for DMR_PC_TP_DEVREFF_TPEFF_M&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27; STORED AS PARQUET&#x3b;</sql>
