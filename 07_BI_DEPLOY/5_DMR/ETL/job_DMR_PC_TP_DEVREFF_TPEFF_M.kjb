    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--target table &#x3a; DMP_PC_TP_DEVREFF_TPEFF_M &#x4ea4;&#x4ed8;&#x4e0e;&#x6548;&#x7387;&#x7ba1;&#x7406;&#xff08;&#x6708;&#xff09;-&#x8fd0;&#x8f93;&#x6548;&#x7387;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_DEVREFF_TPEFF_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID&#xd;&#xa;    ,T.SUPPLIER_ID&#xd;&#xa;    ,T.SUPPLER_NAME&#xd;&#xa;    ,sum&#x28;CUR_M_PAKING_QTY&#x29; AS CUR_M_PAKING_QTY&#xd;&#xa;    ,sum&#x28;CUR_M_OT_PAKING_QTY&#x29; AS CUR_M_OT_PAKING_QTY&#xd;&#xa;    ,sum&#x28;CUR_M_SHIPPING_QTY&#x29; AS CUR_M_SHIPPING_QTY&#xd;&#xa;    ,sum&#x28;CUR_M_OT_SHIPPING_QTY&#x29; AS CUR_M_OT_SHIPPING_QTY&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;ODS&#x27;                                                    AS PART_SC&#xd;&#xa;    ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;B.servprov_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID &#xd;&#xa;        ,coalesce&#x28;B.servprov_name,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS SUPPLER_NAME&#xd;&#xa;        ,count&#x28;A.vin&#x29; AS CUR_M_PAKING_QTY&#xd;&#xa;        ,count&#x28;CASE WHEN A.status IN &#x28;&#x27;&#x51c6;&#x65f6;&#x27;,&#x27;&#x63d0;&#x524d;&#x27;&#x29; THEN A.vin ELSE NULL END&#x29; AS CUR_M_OT_PAKING_QTY&#xd;&#xa;        ,0 AS CUR_M_SHIPPING_QTY&#xd;&#xa;        ,0 AS CUR_M_OT_SHIPPING_QTY&#xd;&#xa;    FROM ods_blpt.T_HAND_ON_TIME_BOX_LV A  &#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT servprov_id,servprov_code,servprov_name&#xd;&#xa;        FROM DIM.dim_servprov&#xd;&#xa;        GROUP BY servprov_id,servprov_code,servprov_name&#xd;&#xa;        &#x29; B&#xd;&#xa;    ON case when A.servprov_gid in &#x28;&#x27;&#x91cd;&#x5e86;&#x5f7c;&#x6d0b;&#x27;,&#x27;&#x5b89;&#x5409;&#x946b;&#x7965;&#x6c47;&#x27;,&#x27;&#x5b89;&#x5409;&#x91d1;&#x5802;&#x27;,&#x27;&#x5b89;&#x5409;&#x4e2d;&#x96c6;&#x27;,&#x27;&#x5b89;&#x5409;&#x5f7c;&#x6d0b;&#x27;,&#x27;&#x5b89;&#x5409;&#x5317;&#x4eac;&#x8bda;&#x901a;&#x27;&#x29; then &#x27;&#x91cd;&#x5e86;&#x5f7c;&#x6d0b;&#x27;&#xd;&#xa;            when A.servprov_gid in &#x28;&#x27;&#x91cd;&#x5e86;&#x4e2d;&#x96c6;&#x27;,&#x27;&#x4e2d;&#x8054;&#x7269;&#x6d41;&#x27;,&#x27;&#x5929;&#x6d25;&#x91d1;&#x5802;&#x27;,&#x27;&#x91cd;&#x5e86;&#x8fdc;&#x5fd7;&#x8fbe;&#x27;,&#x27;&#x91cd;&#x5e86;&#x946b;&#x7965;&#x6c47;&#x27;,&#x27;&#x5317;&#x4eac;&#x8bda;&#x901a;&#x27;&#x29; then &#x27;&#x91cd;&#x5e86;&#x4e2d;&#x96c6;&#x27; end &#x3d; B.servprov_code&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;A.dlvr_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;B.servprov_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;B.servprov_name,&#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;B.servprov_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID &#xd;&#xa;        ,coalesce&#x28;B.servprov_name,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS SUPPLER_NAME&#xd;&#xa;        ,0 AS CUR_M_PAKING_QTY&#xd;&#xa;        ,0 AS CUR_M_OT_PAKING_QTY&#xd;&#xa;        ,count&#x28;A.vin&#x29; AS CUR_M_SHIPPING_QTY&#xd;&#xa;        ,count&#x28;CASE WHEN A.status IN &#x28;&#x27;&#x51c6;&#x65f6;&#x27;,&#x27;&#x63d0;&#x524d;&#x27;&#x29; THEN A.vin ELSE NULL END&#x29; AS CUR_M_OT_SHIPPING_QTY&#xd;&#xa;    FROM ods_blpt.T_HAND_ON_TIME_SHIP_LV A  &#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT servprov_id,servprov_code,servprov_name&#xd;&#xa;        FROM DIM.dim_servprov&#xd;&#xa;        GROUP BY servprov_id,servprov_code,servprov_name&#xd;&#xa;        &#x29; B&#xd;&#xa;    ON &#x27;&#x6c11;&#x751f;&#x7269;&#x6d41;&#x27; &#x3d; B.servprov_code&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;A.dlvr_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;B.servprov_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;B.servprov_name,&#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.SUPPLIER_ID,T.SUPPLER_NAME&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT S.PERIOD_WID&#xd;&#xa;      ,SERVPROV.SERVPROV_ID AS SUPPLIER_ID&#xd;&#xa;      ,SERVPROV.SERVPROV_NAME AS SUPPLIER_NAME&#xd;&#xa;      ,S.ORDER_QTY AS ORDER_QTY&#xd;&#xa;      ,T.ONTIME_PARKING_RATE&#xd;&#xa;      ,T.ONTIME_SHIPING_RATE&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;MM&#x3a;SS&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;ODS&#x27; AS PART_SC&#xd;&#xa;      ,S.PERIOD_WID AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT SERVPROV_GID AS SUPPLIER_NAME&#xd;&#xa;              ,SUM&#x28;ONTIME_PARKING_RATE&#x29; AS ONTIME_PARKING_RATE&#xd;&#xa;              ,SUM&#x28;ONTIME_SHIPING_RATE&#x29; AS ONTIME_SHIPING_RATE&#xd;&#xa;          FROM &#x28;SELECT SERVPROV_GID&#xd;&#xa;                      ,ROUND&#x28;SUM&#x28;FENZI&#x29; &#x2f; SUM&#x28;FENMU&#x29;, 2&#x29; AS ONTIME_PARKING_RATE&#xd;&#xa;                      ,0 AS ONTIME_SHIPING_RATE&#xd;&#xa;                  FROM &#x28;SELECT &#x27;&#x91cd;&#x5e86;&#x5f7c;&#x6d0b;&#x27; AS SERVPROV_GID&#xd;&#xa;                              ,COUNT&#x28;VIN&#x29; AS FENZI&#xd;&#xa;                              ,0 AS FENMU&#xd;&#xa;                          FROM ODS_BLPT.T_HAND_ON_TIME_BOX_LV T&#xd;&#xa;                         WHERE DEFAULT_TIME &#x3d; &#x27;&#x51c6;&#x65f6;&#x27;&#xd;&#xa;                           AND &#x28;SERVPROV_GID LIKE &#x27;&#x25;&#x5b89;&#x5409;&#x25;&#x27; OR SERVPROV_GID LIKE &#x27;&#x25;&#x91cd;&#x5e86;&#x5f7c;&#x6d0b;&#x25;&#x27;&#x29;&#xd;&#xa;                           AND SUBSTRING&#x28;DLVR_DAY, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT &#x27;&#x91cd;&#x5e86;&#x5f7c;&#x6d0b;&#x27; AS SERVPROV_GID&#xd;&#xa;                              ,0 AS FENZI&#xd;&#xa;                              ,COUNT&#x28;VIN&#x29; AS FENMU&#xd;&#xa;                          FROM ODS_BLPT.T_HAND_ON_TIME_BOX_LV T&#xd;&#xa;                         WHERE &#x28;SERVPROV_GID LIKE &#x27;&#x25;&#x5b89;&#x5409;&#x25;&#x27; OR SERVPROV_GID LIKE &#x27;&#x25;&#x91cd;&#x5e86;&#x5f7c;&#x6d0b;&#x25;&#x27;&#x29;&#xd;&#xa;                           AND SUBSTRING&#x28;DLVR_DAY, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT &#x27;&#x91cd;&#x5e86;&#x4e2d;&#x96c6;&#x27; AS SERVPROV_GID&#xd;&#xa;                              ,COUNT&#x28;VIN&#x29; AS FENZI&#xd;&#xa;                              ,0 AS FENMU&#xd;&#xa;                          FROM ODS_BLPT.T_HAND_ON_TIME_BOX_LV T&#xd;&#xa;                         WHERE DEFAULT_TIME &#x3d; &#x27;&#x51c6;&#x65f6;&#x27;&#xd;&#xa;                           AND &#x28;SERVPROV_GID NOT LIKE &#x27;&#x25;&#x5b89;&#x5409;&#x25;&#x27; AND SERVPROV_GID NOT LIKE &#x27;&#x25;&#x91cd;&#x5e86;&#x5f7c;&#x6d0b;&#x25;&#x27;&#x29;&#xd;&#xa;                           AND SUBSTRING&#x28;DLVR_DAY, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT &#x27;&#x91cd;&#x5e86;&#x4e2d;&#x96c6;&#x27; AS SERVPROV_GID&#xd;&#xa;                              ,0 AS FENZI&#xd;&#xa;                              ,COUNT&#x28;VIN&#x29; AS FENMU&#xd;&#xa;                          FROM ODS_BLPT.T_HAND_ON_TIME_BOX_LV T&#xd;&#xa;                         WHERE &#x28;SERVPROV_GID NOT LIKE &#x27;&#x25;&#x5b89;&#x5409;&#x25;&#x27; AND SERVPROV_GID NOT LIKE &#x27;&#x25;&#x91cd;&#x5e86;&#x5f7c;&#x6d0b;&#x25;&#x27;&#x29;&#xd;&#xa;                           AND SUBSTRING&#x28;DLVR_DAY, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; A&#xd;&#xa;                 GROUP BY SERVPROV_GID&#xd;&#xa;                UNION ALL&#xd;&#xa;                SELECT SERVPROV_GID&#xd;&#xa;                      ,0 AS ONTIME_PARKING_RATE&#xd;&#xa;                      ,ROUND&#x28;SUM&#x28;FENZI&#x29; &#x2f; SUM&#x28;FENMU&#x29;, 2&#x29; AS ONTIME_SHIPING_RATE&#xd;&#xa;                  FROM &#x28;SELECT &#x27;&#x6c11;&#x751f;&#x7269;&#x6d41;&#x27; AS SERVPROV_GID&#xd;&#xa;                              ,COUNT&#x28;VIN&#x29; AS FENZI&#xd;&#xa;                              ,0 AS FENMU&#xd;&#xa;                          FROM ODS_BLPT.T_HAND_ON_TIME_SHIP_LV T&#xd;&#xa;                         WHERE STATUS &#x3d; &#x27;&#x51c6;&#x65f6;&#x27;&#xd;&#xa;                           AND SUBSTRING&#x28;DLVR_DAY, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT &#x27;&#x6c11;&#x751f;&#x7269;&#x6d41;&#x27; AS SERVPROV_GID&#xd;&#xa;                              ,0 AS FENZI&#xd;&#xa;                              ,COUNT&#x28;VIN&#x29; AS FENMU&#xd;&#xa;                          FROM ODS_BLPT.T_HAND_ON_TIME_SHIP_LV T&#xd;&#xa;                         WHERE SUBSTRING&#x28;DLVR_DAY, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; B&#xd;&#xa;                 GROUP BY SERVPROV_GID&#x29; A&#xd;&#xa;         GROUP BY SERVPROV_GID&#x29; T&#xd;&#xa;  LEFT JOIN DIM.DIM_SERVPROV SERVPROV&#xd;&#xa;    ON T.SUPPLIER_NAME &#x3d; SERVPROV.SERVPROV_CODE&#xd;&#xa;   AND SERVPROV.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;  LEFT JOIN &#x28;SELECT PERIOD_WID, SUPPLIER_ID, SUM&#x28;ORDER_QTY&#x29; AS ORDER_QTY&#xd;&#xa;               FROM TEMP.TEMP_DMR_PC_TP_DEVREFF_TPEFF_M_1&#xd;&#xa;              GROUP BY PERIOD_WID, SUPPLIER_ID&#x29; S&#xd;&#xa;    ON S.SUPPLIER_ID &#x3d; SERVPROV.SERVPROV_CODE&#xd;&#xa;&#x2a;&#x2f;</sql>
