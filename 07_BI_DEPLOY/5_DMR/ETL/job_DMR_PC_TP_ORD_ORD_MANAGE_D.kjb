    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMR&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_ORD_ORD_MANAGE_D PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT T.DATE_ID&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,COALESCE&#x28;A.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,COALESCE&#x28;B.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,COALESCE&#x28;C.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME&#xd;&#xa;      ,T.SUPPLIER_ID&#xd;&#xa;      ,COALESCE&#x28;D.SERVPROV_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS SUPPLIER_NAME&#xd;&#xa;      ,T.TPMODE_ID&#xd;&#xa;      ,COALESCE&#x28;E.TPMODE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS TPMODE_NAME&#xd;&#xa;      ,SUM&#x28;T.CUR_ORD_QTY&#x29; AS CUR_ORD_QTY&#xd;&#xa;      ,SUM&#x28;T.CUR_SHIPED_QTY&#x29; AS CUR_SHIPED_QTY&#xd;&#xa;      ,SUM&#x28;T.CUR_TGT_SHIPED_QTY&#x29; AS CUR_TGT_SHIPED_QTY&#xd;&#xa;      ,SUM&#x28;T.CUR_WAIT_SHIP_QTY&#x29; AS CUR_WAIT_SHIP_QTY&#xd;&#xa;      ,SUM&#x28;T.CUR_CHANGE_WAY_QTY&#x29; AS CUR_CHANGE_WAY_QTY&#xd;&#xa;      ,SUM&#x28;T.CUR_ONWAY_QTY&#x29; AS CUR_ONWAY_QTY&#xd;&#xa;      ,SUM&#x28;T.CUR_HOLD_QTY&#x29; AS CUR_HOLD_QTY&#xd;&#xa;      ,SUM&#x28;T.CUR_DELAY_QTY&#x29; AS CUR_DELAY_QTY&#xd;&#xa;      ,SUM&#x28;T.CUR_DELETE_QTY&#x29; AS CUR_DELETE_QTY&#xd;&#xa;      ,SUM&#x28;T.CUR_ONTIME_QTY&#x29; AS CUR_ONTIME_QTY&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DATE --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;T.DATE_ID, 1, 6&#x29; AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM &#x28;SELECT DATE_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,SUPPLIER_ID&#xd;&#xa;              ,TPMODE_ID&#xd;&#xa;              ,0                 AS CUR_ORD_QTY&#xd;&#xa;              ,T2.CUR_SHIPED_QTY AS CUR_SHIPED_QTY&#xd;&#xa;              ,T2.CUR_ORD_QTY    AS CUR_TGT_SHIPED_QTY&#xd;&#xa;              ,0                 AS CUR_WAIT_SHIP_QTY&#xd;&#xa;              ,0                 AS CUR_CHANGE_WAY_QTY&#xd;&#xa;              ,0                 AS CUR_ONWAY_QTY&#xd;&#xa;              ,0                 AS CUR_HOLD_QTY&#xd;&#xa;              ,0                 AS CUR_DELAY_QTY&#xd;&#xa;              ,0                 AS CUR_DELETE_QTY&#xd;&#xa;              ,0                 AS CUR_ONTIME_QTY&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_2 T2&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT DATE_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,SUPPLIER_ID&#xd;&#xa;              ,TPMODE_ID&#xd;&#xa;              ,0                AS CUR_ORD_QTY&#xd;&#xa;              ,0                AS CUR_SHIPED_QTY&#xd;&#xa;              ,0                AS CUR_TGT_SHIPED_QTY&#xd;&#xa;              ,0                AS CUR_WAIT_SHIP_QTY&#xd;&#xa;              ,0                AS CUR_CHANGE_WAY_QTY&#xd;&#xa;              ,T3.CUR_ONWAY_QTY AS CUR_ONWAY_QTY&#xd;&#xa;              ,0                AS CUR_HOLD_QTY&#xd;&#xa;              ,0                AS CUR_DELAY_QTY&#xd;&#xa;              ,0                AS CUR_DELETE_QTY&#xd;&#xa;              ,0                AS CUR_ONTIME_QTY&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_3 T3&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT DATE_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,SUPPLIER_ID&#xd;&#xa;              ,TPMODE_ID&#xd;&#xa;              ,0                 AS CUR_ORD_QTY&#xd;&#xa;              ,0                 AS CUR_SHIPED_QTY&#xd;&#xa;              ,0                 AS CUR_TGT_SHIPED_QTY&#xd;&#xa;              ,0                 AS CUR_WAIT_SHIP_QTY&#xd;&#xa;              ,0                 AS CUR_CHANGE_WAY_QTY&#xd;&#xa;              ,0                 AS CUR_ONWAY_QTY&#xd;&#xa;              ,0                 AS CUR_HOLD_QTY&#xd;&#xa;              ,0                 AS CUR_DELAY_QTY&#xd;&#xa;              ,T4.CUR_DELETE_QTY AS CUR_DELETE_QTY&#xd;&#xa;              ,0                 AS CUR_ONTIME_QTY&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_4 T4&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT DATE_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,SUPPLIER_ID&#xd;&#xa;              ,TPMODE_ID&#xd;&#xa;              ,0                    AS CUR_ORD_QTY&#xd;&#xa;              ,0                    AS CUR_SHIPED_QTY&#xd;&#xa;              ,0                    AS CUR_TGT_SHIPED_QTY&#xd;&#xa;              ,T5.CUR_WAIT_SHIP_QTY AS CUR_WAIT_SHIP_QTY&#xd;&#xa;              ,0                    AS CUR_CHANGE_WAY_QTY&#xd;&#xa;              ,0                    AS CUR_ONWAY_QTY&#xd;&#xa;              ,0                    AS CUR_HOLD_QTY&#xd;&#xa;              ,0                    AS CUR_DELAY_QTY&#xd;&#xa;              ,0                    AS CUR_DELETE_QTY&#xd;&#xa;              ,0                    AS CUR_ONTIME_QTY&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_5 T5&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT DATE_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,SUPPLIER_ID&#xd;&#xa;              ,TPMODE_ID&#xd;&#xa;              ,T6.CUR_TOTAL_ARRIVE_QTY AS CUR_ORD_QTY&#xd;&#xa;              ,0                       AS CUR_SHIPED_QTY&#xd;&#xa;              ,0                       AS CUR_TGT_SHIPED_QTY&#xd;&#xa;              ,0                       AS CUR_WAIT_SHIP_QTY&#xd;&#xa;              ,0                       AS CUR_CHANGE_WAY_QTY&#xd;&#xa;              ,0                       AS CUR_ONWAY_QTY&#xd;&#xa;              ,0                       AS CUR_HOLD_QTY&#xd;&#xa;              ,0                       AS CUR_DELAY_QTY&#xd;&#xa;              ,0                       AS CUR_DELETE_QTY&#xd;&#xa;              ,T6.CUR_ONTIME_QTY       AS CUR_ONTIME_QTY&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_6 T6&#x29; T&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; A&#xd;&#xa;    ON T.RGN_ID &#x3d; A.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; B&#xd;&#xa;    ON T.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; C&#xd;&#xa;    ON T.BRAND_ID &#x3d; C.BRAND_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT SERVPROV_ID&#xd;&#xa;                   ,SERVPROV_NAME&#xd;&#xa;               FROM DIM.DIM_SERVPROV&#xd;&#xa;              GROUP BY SERVPROV_ID&#xd;&#xa;                      ,SERVPROV_NAME&#x29; D&#xd;&#xa;    ON T.SUPPLIER_ID &#x3d; D.SERVPROV_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT TPMODE_ID&#xd;&#xa;                   ,TPMODE_NAME&#xd;&#xa;               FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;              GROUP BY TPMODE_ID&#xd;&#xa;                      ,TPMODE_NAME&#x29; E&#xd;&#xa;    ON T.TPMODE_ID &#x3d; E.TPMODE_ID&#xd;&#xa; GROUP BY T.DATE_ID&#xd;&#xa;         ,T.RGN_ID&#xd;&#xa;         ,COALESCE&#x28;A.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,T.BASE_ID&#xd;&#xa;         ,COALESCE&#x28;B.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,T.BRAND_ID&#xd;&#xa;         ,COALESCE&#x28;C.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,T.SUPPLIER_ID&#xd;&#xa;         ,COALESCE&#x28;D.SERVPROV_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,T.TPMODE_ID&#xd;&#xa;         ,COALESCE&#x28;E.TPMODE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
