    <entry>
      <name>CREATE_TEMP_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_2&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_2&#x28;&#xd;&#xa; DATE_ID            STRING	COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID             STRING  COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID            STRING  COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	        STRING	COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID        STRING  COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	        STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CUR_ORD_QTY	    BIGINT	COMMENT	&#x27;&#x672c;&#x671f;&#x8ba2;&#x5355;&#x91cf;&#x27;&#xd;&#xa;,CUR_SHIPED_QTY	    BIGINT  COMMENT	&#x27;&#x672c;&#x671f;&#x5df2;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8ba2;&#x5355;&#x91cf;&#xff0c;&#x5df2;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_3&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_3&#x28;&#xd;&#xa; DATE_ID	    STRING	COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID         STRING  COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID        STRING  COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	    STRING	COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID    STRING  COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	    STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CUR_ONWAY_QTY	BIGINT	COMMENT	&#x27;&#x672c;&#x671f;&#x5728;&#x9014;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x53d1;&#x8fd0;&#x65f6;&#x957f;&#x6807;&#x51c6;&#x8868;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_4&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_4&#x28;&#xd;&#xa; DATE_ID	    STRING	COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID         STRING  COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID        STRING  COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	    STRING	COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID    STRING  COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	    STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CUR_DELETE_QTY	BIGINT	COMMENT	&#x27;&#x672c;&#x671f;&#x5220;&#x5355;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x53d1;&#x8fd0;&#x65f6;&#x957f;&#x6807;&#x51c6;&#x8868;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_5&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_5&#x28;&#xd;&#xa;DATE_ID	STRING	COMMENT &#x27;&#x65e5;&#x671f;ID&#x27;&#xd;&#xa;,RGN_ID  STRING  COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID  STRING  COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	STRING	COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID  STRING  COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CUR_WAIT_SHIP_QTY  BIGINT COMMENT &#x27;&#x672c;&#x671f;&#x5f85;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#x5f85;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_6&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_6&#x28;&#xd;&#xa; DATE_ID	            STRING  COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID                 STRING  COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID                STRING  COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	            STRING  COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID            STRING  COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	            STRING  COMMENT&#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;&#xd;&#xa;,CUR_TOTAL_ARRIVE_QTY	BIGINT  COMMENT	&#x27;&#x672c;&#x671f;&#x5230;&#x8fbe;&#x91cf;&#x27;&#xd;&#xa;,CUR_ONTIME_QTY         BIGINT  COMMENT	&#x27;&#x672c;&#x671f;&#x51c6;&#x5230;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x5728;&#x9014;&#x8d28;&#x635f;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;</sql>
