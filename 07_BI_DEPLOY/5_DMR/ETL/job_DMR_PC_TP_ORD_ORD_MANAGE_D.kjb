      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_2&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x65f6;&#x95f4;dtd&#x5e73;&#x5747;&#x65f6;&#x957f;&#x8868;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_5&#xd;&#xa;SELECT D.PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,COALESCE&#x28;SUPPLIER_ID, &#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;      ,COALESCE&#x28;TRANSPORTMODE_ID, &#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;      ,COUNT&#x28;DISTINCT ORDER_RELEASE_NAME&#x29; AS CUR_ONWAY_QTY  --&#x672c;&#x65e5;&#x5f85;&#x53d1;&#x91cf;&#xd;&#xa;  FROM DIM.DIM_PERIOD_D D&#xd;&#xa;  JOIN DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;    ON 1 &#x3d; 1&#xd;&#xa; WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;EARLY_PICKUP_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;       D.PERIOD_WID&#xd;&#xa;   AND NVL&#x28;SUBSTR&#x28;REGEXP_REPLACE&#x28;FACTORY_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;,&#x27;29991231&#x27;&#x29; &#x3e;&#xd;&#xa;       D.PERIOD_WID&#xd;&#xa;   AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa; GROUP BY D.PERIOD_WID&#xd;&#xa;         ,COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;SUPPLIER_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;TRANSPORTMODE_ID, &#x27;-1&#x27;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
