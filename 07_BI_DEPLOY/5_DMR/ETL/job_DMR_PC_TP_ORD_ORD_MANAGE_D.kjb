      <name>TO_TEMP_2_BAK</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_2&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x65f6;&#x95f4;dtd&#x5e73;&#x5747;&#x65f6;&#x957f;&#x8868;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_2&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; AS DATE_ID&#xd;&#xa;    ,A.RGN_ID&#xd;&#xa;    ,A.BASE_ID&#xd;&#xa;    ,A.BRAND_ID&#xd;&#xa;    ,A.SUPPLIER_ID&#xd;&#xa;    ,A.TPMODE_ID&#xd;&#xa;    ,sum&#x28;CUR_ORD_QTY&#x29; AS CUR_ORD_QTY  --&#x672c;&#x671f;&#x8ba2;&#x5355;&#x91cf;&#xd;&#xa;    ,sum&#x28;CUR_SHIPED_QTY&#x29; AS CUR_SHIPED_QTY  --&#x672c;&#x671f;&#x5df2;&#x53d1;&#x91cf;&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;T1.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;T1.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;T1.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;M.lsp,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;M.segmetn_transportmode,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,count&#x28;DISTINCT T1.order_release_name&#x29; AS CUR_ORD_QTY  --&#x672c;&#x671f;&#x8ba2;&#x5355;&#x91cf;&#xd;&#xa;        ,0 AS CUR_SHIPED_QTY  --&#x672c;&#x671f;&#x5df2;&#x53d1;&#x91cf;&#xd;&#xa;    FROM DW.DW_TP_ORDERRELEASE T1 --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;    LEFT JOIN DW.DW_TP_ORDERMOVEMENT M&#xd;&#xa;        ON T1.ORDER_RELEASE_GID &#x3d; M.ORDER_RELEASE_GID&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT servprov_name AS SUPPLIER_ID FROM dim.dim_servprov WHERE servprov_id LIKE &#x27;&#x25;&#x7279;&#x9510;&#x25;&#x27; &#xd;&#xa;        	OR servprov_id LIKE &#x27;&#x25;&#x957f;&#x4e45;&#x25;&#x27; &#xd;&#xa;        	OR servprov_id LIKE &#x27;&#x25;&#x5b89;&#x76db;&#x25;&#x27; &#xd;&#xa;        	OR servprov_id LIKE &#x27;&#x25;&#x7f8e;&#x8054;&#x25;&#x27; &#xd;&#xa;        	OR servprov_id LIKE &#x27;&#x25;&#x4f17;&#x5347;&#x25;&#x27;&#xd;&#xa;        &#x29; P&#xd;&#xa;    ON M.LSP &#x3d; P.SUPPLIER_ID&#xd;&#xa;    LEFT JOIN DIM.dim_org B&#xd;&#xa;    ON T1.base_id &#x3d; B.base_id AND B.org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;T1.early_pickup_date,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;        AND P.SUPPLIER_ID IS NULL&#xd;&#xa;        AND B.base_type &#x3d; &#x27;VDC&#x27;&#xd;&#xa;        AND NVL&#x28;M.SEQ_NO, 1&#x29; &#x3d; &#x27;1&#x27;&#xd;&#xa;        AND M.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY coalesce&#x28;T1.rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;T1.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T1.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;M.lsp,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;M.segmetn_transportmode,&#x27;-1&#x27;&#x29; &#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;A.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;A.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;A.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;A.supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;A.transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,0 AS CUR_ORD_QTY  --&#x672c;&#x671f;&#x8ba2;&#x5355;&#x91cf;&#xd;&#xa;        ,count&#x28;DISTINCT order_release_name&#x29; AS CUR_SHIPED_QTY  --&#x672c;&#x671f;&#x5df2;&#x53d1;&#x91cf;&#x91cf;&#xd;&#xa;    FROM DMK.dmk_tp_dtd_prog_time_d A&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG B&#xd;&#xa;    ON A.BASE_ID &#x3d; B.BASE_ID AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;    WHERE &#x28;substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;        &#x29; &#xd;&#xa;        AND early_pickup_date IS NOT NULL&#xd;&#xa;        AND B.base_type &#x3d; &#x27;VDC&#x27;&#xd;&#xa;    GROUP BY coalesce&#x28;A.rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;A.base_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;A.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;A.supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;A.transportmode_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;    &#x29; A&#xd;&#xa;GROUP BY A.RGN_ID&#xd;&#xa;    ,A.BASE_ID&#xd;&#xa;    ,A.BRAND_ID&#xd;&#xa;    ,A.SUPPLIER_ID&#xd;&#xa;    ,A.TPMODE_ID</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
