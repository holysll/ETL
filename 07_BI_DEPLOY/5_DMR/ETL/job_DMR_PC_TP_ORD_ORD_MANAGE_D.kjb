    <entry>
      <name>TO_TEMP_2</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_2&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x65f6;&#x95f4;dtd&#x5e73;&#x5747;&#x65f6;&#x957f;&#x8868;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_2&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;    ,A.RGN_ID&#xd;&#xa;    ,A.BASE_ID&#xd;&#xa;    ,A.BRAND_ID&#xd;&#xa;    ,A.SUPPLIER_ID&#xd;&#xa;    ,A.TPMODE_ID&#xd;&#xa;    ,sum&#x28;CUR_ORD_QTY&#x29; AS CUR_ORD_QTY  --&#x672c;&#x671f;&#x8ba2;&#x5355;&#x91cf;&#xd;&#xa;    ,sum&#x28;CUR_SHIPED_QTY&#x29; AS CUR_SHIPED_QTY  --&#x672c;&#x671f;&#x5df2;&#x53d1;&#x91cf;&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT D.PERIOD_WID&#xd;&#xa;        ,coalesce&#x28;T1.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;T1.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;T1.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;M.lsp,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;M.segmetn_transportmode,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,count&#x28;DISTINCT T1.order_release_name&#x29; AS CUR_ORD_QTY  --&#x672c;&#x671f;&#x8ba2;&#x5355;&#x91cf;&#xd;&#xa;        ,0 AS CUR_SHIPED_QTY  --&#x672c;&#x671f;&#x5df2;&#x53d1;&#x91cf;&#xd;&#xa;    FROM dim.dim_period_d D&#xd;&#xa;    JOIN DW.DW_TP_ORDERRELEASE T1 --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;    ON substr&#x28;regexp_replace&#x28;T1.early_pickup_date,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3d; D.PERIOD_WID&#xd;&#xa;    JOIN DW.DW_TP_ORDERMOVEMENT M&#xd;&#xa;        ON T1.ORDER_RELEASE_GID &#x3d; M.ORDER_RELEASE_GID&#xd;&#xa;        AND NVL&#x28;M.SEQ_NO, 1&#x29; &#x3d; &#x27;1&#x27;&#xd;&#xa;        AND M.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT servprov_name AS SUPPLIER_ID FROM dim.dim_servprov WHERE servprov_id LIKE &#x27;&#x25;&#x7279;&#x9510;&#x25;&#x27; &#xd;&#xa;        	OR servprov_id LIKE &#x27;&#x25;&#x957f;&#x4e45;&#x25;&#x27; &#xd;&#xa;        	OR servprov_id LIKE &#x27;&#x25;&#x5b89;&#x76db;&#x25;&#x27; &#xd;&#xa;        	OR servprov_id LIKE &#x27;&#x25;&#x7f8e;&#x8054;&#x25;&#x27; &#xd;&#xa;        	OR servprov_id LIKE &#x27;&#x25;&#x4f17;&#x5347;&#x25;&#x27;&#xd;&#xa;        &#x29; P&#xd;&#xa;    ON M.LSP &#x3d; P.SUPPLIER_ID&#xd;&#xa;    WHERE SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND P.SUPPLIER_ID IS NULL&#xd;&#xa;        AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;    GROUP BY D.PERIOD_WID&#xd;&#xa;        ,coalesce&#x28;T1.rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;T1.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T1.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;M.lsp,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;M.segmetn_transportmode,&#x27;-1&#x27;&#x29; &#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT D.PERIOD_WID&#xd;&#xa;        ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,0 AS CUR_ORD_QTY  --&#x672c;&#x671f;&#x8ba2;&#x5355;&#x91cf;&#xd;&#xa;        ,count&#x28;DISTINCT order_release_name&#x29; AS CUR_SHIPED_QTY  --&#x672c;&#x671f;&#x5df2;&#x53d1;&#x91cf;&#x91cf;&#xd;&#xa;    FROM DIM.dim_period_d D&#xd;&#xa;    JOIN DMK.dmk_tp_dtd_prog_time_d T&#xd;&#xa;    ON D.PERIOD_WID &#x3d; substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;    WHERE &#x28;substr&#x28;D.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29; &#xd;&#xa;        AND D.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;        AND T.early_pickup_date IS NOT NULL&#xd;&#xa;    GROUP BY D.period_wid&#xd;&#xa;        ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;    &#x29; A&#xd;&#xa;GROUP BY A.PERIOD_WID&#xd;&#xa;    ,A.RGN_ID&#xd;&#xa;    ,A.BASE_ID&#xd;&#xa;    ,A.BRAND_ID&#xd;&#xa;    ,A.SUPPLIER_ID&#xd;&#xa;    ,A.TPMODE_ID&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;  SELECT T.PERIOD_WID&#xd;&#xa;        ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,NVL&#x28;T.SUPPLIER_ID, &#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,NVL&#x28;T.TPMODE_ID, &#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,SUM&#x28;NVL&#x28;T.TICKET_QTY, 0&#x29;&#x29; AS CUR_ORD_QTY  --&#x672c;&#x65e5;&#x8ba2;&#x5355;&#x91cf;&#xd;&#xa;        ,SUM&#x28;NVL&#x28;T.SHIPPING_QTY, 0&#x29;&#x29; AS CUR_SHIPED_QTY  --&#x672c;&#x65e5;&#x5df2;&#x53d1;&#x91cf;&#xd;&#xa;    FROM DMK.DMK_TP_ORDERQTY_D T&#xd;&#xa;    JOIN DIM.DIM_ORG B&#xd;&#xa;      ON T.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;   WHERE B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;     AND B.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;     AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;         SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;     AND T.SUPPLIER_ID IS NOT NULL&#xd;&#xa;   GROUP BY T.PERIOD_WID&#xd;&#xa;           ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;           ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;           ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;           ,NVL&#x28;T.SUPPLIER_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;           ,NVL&#x28;T.TPMODE_ID, &#x27;-1&#x27;&#x29;&#x3b;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
