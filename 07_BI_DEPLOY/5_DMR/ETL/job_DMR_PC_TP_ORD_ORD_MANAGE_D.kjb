      <name>TO_TEMP_3_BAK</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_3&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x53d1;&#x8fd0;&#x6807;&#x51c6;&#x65f6;&#x957f;&#x8868;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_D_3&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;  AS DATE_ID&#xd;&#xa;        ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,count&#x28;DISTINCT order_release_name&#x29; AS CUR_ONWAY_QTY  --&#xd;&#xa;    FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;FACTORY_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#xd;&#xa;        AND &#x28;substr&#x28;regexp_replace&#x28;ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3e; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;            OR arrival_day IS NULL&#x29;&#xd;&#xa;        AND on_passage_days IS NOT NULL&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29; as DATE_ID  --&#xd;&#xa;    ,A.RGN_ID&#xd;&#xa;    ,A.BASE_ID&#xd;&#xa;    ,A.BRAND_ID&#xd;&#xa;    ,A.SUPLLIER_ID&#xd;&#xa;    ,A.TPMODE_ID&#xd;&#xa;    ,SUM&#x28;A.ON_PASSAGE_NUM&#x29; AS CUR_ONWAY_QTY&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT D.PERIOD_WID AS PERIOD_WID --From dw&#x3a;FK to DIM_PERIOD_D.PERIOD_WID&#xd;&#xa;          ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;          ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;          ,COALESCE&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID  --&#xd;&#xa;          ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;          ,COALESCE&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29; AS SUPLLIER_ID  --&#xd;&#xa;          ,T.transportmode_id AS TPMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x28;&#x4ec5;&#x542b;&#x7b2c;&#x4e00;&#x6bb5;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#x29; &#xd;&#xa;          ,COUNT&#x28;T.order_release_gid&#x29; AS ON_PASSAGE_NUM --&#x5728;&#x9014;&#x91cf;&#xd;&#xa;      FROM DIM.DIM_PERIOD_D D&#xd;&#xa;      JOIN DMK.dmk_tp_dtd_prog_time_d T --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;        ON 1 &#x3d; 1&#xd;&#xa;     WHERE 1 &#x3d; 1&#xd;&#xa;       AND D.PERIOD_WID &#x3e;&#x3d;&#xd;&#xa;           FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;       AND D.PERIOD_WID &#x3c;&#xd;&#xa;           NVL&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.DLVR_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;,&#x27;29991231&#x27;&#x29;&#xd;&#xa;       AND T.BRAND_ID &#x3c;&#x3e; &#x27;CA&#x27;&#xd;&#xa;       AND D.PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;     GROUP BY D.PERIOD_WID&#xd;&#xa;          ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;          ,T.transportmode_id &#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT D.PERIOD_WID AS PERIOD_WID --From dw&#x3a;FK to DIM_PERIOD_D.PERIOD_WID&#xd;&#xa;          ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;          ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;          ,COALESCE&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID  --&#xd;&#xa;          ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;          ,COALESCE&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29; AS SUPLLIER_ID  --&#xd;&#xa;          ,T.transportmode_id AS TPMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x28;&#x4ec5;&#x542b;&#x7b2c;&#x4e00;&#x6bb5;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#x29; &#xd;&#xa;          ,COUNT&#x28;T.order_release_gid&#x29; AS ON_PASSAGE_NUM --&#x5728;&#x9014;&#x91cf;&#xd;&#xa;      FROM DIM.DIM_PERIOD_D D&#xd;&#xa;      JOIN DMK.dmk_tp_dtd_prog_time_d T --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;        ON 1 &#x3d; 1&#xd;&#xa;     WHERE 1 &#x3d; 1&#xd;&#xa;       AND D.PERIOD_WID &#x3e;&#x3d;&#xd;&#xa;           FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.OUT_STORE_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;       AND D.PERIOD_WID &#x3c;&#xd;&#xa;           NVL&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ARRIVAL_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;,&#x27;29991231&#x27;&#x29;&#xd;&#xa;       AND T.BRAND_ID &#x3d; &#x27;CA&#x27; --&#xd;&#xa;       AND D.PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;     GROUP BY D.PERIOD_WID&#xd;&#xa;          ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;          ,T.transportmode_id&#xd;&#xa;&#x29; A&#xd;&#xa;GROUP BY A.RGN_ID&#xd;&#xa;    ,A.BASE_ID&#xd;&#xa;    ,A.BRAND_ID&#xd;&#xa;    ,A.SUPLLIER_ID&#xd;&#xa;    ,A.TPMODE_ID&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
