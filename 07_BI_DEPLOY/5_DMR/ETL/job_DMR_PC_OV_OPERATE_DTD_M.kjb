    <entry>
      <name>TO_TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_1&#xd;&#xa;--&#x660e;&#x7ec6;&#xd;&#xa;SELECT &#xd;&#xa;    T.RGN_ID AS RGN_ID  --&#x5927;&#x533a;ID&#xd;&#xa;    ,T.BASE_ID AS BASE_ID  --&#x57fa;&#x5730;ID&#xd;&#xa;    ,T.BRAND_ID AS BRAND_ID  --&#x54c1;&#x724c;&#xd;&#xa;    ,T.PROJ_ID AS PROJ_ID  --&#x9879;&#x76ee;ID&#xd;&#xa;    ,T.VIN AS VIN  --VIN&#xd;&#xa;    ,T.ORDER_RELEASE_GID AS ORDER_RELEASE_GID  --&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;    ,T.SEQ_NO AS SEQ_NO  --&#x8fd0;&#x8f93;&#x6bb5;&#xd;&#xa;    ,T.ORDER_MOVEMENT_GID  --&#x5206;&#x6bb5;&#x8def;&#x7531;GID&#xd;&#xa;    ,T.TRANSPORTMODE_ID AS TRANSPORTMODE_ID  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;    ,T.EARLY_PICKUP_DATE AS EARLY_PICKUP_DATE  --&#x63a5;&#x5355;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.ALLOT_PLN_DAY AS ALLOT_PLN_DAY  --&#x5206;&#x914d;&#x65f6;&#x95f4; &#x82e5;&#x6ca1;&#x6709;&#x5219;&#x53d6;&#x627f;&#x8fd0;&#x5546;&#x6700;&#x65b0;&#x786e;&#x8ba4;&#x65f6;&#x95f4;  &#xd;&#xa;    ,T.OUT_STORE_DAY AS OUT_STORE_DAY  --&#x51fa;&#x5e93;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.FACTORY_DAY AS FACTORY_DAY  --&#x6253;&#x5355;&#x65f6;&#x95f4;  &#x51fa;&#x573a;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.ARRIVAL_DAY AS ARRIVAL_DAY  --&#x4ea4;&#x4ed8;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.RECEIPT_DATE AS RECEIPT_DATE  --&#x56de;&#x5355;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.BILL_DATE AS BILL_DATE  --&#x7ed3;&#x7b97;&#x65f6;&#x95f4;&#x28;&#x4e0e;&#x627f;&#x8fd0;&#x5546;&#xff09;&#xd;&#xa;    ,T.DETAIL_FLAG AS DETAIL_FLAG  --&#x6563;&#x5355;&#x6807;&#x5fd7;&#x28;&#x6563;&#x5355;&#x7684;&#x5224;&#x65ad;&#x903b;&#x8f91;&#x29;&#xd;&#xa;FROM DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;WHERE FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ALLOT_PLN_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;	OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ALLOT_PLN_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;	OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ALLOT_PLN_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.OUT_STORE_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.OUT_STORE_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.OUT_STORE_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ARRIVAL_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ARRIVAL_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ARRIVAL_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.RECEIPT_DATE, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.RECEIPT_DATE, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.RECEIPT_DATE, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;</sql>
