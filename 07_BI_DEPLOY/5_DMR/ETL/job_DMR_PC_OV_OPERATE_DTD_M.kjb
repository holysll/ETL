    <entry>
      <name>CREATE_TEMP_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_1&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_1&#x28;&#xd;&#xa;RGN_ID STRING    COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID STRING    COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID STRING   COMMENT&#x27;&#x54c1;&#x724c;&#x27;&#xd;&#xa;,PROJ_ID STRING   COMMENT&#x27;&#x9879;&#x76ee;ID&#x27;&#xd;&#xa;,VIN STRING   COMMENT&#x27;VIN&#x27;&#xd;&#xa;,ORDER_RELEASE_GID STRING   COMMENT&#x27;OTM&#x5355;&#x53f7;&#x27;&#xd;&#xa;,SEQ_NO STRING   COMMENT&#x27;&#x8fd0;&#x8f93;&#x6bb5;&#x27;&#xd;&#xa;,ORDER_MOVEMENT_GID STRING COMMENT&#x27;&#x5206;&#x6bb5;&#x8def;&#x7531;GID&#x27;&#xd;&#xa;,TRANSPORTMODE_ID STRING   COMMENT&#x27;&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,EARLY_PICKUP_DATE STRING   COMMENT&#x27;&#x63a5;&#x5355;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,ALLOT_PLN_DAY STRING   COMMENT&#x27;&#x5206;&#x914d;&#x65f6;&#x95f4; &#x82e5;&#x6ca1;&#x6709;&#x5219;&#x53d6;&#x627f;&#x8fd0;&#x5546;&#x6700;&#x65b0;&#x786e;&#x8ba4;&#x65f6;&#x95f4;  &#x27;&#xd;&#xa;,OUT_STORE_DAY STRING   COMMENT&#x27;&#x51fa;&#x5e93;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,FACTORY_DAY STRING   COMMENT&#x27;&#x6253;&#x5355;&#x65f6;&#x95f4;  &#x51fa;&#x573a;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,ARRIVAL_DAY STRING   COMMENT&#x27;&#x4ea4;&#x4ed8;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,RECEIPT_DATE STRING   COMMENT&#x27;&#x56de;&#x5355;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,BILL_DATE STRING   COMMENT&#x27;&#x7ed3;&#x7b97;&#x65f6;&#x95f4;&#x28;&#x4e0e;&#x627f;&#x8fd0;&#x5546;&#xff09;&#x27;&#xd;&#xa;,DETAIL_FLAG S