    <entry>
      <name>TO_TEMP_6</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_6&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x5728;&#x9014;&#x8d28;&#x635f;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_6&#xd;&#xa;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID&#xd;&#xa;    ,coalesce&#x28;T.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;T.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,sum&#x28;T.PLAN_QTY&#x29; AS ON_PASSAGE_QTY&#xd;&#xa;    ,sum&#x28;T.LOSS_ONWAY_QTY&#x29; AS ON_PASSAGE_LOSS_QTY&#xd;&#xa;FROM &#x28;&#xd;&#xa;    SELECT coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,count&#x28;VIN&#x29; AS LOSS_ONWAY_QTY&#xd;&#xa;        ,0 AS PLAN_QTY&#xd;&#xa;    FROM DW.DW_TP_INROAD_LOSS&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;receiving_date,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,0 AS LOSS_ONWAY_QTY&#xd;&#xa;        ,sum&#x28;biz_qty&#x29; AS PLAN_QTY&#xd;&#xa;    FROM DW.DW_FIN_TGT_BIZ_QTY&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;period_wid,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.BRAND_ID&#xd;&#xa;    ,T.PROJ_ID WITH CUBE</sql>
