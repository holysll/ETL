#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END&#x29; AS LST_Y_M_ALL_TIME_2&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;out_store_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS LST_Y_M_BIZ_QTY_2&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                THEN &#x28;unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END&#x29; AS CUR_M_ALL_TIME_3&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS CUR_M_BIZ_QTY_3&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                THEN &#x28;unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END&#x29; AS LST_M_ALL_TIME_3&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS LST_M_BIZ_QTY_3&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                THEN &#x28;unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END&#x29; AS LST_Y_M_ALL_TIME_3&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS LST_Y_M_BIZ_QTY_3&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                THEN &#x28;unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;&#x28;24&#x2a;3600&#x29; ELSE 0 END&#x29; AS CUR_M_ALL_TIME_4&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS CUR_M_BIZ_QTY_4&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                THEN &#x28;unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;&#x28;24&#x2a;3600&#x29; ELSE 0 END&#x29; AS LST_M_ALL_TIME_4&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS LST_M_BIZ_QTY_4&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                THEN &#x28;unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;&#x28;24&#x2a;3600&#x29; ELSE 0 END&#x29; AS LST_Y_M_ALL_TIME_4&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS LST_Y_M_BIZ_QTY_4&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;receipt_date,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                THEN &#x28;unix_timestamp&#x28;receipt_date,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;&#x28;24&#x2a;3600&#x29; ELSE 0 END&#x29; AS CUR_M_ALL_TIME_5&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;receipt_date,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS CUR_M_BIZ_QTY_5&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;receipt_date,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                THEN &#x28;unix_timestamp&#x28;receipt_date,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;&#x28;24&#x2a;3600&#x29; ELSE 0 END&#x29; AS LST_M_ALL_TIME_5&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;receipt_date,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS LST_M_BIZ_QTY_5&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;receipt_date,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                THEN &#x28;unix_timestamp&#x28;receipt_date,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;&#x28;24&#x2a;3600&#x29; ELSE 0 END&#x29; AS LST_Y_M_ALL_TIME_5&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;receipt_date,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS LST_Y_M_BIZ_QTY_5&#xd;&#xa;    FROM TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_1 &#xd;&#xa;    GROUP BY coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.BRAND_ID,T.PROJ_ID,T.TPMODE_ID WITH CUBE</sql>
