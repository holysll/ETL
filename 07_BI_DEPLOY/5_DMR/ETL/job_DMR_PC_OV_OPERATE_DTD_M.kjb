      <name>TO_TEMP_2</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_2&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x65f6;&#x95f4;dtd&#x5e73;&#x5747;&#x65f6;&#x957f;&#x8868;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_2&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID  --&#x671f;&#x95f4;ID&#xd;&#xa;    ,coalesce&#x28;BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,coalesce&#x28;TPMODE_ID,&#x27;ALL&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;    ,sum&#x28;CUR_M_DTD_TIME&#x29;&#x2f;sum&#x28;CUR_M_DTD_BIZ_QTY&#x29; AS DTD&#xd;&#xa;    ,sum&#x28;LST_M_DTD_TIME&#x29;&#x2f;sum&#x28;LST_M_DTD_BIZ_QTY&#x29; AS LST_DTD&#xd;&#xa;    ,sum&#x28;LST_Y_M_DTD_TIME&#x29;&#x2f;sum&#x28;LST_Y_M_DTD_BIZ_QTY&#x29; AS LST_Y_DTD&#xd;&#xa;    ,sum&#x28;CUR_M_ALL_TIME_1&#x29;&#x2f;sum&#x28;CUR_M_BIZ_QTY_1&#x29; AS ORDER_ALLOC&#xd;&#xa;    ,sum&#x28;LST_M_ALL_TIME_1&#x29;&#x2f;sum&#x28;LST_M_BIZ_QTY_1&#x29; AS LST_ORDER_ALLOC&#xd;&#xa;    ,sum&#x28;LST_Y_M_ALL_TIME_1&#x29;&#x2f;sum&#x28;LST_Y_M_BIZ_QTY_1&#x29; AS LST_Y_ORDER_ALLOC&#xd;&#xa;    ,sum&#x28;CUR_M_ALL_TIME_2&#x29;&#x2f;sum&#x28;CUR_M_BIZ_QTY_2&#x29; AS ALLOC_OUTSTORE&#xd;&#xa;    ,sum&#x28;LST_M_ALL_TIME_2&#x29;&#x2f;sum&#x28;LST_M_BIZ_QTY_2&#x29; AS LST_ALLOC_OUTSTORE&#xd;&#xa;    ,sum&#x28;LST_Y_M_ALL_TIME_2&#x29;&#x2f;sum&#x28;LST_Y_M_BIZ_QTY_2&#x29; AS LST_Y_ALLOC_OUTSTORE&#xd;&#xa;    ,sum&#x28;CUR_M_ALL_TIME_3&#x29;&#x2f;sum&#x28;CUR_M_BIZ_QTY_3&#x29; AS OUTSTORE_SHIPPING&#xd;&#xa;    ,sum&#x28;LST_M_ALL_TIME_3&#x29;&#x2f;sum&#x28;LST_M_BIZ_QTY_3&#x29; AS LST_OUTSTORE_SHIPPING&#xd;&#xa;    ,sum&#x28;LST_Y_M_ALL_TIME_3&#x29;&#x2f;sum&#x28;LST_Y_M_BIZ_QTY_3&#x29; AS LST_Y_OUTSTORE_SHIPPING&#xd;&#xa;    ,sum&#x28;CUR_M_ALL_TIME_4&#x29;&#x2f;sum&#x28;CUR_M_BIZ_QTY_4&#x29; AS SHIPPING_DELIVER&#xd;&#xa;    ,sum&#x28;LST_M_ALL_TIME_4&#x29;&#x2f;sum&#x28;LST_M_BIZ_QTY_4&#x29; AS LST_SHIPPING_DELIVER&#xd;&#xa;    ,sum&#x28;LST_Y_M_ALL_TIME_4&#x29;&#x2f;sum&#x28;LST_Y_M_BIZ_QTY_4&#x29; AS LST_Y_SHIPPING_DELIVER&#xd;&#xa;    ,sum&#x28;CUR_M_ALL_TIME_5&#x29;&#x2f;sum&#x28;CUR_M_BIZ_QTY_5&#x29; AS RECEIPT_BALANCE&#xd;&#xa;    ,sum&#x28;LST_M_ALL_TIME_5&#x29;&#x2f;sum&#x28;LST_M_BIZ_QTY_5&#x29; AS LST_RECEIPT_BALANCE&#xd;&#xa;    ,sum&#x28;LST_Y_M_ALL_TIME_5&#x29;&#x2f;sum&#x28;LST_Y_M_BIZ_QTY_5&#x29; AS LST_Y_RECEIPT_BALANCE&#xd;&#xa;FROM &#x28;&#xd;&#xa;    SELECT coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                THEN &#x28;unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;early_pickup_date,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;&#x28;24&#x2a;3600&#x29; ELSE 0 END&#x29; AS CUR_M_DTD_TIME&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS CUR_M_DTD_BIZ_QTY&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b