      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_6&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x5728;&#x9014;&#x8d28;&#x635f;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_7&#xd;&#xa;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID&#xd;&#xa;    ,coalesce&#x28;T.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;T.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,sum&#x28;T.LOSS_STORE_QTY&#x29; AS IN_STORE_LOSS_QTY&#xd;&#xa;    ,sum&#x28;T.INSTORE_QTY&#x29; AS IN_STORE_QTY&#xd;&#xa;FROM &#x28;&#xd;&#xa;    SELECT coalesce&#x28;M1.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;M2.proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,sum&#x28;loss_cnt&#x29; AS LOSS_STORE_QTY&#xd;&#xa;        ,0 AS INSTORE_QTY&#xd;&#xa;    FROM ODS_BLPT.T_HAND_IN_STOCK_LOSS_LV M&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT brand_id,brand_name&#xd;&#xa;        FROM DIM.dim_brand&#xd;&#xa;        GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29; M1&#xd;&#xa;    ON M.brand_name &#x3d; M1.brand_name&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT proj_id,proj_name&#xd;&#xa;        FROM DIM.dim_proj&#xd;&#xa;        GROUP BY proj_id,proj_name&#xd;&#xa;    &#x29; M2&#xd;&#xa;    ON M.pro_name &#x3d; M2.proj_name&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;check_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;M1.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;M2.proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,0 AS LOSS_STORE_QTY&#xd;&#xa;        ,sum&#x28;instock_qty&#x29; AS INSTORE_QTY&#xd;&#xa;    FROM DMK.DMK_STR_OPERQTY_D&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;period_wid,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.BRAND_ID&#xd;&#xa;    ,T.PROJ_ID WITH CUBE</sql>
