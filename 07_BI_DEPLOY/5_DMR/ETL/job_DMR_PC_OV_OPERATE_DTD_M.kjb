      <name>TO_TEMP_4</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_4&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x51c6;&#x65f6;&#x5230;&#x8fbe;&#xff0c;&#x603b;&#x5230;&#x8fbe;&#x6570;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_4&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID&#xd;&#xa;    ,coalesce&#x28;A.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;A.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,sum&#x28;OT_ARRIVE_QTY&#x29; AS OT_ARRIVE_QTY&#xd;&#xa;    ,sum&#x28;TOTAL_ARRIVE_QTY&#x29; AS TOTAL_ARRIVE_QTY&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,sum&#x28;arrive_ontime_qty&#x29; AS OT_ARRIVE_QTY&#xd;&#xa;        ,sum&#x28;arrive_plan_qty&#x29; AS TOTAL_ARRIVE_QTY&#xd;&#xa;    FROM dmk.dmk_tp_shipqty_d&#xd;&#xa;    WHERE substr&#x28;period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        and tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;        AND brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; A&#xd;&#xa;GROUP BY A.BRAND_ID,A.PROJ_ID WITH CUBE&#xd;&#xa;</sql>
