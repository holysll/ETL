    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMR&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_OV_OPERATE_DTD_M PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;	,T.BRAND_ID&#xd;&#xa;	,CASE WHEN T.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27; &#xd;&#xa;		ELSE T1.BRAND_NAME END AS BRAND_NAME&#xd;&#xa;	,T.PROJ_ID&#xd;&#xa;	,CASE WHEN T.PROJ_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;		ELSE T2.PROJ_NAME END AS PROJ_NAME&#xd;&#xa;	,T.TPMODE_ID&#xd;&#xa;	,T3.TPMODE_NAME&#xd;&#xa;	,T.DTD&#xd;&#xa;	,T.DTD_YOY&#xd;&#xa;	,T.DTD_MOM&#xd;&#xa;	,T.ORDER_ALLOC&#xd;&#xa;	,T.ORDER_ALLOC_TGT&#xd;&#xa;	,T.ORDER_ALLOC_YOY&#xd;&#xa;	,T.ORDER_ALLOC_MOM&#xd;&#xa;	,T.ALLOC_OUTSTORE&#xd;&#xa;	,T.ALLOC_OUTSTORE_TGT&#xd;&#xa;	,T.ALLOC_OUTSTORE_YOY&#xd;&#xa;	,T.ALLOC_OUTSTORE_MOM&#xd;&#xa;	,T.OUTSTORE_SHIPPING&#xd;&#xa;	,T.OUTSTORE_SHIPPING_TGT&#xd;&#xa;	,T.OUTSTORE_SHIPPING_YOY&#xd;&#xa;	,T.OUTSTORE_SHIPPING_MOM&#xd;&#xa;	,T.SHIPPING_DELIVER&#xd;&#xa;	,T.SHIPPING_DELIVER_TGT&#xd;&#xa;	,T.SHIPPING_DELIVER_YOY&#xd;&#xa;	,T.SHIPPING_DELIVER_MOM&#xd;&#xa;	,T.RECEIPT_BALANCE&#xd;&#xa;	,T.RECEIPT_BALANCE_TGT&#xd;&#xa;	,T.RECEIPT_BALANCE_YOY&#xd;&#xa;	,T.RECEIPT_BALANCE_MOM&#xd;&#xa;	,T.OT_ARRIVE_QTY&#xd;&#xa;	,T.TOTAL_ARRIVE_QTY&#xd;&#xa;	,T.STOCK_OUT_OT_QTY&#xd;&#xa;	,T.OUT_STORE_QTY&#xd;&#xa;	,T.ON_PASSAGE_LOSS_QTY&#xd;&#xa;	,T.ON_PASSAGE_QTY&#xd;&#xa;	,T.IN_STORE_LOSS_QTY&#xd;&#xa;	,T.IN_STORE_QTY&#xd;&#xa;	,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;	,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DATE  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;	,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;	,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT  --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;FROM&#x28;&#xd;&#xa;	SELECT coalesce&#x28;A.PERIOD_WID,B.period_wid&#x29; AS PERIOD_WID&#xd;&#xa;		,COALESCE&#x28;A.BRAND_ID,B.BRAND_ID&#x29; AS BRAND_ID&#xd;&#xa;		,COALESCE&#x28;A.PROJ_ID,B.PROJ_ID&#x29; AS PROJ_ID&#xd;&#xa;		,COALESCE&#x28;A.TPMODE_ID,B.TPMODE_ID&#x29; AS TPMODE_ID&#xd;&#xa;		,B.dtd AS DTD&#xd;&#xa;		,B.lst_y_dtd AS DTD_YOY&#xd;&#xa;		,B.lst_dtd AS DTD_MOM&#xd;&#xa;		,B.order_alloc AS ORDER_ALLOC&#xd;&#xa;		,A.order_alloc_tgt AS ORDER_ALLOC_TGT&#xd;&#xa;		,B.lst_y_order_alloc AS ORDER_ALLOC_YOY&#xd;&#xa;		,B.lst_order_alloc AS ORDER_ALLOC_MOM&#xd;&#xa;		,B.alloc_outstore AS ALLOC_OUTSTORE&#xd;&#xa;		,A.alloc_outstore_tgt AS ALLOC_OUTSTORE_TGT&#xd;&#xa;		,B.lst_y_alloc_outstore AS ALLOC_OUTSTORE_YOY&#xd;&#xa;		,B.lst_alloc_outstore AS ALLOC_OUTSTORE_MOM&#xd;&#xa;		,B.outstore_shipping AS OUTSTORE_SHIPPING&#xd;&#xa;		,A.outstore_shipping_tgt AS OUTSTORE_SHIPPING_TGT&#xd;&#xa;		,B.lst_y_outstore_shipping AS OUTSTORE_SHIPPING_YOY&#xd;&#xa;		,B.lst_outstore_shipping AS OUTSTORE_SHIPPING_MOM&#xd;&#xa;		,B.shipping_deliver AS SHIPPING_DELIVER&#xd;&#xa;		,A.shipping_deliver_tgt AS SHIPPING_DELIVER_TGT&#xd;&#xa;		,B.lst_y_shipping_deliver AS SHIPPING_DELIVER_YOY&#xd;&#xa;		,B.lst_shipping_deliver AS SHIPPING_DELIVER_MOM&#xd;&#xa;		,B.receipt_balance AS RECEIPT_BALANCE&#xd;&#xa;		,A.receipt_balance_tgt AS RECEIPT_BALANCE_TGT&#xd;&#xa;		,B.lst_y_receipt_balance AS RECEIPT_BALANCE_YOY&#xd;&#xa;		,B.lst_receipt_balance AS RECEIPT_BALANCE_MOM&#xd;&#xa;		,NULL AS OT_ARRIVE_QTY	--&#x51c6;&#x65f6;&#x5230;&#x8fbe;&#x6570;&#xd;&#xa;		,NULL AS TOTAL_ARRIVE_QTY	--&#x603b;&#x5230;&#x8fbe;&#x91cf;&#xd;&#xa;		,NULL AS STOCK_OUT_OT_QTY	--&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x91cf;&#xd;&#xa;		,NULL AS OUT_STORE_QTY	--&#x603b;&#x51fa;&#x5e93;&#x91cf;&#xd;&#xa;		,NULL AS ON_PASSAGE_LOSS_QTY	--&#x5728;&#x9014;&#x8d28;&#x635f;&#x6570;&#xd;&#xa;		,NULL AS ON_PASSAGE_QTY	--&#x5728;&#x9014;&#x6570;&#xd;&#xa;		,NULL AS IN_STORE_LOSS_QTY	--&#x5728;&#x5e93;&#x8d28;&#x635f;&#x6570;&#xd;&#xa;		,NULL AS IN_STORE_QTY	--&#x5728;&#x5e93;&#x6570;&#xd;&#xa;	FROM TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_3 A &#xd;&#xa;	FULL JOIN TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_2 B&#xd;&#xa;	ON A.PERIOD_WID &#x3d; B.PERIOD_WID AND A.BRAND_ID &#x3d; B.BRAND_ID AND A.PROJ_ID &#x3d; B.PROJ_ID AND A.TPMODE_ID &#x3d; B.TPMODE_ID&#xd;&#xa;	WHERE B.brand_id &#x3c;&#x3e; &#x27;-1&#x27; AND B.proj_id &#x3c;&#x3e; &#x27;-1&#x27; AND B.tpmode_id NOT IN &#x28;&#x27;ALL&#x27;,&#x27;-1&#x27;,&#x27;ZT&#x27;&#x29;&#xd;&#xa;	UNION ALL &#xd;&#xa;	SELECT coalesce&#x28;C.period_wid,D.period_wid,E.period_wid,F.period_wid&#x29; AS PERIOD_WID&#xd;&#xa;		,COALESCE&#x28;C.BRAND_ID,D.BRAND_ID,E.BRAND_ID,F.BRAND_ID&#x29; AS BRAND_ID&#xd;&#xa;		,COALESCE&#x28;C.PROJ_ID,D.PROJ_ID,E.PROJ_ID,F.PROJ_ID&#x29; AS PROJ_ID&#xd;&#xa;		,NULL AS TPMODE_ID&#xd;&#xa;		,NULL AS DTD&#xd;&#xa;		,NULL AS DTD_YOY&#xd;&#xa;		,NULL AS DTD_MOM&#xd;&#xa;		,NULL AS ORDER_ALLOC&#xd;&#xa;		,NULL AS ORDER_ALLOC_TGT&#xd;&#xa;		,NULL AS ORDER_ALLOC_YOY&#xd;&#xa;		,NULL AS ORDER_ALLOC_MOM&#xd;&#xa;		,NULL AS ALLOC_OUTSTORE&#xd;&#xa;		,NULL AS ALLOC_OUTSTORE_TGT&#xd;&#xa;		,NULL AS ALLOC_OUTSTORE_YOY&#xd;&#xa;		,NULL AS ALLOC_OUTSTORE_MOM&#xd;&#xa;		,NULL AS OUTSTORE_SHIPPING&#xd;&#xa;		,NULL AS OUTSTORE_SHIPPING_TGT&#xd;&#xa;		,NULL AS OUTSTORE_SHIPPING_YOY&#xd;&#xa;		,NULL AS OUTSTORE_SHIPPING_MOM&#xd;&#xa;		,NULL AS SHIPPING_DELIVER&#xd;&#xa;		,NULL AS SHIPPING_DELIVER_TGT&#xd;&#xa;		,NULL AS SHIPPING_DELIVER_YOY&#xd;&#xa;		,NULL AS SHIPPING_DELIVER_MOM&#xd;&#xa;		,NULL AS RECEIPT_BALANCE&#xd;&#xa;		,NULL AS RECEIPT_BALANCE_TGT&#xd;&#xa;		,NULL AS RECEIPT_BALANCE_YOY&#xd;&#xa;		,NULL AS RECEIPT_BALANCE_MOM&#xd;&#xa;		,C.OT_ARRIVE_QTY&#xd;&#xa;		,C.TOTAL_ARRIVE_QTY&#xd;&#xa;		,D.STOCK_OUT_QTY AS STOCK_OUT_OT_QTY&#xd;&#xa;		,D.OUT_STORE_QTY&#xd;&#xa;		,E.ON_PASSAGE_LOSS_QTY&#xd;&#xa;		,E.ON_PASSAGE_QTY&#xd;&#xa;		,F.IN_STORE_LOSS_QTY&#xd;&#xa;		,F.IN_STORE_QTY&#xd;&#xa;	FROM TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_4 C&#xd;&#xa;	FULL JOIN TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_5 D&#xd;&#xa;	ON C.BRAND_ID &#x3d; D.BRAND_ID AND C.PROJ_ID &#x3d; D.PROJ_ID AND C.PERIOD_WID &#x3d; D.PERIOD_WID&#xd;&#xa;	FULL JOIN TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_6 E&#xd;&#xa;	ON C.BRAND_ID &#x3d; E.BRAND_ID AND C.PROJ_ID &#x3d; E.PROJ_ID AND C.PERIOD_WID &#x3d; E.PERIOD_WID&#xd;&#xa;	FULL JOIN TEMP.TEMP_DMR_PC_OV_OPERATE_DTD_M_7 F&#xd;&#xa;	ON C.BRAND_ID &#x3d; F.BRAND_ID AND C.PROJ_ID &#x3d; F.PROJ_ID AND C.PERIOD_WID &#x3d; F.PERIOD_WID&#xd;&#xa;	&#x29; T&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.dim_brand&#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29;T1&#xd;&#xa;ON T.brand_id &#x3d; T1.brand_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT proj_id,proj_name&#xd;&#xa;    FROM DIM.dim_proj&#xd;&#xa;    GROUP BY proj_id,proj_name&#xd;&#xa;    &#x29;T2&#xd;&#xa;ON T.proj_id &#x3d; T2.proj_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT tpmode_id,tpmode_name&#xd;&#xa;    FROM DIM.dim_transport_mode&#xd;&#xa;    GROUP BY tpmode_id,tpmode_name&#xd;&#xa;    &#x29;T3&#xd;&#xa;ON T.tpmode_id &#x3d; T3.tpmode_id</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>hive</connection>
