    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--target table in hive&#x3a; DMR.DMR_PC_TP_DEVREFF_TPMODE_M &#x4ea4;&#x4ed8;&#x4e0e;&#x6548;&#x7387;&#x7ba1;&#x7406;&#xff08;&#x6708;&#xff09;-&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_DEVREFF_CARLOAD_M PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,coalesce&#x28;T1.base_name,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME&#xd;&#xa;    ,NULL AS CAR_TP_LOAD_QTY&#xd;&#xa;    ,sum&#x28;MILEGE&#x29;&#x2f;COUNT&#x28;DISTINCT lic_no&#x29; AS CAR_TP_CAPABILITY&#xd;&#xa;    ,sum&#x28;VIN_QTY&#x29;&#x2f;SUM&#x28;MAX_CAR_QTY&#x29; AS CAR_TP_TP_LOAD_RATE&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;    AS INSERT_DATE --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;    AS UPDATE_DATE --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DW&#x27;                                                     AS PART_SC&#xd;&#xa;    ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT A.BASE_ID&#xd;&#xa;        ,A.budget_shipment_gid&#xd;&#xa;        ,A.lic_no&#xd;&#xa;        ,A.VIN_QTY&#xd;&#xa;        ,A.MILEGE&#xd;&#xa;        ,B.MAX_CAR_QTY&#xd;&#xa;    FROM &#x28;&#xd;&#xa;        SELECT coalesce&#x28;A.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID,A.budget_shipment_gid,A.lic_no,count&#x28;A.vin&#x29; AS VIN_QTY,sum&#x28;A.milege&#x29; AS MILEGE &#xd;&#xa;        FROM DW.dw_tp_ordermovement A&#xd;&#xa;        INNER JOIN DW.dw_tp_orderrelease B&#xd;&#xa;        ON A.order_release_gid &#x3d; B.order_release_gid&#xd;&#xa;        WHERE A.perspective &#x3d; &#x27;B&#x27;&#xd;&#xa;        AND A.segmetn_transportmode &#x3d; &#x27;GL&#x27;&#xd;&#xa;        AND substr&#x28;regexp_replace&#x28;CASE WHEN A.brand_id &#x3d; &#x27;CA&#x27; THEN A.unload_day WHEN A.brand_id &#x3d; &#x27;FT&#x27; THEN B.factory_day END,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        GROUP BY coalesce&#x28;A.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,A.budget_shipment_gid&#xd;&#xa;            ,A.lic_no &#xd;&#xa;        &#x29; A&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT power_unit_id,power_unit_xid,if&#x28;max_car_qty &#x3e;&#x3d; max_minicar_qty, max_car_qty,max_minicar_qty&#x29; AS MAX_CAR_QTY&#xd;&#xa;        FROM DIM.dim_power_unit&#xd;&#xa;        &#x29; B &#xd;&#xa;    ON A.lic_no &#x3d; B.power_unit_id  &#xd;&#xa;    &#x29; T&#xd;&#xa;left join &#x28;&#xd;&#xa;    select base_id,base_name&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    where org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    group by base_id,base_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.BASE_ID &#x3d; T1.base_id&#xd;&#xa;GROUP BY T.BASE_ID&#xd;&#xa;    ,coalesce&#x28;T1.base_name,&#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID&#xd;&#xa;    ,&#x27;ALL&#x27; AS BASE_ID&#xd;&#xa;    ,&#x27;&#x5168;&#x90e8;&#x27; AS BASE_NAME&#xd;&#xa;    ,sum&#x28;VIN_QTY&#x29;&#x2f;COUNT&#x28;budget_shipment_gid&#x29; AS CAR_TP_LOAD_QTY&#xd;&#xa;    ,NULL AS CAR_TP_CAPABILITY&#xd;&#xa;    ,NULL AS CAR_TP_TP_LOAD_RATE&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;    AS INSERT_DATE --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;    AS UPDATE_DATE --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DW&#x27;                                                     AS PART_SC&#xd;&#xa;    ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT A.BASE_ID&#xd;&#xa;        ,A.budget_shipment_gid&#xd;&#xa;        ,A.lic_no&#xd;&#xa;        ,A.VIN_QTY&#xd;&#xa;        ,A.MILEGE&#xd;&#xa;        ,B.MAX_CAR_QTY&#xd;&#xa;    FROM &#x28;&#xd;&#xa;        SELECT coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID,budget_shipment_gid,lic_no,count&#x28;vin&#x29; AS VIN_QTY,sum&#x28;milege&#x29; AS MILEGE &#xd;&#xa;        FROM DW.dw_tp_ordermovement&#xd;&#xa;        WHERE perspective &#x3d; &#x27;B&#x27;&#xd;&#xa;        AND segmetn_transportmode &#x3d; &#x27;GL&#x27;&#xd;&#xa;        AND substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        GROUP BY coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,budget_shipment_gid&#xd;&#xa;            ,lic_no &#xd;&#xa;        &#x29; A&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT power_unit_id,power_unit_xid,if&#x28;max_car_qty &#x3e;&#x3d; max_minicar_qty, max_car_qty,max_minicar_qty&#x29; AS MAX_CAR_QTY&#xd;&#xa;        FROM DIM.dim_power_unit&#xd;&#xa;        &#x29; B &#xd;&#xa;    ON A.lic_no &#x3d; B.power_unit_id  &#xd;&#xa;    &#x29; T</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
