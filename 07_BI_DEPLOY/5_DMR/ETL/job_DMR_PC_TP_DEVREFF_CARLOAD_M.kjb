    <entry>
      <name>TO_TEMP</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_DEVREFF_TPMODE_M_1&#xd;&#xa;SELECT T.PERIOD_WID, NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID, NVL&#x28;T.TPMODE_ID, &#x27;-1&#x27;&#x29; AS TPMODE_ID, SUM&#x28;VIN_CNT&#x29; AS BIZ_QTY&#xd;&#xa;  FROM &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID --&#x671f;&#x95f4;ID&#xd;&#xa;              ,F.RGN_ID --&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;              ,F.SEGMETN_TRANSPORTMODE AS TPMODE_ID&#xd;&#xa;              ,COUNT&#x28;DISTINCT F.VIN&#x29; AS VIN_CNT&#xd;&#xa;          FROM DW.DW_TP_ORDERMOVEMENT F --&#x539f;&#x8868; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;          LEFT JOIN DIM.DIM_BRAND D&#xd;&#xa;            ON F.BRAND_ID &#x3d; D.BRAND_ID&#xd;&#xa;         WHERE F.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND F.SEGMETN_TRANSPORTMODE NOT IN &#x28;&#x27;ZT&#x27;&#x29; --&#x81ea;&#x63d0;&#x4e0d;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTRING&#x28;F.UNLOAD_DAY, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d; --&#x957f;&#x5b89;&#x4f7f;&#x7528;&#x65e5;&#x671f;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND F.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;         GROUP BY F.RGN_ID, F.SEGMETN_TRANSPORTMODE&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID --&#x671f;&#x95f4;ID&#xd;&#xa;              ,F.RGN_ID --&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;              ,F.SEGMETN_TRANSPORTMODE AS TPMODE_ID&#xd;&#xa;              ,COUNT&#x28;DISTINCT F.VIN&#x29; AS VIN_CNT&#xd;&#xa;          FROM DW.DW_TP_ORDERMOVEMENT F --&#x539f;&#x8868; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;          LEFT JOIN DIM.DIM_BRAND D&#xd;&#xa;            ON F.BRAND_ID &#x3d; D.BRAND_ID&#xd;&#xa;         WHERE F.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND F.SEGMETN_TRANSPORTMODE NOT IN &#x28;&#x27;ZT&#x27;&#x29; --&#x81ea;&#x63d0;&#x4e0d;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTRING&#x28;F.FACTORY_DAY, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d; --&#x798f;&#x7279;&#x4f7f;&#x7528;&#x51fa;&#x5382;&#x65e5;&#x671f;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND F.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;         GROUP BY F.RGN_ID, F.SEGMETN_TRANSPORTMODE&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID --&#x671f;&#x95f4;ID&#xd;&#xa;              ,F.RGN_ID --&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;              ,F.SEGMETN_TRANSPORTMODE AS TPMODE_ID&#xd;&#xa;              ,COUNT&#x28;DISTINCT F.VIN&#x29; AS VIN_CNT&#xd;&#xa;          FROM DW.DW_TP_ORDERMOVEMENT F --&#x539f;&#x8868; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;          LEFT JOIN DIM.DIM_BRAND D&#xd;&#xa;            ON F.BRAND_ID &#x3d; D.BRAND_ID&#xd;&#xa;         WHERE F.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND F.SEGMETN_TRANSPORTMODE NOT IN &#x28;&#x27;ZT&#x27;&#x29; --&#x81ea;&#x63d0;&#x4e0d;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTRING&#x28;F.EARLY_PICKUP_DATE, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d; --&#x5176;&#x5b83;&#x54c1;&#x724c;&#x4f7f;&#x7528;&#x8ba1;&#x5212;&#x65e5;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND F.BRAND_ID NOT IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;         GROUP BY F.RGN_ID, F.SEGMETN_TRANSPORTMODE&#x29; AS T&#xd;&#xa; GROUP BY T.PERIOD_WID, NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;, NVL&#x28;T.TPMODE_ID, &#x27;-1&#x27;&#x29;&#x3b;</sql>
