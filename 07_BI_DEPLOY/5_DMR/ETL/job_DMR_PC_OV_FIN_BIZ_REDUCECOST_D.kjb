    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--target table &#x3a; DMR_PC_OV_FIN_BIZ_REDUCECOST_D &#x6982;&#x89c8;-&#x5546;&#x52a1;&#x7ba1;&#x7406;-&#x91c7;&#x8d2d;&#x964d;&#x672c;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_OV_FIN_BIZ_REDUCECOST_D PARTITION &#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;      ,A.PERIOD_TYPE&#xd;&#xa;      ,A.PUR_TYPE_ID&#xd;&#xa;      ,A.PUR_TYPE_NAME&#xd;&#xa;      ,A.PUR_NUM&#xd;&#xa;      ,A.BUDGET_AMT&#xd;&#xa;      ,A.BRGN_AMT&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN A.BRGN_AMT IS NULL THEN&#xd;&#xa;          BUDGET_AMT&#xd;&#xa;         WHEN A.BRGN_AMT &#x3d; &#x27;&#x27; THEN&#xd;&#xa;          BUDGET_AMT&#xd;&#xa;         WHEN A.BUDGET_AMT IS NULL THEN&#xd;&#xa;          0&#xd;&#xa;         WHEN A.BUDGET_AMT &#x3d; &#x27;&#x27; THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          NVL&#x28;BUDGET_AMT, 0&#x29; - NVL&#x28;BRGN_AMT, 0&#x29;&#xd;&#xa;       END AS REDUCE_COST_AMT&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;PROJECT_BUDGET, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          CASE&#xd;&#xa;            WHEN A.BRGN_AMT IS NULL THEN&#xd;&#xa;             BUDGET_AMT&#xd;&#xa;            WHEN A.BRGN_AMT &#x3d; &#x27;&#x27; THEN&#xd;&#xa;             BUDGET_AMT&#xd;&#xa;            WHEN A.BUDGET_AMT IS NULL THEN&#xd;&#xa;             0&#xd;&#xa;            WHEN A.BUDGET_AMT &#x3d; &#x27;&#x27; THEN&#xd;&#xa;             0&#xd;&#xa;            ELSE&#xd;&#xa;             NVL&#x28;BUDGET_AMT, 0&#x29; - NVL&#x28;BRGN_AMT, 0&#x29;&#xd;&#xa;          END &#x2f; A.PROJECT_BUDGET&#xd;&#xa;       END AS REDUCE_COST_RATE&#xd;&#xa;      ,A.PUR_APPR_HOURS&#xd;&#xa;      ,A.INSERT_TIME&#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;      ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;T.APPROVE_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PERIOD_WID --&#x671f;&#x95f4;ID&#xd;&#xa;              ,&#x27;M&#x27; AS PERIOD_TYPE&#xd;&#xa;              ,ENUM.ID AS PUR_TYPE_ID --&#x91c7;&#x8d2d;&#x7c7b;&#x578b;ID&#xd;&#xa;              ,T.PUR_TYPE AS PUR_TYPE_NAME --&#x91c7;&#x8d2d;&#x7c7b;&#x578b;&#x540d;&#x79f0;&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.PUR_ORDER_NAME &#x3d; &#x27;FORMMAIN_1785-&#x91c7;&#x8d2d;&#x7533;&#x8bf7;&#x5355;&#x28;&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#xff09;&#x27; --&#x6570;&#x91cf;&#x53d6;&#x91c7;&#x8d2d;&#x7533;&#x8bf7;&#x5355;  &#xd;&#xa;                        THEN&#xd;&#xa;                        PUR_ORDER_NAME&#xd;&#xa;                       ELSE&#xd;&#xa;                        NULL&#xd;&#xa;                     END&#x29; AS PUR_NUM --&#x91c7;&#x8d2d;&#x6570;&#x91cf;&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN T.PUR_ORDER_NAME IN&#xd;&#xa;                          &#x28;&#x27;FORMMAIN_0640-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;old20180816&#x27;,&#xd;&#xa;                           &#x27;FORMMAIN_1798-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;&#x27;&#x29; THEN&#xd;&#xa;                      PUR_BUDGET_AMT&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BUDGET_AMT --&#x9884;&#x7b97;&#x91d1;&#x989d; &#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN T.PUR_ORDER_NAME IN&#xd;&#xa;                          &#x28;&#x27;FORMMAIN_0640-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;old20180816&#x27;,&#xd;&#xa;                           &#x27;FORMMAIN_1798-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;&#x27;&#x29; THEN&#xd;&#xa;                      NEGOTIATED_RESULTS&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BRGN_AMT --&#x8bae;&#x4ef7;&#x91d1;&#x989d;&#xd;&#xa;              ,0 AS REDUCE_COST_AMT --&#x964d;&#x672c;&#x91d1;&#x989d;&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN T.PUR_ORDER_NAME IN&#xd;&#xa;                          &#x28;&#x27;FORMMAIN_0640-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;old20180816&#x27;,&#xd;&#xa;                           &#x27;FORMMAIN_1798-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;&#x27;&#x29; THEN&#xd;&#xa;                      PROJECT_BUDGET&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS PROJECT_BUDGET&#xd;&#xa;              ,SUM&#x28;DATEDIFF&#x28;HOPE_COMP_DATE, START_DATE&#x29;&#x29; AS PUR_APPR_HOURS --&#x91c7;&#x8d2d;&#x5ba1;&#x6279;&#x65f6;&#x957f;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;          FROM DW.DW_BIZ_DIRECT_MINING_PUR T&#xd;&#xa;          LEFT JOIN ODS_OA.CTP_ENUM_ITEM ENUM&#xd;&#xa;            ON T.PUR_TYPE &#x3d; ENUM.SHOWVALUE&#xd;&#xa;         WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;T.APPROVE_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND ENUM.IFUSE &#x3d; &#x27;Y&#x27;&#xd;&#xa;           AND ENUM.REF_ENUMID IN &#x28;&#x27;395611239263390757&#x27;&#x29;&#xd;&#xa;         GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;T.APPROVE_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                 ,ENUM.ID&#xd;&#xa;                 ,T.PUR_TYPE&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;T.APPROVE_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 4&#x29; AS PERIOD_WID --&#x671f;&#x95f4;ID&#xd;&#xa;              ,&#x27;Y&#x27; AS PERIOD_TYPE&#xd;&#xa;              ,ENUM.ID AS PUR_TYPE_ID --&#x91c7;&#x8d2d;&#x7c7b;&#x578b;ID&#xd;&#xa;              ,T.PUR_TYPE AS PUR_TYPE_NAME --&#x91c7;&#x8d2d;&#x7c7b;&#x578b;&#x540d;&#x79f0;&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.PUR_ORDER_NAME &#x3d; &#x27;FORMMAIN_1785-&#x91c7;&#x8d2d;&#x7533;&#x8bf7;&#x5355;&#x28;&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#xff09;&#x27; --&#x6570;&#x91cf;&#x53d6;&#x91c7;&#x8d2d;&#x7533;&#x8bf7;&#x5355;  &#xd;&#xa;                        THEN&#xd;&#xa;                        PUR_ORDER_NAME&#xd;&#xa;                       ELSE&#xd;&#xa;                        NULL&#xd;&#xa;                     END&#x29; AS PUR_NUM --&#x91c7;&#x8d2d;&#x6570;&#x91cf;&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN T.PUR_ORDER_NAME IN&#xd;&#xa;                          &#x28;&#x27;FORMMAIN_0640-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;old20180816&#x27;,&#xd;&#xa;                           &#x27;FORMMAIN_1798-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;&#x27;&#x29; THEN&#xd;&#xa;                      PUR_BUDGET_AMT&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BUDGET_AMT --&#x9884;&#x7b97;&#x91d1;&#x989d; &#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN T.PUR_ORDER_NAME IN&#xd;&#xa;                          &#x28;&#x27;FORMMAIN_0640-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;old20180816&#x27;,&#xd;&#xa;                           &#x27;FORMMAIN_1798-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;&#x27;&#x29; THEN&#xd;&#xa;                      NEGOTIATED_RESULTS&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BRGN_AMT --&#x8bae;&#x4ef7;&#x91d1;&#x989d;&#xd;&#xa;              ,0 AS REDUCE_COST_AMT --&#x964d;&#x672c;&#x91d1;&#x989d;&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN T.PUR_ORDER_NAME IN&#xd;&#xa;                          &#x28;&#x27;FORMMAIN_0640-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;old20180816&#x27;,&#xd;&#xa;                           &#x27;FORMMAIN_1798-&#x76f4;&#x91c7;&#x8bae;&#x4ef7;&#x5b9a;&#x70b9;&#x5b9a;&#x4ef7;&#x5ba1;&#x6279;&#x8868;&#x27;&#x29; THEN&#xd;&#xa;                      PROJECT_BUDGET&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS PROJECT_BUDGET&#xd;&#xa;              ,SUM&#x28;DATEDIFF&#x28;HOPE_COMP_DATE, START_DATE&#x29;&#x29; AS PUR_APPR_HOURS --&#x91c7;&#x8d2d;&#x5ba1;&#x6279;&#x65f6;&#x957f;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;          FROM DW.DW_BIZ_DIRECT_MINING_PUR T&#xd;&#xa;          LEFT JOIN ODS_OA.CTP_ENUM_ITEM ENUM&#xd;&#xa;            ON T.PUR_TYPE &#x3d; ENUM.SHOWVALUE&#xd;&#xa;         WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;T.APPROVE_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 4&#x29; &#x3d;&#xd;&#xa;               SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;&#xd;&#xa;           AND ENUM.IFUSE &#x3d; &#x27;Y&#x27;&#xd;&#xa;           AND ENUM.REF_ENUMID IN &#x28;&#x27;395611239263390757&#x27;&#x29;&#xd;&#xa;         GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;T.APPROVE_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;