      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DMK_TP_OPERATE_D&#xd;&#xa;--Target Table in Hive&#x3a; DMR_OV_CONVECTION_RANK_M&#xd;&#xa;&#xd;&#xa;--hive conf setttings&#xd;&#xa;--set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMR&#x3b;&#xd;&#xa;--set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;--set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;--set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;--set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_CONVECTION_RANK_M PARTITION&#x28;PART_DT&#x29;&#xd;&#xa;SELECT &#xd;&#xa; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;          AS MONTH_ID         --&#x6708;&#x4efd;ID&#xd;&#xa;      ,C.SOURCE_CTY_ID                                                  AS CITY_ID          --&#x57ce;&#x5e02;ID&#xd;&#xa;      ,REGEXP_REPLACE&#x28;CONCAT_WS&#x28;&#x27;--&#x27;,C.SOURCE_CTY_ID,C.DEST_CTY_ID&#x29;,&#x27;&#x5e02;&#x27;,&#x27;&#x27;&#x29;         AS CITY_NAME        --&#x57ce;&#x5e02;&#x540d;&#x79f0;&#xd;&#xa;      ,ROW_NUMBER&#x28;&#x29; OVER&#x28;ORDER BY LEAST&#x28;NVL&#x28;C.FORWARD_QTY, 0&#x29;, NVL&#x28;C.BACKWARD_QTY, 0&#x29;&#x29; ASC,C.FORWARD_QTY ASC,C.BACKWARD_QTY ASC&#x29;          AS ASC_RANK_ID      --&#x5347;&#x5e8f;&#x6392;&#x540d;&#x53f7;&#xd;&#xa;      ,ROW_NUMBER&#x28;&#x29; OVER&#x28;ORDER BY LEAST&#x28;NVL&#x28;C.FORWARD_QTY, 0&#x29;, NVL&#x28;C.BACKWARD_QTY, 0&#x29;&#x29; DESC,C.FORWARD_QTY DESC,C.BACKWARD_QTY DESC&#x29;           AS DESC_RANK_ID     --&#x964d;&#x5e8f;&#x6392;&#x540d;&#x53f7;&#xd;&#xa;      ,C.FORWARD_QTY            --&#x987a;&#x5411;&#x6570;&#x91cf;&#xd;&#xa;      ,C.BACKWARD_QTY           --&#x9006;&#x5411;&#x6570;&#x91cf;&#xd;&#xa;      ,LEAST&#x28;NVL&#x28;C.FORWARD_QTY, 0&#x29;, NVL&#x28;C.BACKWARD_QTY, 0&#x29;&#x29;      AS CONVECTION_QTY   --&#x5bf9;&#x6d41;&#x91cf;, MIN VALUE BETWEEN FORWARD AND BACKWARD QTY&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;     AS INSERT_TIME      --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;     AS UPDATE_TIME      --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;  AS PART_DT&#xd;&#xa;FROM&#xd;&#xa;&#x28;SELECT &#xd;&#xa;      REGEXP_REPLACE&#x28;CASE WHEN NVL&#x28;F.SOURCE_CTY_ID,&#x27;-1&#x27;&#x29;&#x3d;&#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; ELSE F.SOURCE_CTY_ID END,&#x27;&#x5e02;&#x27;,&#x27;&#x27;&#x29; AS SOURCE_CTY_ID&#xd;&#xa;	  ,REGEXP_REPLACE&#x28;CASE WHEN NVL&#x28;F.DEST_CTY_ID,&#x27;-1&#x27;&#x29;&#x3d;&#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; ELSE F.DEST_CTY_ID END,&#x27;&#x5e02;&#x27;,&#x27;&#x27;&#x29; AS DEST_CTY_ID&#xd;&#xa;	  ,SUM&#x28;CASE WHEN F.FLAG&#x3d;&#x27;1&#x27; THEN F.SHIPPING_NUM ELSE 0 END&#x29; AS FORWARD_QTY&#xd;&#xa;	  ,SUM&#x28;CASE WHEN F.FLAG&#x3d;&#x27;0&#x27; THEN F.SHIPPING_NUM ELSE 0 END&#x29; AS BACKWARD_QTY&#xd;&#xa;     FROM&#xd;&#xa;    &#x28;SELECT &#xd;&#xa;       GREATEST&#x28;src_city_id,dest_city_id&#x29; AS SOURCE_CTY_ID&#xd;&#xa;       ,LEAST&#x28;src_city_id,dest_city_id&#x29; AS DEST_CTY_ID&#xd;&#xa;       ,CASE WHEN GREATEST&#x28;src_city_id,dest_city_id&#x29; &#x3d; src_city_id THEN &#x27;1&#x27; ELSE &#x27;0&#x27; END AS FLAG&#xd;&#xa;       ,shipping_qty AS SHIPPING_NUM&#xd;&#xa;    FROM  DMK.dmk_tp_shipqty_d &#xd;&#xa;    WHERE PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; and tpmode_id &#x3d; &#x27;GL&#x27;&#xd;&#xa;	&#x29; F&#xd;&#xa;    GROUP BY REGEXP_REPLACE&#x28;CASE WHEN nvl&#x28;F.SOURCE_CTY_ID,&#x27;-1&#x27;&#x29;&#x3d;&#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; ELSE F.SOURCE_CTY_ID END,&#x27;&#x5e02;&#x27;,&#x27;&#x27;&#x29;,REGEXP_REPLACE&#x28;CASE WHEN NVL&#x28;F.DEST_CTY_ID,&#x27;-1&#x27;&#x29;&#x3d;&#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; ELSE F.DEST_CTY_ID END,&#x27;&#x5e02;&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;   &#x29; C</sql>
