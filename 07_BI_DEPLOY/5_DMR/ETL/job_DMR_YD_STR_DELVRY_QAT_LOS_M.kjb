    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_STR_DELVRY_QAT_LOS_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID AS DATE_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN D.PERIOD_WID &#x3d; D.MONTH_END_DT_WID THEN&#xd;&#xa;          &#x27;Y&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;N&#x27;&#xd;&#xa;       END AS LST_DAY_FLAG --&#x6708;&#x672b;&#x6807;&#x8bb0;&#xd;&#xa;      ,T.RGN_ID AS RNG_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T1.RGN_NAME&#xd;&#xa;       END AS RNG_NAME&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.BASE_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.BASE_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T2.BASE_NAME&#xd;&#xa;       END AS BASE_NAME&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T3.BRAND_NAME&#xd;&#xa;       END AS BRAND_NAME&#xd;&#xa;      ,STR_BIZ_QTY AS STR_BIZ_QTY --&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,CUR_M_DELVRY_QTY AS CUR_M_DELVRY_QTY --&#x5f53;&#x6708;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;      ,CUR_Y_DELVRY_QTY AS CUR_Y_DELVRY_QTY --&#x5f53;&#x5e74;&#x5f53;&#x6708;&#x7d2f;&#x8ba1;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;      ,ONTIME_M_DELVRY_QTY AS ONTIME_M_DELVRY_QTY --&#x5f53;&#x6708;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;      ,ONTIME_Y_DELVRY_QTY AS ONTIME_Y_DELVRY_QTY --&#x5f53;&#x5e74;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;      ,&#x28;ONTIME_M_DELVRY_QTY &#x2f; CUR_M_DELVRY_QTY&#x29; &#x2a; 100 AS ONTIME_M_DELVRY_RATE --&#x5f53;&#x6708;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x7387;&#xd;&#xa;      ,&#x28;ONTIME_Y_DELVRY_QTY &#x2f; CUR_Y_DELVRY_QTY&#x29; &#x2a; 100 AS ONTIME_Y_DELVRY_RATE --&#x5f53;&#x5e74;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x7387; &#xd;&#xa;      ,CUR_M_IN_STORE_QTY AS CUR_M_IN_STORE_QTY --&#x5f53;&#x6708;&#x5728;&#x5e93;&#x6570;&#xd;&#xa;      ,CUR_Y_IN_STORE_QTY AS CUR_Y_IN_STORE_QTY --&#x5f53;&#x5e74;&#x5728;&#x5e93;&#x6570;&#xd;&#xa;      ,QAT_LOS_M_QTY AS QAT_LOS_M_QTY --&#x5f53;&#x6708;&#x5728;&#x5e93;&#x8d28;&#x635f;&#x6570;&#xd;&#xa;      ,QAT_LOS_Y_QTY AS QAT_LOS_Y_QTY --&#x5f53;&#x5e74;&#x5728;&#x5e93;&#x8d28;&#x635f;&#x6570;&#xd;&#xa;      ,&#x28;QAT_LOS_M_QTY &#x2f; CUR_M_IN_STORE_QTY&#x29; &#x2a; 100 AS QAT_LOS_M_RATE --&#x5f53;&#x6708;&#x5728;&#x5e93;&#x8d28;&#x635f;&#x7387;&#xd;&#xa;      ,&#x28;QAT_LOS_Y_QTY &#x2f; CUR_Y_IN_STORE_QTY&#x29; &#x2a; 100 AS QAT_LOS_Y_RATE --&#x5f53;&#x5e74;&#x5728;&#x5e93;&#x8d28;&#x635f;&#x7387;&#xd;&#xa;      ,NULL AS ONTIME_WRN_M_FLAG --&#x5f53;&#x6708;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x7387;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,NULL AS ONTIME_WRN_Y_FLAG --&#x5f53;&#x5e74;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x7387;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,NULL AS QAT_WRN_M_FLAG --&#x5f53;&#x6708;&#x5728;&#x5e93;&#x8d28;&#x635f;&#x7387;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,NULL AS QAT_WRN_Y_FLAG --&#x5f53;&#x5e74;&#x5728;&#x5e93;&#x8d28;&#x635f;&#x7387;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT T.PERIOD_WID&#xd;&#xa;        ,RGN_ID&#xd;&#xa;        ,BASE_ID&#xd;&#xa;        ,BRAND_ID&#xd;&#xa;        ,0 AS STR_BIZ_QTY&#xd;&#xa;        ,sum&#x28;CUR_D_DELVRY_QTY&#x29; OVER&#x28;PARTITION BY substr&#x28;T.PERIOD_WID,1,6&#x29;,T.RGN_ID,T.BASE_ID,T.BRAND_ID ORDER BY T.PERIOD_WID&#x29; AS CUR_M_DELVRY_QTY&#xd;&#xa;        ,sum&#x28;CUR_D_DELVRY_QTY&#x29; OVER&#x28;PARTITION BY substr&#x28;T.PERIOD_WID,1,4&#x29;,T.RGN_ID,T.BASE_ID,T.BRAND_ID ORDER BY T.PERIOD_WID&#x29; AS CUR_Y_DELVRY_QTY&#xd;&#xa;        ,sum&#x28;ONTIME_D_DELVRY_QTY&#x29; OVER&#x28;PARTITION BY substr&#x28;T.PERIOD_WID,1,6&#x29;,T.RGN_ID,T.BASE_ID,T.BRAND_ID ORDER BY T.PERIOD_WID&#x29; AS ONTIME_M_DELVRY_QTY&#xd;&#xa;        ,sum&#x28;ONTIME_D_DELVRY_QTY&#x29; OVER&#x28;PARTITION BY substr&#x28;T.PERIOD_WID,1,4&#x29;,T.RGN_ID,T.BASE_ID,T.BRAND_ID ORDER BY T.PERIOD_WID&#x29; AS ONTIME_Y_DELVRY_QTY&#xd;&#xa;        ,sum&#x28;CUR_D_IN_STORE_QTY&#x29; OVER&#x28;PARTITION BY substr&#x28;T.PERIOD_WID,1,6&#x29;,T.RGN_ID,T.BASE_ID,T.BRAND_ID ORDER BY T.PERIOD_WID&#x29; AS CUR_M_IN_STORE_QTY&#xd;&#xa;        ,sum&#x28;CUR_D_IN_STORE_QTY&#x29; OVER&#x28;PARTITION BY substr&#x28;T.PERIOD_WID,1,4&#x29;,T.RGN_ID,T.BASE_ID,T.BRAND_ID ORDER BY T.PERIOD_WID&#x29; AS CUR_Y_IN_STORE_QTY&#xd;&#xa;        ,sum&#x28;QAT_LOS_D_QTY&#x29; OVER&#x28;PARTITION BY substr&#x28;T.PERIOD_WID,1,6&#x29;,T.RGN_ID,T.BASE_ID,T.BRAND_ID ORDER BY T.PERIOD_WID&#x29; AS QAT_LOS_M_QTY&#xd;&#xa;        ,sum&#x28;QAT_LOS_D_QTY&#x29; OVER&#x28;PARTITION BY substr&#x28;T.PERIOD_WID,1,4&#x29;,T.RGN_ID,T.BASE_ID,T.BRAND_ID ORDER BY T.PERIOD_WID&#x29; AS QAT_LOS_Y_QTY&#xd;&#xa;    FROM &#x28;&#xd;&#xa;        SELECT C.PERIOD_WID&#xd;&#xa;            ,C.RGN_ID&#xd;&#xa;            ,C.BASE_ID&#xd;&#xa;            ,C.BRAND_ID&#xd;&#xa;            ,SUM&#x28;C.CUR_D_DELVRY_QTY&#x29; AS CUR_D_DELVRY_QTY&#xd;&#xa;            ,SUM&#x28;C.ONTIME_D_DELVRY_QTY&#x29; AS ONTIME_D_DELVRY_QTY&#xd;&#xa;            ,SUM&#x28;C.CUR_D_IN_STORE_QTY&#x29; AS CUR_D_IN_STORE_QTY&#xd;&#xa;            ,SUM&#x28;C.QAT_LOS_D_QTY&#x29; AS QAT_LOS_D_QTY&#xd;&#xa;        FROM&#x28;&#xd;&#xa;            SELECT T.period_wid&#xd;&#xa;                ,NVL&#x28;nvl&#x28;T.rgn_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS RGN_ID &#xd;&#xa;                ,NVL&#x28;nvl&#x28;T.base_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS BASE_ID &#xd;&#xa;                ,NVL&#x28;nvl&#x28;T.brand_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                ,sum&#x28;T.outstock_plan_qty&#x29; AS CUR_D_DELVRY_QTY --&#x5f53;RI&#x8ba1;&#x5212;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;                ,sum&#x28;T.outstock_ontime_qty&#x29; AS ONTIME_D_DELVRY_QTY --&#x5f53;RI&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;                ,sum&#x28;T.instock_qty&#x29; AS CUR_D_IN_STORE_QTY --&#x5f53;RI&#x5728;&#x5e93;&#x6570;&#x28;&#x6536;&#x8f66;&#x29;&#xd;&#xa;                ,0 AS QAT_LOS_D_QTY --&#x5f53;RI&#x5728;&#x5e93;&#x8d28;&#x635f;&#x6570;&#xd;&#xa;            FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;            WHERE substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;                AND substr&#x28;T.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                AND substr&#x28;T.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;            GROUP BY T.period_wid&#xd;&#xa;                ,nvl&#x28;T.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;                ,nvl&#x28;T.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;                ,nvl&#x28;T.brand_id,&#x27;-1&#x27;&#x29; WITH CUBE&#xd;&#xa;            UNION ALL&#xd;&#xa;            SELECT substr&#x28;regexp_replace&#x28;A.check_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; AS PERIOD_WID&#xd;&#xa;                ,NVL&#x28;nvl&#x28;B.rgn_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS RGN_ID &#xd;&#xa;                ,NVL&#x28;nvl&#x28;C.base_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS BASE_ID &#xd;&#xa;                ,NVL&#x28;nvl&#x28;D.brand_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                ,0 AS CUR_D_DELVRY_QTY --&#x5f53;RI&#x8ba1;&#x5212;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;                ,0 AS ONTIME_D_DELVRY_QTY --&#x5f53;RI&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;                ,0 AS CUR_D_IN_STORE_QTY --&#x5f53;RI&#x5728;&#x5e93;&#x6570;&#x28;&#x6536;&#x8f66;&#x29;&#xd;&#xa;                ,sum&#x28;A.loss_cnt&#x29; AS QAT_LOS_D_QTY --&#x5f53;RI&#x5728;&#x5e93;&#x8d28;&#x635f;&#x6570;&#xd;&#xa;            FROM ODS_BLPT.T_HAND_IN_STOCK_LOSS_LV A&#xd;&#xa;            LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                            ,RGN_NAME&#xd;&#xa;                        FROM DIM.DIM_ORG&#xd;&#xa;                       WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                       GROUP BY RGN_ID&#xd;&#xa;                               ,RGN_NAME&#x29; B&#xd;&#xa;              ON A.RGN_NAME &#x3d; B.RGN_NAME&#xd;&#xa;            LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                            ,BASE_NAME&#xd;&#xa;                        FROM DIM.DIM_ORG&#xd;&#xa;                       WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                       GROUP BY BASE_ID&#xd;&#xa;                               ,BASE_NAME&#x29; C&#xd;&#xa;              ON A.BASE_NAME &#x3d; C.BASE_NAME&#xd;&#xa;            LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                            ,BRAND_NAME&#xd;&#xa;                        FROM DIM.DIM_BRAND&#xd;&#xa;                       GROUP BY BRAND_ID&#xd;&#xa;                               ,BRAND_NAME&#x29; D&#xd;&#xa;              ON A.BRAND_NAME &#x3d; D.BRAND_NAME&#xd;&#xa;            WHERE substr&#x28;regexp_replace&#x28;A.check_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;                AND substr&#x28;regexp_replace&#x28;A.check_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                AND substr&#x28;regexp_replace&#x28;A.check_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;            GROUP BY substr&#x28;regexp_replace&#x28;A.check_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;                ,nvl&#x28;B.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;                ,nvl&#x28;C.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;                ,nvl&#x28;D.brand_id,&#x27;-1&#x27;&#x29; WITH CUBE&#xd;&#xa;            &#x29; C&#xd;&#xa;        GROUP BY C.PERIOD_WID&#xd;&#xa;            ,C.RGN_ID&#xd;&#xa;            ,C.BASE_ID&#xd;&#xa;            ,C.BRAND_ID&#xd;&#xa;        &#x29; T&#xd;&#xa;    &#x29; T&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; T1&#xd;&#xa;    ON T.RGN_ID &#x3d; T1.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; T2&#xd;&#xa;    ON T.BASE_ID &#x3d; T2.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; T3&#xd;&#xa;    ON T.BRAND_ID &#x3d; T3.BRAND_ID&#xd;&#xa;  LEFT JOIN DIM.DIM_PERIOD_D D&#xd;&#xa;    ON T.PERIOD_WID &#x3d; D.PERIOD_WID&#xd;&#xa;   AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa; WHERE 1&#x3d;1&#xd;&#xa;       AND substr&#x28;T.PERIOD_WID,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;       AND substr&#x28;T.PERIOD_WID,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
