    <entry>
      <name>TO_TMP2</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_STR_DELVRY_QAT_LOS_M_01&#xd;&#xa;SELECT COALESCE&#x28;T1.RGN_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;T1.BASE_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       SUM&#x28;T1.BIZ_QTY&#x29;,&#xd;&#xa;       SUM&#x28;T2.ONTIME_QTY_M&#x29;,&#xd;&#xa;       SUM&#x28;T2.ALL_QTY_M&#x29;,&#xd;&#xa;       SUM&#x28;T2.ONTIME_QTY_Y&#x29;,&#xd;&#xa;       SUM&#x28;T2.ALL_QTY_Y&#x29;&#xd;&#xa;  FROM &#x28;SELECT RGN_ID,&#xd;&#xa;               BASE_ID,&#xd;&#xa;               SUM&#x28;BIZ_QTY&#x29; AS BIZ_QTY  --&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;          FROM DMK.DMK_STR_PROJBIZQTY_M&#xd;&#xa;            WHERE SUBSTR&#x28;PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY RGN_ID,&#xd;&#xa;                       BASE_ID&#xd;&#xa;       &#x29; T1&#xd;&#xa;    LEFT JOIN &#x28;SELECT RGN_ID,&#xd;&#xa;                      BASE_ID,&#xd;&#xa;                      SUM&#x28;CASE WHEN SUBSTR&#x28;OUT_STORE_DAY,1,7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;&#xd;&#xa;                                AND UNIX_TIMESTAMP&#x28;OUT_STORE_DAY&#x29;-UNIX_TIMESTAMP&#x28;ALLOT_PLN_DAY&#x29; &#x3c;&#x3d; 86400&#xd;&#xa;                               THEN 1 ELSE 0 END&#x29; AS ONTIME_QTY_M,&#xd;&#xa;                      SUM&#x28;CASE WHEN SUBSTR&#x28;OUT_STORE_DAY,1,7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;&#xd;&#xa;                               THEN 1 ELSE 0 END&#x29; AS ALL_QTY_M,&#xd;&#xa;                      SUM&#x28;CASE WHEN UNIX_TIMESTAMP&#x28;OUT_STORE_DAY&#x29;-UNIX_TIMESTAMP&#x28;ALLOT_PLN_DAY&#x29; &#x3c;&#x3d; 86400&#xd;&#xa;                               THEN 1 ELSE 0 END&#x29; AS ONTIME_QTY_Y,&#xd;&#xa;                      SUM&#x28;1&#x29; AS ALL_QTY_Y  &#xd;&#xa;                FROM &#x28;SELECT RGN_ID,             --&#x5927;&#x533a;ID&#xd;&#xa;                             BASE_ID,            --&#x57fa;&#x5730;ID&#xd;&#xa;                             ALLOT_PLN_DAY,      --&#x8c03;&#x5ea6;&#x5206;&#x914d;&#x8ba1;&#x5212;&#x65e5;&#x671f;&#xd;&#xa;                             OUT_STORE_DAY,      --&#x51fa;&#x5e93;&#x65e5;&#xd;&#xa;                             ROW_NUMBER&#x28;&#x29; OVER&#x28;PARTITION BY VIN ORDER BY OUT_STORE_DAY ASC&#x29; AS ROW_NUM &#xd;&#xa;                        FROM DMK.DMK_TP_DTD_PROG_TIME_D&#xd;&#xa;                          WHERE VIN IS NOT NULL&#xd;&#xa;                            AND BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;                            AND SUBSTR&#x28;OUT_STORE_DAY,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#x29; TMP&#xd;&#xa;                  WHERE ROW_NUM &#x3d; 1&#xd;&#xa;                    GROUP BY RGN_ID,&#xd;&#xa;                             BASE_ID&#xd;&#xa;              &#x29; T2 ON T1.RGN_ID &#x3d; T2.RGN_ID&#xd;&#xa;                  AND T1.BASE_ID &#x3d; T2.BASE_ID&#xd;&#xa;    GROUP BY T1.RGN_ID,&#xd;&#xa;             T1.BASE_ID&#xd;&#xa;      WITH CUBE&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_STR_DELVRY_QAT_LOS_M_02&#xd;&#xa;SELECT TMP.RGN_ID,&#xd;&#xa;       CASE WHEN TMP.RGN_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       TMP.BASE_ID,&#xd;&#xa;       CASE WHEN TMP.BASE_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       NULL AS BRAND_ID,&#xd;&#xa;       NULL AS BRAND_NAME,&#xd;&#xa;       TMP.BIZ_QTY,&#xd;&#xa;       TMP.ALL_QTY_M,&#xd;&#xa;       TMP.ALL_QTY_Y,&#xd;&#xa;       TMP.ONTIME_QTY_M,&#xd;&#xa;       TMP.ONTIME_QTY_Y,&#xd;&#xa;       ROUND&#x28;TMP.ONTIME_QTY_M &#x2f; TMP.ALL_QTY_M &#x2a; 100,2&#x29;,&#xd;&#xa;       ROUND&#x28;TMP.ONTIME_QTY_Y &#x2f; TMP.ALL_QTY_Y &#x2a; 100,2&#x29;,&#xd;&#xa;       NULL,&#xd;&#xa;       NULL,&#xd;&#xa;       NULL,&#xd;&#xa;       NULL,&#xd;&#xa;       NULL,&#xd;&#xa;       NULL,&#xd;&#xa;       CASE WHEN ROUND&#x28;TMP.ONTIME_QTY_M &#x2f; TMP.ALL_QTY_M &#x2a; 100,2&#x29; &#x3c; &#xd;&#xa;            CASE MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; WHEN 1 THEN TAR.JAN_PLAN_AMT&#xd;&#xa;                                        WHEN 2 THEN TAR.FEB_PLAN_AMT&#xd;&#xa;                                        WHEN 3 THEN TAR.MAR_PLAN_AMT&#xd;&#xa;                                        WHEN 4 THEN TAR.APR_PLAN_AMT&#xd;&#xa;                                        WHEN 5 THEN TAR.MAY_PLAN_AMT &#xd;&#xa;                                        WHEN 6 THEN TAR.JUNE_PLAN_AMT&#xd;&#xa;                                        WHEN 7 THEN TAR.JULY_PLAN_AMT&#xd;&#xa;                                        WHEN 8 THEN TAR.AUG_PLAN_AMT&#xd;&#xa;                                        WHEN 9 THEN TAR.SEPT_PLAN_AMT&#xd;&#xa;                                        WHEN 10 THEN TAR.OCT_PLAN_AMT&#xd;&#xa;                                        WHEN 11 THEN TAR.NOV_PLAN_AMT&#xd;&#xa;                                        ELSE TAR.DEC_PLAN_AMT END&#xd;&#xa;            THEN 1 ELSE 0 END,&#xd;&#xa;       CASE WHEN ROUND&#x28;TMP.ONTIME_QTY_Y &#x2f; TMP.ALL_QTY_Y &#x2a; 100,2&#x29; &#x3c; &#xd;&#xa;            CASE MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; WHEN 1 THEN TAR.JAN_PLAN_AMT&#xd;&#xa;                                        WHEN 2 THEN TAR.FEB_PLAN_AMT&#xd;&#xa;                                        WHEN 3 THEN TAR.MAR_PLAN_AMT&#xd;&#xa;                                        WHEN 4 THEN TAR.APR_PLAN_AMT&#xd;&#xa;                                        WHEN 5 THEN TAR.MAY_PLAN_AMT &#xd;&#xa;                                        WHEN 6 THEN TAR.JUNE_PLAN_AMT&#xd;&#xa;                                        WHEN 7 THEN TAR.JULY_PLAN_AMT&#xd;&#xa;                                        WHEN 8 THEN TAR.AUG_PLAN_AMT&#xd;&#xa;                                        WHEN 9 THEN TAR.SEPT_PLAN_AMT&#xd;&#xa;                                        WHEN 10 THEN TAR.OCT_PLAN_AMT&#xd;&#xa;                                        WHEN 11 THEN TAR.NOV_PLAN_AMT&#xd;&#xa;                                        ELSE TAR.DEC_PLAN_AMT END&#xd;&#xa;            THEN 1 ELSE 0 END,&#xd;&#xa;       NULL,&#xd;&#xa;       NULL&#xd;&#xa;  FROM TEMP.TEMP_DMR_YD_STR_DELVRY_QAT_LOS_M_01 TMP&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;           ON TMP.RGN_ID &#x3d; RGN.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;           ON TMP.BASE_ID &#x3d; BAS.ORG_ID&#xd;&#xa;    LEFT JOIN ODS_BLPT.T_HAND_WARNING_TARGET TAR&#xd;&#xa;           ON TAR.YEAR_ID &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;          AND TAR.INDEX_NAME &#x3d; &#x27;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x7387;&#x27;&#xd;&#xa;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
