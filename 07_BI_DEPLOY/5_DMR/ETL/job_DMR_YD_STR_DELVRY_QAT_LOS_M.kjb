      <name>CREATE_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_STR_DELVRY_QAT_LOS_M_01&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_STR_DELVRY_QAT_LOS_M_01&#x28;&#xd;&#xa;RGN_ID           STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID          STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BIZ_QTY          BIGINT        COMMENT  &#x27;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;ONTIME_QTY_M     BIGINT        COMMENT  &#x27;&#x6708;&#x7d2f;&#x8ba1;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x6570;&#x27;,&#xd;&#xa;ALL_QTY_M        BIGINT        COMMENT  &#x27;&#x6708;&#x7d2f;&#x8ba1;&#x51fa;&#x5e93;&#x6570;&#x27;,&#xd;&#xa;ONTIME_QTY_Y     BIGINT        COMMENT  &#x27;&#x5e74;&#x7d2f;&#x8ba1;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x6570;&#x27;,&#xd;&#xa;ALL_QTY_Y        BIGINT        COMMENT  &#x27;&#x5e74;&#x7d2f;&#x8ba1;&#x51fa;&#x5e93;&#x6570;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x7387;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_STR_DELVRY_QAT_LOS_M_02&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_STR_DELVRY_QAT_LOS_M_02&#x28;&#xd;&#xa;RGN_ID           STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;RGN_NAME         STRING        COMMENT  &#x27;&#x5927;&#x533a;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;BASE_ID          STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BASE_NAME        STRING        COMMENT  &#x27;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;BRAND_ID         STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;BRAND_