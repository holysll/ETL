    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_FIN_TPBUDGET_D	&#x8fd0;&#x8f93;&#x8d22;&#x52a1;&#x9884;&#x7b97;&#x65e5;&#x660e;&#x7ec6;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;05_DMK&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_OPERATE_ONE_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;&#xd;&#xa;SELECT T.YEAR AS YEAR_ID --  &#x5e74;&#x4efd;ID&#xd;&#xa;      ,T.PERIOD_WID AS MONTH_ID --  &#x6708;&#x4efd;ID&#xd;&#xa;      ,ROUND&#x28;&#x28;T.MONTH &#x2f; 12&#x29;&#x2a;100,0&#x29; AS TIME_RATE  --  &#x65f6;&#x95f4;&#x8fdb;&#x5ea6;&#xd;&#xa;      ,ROUND&#x28;M1.VCL_INCOME_AMT,2&#x29; AS CUR_M_BIZ_INCOME_AMT --  &#x5f53;&#x6708;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;      ,ROUND&#x28;Y1.VCL_INCOME_AMT,2&#x29; AS CUR_Y_BIZ_INCOME_AMT --  &#x5f53;&#x5e74;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;IMCOME_TGT.VCL_INCOME_TGT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;          ROUND&#x28;&#x28;Y1.VCL_INCOME_AMT &#x2f; IMCOME_TGT.VCL_INCOME_TGT_AMT&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_Y_BIZ_INCOME_FINISH_RATE --  &#x5f53;&#x5e74;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;IMCOME_TGT.VCL_INCOME_TGT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;         WHEN NVL&#x28;Y1.VCL_INCOME_AMT &#x2f; IMCOME_TGT.VCL_INCOME_TGT_AMT, 0&#x29; &#x3e;&#x3d; CAST&#x28;T.MONTH AS BIGINT&#x29; &#x2f; 12 THEN&#xd;&#xa;          &#x27;0&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;       END AS CUR_Y_BIZ_INCOME_FLAG --  &#x5f53;&#x5e74;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x6b63;&#x5e38;&#x6807;&#x5fd7;&#xff08;0&#x6b63;&#x5e38; 1&#x4e0d;&#x6b63;&#x5e38;&#xff09;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;TM1.VCL_INCOME_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;          ROUND&#x28;&#x28;&#x28;NVL&#x28;M1.VCL_INCOME_AMT, 0&#x29; - TM1.VCL_INCOME_AMT&#x29; &#x2f;&#xd;&#xa;          ABS&#x28;TM1.VCL_INCOME_AMT&#x29;&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_M_BIZ_INCOME_TB_RATE --  &#x5f53;&#x6708;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x540c;&#x6bd4;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;LM1.VCL_INCOME_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;          ROUND&#x28;&#x28;&#x28;NVL&#x28;M1.VCL_INCOME_AMT, 0&#x29; - LM1.VCL_INCOME_AMT&#x29; &#x2f;&#xd;&#xa;          ABS&#x28;LM1.VCL_INCOME_AMT&#x29;&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_M_BIZ_INCOME_HB_RATE --  &#x5f53;&#x6708;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x73af;&#x6bd4;&#xd;&#xa;      ,ROUND&#x28;M1.PROFIT_AMT,2&#x29; AS CUR_M_PROFIT_AMT --&#x5f53;&#x6708;&#x5229;&#x6da6;&#x91d1;&#x989d;&#xd;&#xa;      ,ROUND&#x28;Y1.PROFIT_AMT,2&#x29; AS CUR_Y_PROFIT_AMT --&#x5f53;&#x5e74;&#x5229;&#x6da6;&#x91d1;&#x989d;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;PROFIT_TGT.PROFIT_TGT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;          ROUND&#x28;&#x28;1&#x2b;&#x28;NVL&#x28;Y1.PROFIT_AMT,0&#x29;-NVL&#x28;PROFIT_TGT.PROFIT_TGT_AMT,0&#x29;&#x29; &#x2f; ABS&#x28;PROFIT_TGT.PROFIT_TGT_AMT&#x29;&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_Y_PROFIT_PLAN_FINISH_RATE --  &#x5f53;&#x5e74;&#x5229;&#x6da6;&#x8ba1;&#x5212;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;PROFIT_TGT.PROFIT_TGT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;         WHEN NVL&#x28;Y1.PROFIT_AMT &#x2f; PROFIT_TGT.PROFIT_TGT_AMT, 0&#x29; &#x3e;&#x3d; CAST&#x28;T.MONTH AS BIGINT&#x29; &#x2f; 12 THEN&#xd;&#xa;          &#x27;0&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;       END AS CUR_Y_PROFIT_FLAG --  &#x5f53;&#x5e74;&#x5229;&#x6da6;&#x6b63;&#x5e38;&#x6807;&#x5fd7;&#xff08;0&#x6b63;&#x5e38; 1&#x4e0d;&#x6b63;&#x5e38;&#xff09;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;TM1.PROFIT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;         ROUND&#x28;&#x28;&#x28;NVL&#x28;M1.PROFIT_AMT, 0&#x29; - TM1.PROFIT_AMT&#x29; &#x2f; ABS&#x28;TM1.PROFIT_AMT&#x29;&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_M_PROFIT_TB_RATE --  &#x5f53;&#x6708;&#x5229;&#x6da6;&#x540c;&#x6bd4;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;LM1.PROFIT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;         ROUND&#x28;&#x28;&#x28;NVL&#x28;M1.PROFIT_AMT, 0&#x29; - LM1.PROFIT_AMT&#x29; &#x2f; ABS&#x28;LM1.PROFIT_AMT&#x29;&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_M_PROFIT_HB_RATE --  &#x5f53;&#x6708;&#x5229;&#x6da6;&#x73af;&#x6bd4;&#xd;&#xa;      ,ROUND&#x28;M1.AR_RETURN_AMT,2&#x29; AS CUR_M_RECV_RFND_AMT --  &#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x56de;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;      ,ROUND&#x28;Y1.AR_RETURN_AMT,2&#x29; AS CUR_Y_RECV_RFND_AMT --  &#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x56de;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;Y1.AR_RETURN_TGT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;         ROUND&#x28;&#x28;Y1.AR_RETURN_AMT &#x2f; Y1.AR_RETURN_TGT_AMT&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_Y_RECV_RFND_FINISH_RATE --  &#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x56de;&#x6b3e;&#x5b8c;&#x6210;&#x7387;  &#x7d2f;&#x8ba1;&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x56de;&#x6b3e;&#x91d1;&#x989d;&#x2f;&#x7d2f;&#x8ba1;&#x4e0a;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x56de;&#x6b3e;&#x91d1;&#x989d;&#x2b;&#x5f53;&#x6708;&#x56de;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#x6708;&#x76ee;&#x6807;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;Y1.AR_RETURN_TGT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;         WHEN NVL&#x28;Y1.AR_RETURN_AMT &#x2f; Y1.AR_RETURN_TGT_AMT, 0&#x29; &#x3e;&#x3d; CAST&#x28;T.MONTH AS BIGINT&#x29; &#x2f; 12 THEN&#xd;&#xa;          &#x27;0&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;       END AS CUR_Y_RECV_RFND_FLAG --  &#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x56de;&#x6b3e;&#x6b63;&#x5e38;&#x6807;&#x5fd7;&#xff08;0&#x6b63;&#x5e38; 1&#x4e0d;&#x6b63;&#x5e38;&#xff09;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;TM1.AR_RETURN_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;         ROUND&#x28;&#x28;&#x28;NVL&#x28;M1.AR_RETURN_AMT, 0&#x29; - TM1.AR_RETURN_AMT&#x29; &#x2f; ABS&#x28;TM1.AR_RETURN_AMT&#x29;&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_M_RECV_RFND_TB_RATE --  &#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x56de;&#x6b3e;&#x540c;&#x6bd4;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;LM1.AR_RETURN_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;         ROUND&#x28;&#x28;&#x28;NVL&#x28;M1.AR_RETURN_AMT, 0&#x29; - LM1.AR_RETURN_AMT&#x29; &#x2f; ABS&#x28;LM1.AR_RETURN_AMT&#x29;&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_M_RECV_RFND_HB_RATE --  &#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x56de;&#x6b3e;&#x73af;&#x6bd4;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;M1.VCL_INCOME_TGT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;         ROUND&#x28;&#x28;&#x28;M1.VCL_INCOME_AMT &#x2f; M1.VCL_INCOME_TGT_AMT&#x29;&#x2a;100&#x29;,2&#x29;&#xd;&#xa;       END AS CUR_M_INCOME_FNSH_RATE --  &#x5f53;&#x6708;&#x6536;&#x5165;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;M1.AR_RETURN_TGT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;          ROUND&#x28;&#x28;M1.AR_RETURN_AMT &#x2f; M1.AR_RETURN_TGT_AMT&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_M_INCOME_RFND_RATE --  &#x5f53;&#x6708;&#x56de;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;M1.PROFIT_TGT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0.00&#xd;&#xa;         ELSE&#xd;&#xa;          ROUND&#x28;&#x28;1&#x2b;&#x28;NVL&#x28;M1.PROFIT_AMT,0&#x29;-NVL&#x28;M1.PROFIT_TGT_AMT,0&#x29;&#x29; &#x2f; ABS&#x28;M1.PROFIT_TGT_AMT&#x29;&#x29;&#x2a;100,2&#x29;&#xd;&#xa;       END AS CUR_M_PROFIT_FNSH_RATE --  &#x5f53;&#x6708;&#x5229;&#x6da6;&#x603b;&#x989d;&#x7684;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,0 AS SORTING --  &#x6392;&#x5e8f;&#x6807;&#x5fd7;&#x9ed8;&#x8ba4;&#x503c;&#x662f;0&#xd;&#xa;      ,ROUND&#x28;M1.AR_RETURN_TGT_AMT,2&#x29; AS INCOME_RFND_RATE_M_TARGT --  &#x56de;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#x6708;&#x76ee;&#x6807;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --  &#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --  &#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,T.PERIOD_WID AS PART_DT&#xd;&#xa;  FROM DIM.DIM_PERIOD_D T&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,SUM&#x28;T.VCL_INCOME_AMT&#x29; AS VCL_INCOME_AMT&#xd;&#xa;                   ,SUM&#x28;T.VCL_INCOME_TGT_AMT&#x29; AS VCL_INCOME_TGT_AMT&#xd;&#xa;                   ,SUM&#x28;T.PROFIT_AMT&#x29; AS PROFIT_AMT&#xd;&#xa;                   ,SUM&#x28;T.PROFIT_TGT_AMT&#x29; AS PROFIT_TGT_AMT&#xd;&#xa;                   ,SUM&#x28;T.AR_RETURN_AMT&#x29; AS AR_RETURN_AMT&#xd;&#xa;                   ,SUM&#x28;T.AR_RETURN_TGT_AMT&#x29; AS AR_RETURN_TGT_AMT&#xd;&#xa;               FROM DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;              WHERE T.PERIOD_WID &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; AS M1&#xd;&#xa;    ON T.PERIOD_WID &#x3d; M1.PERIOD_WID                --&#x6708;&#x5ea6;&#x6536;&#x5165;&#x3001;&#x5229;&#x6da6;&#x548c;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,SUM&#x28;T.VCL_INCOME_AMT&#x29; AS VCL_INCOME_AMT&#xd;&#xa;                   ,SUM&#x28;T.PROFIT_AMT&#x29; AS PROFIT_AMT&#xd;&#xa;                   ,SUM&#x28;T.AR_RETURN_AMT&#x29;  AS AR_RETURN_AMT&#xd;&#xa;                   ,SUM&#x28;CASE WHEN T.PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN T.AR_RETURN_TGT_AMT  --&#x5e74;&#x8ba1;&#x5212;&#x903b;&#x8f91;&#x3a;&#x5f53;&#x6708;&#x8ba1;&#x5212;&#x503c;&#x2b;&#x4ee5;&#x5f80;&#x6708;&#x4efd;&#x5b9e;&#x9645;&#x503c;&#xd;&#xa;                             WHEN T.PERIOD_WID &#x3c; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN T.AR_RETURN_AMT&#xd;&#xa;                             ELSE 0&#xd;&#xa;                    END&#x29;                    AS AR_RETURN_TGT_AMT  --&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x76ee;&#x6807;&#xd;&#xa;               FROM DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29;&#x3c;&#x3d;REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; AS Y1&#xd;&#xa;    ON T.PERIOD_WID &#x3d; Y1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT YEAR&#xd;&#xa;                    ,SUM&#x28;INCOME_PLAN_AMT&#x29;  AS VCL_INCOME_TGT_AMT &#xd;&#xa;               FROM DW.DW_FIN_INCOME_PLAN &#xd;&#xa;              WHERE YEAR &#x3d;REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;               AND PROJ_NAME &#x3c;&#x3e; &#x27;&#x4e00;&#x4f53;&#x5316;&#x8d2d;&#x8f66;&#x9879;&#x76ee;&#x7ec4;&#x27;&#xd;&#xa;              GROUP BY YEAR&#xd;&#xa;            &#x29; IMCOME_TGT&#xd;&#xa;         ON SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d; IMCOME_TGT.YEAR&#xd;&#xa;&#xd;&#xa;  LEFT JOIN &#x28;SELECT YEAR&#xd;&#xa;                    ,SUM&#x28;PROFIT_PLAN_AMT&#x29;  AS PROFIT_TGT_AMT &#xd;&#xa;               FROM DW.DW_FIN_PROFIT_PLAN &#xd;&#xa;              WHERE YEAR &#x3d;REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#xd;&#xa;              GROUP BY YEAR&#xd;&#xa;            &#x29; PROFIT_TGT&#xd;&#xa;         ON SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d; PROFIT_TGT.YEAR         &#xd;&#xa;         &#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,SUM&#x28;T.VCL_INCOME_AMT&#x29; AS VCL_INCOME_AMT&#xd;&#xa;                   ,SUM&#x28;T.PROFIT_AMT&#x29; AS PROFIT_AMT&#xd;&#xa;                   ,SUM&#x28;T.AR_RETURN_AMT&#x29; AS AR_RETURN_AMT&#xd;&#xa;               FROM DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;              WHERE T.PERIOD_WID &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#x29; AS LM1&#xd;&#xa;    ON T.PERIOD_WID &#x3d; LM1.PERIOD_WID    	 &#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,SUM&#x28;T.VCL_INCOME_AMT&#x29; AS VCL_INCOME_AMT&#xd;&#xa;                   ,SUM&#x28;T.PROFIT_AMT&#x29; AS PROFIT_AMT&#xd;&#xa;                   ,SUM&#x28;T.AR_RETURN_AMT&#x29; AS AR_RETURN_AMT&#xd;&#xa;               FROM DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;              WHERE T.PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#x29; AS TM1&#xd;&#xa;    ON T.PERIOD_WID &#x3d; TM1.PERIOD_WID&#xd;&#xa; WHERE T.PERIOD_TYPE &#x3d; &#x27;M&#x27;&#xd;&#xa;   AND T.PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x3b;</sql>
