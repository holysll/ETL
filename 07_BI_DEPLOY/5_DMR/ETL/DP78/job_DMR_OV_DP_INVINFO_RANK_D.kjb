      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_1&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_DP_INVINFO_RANK_D_1&#xd;&#xa;--&#x57fa;&#x5730;&#x79fb;&#x5e93;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,&#x27;B&#x27; AS ORG_TYPE&#xd;&#xa;      ,COALESCE&#x28;T.OC_ID, &#x27;CMAL&#x27;&#x29; AS OC_ID&#xd;&#xa;      ,COALESCE&#x28;OC.OC_NAME, &#x27;&#x6574;&#x8f66;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27;&#x29; AS OC_NAME&#xd;&#xa;      ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,NVL&#x28;RGN.RGN_NAME, &#x27;-1&#x27;&#x29; AS RGN_NAME&#xd;&#xa;      ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,NVL&#x28;BASE.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME&#xd;&#xa;      ,NVL&#x28;BASE.BASE_TYPE, &#x27;-1&#x27;&#x29; AS BASE_TYPE &#xd;&#xa;      ,NULL AS INV_QTY&#xd;&#xa;      ,NULL AS INV_REST_QTY&#xd;&#xa;      ,NULL AS INV_USE_RATE&#xd;&#xa;      ,NULL AS INV_WARNING_FLAG&#xd;&#xa;      ,SUM&#x28;T.MOVEOUT_QTY&#x29; TRANS_INV_QTY&#xd;&#xa;      ,NULL AS TOTAL_INSTOCK_QTY&#xd;&#xa;  FROM DMK.dmk_str_operqty_d T&#xd;&#xa;  LEFT JOIN &#x28;SELECT OC_ID, OC_NAME FROM DIM.DIM_ORG WHERE ORG_TYPE &#x3d; &#x27;O&#x27; AND ENABLE_FLAG &#x3d; &#x27;T&#x27;&#x29; OC ON T.OC_ID &#x3d; OC.OC_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID, RGN_NAME FROM DIM.DIM_ORG WHERE ORG_TYPE &#x3d; &#x27;R&#x27; AND ENABLE_FLAG &#x3d; &#x27;T&#x27;&#x29; RGN ON T.RGN_ID &#x3d; RGN.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID, BASE_NAME, BASE_TYPE FROM DIM.DIM_ORG WHERE ORG_TYPE &#x3d; &#x27;B&#x27; AND ENABLE_FLAG &#x3d; &#x27;T&#x27;&#x29; BASE ON T.BASE_ID &#x3d; BASE.BASE_ID&#xd;&#xa; WHERE 1 &#x3d; 1&#xd;&#xa;   AND T.PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;  &#xd;&#xa;   and T.RGN_ID &#x3c;&#x3e; &#x27;-1&#x27;&#xd;&#xa; GROUP BY T.PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;T.OC_ID, &#x27;CMAL&#x27;&#x29;  &#xd;&#xa;      ,COALESCE&#x28;OC.OC_NAME, &#x27;&#x6574;&#x8f66;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27;&#x29;  &#xd;&#xa;      ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;  &#xd;&#xa;      ,NVL&#x28;RGN.RGN_NAME, &#x27;-1&#x27;&#x29;  &#xd;&#xa;      ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;  &#xd;&#xa;      ,NVL&#x28;BASE.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;  &#xd;&#xa;      ,NVL&#x28;BASE.BASE_TYPE, &#x27;-1&#x27;&#x29;  &#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;T.ARRIVE_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;      ,&#x27;B&#x27; AS ORG_TYPE&#xd;&#xa;      ,O.OC_ID&#xd;&#xa;      ,O.OC_NAME&#xd;&#xa;      ,O.RGN_ID&#xd;&#xa;      ,O.RGN_NAME&#xd;&#xa;      ,O.BASE_ID&#xd;&#xa;      ,O.BASE_NAME&#xd;&#xa;      ,O.BASE_TYPE&#xd;&#xa;      ,NULL AS INV_QTY&#xd;&#xa;      ,NULL AS INV_REST_QTY&#xd;&#xa;      ,NULL AS INV_USE_RATE&#xd;&#xa;      ,NULL AS INV_WARNING_FLAG&#xd;&#xa;      ,COUNT&#x28;DISTINCT T.VIN&#x29; TRANS_INV_QTY&#xd;&#xa;      ,NULL AS TOTAL_INSTOCK_QTY&#xd;&#xa;  FROM DW.DW_TP_ORDERRELEASE T&#xd;&#xa;  JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.DEST_LOCATION_ID &#x3d; O.ORG_ID&#xd;&#xa;  LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;    ON O.BASE_ID &#x3d; P.BASE_ID&#xd;&#xa;   AND T.BRAND_ID &#x3d; P.BRAND_ID&#xd;&#xa; WHERE 1 &#x3d; 1&#xd;&#xa;   AND REGEXP_REPLACE&#x28;SUBSTR&#x28;T.ARRIVE_DAY, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;       REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;  &#xd;&#xa;   AND T.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;   AND O.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;   AND T.OR_PLAN_TYPE &#x3d; &#x27;&#x79fb;&#x5e93;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;   AND T.IS_KNOWN_SHIPPER &#x3d; &#x27;N&#x27; --&#x53bb;&#x9664;&#x5728;&#x9014;&#x542f;&#x7968;&#xd;&#xa;   AND T.VIN IS NOT NULL&#xd;&#xa;   AND T.ARRIVE_DAY IS NOT NULL&#xd;&#xa; GROUP BY REGEXP_REPLACE&#x28;SUBSTR&#x28;T.ARRIVE_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;         ,O.OC_ID&#xd;&#xa;         ,O.OC_NAME&#xd;&#xa;         ,O.RGN_ID&#xd;&#xa;         ,O.RGN_NAME&#xd;&#xa;         ,O.BASE_ID&#xd;&#xa;         ,O.BASE_NAME&#xd;&#xa;         ,O.BASE_TYPE&#x3b;&#x2a;&#x2f;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
