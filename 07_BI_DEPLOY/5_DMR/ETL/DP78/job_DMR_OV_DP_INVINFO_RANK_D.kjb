      <name>TEMP_2</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_2&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_DP_INVINFO_RANK_D_2&#xd;&#xa; --&#x5927;&#x533a;&#x79fb;&#x5e93;      &#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,&#x27;O&#x27; AS ORG_TYPE&#xd;&#xa;      ,COALESCE&#x28;T.OC_ID, &#x27;CMAL&#x2f;ZC&#x27;&#x29; AS OC_ID&#xd;&#xa;      ,COALESCE&#x28;OC.OC_NAME, &#x27;&#x6574;&#x8f66;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27;&#x29; AS OC_NAME&#xd;&#xa;      ,&#x27;ALL&#x27; AS RGN_ID&#xd;&#xa;      ,&#x27;&#x5168;&#x90e8;&#x27; AS RGN_NAME&#xd;&#xa;      ,&#x27;ALL&#x27; AS BASE_ID&#xd;&#xa;      ,&#x27;&#x5168;&#x90e8;&#x27; AS BASE_NAME&#xd;&#xa;      ,NULL AS BASE_TYPE&#xd;&#xa;      ,NULL AS INV_QTY&#xd;&#xa;      ,NULL AS INV_REST_QTY&#xd;&#xa;      ,NULL AS INV_USE_RATE&#xd;&#xa;      ,NULL AS INV_WARNING_FLAG&#xd;&#xa;      ,SUM&#x28;T.MOVEOUT_QTY&#x29; TRANS_INV_QTY&#xd;&#xa;      ,NULL AS TOTAL_INSTOCK_QTY&#xd;&#xa;  FROM DMK.dmk_str_operqty_d T&#xd;&#xa;  LEFT JOIN &#x28;SELECT OC_ID, OC_NAME FROM DIM.DIM_ORG WHERE ORG_TYPE &#x3d; &#x27;O&#x27; AND ENABLE_FLAG &#x3d; &#x27;T&#x27;&#x29; OC ON T.OC_ID &#x3d; OC.OC_ID&#xd;&#xa; WHERE 1 &#x3d; 1&#xd;&#xa;   AND T.PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;  &#xd;&#xa;   and T.OC_ID &#x3c;&#x3e; &#x27;-1&#x27;&#xd;&#xa; GROUP BY T.PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;T.OC_ID, &#x27;CMAL&#x2f;ZC&#x27;&#x29;  &#xd;&#xa;      ,COALESCE&#x28;OC.OC_NAME, &#x27;&#x6574;&#x8f66;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27;&#x29;  &#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
