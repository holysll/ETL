    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_FIN_TPBUDGET_D	&#x8fd0;&#x8f93;&#x8d22;&#x52a1;&#x9884;&#x7b97;&#x65e5;&#x660e;&#x7ec6;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;05_DMK&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_DP_INVINFO_RANK_D PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT PERIOD_WID AS PERIOD_WID&#xd;&#xa;      ,ORG_TYPE AS ORG_TYPE&#xd;&#xa;      ,OC_ID AS OC_ID&#xd;&#xa;      ,OC_NAME AS OC_NAME&#xd;&#xa;      ,RGN_ID AS RGN_ID&#xd;&#xa;      ,RGN_NAME AS RGN_NAME&#xd;&#xa;      ,BASE_ID AS BASE_ID&#xd;&#xa;      ,BASE_NAME AS BASE_NAME&#xd;&#xa;      ,BASE_TYPE AS BASE_TYPE&#xd;&#xa;      ,SUM&#x28;NVL&#x28;T.INV_QTY, 0&#x29;&#x29; AS INV_QTY&#xd;&#xa;      ,SUM&#x28;NVL&#x28;T.INV_REST_QTY, 0&#x29;&#x29; AS INV_REST_QTY&#xd;&#xa;      ,SUM&#x28;CASE WHEN NVL&#x28;T.INV_QTY, 0&#x29; &#x3d; 0 THEN 0 ELSE NVL&#x28;T.INV_QTY, 0&#x29; &#x2f; &#x28;NVL&#x28;T.INV_QTY, 0&#x29; &#x2b; NVL&#x28;T.INV_REST_QTY, 0&#x29;&#x29; END&#x29; AS INV_USE_RATE&#xd;&#xa;      ,NULL AS INV_WARNING_FLAG&#xd;&#xa;      ,SUM&#x28;NVL&#x28;T.TRANS_INV_QTY, 0&#x29;&#x29; AS TRANS_INV_QTY&#xd;&#xa;      ,SUM&#x28;NVL&#x28;T.TOTAL_INSTOCK_QTY, 0&#x29;&#x29; AS TOTAL_INSTOCK_QTY&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x63d2;&#x5165;&#x65f6;&#x95f4; &#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_OV_DP_INVINFO_RANK_D_1&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_OV_DP_INVINFO_RANK_D_2&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_OV_DP_INVINFO_RANK_D_3&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a; FROM TEMP.TEMP_DMR_OV_DP_INVINFO_RANK_D_4&#x29; AS T&#xd;&#xa; GROUP BY PERIOD_WID&#xd;&#xa;         ,ORG_TYPE&#xd;&#xa;         ,OC_ID&#xd;&#xa;         ,OC_NAME&#xd;&#xa;         ,RGN_ID&#xd;&#xa;         ,RGN_NAME&#xd;&#xa;         ,BASE_ID&#xd;&#xa;         ,BASE_NAME&#xd;&#xa;         ,BASE_TYPE&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
