      <name>TEMP_3</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_3&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_DP_INVINFO_RANK_D_3&#xd;&#xa;--&#x5f53;&#x524d;&#x57fa;&#x5730;&#x5e93;&#x4f4d;&#x5e93;&#x5b58;&#x60c5;&#x51b5;         &#xd;&#xa;SELECT PERIOD_WID&#xd;&#xa;      ,&#x27;B&#x27; AS ORG_TYPE&#xd;&#xa;      ,COALESCE&#x28;O.OC_ID, &#x27;CMAL&#x27;&#x29; AS OC_ID&#xd;&#xa;      ,COALESCE&#x28;O.OC_NAME, &#x27;&#x6574;&#x8f66;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27;&#x29; AS OC_NAME&#xd;&#xa;      ,NVL&#x28;O.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,NVL&#x28;O.RGN_NAME, &#x27;-1&#x27;&#x29; AS RGN_NAME&#xd;&#xa;      ,NVL&#x28;O.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,NVL&#x28;O.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME&#xd;&#xa;      ,NVL&#x28;O.BASE_TYPE, &#x27;-1&#x27;&#x29; AS BASE_TYPE &#xd;&#xa;      ,SUM&#x28;USE_QTY&#x29; AS INV_QTY&#xd;&#xa;      ,SUM&#x28;MAX_QTY&#x29; - SUM&#x28;USE_QTY&#x29; AS INV_REST_QTY&#xd;&#xa;      ,NULL AS INV_USE_RATE&#xd;&#xa;      ,NULL AS INV_WARNING_FLAG&#xd;&#xa;      ,NULL AS TRANS_INV_QTY&#xd;&#xa;      ,NULL AS TOTAL_INSTOCK_QTY&#xd;&#xa;  FROM &#x28;SELECT NVL&#x28;EMP.PERIOD_WID, REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; AS PERIOD_WID&#xd;&#xa;              ,EMP.RGN_ID&#xd;&#xa;              ,EMP.BASE_ID&#xd;&#xa;              ,EMP.STR_ID&#xd;&#xa;              ,COALESCE&#x28;EMP.USE_QTY, 0&#x29; AS USE_QTY&#xd;&#xa;              ,TOT.MAX_QTY&#xd;&#xa;          FROM &#x28;SELECT RGN_ID&#xd;&#xa;                      ,BASE_ID&#xd;&#xa;                      ,STR_ID&#xd;&#xa;                      ,SUM&#x28;MAX_STORE_QTY&#x29; AS MAX_QTY --&#x603b;&#x91cf;&#xd;&#xa;                  FROM DMK.DMK_STR_MAXSTORE_M T&#xd;&#xa;                 WHERE PART_DT &#x3d;&#xd;&#xa;                       REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                      &#x27;-&#x27;,&#xd;&#xa;                                      &#x27;&#x27;&#x29;&#xd;&#xa;                   AND T.PART_SC &#x3d; &#x27;DIM&#x27;&#xd;&#xa;                 GROUP BY RGN_ID&#xd;&#xa;                         ,BASE_ID&#xd;&#xa;                         ,STR_ID&#x29; TOT&#xd;&#xa;          RIGHT JOIN &#x28;SELECT A.PERIOD_WID&#xd;&#xa;                          ,A.RGN_ID&#xd;&#xa;                          ,A.BASE_ID&#xd;&#xa;                          ,A.STR_ID&#xd;&#xa;                          ,SUM&#x28;INVENTORY_QTY&#x29; AS USE_QTY --&#x4f7f;&#x7528;&#x91cf; &#xd;&#xa;                      FROM DMK.DMK_STR_INVENTORY_D A&#xd;&#xa;                     WHERE SUBSTRING&#x28;A.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                           REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;&#xd;&#xa;                    --AND RGN_ID &#x3c;&#x3e; &#x27;ZMAL&#x2f;ZC&#x2f;-1&#x27;&#xd;&#xa;                     GROUP BY A.PERIOD_WID&#xd;&#xa;                             ,A.RGN_ID&#xd;&#xa;                             ,A.BASE_ID&#xd;&#xa;                             ,A.STR_ID&#x29; EMP&#xd;&#xa;            ON TOT.STR_ID &#x3d; EMP.STR_ID&#x29; T&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.STR_ID &#x3d; O.STR_ID&#xd;&#xa;   AND O.ORG_TYPE &#x3d; &#x27;S&#x27;&#xd;&#xa; GROUP BY PERIOD_WID&#xd;&#xa;         ,COALESCE&#x28;O.OC_ID, &#x27;CMAL&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;O.OC_NAME, &#x27;&#x6574;&#x8f66;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;O.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;O.RGN_NAME, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;O.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;O.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;O.BASE_TYPE, &#x27;-1&#x27;&#x29; &#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
