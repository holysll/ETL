      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_FIN_TPBUDGET_D	&#x8fd0;&#x8f93;&#x8d22;&#x52a1;&#x9884;&#x7b97;&#x65e5;&#x660e;&#x7ec6;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;05_DMK&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_DP_LYC_PDI_D PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT COALESCE&#x28;A.PERIOD_WID, B.PERIOD_WID&#x29; AS PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;A.ORG_TYPE, B.ORG_TYPE&#x29; AS ORG_TYPE&#xd;&#xa;      ,COALESCE&#x28;A.ORG_ID, B.ORG_ID&#x29; AS ORG_ID&#xd;&#xa;      ,COALESCE&#x28;A.ORG_NAME, B.ORG_NAME&#x29; AS ORG_NAME&#xd;&#xa;      ,A.LYC_QTY&#xd;&#xa;      ,A.LYC_RATE&#xd;&#xa;      ,A.LYC_YOY_RATE&#xd;&#xa;      ,A.LYC_MOM_RATE&#xd;&#xa;      ,B.PDI_QTY&#xd;&#xa;      ,B.PDI_RATE&#xd;&#xa;      ,B.PDI_YOY_RATE&#xd;&#xa;      ,B.PDI_MOM_RATE&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x63d2;&#x5165;&#x65f6;&#x95f4; &#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;COALESCE&#x28;A.PERIOD_WID, B.PERIOD_WID&#x29;, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM TEMP.TEMP_DMR_OV_DP_LYC_PDI_D_1 A&#xd;&#xa;  FULL JOIN TEMP.TEMP_DMR_OV_DP_LYC_PDI_D_2 B&#xd;&#xa;    ON A.PERIOD_WID &#x3d; B.PERIOD_WID&#xd;&#xa;   AND A.ORG_TYPE   &#x3d; B.ORG_TYPE&#xd;&#xa;   AND A.ORG_ID     &#x3d; B.ORG_ID&#xd;&#xa;   AND A.ORG_NAME   &#x3d; B.ORG_NAME</sql>
