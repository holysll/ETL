    <entry>
      <name>CREATE_TEMP_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Udf&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_OV_DP_LYC_PDI_D_1&#x28;  &#xd;&#xa;PERIOD_WID                    BIGINT                             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;ORG_TYPE                      STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;&#x7c7b;&#x578b;&#x28;O,R,B,S&#x29;&#x27;,&#xd;&#xa;ORG_ID                        STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;ID&#x27;,&#xd;&#xa;ORG_NAME                      STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;LYC_QTY                       DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x6708;&#x7d2f;&#x8ba1;&#x8054;&#x8fd0;&#x8f66;&#x27;,&#xd;&#xa;LYC_RATE                      DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x6708;&#x7d2f;&#x8ba1;&#x8054;&#x8fd0;&#x8f66;&#x5360;&#x6bd4;&#x27;,&#xd;&#xa;LYC_YOY_RATE                  DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x6708;&#x7d2f;&#x8ba1;&#x8054;&#x8fd0;&#x8f66;&#x540c;&#x6bd4;&#x27;,&#xd;&#xa;LYC_MOM_RATE                  DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x6708;&#x7d2f;&#x8ba1;&#x8054;&#x8fd0;&#x8f66;&#x73af;&#x6bd4;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x8054;&#x8fd0;&#x8f66;&#x53d1;&#x8fd0;&#x60c5;&#x51b5;&#x26;PDI&#x68c0;&#x51fa;&#x60c5;&#x51b5;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_OV_DP_LYC_PDI_D_2&#x28;&#xd;&#xa;PERIOD_WID                    BIGINT                             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;ORG_TYPE                      STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;&#x7c7b;&#x578b;&#x28;O,R,B,S&#x29;&#x27;,&#xd;&#xa;ORG_ID                        STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;ID&#x27;,&#xd;&#xa;ORG_NAME                      STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;PDI_QTY                       DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x6708;PDI&#x68c0;&#x51fa;&#x8f66;&#x8f86;&#x27;,&#xd;&#xa;PDI_RATE                      DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x6708;PDI&#x68c0;&#x51fa;&#x8f66;&#x8f86;&#x5360;&#x6bd4;&#x27;,&#xd;&#xa;PDI_YOY_RATE                  DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x6708;PDI&#x68c0;&#x51fa;&#x8f66;&#x8f86;&#x540c;&#x6bd4;&#x27;,&#xd;&#xa;PDI_MOM_RATE                  DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5f53;&#x6708;PDI&#x68c0;&#x51fa;&#x8f66;&#x8f86;&#x73af;&#x6bd4;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x8054;&#x8fd0;&#x8f66;&#x53d1;&#x8fd0;&#x60c5;&#x51b5;&#x26;PDI&#x68c0;&#x51fa;&#x60c5;&#x51b5;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
