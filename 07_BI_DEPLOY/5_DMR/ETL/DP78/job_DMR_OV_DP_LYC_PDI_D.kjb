      <name>TEMP_PDI</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_2&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_DP_LYC_PDI_D_2&#xd;&#xa;--PDI    &#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,&#x27;O&#x27; AS ORG_TYPE&#xd;&#xa;      ,&#x27;CMAL&#x2f;ZC&#x27; AS ORG_ID&#xd;&#xa;      ,&#x27;&#x6574;&#x8f66;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27; AS ORG_NAME&#xd;&#xa;      ,T.PDI_QTY AS PDI_QTY&#xd;&#xa;      ,CASE WHEN NVL&#x28;OPER_QTY, 0&#x29; &#x3d; 0 THEN 0 ELSE T.PDI_QTY &#x2f; OPER_QTY END AS PDI_RATE&#xd;&#xa;      ,CASE WHEN NVL&#x28;TB.PDI_QTY, 0&#x29; &#x3d; 0 THEN 0 ELSE T.PDI_QTY &#x2f; TB.PDI_QTY END AS PDI_RATE_YOY&#xd;&#xa;      ,CASE WHEN NVL&#x28;HB.PDI_QTY, 0&#x29; &#x3d; 0 THEN 0 ELSE T.PDI_QTY &#x2f; HB.PDI_QTY END AS PDI_RATE_MOM&#xd;&#xa;  FROM &#x28;&#xd;&#xa;  SELECT PD.PERIOD_WID, SUM&#x28;PDI_QTY&#x29; AS PDI_QTY, SUM&#x28;OPER_QTY&#x29; AS OPER_QTY&#xd;&#xa;  FROM DIM.DIM_PERIOD_D PD&#xd;&#xa;JOIN&#xd;&#xa;&#x28;&#xd;&#xa; SELECT PERIOD_WID, SUM&#x28;PDI_QTY&#x29; AS PDI_QTY, SUM&#x28;OPER_QTY&#x29; AS OPER_QTY&#xd;&#xa; FROM&#x28; &#xd;&#xa; SELECT T.PERIOD_WID&#xd;&#xa;  ,0 AS PDI_QTY&#xd;&#xa;              ,SUM&#x28;INSTOCK_QTY&#x29; AS OPER_QTY&#xd;&#xa;          FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;         WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           GROUP BY T.PERIOD_WID&#xd;&#xa;UNION ALL&#xd;&#xa; SELECT SUBSTRING&#x28;REGEXP_REPLACE&#x28;T.CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; as period_wid&#xd;&#xa;                   ,SUM&#x28;T.TOTAL_CNT&#x29; AS PDI_QTY&#xd;&#xa;                   ,0 AS OPER_QTY&#xd;&#xa;               FROM ODS_BLPT.T_HAND_PDI_CHECK_LV T&#xd;&#xa;               WHERE SUBSTRING&#x28;REGEXP_REPLACE&#x28;T.CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;               group by SUBSTRING&#x28;REGEXP_REPLACE&#x28;T.CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;&#xd;&#xa;&#x29;TT GROUP BY PERIOD_WID&#xd;&#xa;&#x29;S WHERE PD.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;              AND SUBSTR&#x28;PD.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              AND PD.PERIOD_WID &#x3e;&#x3d; NVL&#x28;S.PERIOD_WID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29; GROUP BY PD.PERIOD_WID&#x29;T&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS MONTH_ID&#xd;&#xa;                   ,SUM&#x28;T.TOTAL_CNT&#x29; AS PDI_QTY&#xd;&#xa;               FROM ODS_BLPT.T_HAND_PDI_CHECK_LV T&#xd;&#xa;              WHERE SUBSTRING&#x28;REGEXP_REPLACE&#x28;T.CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; AS TB&#xd;&#xa;    ON SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d; NVL&#x28;TB.MONTH_ID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS MONTH_ID&#xd;&#xa;                   ,SUM&#x28;T.TOTAL_CNT&#x29; AS PDI_QTY&#xd;&#xa;               FROM ODS_BLPT.T_HAND_PDI_CHECK_LV T&#xd;&#xa;              WHERE SUBSTRING&#x28;REGEXP_REPLACE&#x28;T.CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; AS HB&#xd;&#xa;    ON SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d; NVL&#x28;HB.MONTH_ID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29;&#xd;&#xa; &#x3b;</sql>
