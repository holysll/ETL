    <entry>
      <name>TEMP_LY</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_1&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_DP_LYC_PDI_D_1&#xd;&#xa;--&#x8054;&#x8fd0;&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;      ,&#x27;O&#x27; AS ORG_TYPE&#xd;&#xa;      ,&#x27;CMAL&#x2f;ZC&#x27; AS ORG_ID&#xd;&#xa;      ,&#x27;&#x6574;&#x8f66;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27; AS ORG_NAME&#xd;&#xa;      ,LY_QTY AS LYC_QTY&#xd;&#xa;      ,CASE WHEN NVL&#x28;TOTAL_SHIP_QTY, 0&#x29; &#x3d; 0 THEN 0 ELSE LY_QTY &#x2f; TOTAL_SHIP_QTY END AS LYC_RATE&#xd;&#xa;      ,CASE WHEN NVL&#x28;TB_LY_QTY, 0&#x29; &#x3d; 0 THEN 0 ELSE &#x28;NVL&#x28;LY_QTY, 0&#x29; - NVL&#x28;TB_LY_QTY, 0&#x29;&#x29; &#x2f; TB_LY_QTY END AS LYC_YOY_RATE&#xd;&#xa;      ,CASE WHEN NVL&#x28;HB_LY_QTY, 0&#x29; &#x3d; 0 THEN 0 ELSE &#x28;NVL&#x28;LY_QTY, 0&#x29; - NVL&#x28;HB_LY_QTY, 0&#x29;&#x29; &#x2f; HB_LY_QTY END AS LYC_MOM_RATE&#xd;&#xa;  FROM &#x28;SELECT T.PERIOD_WID&#xd;&#xa;              ,T.TOTAL_SHIP_QTY&#xd;&#xa;              ,T.LY_QTY&#xd;&#xa;              ,TB.LY_QTY AS TB_LY_QTY&#xd;&#xa;              ,HB.LY_QTY AS HB_LY_QTY&#xd;&#xa;          FROM &#x28;&#xd;&#xa;          SELECT PD.PERIOD_WID&#xd;&#xa;                ,SUM&#x28;M.TOTAL_SHIP_QTY&#x29; AS TOTAL_SHIP_QTY&#xd;&#xa;                ,SUM&#x28;M.LY_QTY&#x29; AS LY_QTY&#xd;&#xa;            FROM DIM.DIM_PERIOD_D PD&#xd;&#xa;            JOIN &#x28;SELECT PERIOD_WID&#xd;&#xa;                          ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS MONTH_ID&#xd;&#xa;                          ,SUM&#x28;SHIPPING_QTY&#x29; AS TOTAL_SHIP_QTY&#xd;&#xa;                          ,SUM&#x28;TRANS_QTY&#x29; AS LY_QTY&#xd;&#xa;                      FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                     WHERE T.PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                     GROUP BY PERIOD_WID&#x29; M&#xd;&#xa;            WHERE PD.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;              AND SUBSTR&#x28;PD.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              AND PD.PERIOD_WID &#x3e;&#x3d; NVL&#x28;M.PERIOD_WID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;            GROUP BY PD.PERIOD_WID&#x29;T&#xd;&#xa;          LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS MONTH_ID&#xd;&#xa;                          ,SUM&#x28;TRANS_QTY&#x29; AS LY_QTY&#xd;&#xa;                      FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                     WHERE T.PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; AS TB&#xd;&#xa;            ON SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d; NVL&#x28;TB.MONTH_ID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;          LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS MONTH_ID&#xd;&#xa;                          ,SUM&#x28;TRANS_QTY&#x29; AS LY_QTY&#xd;&#xa;                      FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                     WHERE T.PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; AS HB&#xd;&#xa;            ON SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d; NVL&#x28;HB.MONTH_ID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29;&#x29; A&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
