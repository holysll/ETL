      <name>TEMP_2</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_2&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_DP_DELIVER_ARRIVE_D_2&#xd;&#xa;--&#x540c;&#x671f;&#xd;&#xa;SELECT PD.PERIOD_WID&#xd;&#xa;      ,S.ORG_TYPE&#xd;&#xa;      ,S.OC_ID&#xd;&#xa;      ,S.OC_NAME&#xd;&#xa;      ,S.RGN_ID&#xd;&#xa;      ,S.RGN_NAME&#xd;&#xa;      ,S.BRAND_ID&#xd;&#xa;      ,S.BRAND_NAME&#xd;&#xa;      ,SUM&#x28;S.ARRIVAL_QTY_M&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;      ,SUM&#x28;S.ARRIVAL_TAR_QTY_M&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;  FROM DIM.DIM_PERIOD_D PD&#xd;&#xa;  JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;ARRIVAL_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,&#x27;O&#x27; AS ORG_TYPE&#xd;&#xa;              ,&#x27;CMAL&#x2f;ZC&#x27; AS OC_ID&#xd;&#xa;              ,&#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27; AS OC_NAME&#xd;&#xa;              ,&#x27;ALL&#x27; AS RGN_ID&#xd;&#xa;              ,&#x27;&#x5168;&#x90e8;&#x27; AS RGN_NAME&#xd;&#xa;              ,&#x27;ALL&#x27; AS BRAND_ID&#xd;&#xa;              ,&#x27;&#x5168;&#x90e8;&#x27; AS BRAND_NAME&#xd;&#xa;              ,SUM&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                AND  SUBSTR&#x28;REGEXP_REPLACE&#x28;ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY, 1, 10&#x29;, CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;&#xd;&#xa;             THEN 1 ELSE 0 END&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;              ,SUM&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;              THEN 1 ELSE 0 END&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;          FROM DMK.DMK_TP_DTD_PROG_TIME_D&#xd;&#xa;         WHERE RGN_ID &#x3c;&#x3e; &#x27;CMAL&#x2f;ZC&#x2f;-1&#x27;&#xd;&#xa;           AND BRAND_ID IS NOT NULL&#xd;&#xa;           AND SUBSTR&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,1,4&#x29; &#x3e;&#x3d; YEAR&#x28;ADD_MONTHS&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,-12&#x29;&#x29;&#xd;&#xa;           AND SUBSTR&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,1,4&#x29; &#x3c;&#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;&#x29;&#xd;&#xa;           AND SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3c;&#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;         GROUP BY REGEXP_REPLACE&#x28;SUBSTRING&#x28;ARRIVAL_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; S&#xd;&#xa; WHERE PD.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;PD.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;   AND PD.PERIOD_WID &#x3e;&#x3d; NVL&#x28;S.PERIOD_WID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29;&#xd;&#xa; GROUP BY PD.PERIOD_WID&#xd;&#xa;         ,S.ORG_TYPE&#xd;&#xa;         ,S.OC_ID&#xd;&#xa;         ,S.OC_NAME&#xd;&#xa;         ,S.RGN_ID&#xd;&#xa;         ,S.RGN_NAME&#xd;&#xa;         ,S.BRAND_ID&#xd;&#xa;         ,S.BRAND_NAME&#xd;&#xa;&#x3b;</sql>
