      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_FIN_TPBUDGET_D	&#x8fd0;&#x8f93;&#x8d22;&#x52a1;&#x9884;&#x7b97;&#x65e5;&#x660e;&#x7ec6;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;05_DMK&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_DP_DELIVER_ARRIVE_D PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT PERIOD_WID&#xd;&#xa;      ,ORG_TYPE&#xd;&#xa;      ,OC_ID&#xd;&#xa;      ,OC_NAME&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,RGN_NAME&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,BRAND_NAME&#xd;&#xa;      ,ARRIVAL_QTY_M AS FACT_DELIVER_QTY&#xd;&#xa;      ,ARRIVAL_TAR_QTY_M AS PLAN_DELIVER_QTY&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;ARRIVAL_QTY_M, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          ARRIVAL_QTY_M &#x2f; ARRIVAL_TAR_QTY_M&#xd;&#xa;       END AS DELIVER_RATE&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;ARRIVAL_QTY_LYM, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;ARRIVAL_QTY_M - ARRIVAL_QTY_LYM&#x29; &#x2f; ARRIVAL_QTY_LYM&#xd;&#xa;       END AS DELIVER_RATE_YOY&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;ARRIVAL_QTY_LM, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;ARRIVAL_QTY_M - ARRIVAL_QTY_LM&#x29; &#x2f; ARRIVAL_QTY_LM&#xd;&#xa;       END AS DELIVER_RATE_MOM&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM TEMP.TEMP_DMR_OV_DP_DELIVER_ARRIVE_D_1&#x3b;&#xd;&#xa;</sql>
