    <entry>
      <name>TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_1&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_DP_DELIVER_ARRIVE_D_1&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;      ,&#x27;O&#x27; AS ORG_TYPE&#xd;&#xa;      ,&#x27;CMAL&#x2f;ZC&#x27; AS OC_ID&#xd;&#xa;      ,&#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27; AS OC_NAME&#xd;&#xa;      ,&#x27;ALL&#x27; AS RGN_ID&#xd;&#xa;      ,&#x27;&#x5168;&#x90e8;&#x27; AS RGN_NAME&#xd;&#xa;      ,&#x27;ALL&#x27; AS BRAND_ID&#xd;&#xa;      ,&#x27;&#x5168;&#x90e8;&#x27; AS BRAND_NAME&#xd;&#xa;      ,SUM&#x28;A.ARRIVAL_QTY_M&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;A.PERIOD_WID, 1, 6&#x29; ORDER BY A.PERIOD_WID&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;      ,SUM&#x28;A.ARRIVAL_TAR_QTY_M&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;A.PERIOD_WID, 1, 6&#x29; ORDER BY A.PERIOD_WID&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;      ,B.ARRIVAL_QTY_LM&#xd;&#xa;      ,B.ARRIVAL_TAR_QTY_LM&#xd;&#xa;      ,C.ARRIVAL_QTY_LYM&#xd;&#xa;      ,C.ARRIVAL_TAR_QTY_LYM&#xd;&#xa;  FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;              ,SUM&#x28;T.ARRIVAL_QTY_M&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;              ,SUM&#x28;T.ARRIVAL_TAR_QTY_M&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;          FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                      ,D.PERIOD_DATE&#xd;&#xa;                  FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                 WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                   AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                   AND SUBSTR&#x28;PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;                       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; D&#xd;&#xa;          LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                          ,SUM&#x28;T.DLVR_ONTIME_QTY&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;                          ,SUM&#x28;T.DLVR_PLAN_QTY&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;                      FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                     WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29;&#xd;&#xa;                       AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                       AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;                     GROUP BY T.PERIOD_WID&#x29; AS T&#xd;&#xa;            ON D.PERIOD_WID &#x3d; T.PERIOD_WID&#xd;&#xa;         GROUP BY D.PERIOD_WID&#x29; AS A&#xd;&#xa;  LEFT JOIN &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PERIOD_WID&#xd;&#xa;                   ,SUM&#x28;T.DLVR_ONTIME_QTY&#x29; AS ARRIVAL_QTY_LM&#xd;&#xa;                   ,SUM&#x28;T.DLVR_PLAN_QTY&#x29; AS ARRIVAL_TAR_QTY_LM&#xd;&#xa;               FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                           1,&#xd;&#xa;                           6&#x29;&#xd;&#xa;                AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#x29; AS B&#xd;&#xa;    ON SUBSTR&#x28;A.PERIOD_WID, 1, 6&#x29; &#x3d; SUBSTR&#x28;B.PERIOD_WID, 1, 6&#x29;&#xd;&#xa;  LEFT JOIN &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PERIOD_WID&#xd;&#xa;                   ,SUM&#x28;T.DLVR_ONTIME_QTY&#x29; AS ARRIVAL_QTY_LYM&#xd;&#xa;                   ,SUM&#x28;T.DLVR_PLAN_QTY&#x29; AS ARRIVAL_TAR_QTY_LYM&#xd;&#xa;               FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                           1,&#xd;&#xa;                           6&#x29;&#xd;&#xa;                AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#x29; AS C&#xd;&#xa;    ON SUBSTR&#x28;A.PERIOD_WID, 1, 6&#x29; &#x3d; SUBSTR&#x28;C.PERIOD_WID, 1, 6&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;      ,&#x27;R&#x27; AS ORG_TYPE&#xd;&#xa;      ,&#x27;CMAL&#x2f;ZC&#x27; AS OC_ID&#xd;&#xa;      ,&#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27; AS OC_NAME&#xd;&#xa;      ,A.RGN_ID&#xd;&#xa;      ,A.RGN_NAME&#xd;&#xa;      ,A.BRAND_ID&#xd;&#xa;      ,A.BRAND_NAME&#xd;&#xa;      ,SUM&#x28;A.ARRIVAL_QTY_M&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;A.PERIOD_WID, 1, 6&#x29;, A.RGN_ID, A.RGN_NAME, A.BRAND_ID, A.BRAND_NAME ORDER BY A.PERIOD_WID&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;      ,SUM&#x28;A.ARRIVAL_TAR_QTY_M&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;A.PERIOD_WID, 1, 6&#x29;, A.RGN_ID, A.RGN_NAME, A.BRAND_ID, A.BRAND_NAME ORDER BY A.PERIOD_WID&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;      ,B.ARRIVAL_QTY_LM&#xd;&#xa;      ,B.ARRIVAL_TAR_QTY_LM&#xd;&#xa;      ,C.ARRIVAL_QTY_LYM&#xd;&#xa;      ,C.ARRIVAL_TAR_QTY_LYM&#xd;&#xa;  FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;              ,D.RGN_ID&#xd;&#xa;              ,D.RGN_NAME&#xd;&#xa;              ,D.BRAND_ID&#xd;&#xa;              ,D.BRAND_NAME&#xd;&#xa;              ,SUM&#x28;T.ARRIVAL_QTY_M&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;              ,SUM&#x28;T.ARRIVAL_TAR_QTY_M&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;          FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                      ,T.RGN_ID&#xd;&#xa;                      ,T.RGN_NAME&#xd;&#xa;                      ,T.BRAND_ID&#xd;&#xa;                      ,T.BRAND_NAME&#xd;&#xa;                  FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                              ,D.PERIOD_DATE&#xd;&#xa;                          FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                         WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                           AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      6&#x29;&#xd;&#xa;                           AND SUBSTR&#x28;PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;                               FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; D&#xd;&#xa;                  JOIN &#x28;SELECT T.RGN_ID&#xd;&#xa;                             ,O.RGN_NAME&#xd;&#xa;                             ,T.BRAND_ID&#xd;&#xa;                             ,BR.BRAND_NAME&#xd;&#xa;                         FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                         LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;                           ON T.RGN_ID &#x3d; O.ORG_ID&#xd;&#xa;                          AND O.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                         LEFT JOIN DIM.DIM_BRAND BR&#xd;&#xa;                           ON T.BRAND_ID &#x3d; BR.BRAND_ID&#xd;&#xa;                        WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                              SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                     1,&#xd;&#xa;                                     6&#x29;&#xd;&#xa;                          AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                          AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;                        GROUP BY T.RGN_ID&#xd;&#xa;                                ,O.RGN_NAME&#xd;&#xa;                                ,T.BRAND_ID&#xd;&#xa;                                ,BR.BRAND_NAME&#x29; T&#xd;&#xa;                    ON 1 &#x3d; 1&#x29; AS D&#xd;&#xa;          LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                          ,T.RGN_ID&#xd;&#xa;                          ,O.RGN_NAME&#xd;&#xa;                          ,T.BRAND_ID&#xd;&#xa;                          ,BR.BRAND_NAME&#xd;&#xa;                          ,SUM&#x28;T.ARRIVE_ONTIME_QTY&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;                          ,SUM&#x28;T.ARRIVE_PLAN_QTY&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;                      FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                      LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;                        ON T.RGN_ID &#x3d; O.ORG_ID&#xd;&#xa;                       AND O.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                      LEFT JOIN DIM.DIM_BRAND BR&#xd;&#xa;                        ON T.BRAND_ID &#x3d; BR.BRAND_ID&#xd;&#xa;                     WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29;&#xd;&#xa;                       AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                       AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;                     GROUP BY T.PERIOD_WID&#xd;&#xa;                             ,T.RGN_ID&#xd;&#xa;                             ,O.RGN_NAME&#xd;&#xa;                             ,T.BRAND_ID&#xd;&#xa;                             ,BR.BRAND_NAME&#x29; AS T&#xd;&#xa;            ON D.PERIOD_WID &#x3d; T.PERIOD_WID&#xd;&#xa;           AND D.RGN_ID &#x3d; T.RGN_ID&#xd;&#xa;           AND D.RGN_NAME &#x3d; T.RGN_NAME&#xd;&#xa;           AND D.BRAND_ID &#x3d; T.BRAND_ID&#xd;&#xa;           AND D.BRAND_NAME &#x3d; T.BRAND_NAME&#xd;&#xa;         GROUP BY D.PERIOD_WID&#xd;&#xa;                 ,D.RGN_ID&#xd;&#xa;                 ,D.RGN_NAME&#xd;&#xa;                 ,D.BRAND_ID&#xd;&#xa;                 ,D.BRAND_NAME&#x29; AS A&#xd;&#xa;  LEFT JOIN &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.RGN_ID&#xd;&#xa;                   ,O.RGN_NAME&#xd;&#xa;                   ,T.BRAND_ID&#xd;&#xa;                   ,BR.BRAND_NAME&#xd;&#xa;                   ,SUM&#x28;T.ARRIVE_ONTIME_QTY&#x29; AS ARRIVAL_QTY_LM&#xd;&#xa;                   ,SUM&#x28;T.ARRIVE_PLAN_QTY&#x29; AS ARRIVAL_TAR_QTY_LM&#xd;&#xa;               FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;               LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;                 ON T.RGN_ID &#x3d; O.ORG_ID&#xd;&#xa;                AND O.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;               LEFT JOIN DIM.DIM_BRAND BR&#xd;&#xa;                 ON T.BRAND_ID &#x3d; BR.BRAND_ID&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                           1,&#xd;&#xa;                           6&#x29;&#xd;&#xa;                AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;              GROUP BY T.RGN_ID&#xd;&#xa;                      ,O.RGN_NAME&#xd;&#xa;                      ,T.BRAND_ID&#xd;&#xa;                      ,BR.BRAND_NAME&#x29; AS B&#xd;&#xa;    ON SUBSTR&#x28;A.PERIOD_WID, 1, 6&#x29; &#x3d; SUBSTR&#x28;B.PERIOD_WID, 1, 6&#x29;&#xd;&#xa;   AND A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;   AND A.RGN_NAME &#x3d; B.RGN_NAME&#xd;&#xa;   AND A.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;   AND A.BRAND_NAME &#x3d; B.BRAND_NAME&#xd;&#xa;  LEFT JOIN &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.RGN_ID&#xd;&#xa;                   ,O.RGN_NAME&#xd;&#xa;                   ,T.BRAND_ID&#xd;&#xa;                   ,BR.BRAND_NAME&#xd;&#xa;                   ,SUM&#x28;T.ARRIVE_ONTIME_QTY&#x29; AS ARRIVAL_QTY_LYM&#xd;&#xa;                   ,SUM&#x28;T.ARRIVE_PLAN_QTY&#x29; AS ARRIVAL_TAR_QTY_LYM&#xd;&#xa;               FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;               LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;                 ON T.RGN_ID &#x3d; O.ORG_ID&#xd;&#xa;                AND O.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;               LEFT JOIN DIM.DIM_BRAND BR&#xd;&#xa;                 ON T.BRAND_ID &#x3d; BR.BRAND_ID&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                           1,&#xd;&#xa;                           6&#x29;&#xd;&#xa;                AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;              GROUP BY T.RGN_ID&#xd;&#xa;                      ,O.RGN_NAME&#xd;&#xa;                      ,T.BRAND_ID&#xd;&#xa;                      ,BR.BRAND_NAME&#x29; AS C&#xd;&#xa;    ON SUBSTR&#x28;A.PERIOD_WID, 1, 6&#x29; &#x3d; SUBSTR&#x28;C.PERIOD_WID, 1, 6&#x29;&#xd;&#xa;   AND A.RGN_ID &#x3d; C.RGN_ID&#xd;&#xa;   AND A.RGN_NAME &#x3d; C.RGN_NAME&#xd;&#xa;   AND A.BRAND_ID &#x3d; C.BRAND_ID&#xd;&#xa;   AND A.BRAND_NAME &#x3d; C.BRAND_NAME&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
