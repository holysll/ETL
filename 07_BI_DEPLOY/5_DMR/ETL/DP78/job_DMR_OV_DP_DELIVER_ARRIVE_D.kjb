    <entry>
      <name>TEMP_4</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_4&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_DP_DELIVER_ARRIVE_D_4&#xd;&#xa;select &#xd;&#xa;pd.period_wid, s.org_type, s.oc_id, oc.oc_name, s.rgn_id, rgn.rgn_name, s.brand_id, brand.brand_name, sum&#x28;arrival_qty_m&#x29; as arrival_qty_m,&#xd;&#xa;sum&#x28;arrival_tar_qty_m&#x29; as arrival_tar_qty_m--, sum&#x28;arrival_qty_m&#x29;&#x2f;sum&#x28;arrival_tar_qty_m&#x29; as rate&#xd;&#xa;FROM DIM.DIM_PERIOD_D PD&#xd;&#xa;join&#x28;&#xd;&#xa;SELECT T5.period_wid,&#x27;R&#x27; as org_type, T5.OC_ID, T5.rgn_id, &#x27;CA&#x27; as brand_id,&#xd;&#xa;SUM&#x28;T5.ONTIME_ARRIVE_NUM&#x29;&#x2b;SUM&#x28;T5.ADVANCE_ARRIVE_NUM&#x29; as ARRIVAL_QTY_M,&#xd;&#xa;SUM&#x28;T5.TOTAL_ARRIVE_NUM&#x29; as arrival_tar_qty_m&#xd;&#xa;                   ,CASE WHEN SUM&#x28;T5.TOTAL_ARRIVE_NUM&#x29; &#x3d; 0 THEN 0&#xd;&#xa;                    ELSE &#x28;SUM&#x28;T5.ONTIME_ARRIVE_NUM&#x29;&#x2b;SUM&#x28;T5.ADVANCE_ARRIVE_NUM&#x29;&#x29; &#x2f; SUM&#x28;T5.TOTAL_ARRIVE_NUM&#x29; &#x2a;100 END AS ONTIME_RATE&#xd;&#xa;               FROM DMK.DMK_TP_ARRIVE_D T5&#xd;&#xa;              WHERE T5.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;                AND T5.PART_DT  &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T5.period_wid, T5.OC_ID,T5.rgn_id&#xd;&#xa;union all&#xd;&#xa;SELECT T6.period_wid,&#x27;R&#x27; as org_type, T6.OC_ID, T6.rgn_id, &#x27;FT&#x27; as brand_id,&#xd;&#xa;SUM&#x28;T6.ONTIME_ARRIVE_NUM&#x29;&#x2b;SUM&#x28;T6.ADVANCE_ARRIVE_NUM&#x29; as ARRIVAL_QTY_M,&#xd;&#xa;SUM&#x28;T6.TOTAL_ARRIVE_NUM&#x29; as arrival_tar_qty_m&#xd;&#xa;                   ,CASE WHEN SUM&#x28;T6.TOTAL_ARRIVE_NUM&#x29; &#x3d; 0 THEN 0&#xd;&#xa;                    ELSE &#x28;SUM&#x28;T6.ONTIME_ARRIVE_NUM&#x29;&#x2b;SUM&#x28;T6.ADVANCE_ARRIVE_NUM&#x29;&#x29; &#x2f; SUM&#x28;T6.TOTAL_ARRIVE_NUM&#x29; &#x2a;100 END AS ONTIME_RATE&#xd;&#xa;               FROM DMK.DMK_TP_ARRIVE_D T6&#xd;&#xa;              WHERE T6.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;                AND T6.PART_DT  &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T6.period_wid, T6.OC_ID, T6.rgn_id&#xd;&#xa;&#x29;s&#xd;&#xa;left join &#x28;select oc_id, oc_name from dim.dim_org where org_type &#x3d; &#x27;O&#x27; and enable_flag &#x3d; &#x27;T&#x27;&#x29; oc&#xd;&#xa;on oc.oc_id &#x3d; s.oc_id&#xd;&#xa;left join &#x28;select rgn_id, rgn_name from dim.dim_org where org_type &#x3d; &#x27;R&#x27; and enable_flag &#x3d; &#x27;T&#x27;&#x29; rgn&#xd;&#xa;on rgn.rgn_id &#x3d; s.rgn_id&#xd;&#xa;left join &#x28;select distinct brand_id, brand_name from dim.dim_brand where enable_flag &#x3d; &#x27;T&#x27;&#x29; brand&#xd;&#xa;on brand.brand_id &#x3d; s.brand_id&#xd;&#xa;WHERE PD.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;PD.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;   AND PD.PERIOD_WID &#x3e;&#x3d; NVL&#x28;S.PERIOD_WID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;   --and pd.period_wid &#x3d; 20180731&#xd;&#xa;   group by pd.period_wid, s.org_type, s.oc_id, oc.oc_name, s.rgn_id, rgn.rgn_name, s.brand_id, brand.brand_name&#xd;&#xa;union all&#xd;&#xa;select &#xd;&#xa;pd.period_wid, s.org_type, &#x27;-1&#x27; as oc_id, &#x27;-1&#x27; as oc_name, &#x27;-1&#x27; as rgn_id, &#x27;-1&#x27; as rgn_name, s.brand_id, brand.brand_name, sum&#x28;arrival_qty_m&#x29; as arrival_qty_m,&#xd;&#xa;sum&#x28;arrival_tar_qty_m&#x29; as arrival_tar_qty_m--, sum&#x28;arrival_qty_m&#x29;&#x2f;sum&#x28;arrival_tar_qty_m&#x29; as rate&#xd;&#xa;FROM DIM.DIM_PERIOD_D PD&#xd;&#xa;join&#x28;&#xd;&#xa;SELECT T5.period_wid,&#x27;R&#x27; as org_type, T5.OC_ID, T5.rgn_id, &#x27;CA&#x27; as brand_id,&#xd;&#xa;SUM&#x28;T5.ONTIME_ARRIVE_NUM&#x29;&#x2b;SUM&#x28;T5.ADVANCE_ARRIVE_NUM&#x29; as ARRIVAL_QTY_M,&#xd;&#xa;SUM&#x28;T5.TOTAL_ARRIVE_NUM&#x29; as arrival_tar_qty_m&#xd;&#xa;                   ,CASE WHEN SUM&#x28;T5.TOTAL_ARRIVE_NUM&#x29; &#x3d; 0 THEN 0&#xd;&#xa;                    ELSE &#x28;SUM&#x28;T5.ONTIME_ARRIVE_NUM&#x29;&#x2b;SUM&#x28;T5.ADVANCE_ARRIVE_NUM&#x29;&#x29; &#x2f; SUM&#x28;T5.TOTAL_ARRIVE_NUM&#x29; &#x2a;100 END AS ONTIME_RATE&#xd;&#xa;               FROM DMK.DMK_TP_ARRIVE_D T5&#xd;&#xa;              WHERE T5.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;                AND T5.PART_DT  &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T5.period_wid, T5.OC_ID,T5.rgn_id&#xd;&#xa;union all&#xd;&#xa;SELECT T6.period_wid,&#x27;R&#x27; as org_type, T6.OC_ID, T6.rgn_id, &#x27;FT&#x27; as brand_id,&#xd;&#xa;SUM&#x28;T6.ONTIME_ARRIVE_NUM&#x29;&#x2b;SUM&#x28;T6.ADVANCE_ARRIVE_NUM&#x29; as ARRIVAL_QTY_M,&#xd;&#xa;SUM&#x28;T6.TOTAL_ARRIVE_NUM&#x29; as arrival_tar_qty_m&#xd;&#xa;                   ,CASE WHEN SUM&#x28;T6.TOTAL_ARRIVE_NUM&#x29; &#x3d; 0 THEN 0&#xd;&#xa;                    ELSE &#x28;SUM&#x28;T6.ONTIME_ARRIVE_NUM&#x29;&#x2b;SUM&#x28;T6.ADVANCE_ARRIVE_NUM&#x29;&#x29; &#x2f; SUM&#x28;T6.TOTAL_ARRIVE_NUM&#x29; &#x2a;100 END AS ONTIME_RATE&#xd;&#xa;               FROM DMK.DMK_TP_ARRIVE_D T6&#xd;&#xa;              WHERE T6.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;                AND T6.PART_DT  &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T6.period_wid, T6.OC_ID, T6.rgn_id&#xd;&#xa;&#x29;s&#xd;&#xa;left join &#x28;select oc_id, oc_name from dim.dim_org where org_type &#x3d; &#x27;O&#x27; and enable_flag &#x3d; &#x27;T&#x27;&#x29; oc&#xd;&#xa;on oc.oc_id &#x3d; s.oc_id&#xd;&#xa;left join &#x28;select rgn_id, rgn_name from dim.dim_org where org_type &#x3d; &#x27;R&#x27; and enable_flag &#x3d; &#x27;T&#x27;&#x29; rgn&#xd;&#xa;on rgn.rgn_id &#x3d; s.rgn_id&#xd;&#xa;left join &#x28;select distinct brand_id, brand_name from dim.dim_brand where enable_flag &#x3d; &#x27;T&#x27;&#x29; brand&#xd;&#xa;on brand.brand_id &#x3d; s.brand_id&#xd;&#xa;WHERE PD.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;PD.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;   AND PD.PERIOD_WID &#x3e;&#x3d; NVL&#x28;S.PERIOD_WID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;   --and pd.period_wid &#x3d; 20180731&#xd;&#xa;   group by pd.period_wid, s.org_type, s.brand_id, brand.brand_name&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT PD.PERIOD_WID&#xd;&#xa;      ,S.ORG_TYPE&#xd;&#xa;      ,S.OC_ID&#xd;&#xa;      ,S.OC_NAME&#xd;&#xa;      ,S.RGN_ID&#xd;&#xa;      ,S.RGN_NAME&#xd;&#xa;      ,S.BRAND_ID&#xd;&#xa;      ,S.BRAND_NAME&#xd;&#xa;      ,SUM&#x28;S.ARRIVAL_QTY_M&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;      ,SUM&#x28;S.ARRIVAL_TAR_QTY_M&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;  FROM DIM.DIM_PERIOD_D PD&#xd;&#xa;  JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;ARRIVAL_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,&#x27;R&#x27; AS ORG_TYPE&#xd;&#xa;              ,T.OC_ID&#xd;&#xa;              ,O.OC_NAME&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,R.RGN_NAME&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,B.BRAND_NAME&#xd;&#xa;              ,SUM&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#xd;&#xa;                               &#x3e;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;                        AND SUBSTR&#x28;ARRIVAL_DAY,1,7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29; THEN 1 ELSE 0 END&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;              ,COUNT&#x28;1&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;          FROM DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;         LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.OC_ID &#x3d; O.OC_ID&#xd;&#xa;   AND O.ORG_TYPE &#x3d; &#x27;O&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG R&#xd;&#xa;    ON T.RGN_ID &#x3d; R.RGN_ID&#xd;&#xa;   AND R.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_BRAND B&#xd;&#xa;    ON T.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;         WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;               &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;           AND SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;         GROUP BY REGEXP_REPLACE&#x28;SUBSTRING&#x28;ARRIVAL_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;               T.OC_ID&#xd;&#xa;              ,O.OC_NAME&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,R.RGN_NAME&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,B.BRAND_NAME&#xd;&#xa;              &#x29; S&#xd;&#xa; WHERE PD.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;PD.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;   AND PD.PERIOD_WID &#x3e;&#x3d; NVL&#x28;S.PERIOD_WID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29;&#xd;&#xa; GROUP BY PD.PERIOD_WID&#xd;&#xa;         ,S.ORG_TYPE&#xd;&#xa;         ,S.OC_ID&#xd;&#xa;         ,S.OC_NAME&#xd;&#xa;         ,S.RGN_ID&#xd;&#xa;         ,S.RGN_NAME&#xd;&#xa;         ,S.BRAND_ID&#xd;&#xa;         ,S.BRAND_NAME&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT PD.PERIOD_WID&#xd;&#xa;      ,S.ORG_TYPE&#xd;&#xa;      ,&#x27;-1&#x27; AS OC_ID&#xd;&#xa;      ,&#x27;-1&#x27; AS OC_NAME&#xd;&#xa;      ,&#x27;-1&#x27; AS RGN_ID&#xd;&#xa;      ,&#x27;-1&#x27; AS RGN_NAME&#xd;&#xa;      ,S.BRAND_ID&#xd;&#xa;      ,S.BRAND_NAME&#xd;&#xa;      ,SUM&#x28;S.ARRIVAL_QTY_M&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;      ,SUM&#x28;S.ARRIVAL_TAR_QTY_M&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;  FROM DIM.DIM_PERIOD_D PD&#xd;&#xa;  JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;ARRIVAL_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,&#x27;R&#x27; AS ORG_TYPE&#xd;&#xa;              ,T.OC_ID&#xd;&#xa;              ,O.OC_NAME&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,R.RGN_NAME&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,B.BRAND_NAME&#xd;&#xa;              ,SUM&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#xd;&#xa;                               &#x3e;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;                        AND SUBSTR&#x28;ARRIVAL_DAY,1,7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29; THEN 1 ELSE 0 END&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;              ,COUNT&#x28;1&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;          FROM DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;         LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.OC_ID &#x3d; O.OC_ID&#xd;&#xa;   AND O.ORG_TYPE &#x3d; &#x27;O&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG R&#xd;&#xa;    ON T.RGN_ID &#x3d; R.RGN_ID&#xd;&#xa;   AND R.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_BRAND B&#xd;&#xa;    ON T.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;         WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;               &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;           AND SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;         GROUP BY REGEXP_REPLACE&#x28;SUBSTRING&#x28;ARRIVAL_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;               T.OC_ID&#xd;&#xa;              ,O.OC_NAME&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,R.RGN_NAME&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,B.BRAND_NAME&#xd;&#xa;              &#x29; S&#xd;&#xa; WHERE PD.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;PD.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;   AND PD.PERIOD_WID &#x3e;&#x3d; NVL&#x28;S.PERIOD_WID, CONCAT&#x28;&#x27;hive&#x27;, RAND&#x28;&#x29;&#x29;&#x29;&#xd;&#xa; GROUP BY PD.PERIOD_WID&#xd;&#xa;         ,S.ORG_TYPE&#xd;&#xa;         ,S.BRAND_ID&#xd;&#xa;         ,S.BRAND_NAME&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;&#x2f;&#x2a;SELECT T.PERIOD_WID&#xd;&#xa;      ,&#x27;R&#x27; AS ORG_TYPE&#xd;&#xa;      ,T.OC_ID&#xd;&#xa;      ,O.OC_NAME&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,R.RGN_NAME&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,B.BRAND_NAME&#xd;&#xa;      ,SUM&#x28;ONTIME_ARRIVE_NUM&#x29; AS FACT_DELIVER_QTY&#xd;&#xa;      ,SUM&#x28;TOTAL_ARRIVE_NUM&#x29; AS PLAN_DELIVER_QTY&#xd;&#xa;  FROM DMK.DMK_TP_ARRIVE_D T&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.OC_ID &#x3d; O.OC_ID&#xd;&#xa;   AND O.ORG_TYPE &#x3d; &#x27;O&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG R&#xd;&#xa;    ON T.RGN_ID &#x3d; R.RGN_ID&#xd;&#xa;   AND R.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_BRAND B&#xd;&#xa;    ON T.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa; WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa; GROUP BY T.PERIOD_WID&#xd;&#xa;         ,T.OC_ID&#xd;&#xa;         ,O.OC_NAME&#xd;&#xa;         ,T.RGN_ID&#xd;&#xa;         ,R.RGN_NAME&#xd;&#xa;         ,T.BRAND_ID&#xd;&#xa;         ,B.BRAND_NAME&#x3b;&#x2a;&#x2f;</sql>
