      <name>CREATE_TEMP_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Udf&#xd;&#xa; &#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_OV_DP_DELIVER_ARRIVE_D_1&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_OV_DP_DELIVER_ARRIVE_D_1&#x28;&#xd;&#xa;PERIOD_WID                    BIGINT                             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;ORG_TYPE                      STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;&#x7c7b;&#x578b;&#x27;,&#xd;&#xa;OC_ID                         STRING                             COMMENT  &#x27;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#x27;,&#xd;&#xa;OC_NAME                       STRING                             COMMENT  &#x27;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27;,&#xd;&#xa;RGN_ID                        STRING                             COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;RGN_NAME                      STRING                             COMMENT  &#x27;&#x5927;&#x533a;&#x27;,&#xd;&#xa;BRAND_ID                      STRING                             COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;BRAND_NAME                    STRING                             COMMENT  &#x27;&#x54c1;&#x724c;&#x27;,&#xd;&#xa;ARRIVAL_QTY_M                 DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5b9e;&#x9645;&#x503c;&#x2f;&#x51c6;&#x5230;&#x503c; &#x6708;&#x27;,&#xd;&#xa;ARRIVAL_TAR_QTY_M             DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x8ba1;&#x5212;&#x503c;&#x2f;&#x603b;&#x5230;&#x503c; &#x6708;&#x27;,&#xd;&#xa;ARRIVAL_QTY_LM                DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5b9e;&#x9645;&#x503c;&#x2f;&#x51c6;&#x5230;&#x503c; &#x4e0a;&#x6708;&#x27;,&#xd;&#xa;ARRIVAL_TAR_QTY_LM            DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x8ba1;&#x5212;&#x503c;&#x2f;&#x603b;&#x5230;&#x503c; &#x4e0a;&#x6708;&#x27;,&#xd;&#xa;ARRIVAL_QTY_LYM               DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5b9e;&#x9645;&#x503c;&#x2f;&#x51c6;&#x5230;&#x503c; &#x540c;&#x671f;&#x27;,&#xd;&#xa;ARRIVAL_TAR_QTY_LYM           DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x8ba1;&#x5212;&#x503c;&#x2f;&#x603b;&#x5230;&#x503c; &#x540c;&#x671f;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4ea4;&#x4ed8;&#x4e0e;&#x51c6;&#x5230;-&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x5b8c;&#x6210;&#x7387;-&#x672c;&#x671f;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_OV_DP_DELIVER_ARRIVE_D_2&#x28;&#xd;&#xa;PERIOD_WID                    BIGINT                             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;ORG_TYPE                      STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;&#x7c7b;&#x578b;&#x27;,&#xd;&#xa;OC_ID                         STRING                             COMMENT  &#x27;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#x27;,&#xd;&#xa;OC_NAME                       STRING                             COMMENT  &#x27;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27;,&#xd;&#xa;RGN_ID                        STRING                             COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;RGN_NAME                      STRING                             COMMENT  &#x27;&#x5927;&#x533a;&#x27;,&#xd;&#xa;BRAND_ID                      STRING                             COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;BRAND_NAME                    STRING                             COMMENT  &#x27;&#x54c1;&#x724c;&#x27;,&#xd;&#xa;FACT_DELIVER_QTY              DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5b9e;&#x9645;&#x503c;&#x2f;&#x51c6;&#x5230;&#x503c;&#x27;,&#xd;&#xa;PLAN_DELIVER_QTY              DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x8ba1;&#x5212;&#x503c;&#x2f;&#x603b;&#x5230;&#x503c;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4ea4;&#x4ed8;&#x4e0e;&#x51c6;&#x5230;-&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x5b8c;&#x6210;&#x7387;-&#x4e0a;&#x671f;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_OV_DP_DELIVER_ARRIVE_D_3&#x28;&#xd;&#xa;PERIOD_WID                    BIGINT                             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;ORG_TYPE                      STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;&#x7c7b;&#x578b;&#x27;,&#xd;&#xa;OC_ID                         STRING                             COMMENT  &#x27;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#x27;,&#xd;&#xa;OC_NAME                       STRING                             COMMENT  &#x27;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27;,&#xd;&#xa;RGN_ID                        STRING                             COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;RGN_NAME                      STRING                             COMMENT  &#x27;&#x5927;&#x533a;&#x27;,&#xd;&#xa;BRAND_ID                      STRING                             COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;BRAND_NAME                    STRING                             COMMENT  &#x27;&#x54c1;&#x724c;&#x27;,&#xd;&#xa;FACT_DELIVER_QTY              DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5b9e;&#x9645;&#x503c;&#x2f;&#x51c6;&#x5230;&#x503c;&#x27;,&#xd;&#xa;PLAN_DELIVER_QTY              DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x8ba1;&#x5212;&#x503c;&#x2f;&#x603b;&#x5230;&#x503c;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4ea4;&#x4ed8;&#x4e0e;&#x51c6;&#x5230;-&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x5b8c;&#x6210;&#x7387;-&#x540c;&#x671f;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_OV_DP_DELIVER_ARRIVE_D_4&#x28;&#xd;&#xa;PERIOD_WID                    BIGINT                             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;ORG_TYPE                      STRING                             COMMENT  &#x27;&#x7ec4;&#x7ec7;&#x7c7b;&#x578b;&#x27;,&#xd;&#xa;OC_ID                         STRING                             COMMENT  &#x27;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#x27;,&#xd;&#xa;OC_NAME                       STRING                             COMMENT  &#x27;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#x27;,&#xd;&#xa;RGN_ID                        STRING                             COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;RGN_NAME                      STRING                             COMMENT  &#x27;&#x5927;&#x533a;&#x27;,&#xd;&#xa;BRAND_ID                      STRING                             COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;BRAND_NAME                    STRING                             COMMENT  &#x27;&#x54c1;&#x724c;&#x27;,&#xd;&#xa;FACT_DELIVER_QTY              DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x5b9e;&#x9645;&#x503c;&#x2f;&#x51c6;&#x5230;&#x503c;&#x27;,&#xd;&#xa;PLAN_DELIVER_QTY              DECIMAL&#x28;32,8&#x29;                      COMMENT  &#x27;&#x8ba1;&#x5212;&#x503c;&#x2f;&#x603b;&#x5230;&#x503c;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4ea4;&#x4ed8;&#x4e0e;&#x51c6;&#x5230;-&#x5927;&#x533a;&#x51c6;&#x5230;&#x7387;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;</sql>
