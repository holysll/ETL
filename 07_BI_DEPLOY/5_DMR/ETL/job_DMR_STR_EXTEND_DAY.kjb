      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_STR_EXTEND_DAY_06&#xd;&#xa;SELECT PERIOD_WID&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,STR_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,MODEL_ID&#xd;&#xa;      ,&#x27;&#x8054;&#x8fd0;&#x27; AS INDEX_NAME&#xd;&#xa;      ,SUM&#x28;TMP.FENZI&#x29; AS FENZI&#xd;&#xa;      ,SUM&#x28;TMP.FENMU&#x29; AS FENMU&#xd;&#xa;FROM &#x28;SELECT period_wid&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,STR_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,MODEL_ID&#xd;&#xa;      ,0 AS FENZI&#xd;&#xa;      ,SUM&#x28;INSTOCK_QTY&#x29; AS FENMU&#xd;&#xa;FROM DMK.DMK_STR_OPERQTY_D &#xd;&#xa;      WHERE SUBSTRING&#x28;PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;          GROUP BY period_wid&#xd;&#xa;                  ,RGN_ID&#xd;&#xa;                  ,BASE_ID&#xd;&#xa;                  ,STR_ID&#xd;&#xa;                  ,BRAND_ID&#xd;&#xa;                  ,MODEL_ID&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT period_wid&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,SOURCE_LOCATION_ID AS STR_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,MODEL_ID&#xd;&#xa;      ,SUM&#x28;TRANS_QTY&#x29; AS FENZI&#xd;&#xa;      ,SUM&#x28;TRANS_QTY&#x29; AS FENMU&#xd;&#xa; FROM DMK.DMK_TP_SHIPQTY_D &#xd;&#xa;     WHERE SUBSTR&#x28;PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;      GROUP BY period_wid&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,SOURCE_LOCATION_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;&#xd;&#xa;&#x29;  TMP&#xd;&#xa;GROUP BY PERIOD_WID&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,STR_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,MODEL_ID&#xd;&#xa;&#x3b;</sql>
