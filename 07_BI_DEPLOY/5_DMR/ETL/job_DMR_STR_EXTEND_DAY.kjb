      <name>TMP3</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_STR_EXTEND_DAY_03&#xd;&#xa;SELECT REP.REPAIR_START_DAY&#xd;&#xa;      ,NVL&#x28;RGN.RGN_ID,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,NVL&#x28;BAS.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,NULL AS STR_ID&#xd;&#xa;      ,NVL&#x28;BRD.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,NULL AS MODEL_ID&#xd;&#xa;      ,&#x27;&#x8fd4;&#x4fee;&#x27; AS INDEX_NAME&#xd;&#xa;      ,SUM&#x28;REP.REPAIR_DONR_CNT&#x29; AS FENZI&#xd;&#xa;      ,SUM&#x28;REP.REPAIR_CNT&#x29; AS FENMU&#xd;&#xa;  FROM ODS_BLPT.T_HAND_REPAIR REP&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;           ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;          AND REP.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;           ON BAS.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;          AND REP.BASE_NAME &#x3d; BAS.BASE_NAME&#xd;&#xa;    LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;           ON REP.BRAND_NAME &#x3d; BRD.BRAND_NAME&#xd;&#xa;      WHERE SUBSTR&#x28;REPAIR_START_DAY, 1, 6&#x29;   &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;        GROUP BY REP.REPAIR_START_DAY&#xd;&#xa;                ,RGN.RGN_ID&#xd;&#xa;                ,BAS.BASE_ID&#xd;&#xa;                ,BRD.BRAND_ID&#xd;&#xa;&#x3b;</sql>
