      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_STR_EXTEND_DAY_08&#xd;&#xa;SELECT A.PERIOD_WID AS PERIOD_WID&#xd;&#xa;               ,NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#xd;&#xa;               ,NVL&#x28;A.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#xd;&#xa;               ,NVL&#x28;A.STR_ID, &#x27;-1&#x27;&#x29; AS STR_ID&#xd;&#xa;               ,NVL&#x28;A.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;               ,NVL&#x28;A.MODEL_ID, &#x27;-1&#x27;&#x29; AS MODEL_ID&#xd;&#xa;               ,&#x27;&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x27; AS INDEX_NAME&#xd;&#xa;               ,SUM&#x28;OUTSTOCK_ONTIME_QTY&#x29; AS ON_TIME_DESP_QTY&#xd;&#xa;               ,SUM&#x28;OUTSTOCK_PLAN_QTY&#x29; AS ALL_DESP_QTY&#xd;&#xa;           FROM DMK.DMK_STR_OPERQTY_D A&#xd;&#xa;          WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;PERIOD_WID, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;                SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;          GROUP BY A.PERIOD_WID&#xd;&#xa;                  ,NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                  ,NVL&#x28;A.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                  ,NVL&#x28;A.STR_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                  ,NVL&#x28;A.BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                  ,NVL&#x28;A.MODEL_ID, &#x27;-1&#x27;&#x29;&#x3b;&#xd;&#xa;&#x3b;</sql>
