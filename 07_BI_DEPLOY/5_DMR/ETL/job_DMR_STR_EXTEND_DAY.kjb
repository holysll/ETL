      </entry>
    <entry>
      <name>TMP1</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_STR_EXTEND_DAY_01&#xd;&#xa;SELECT T.PERIOD_WID AS PERIOD_WID&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,T.STR_ID&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,T.MODEL_ID&#xd;&#xa;      ,&#x27;&#x5e93;&#x5b58;&#x27; AS INDEX_NAME&#xd;&#xa;      ,SUM&#x28;T.FENZI&#x29; AS FENZI&#xd;&#xa;      ,SUM&#x28;T.FENMU&#x29; AS FENMU&#xd;&#xa;  FROM &#x28;SELECT PERIOD_WID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,STR_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,SUM&#x28;INVENTORY_QTY&#x29; AS FENZI&#xd;&#xa;              ,NULL AS FENMU&#xd;&#xa;          FROM DMK.DMK_STR_INVENTORY_D&#xd;&#xa;         WHERE SUBSTRING&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;         GROUP BY PERIOD_WID&#xd;&#xa;                 ,RGN_ID&#xd;&#xa;                 ,BASE_ID&#xd;&#xa;                 ,STR_ID&#xd;&#xa;                 ,BRAND_ID&#xd;&#xa;                 ,MODEL_ID&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT D.PERIOD_WID&#xd;&#xa;              ,A.RGN_ID&#xd;&#xa;              ,A.BASE_ID&#xd;&#xa;              ,A.STR_ID&#xd;&#xa;              ,A.BRAND_ID&#xd;&#xa;              ,A.MODEL_ID&#xd;&#xa;              ,A.FENZI&#xd;&#xa;              ,A.FENMU&#xd;&#xa;          FROM DIM.DIM_PERIOD_D D&#xd;&#xa;          JOIN &#x28;SELECT T.RGN_ID&#xd;&#xa;                     ,T.BASE_ID&#xd;&#xa;                     ,T.STR_ID&#xd;&#xa;                     ,T.BRAND_ID&#xd;&#xa;                     ,&#x27;-1&#x27; AS MODEL_ID&#xd;&#xa;                     ,NULL AS FENZI&#xd;&#xa;                     ,SUM&#x28;MAX_STORE_QTY&#x29; AS FENMU&#xd;&#xa;                 FROM DMK.DMK_STR_MAXSTORE_M T&#xd;&#xa;                WHERE PART_DT &#x3d;&#xd;&#xa;                      REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                     &#x27;-&#x27;,&#xd;&#xa;                                     &#x27;&#x27;&#x29;&#xd;&#xa;                  AND T.PART_SC &#x3d; &#x27;DIM&#x27;&#xd;&#xa;                GROUP BY T.RGN_ID&#xd;&#xa;                        ,T.BASE_ID&#xd;&#xa;                        ,T.STR_ID&#xd;&#xa;                        ,T.BRAND_ID&#x29; A&#xd;&#xa;            ON 1 &#x3d; 1&#xd;&#xa;         WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;           AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#x29; AS T&#xd;&#xa; GROUP BY T.PERIOD_WID&#xd;&#xa;         ,T.RGN_ID&#xd;&#xa;         ,T.BASE_ID&#xd;&#xa;         ,T.STR_ID&#xd;&#xa;         ,T.BRAND_ID&#xd;&#xa;         ,T.MODEL_ID&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
