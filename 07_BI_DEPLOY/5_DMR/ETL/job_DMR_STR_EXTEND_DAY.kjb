    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_STR_EXTEND_DAY PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;&#xd;&#xa;SELECT TMP.PERIOD_WID AS PERIOD_WID,&#xd;&#xa;       RGN.RGN_ID,&#xd;&#xa;       RGN.RGN_NAME,&#xd;&#xa;       BAS.BASE_ID,&#xd;&#xa;       BAS.BASE_NAME,&#xd;&#xa;       STR.STR_ID,&#xd;&#xa;       STR.STR_NAME,&#xd;&#xa;       BRD.BRAND_ID,&#xd;&#xa;       BRD.BRAND_NAME,&#xd;&#xa;       MDL.MODEL_ID,&#xd;&#xa;       MDL.MODEL_NAME,&#xd;&#xa;       TMP.INDEX_NAME,&#xd;&#xa;       TMP.FENZI,&#xd;&#xa;       TMP.FENMU,&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;&#x29; AS W_INSERT_DT, --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;       &#x27;DMK&#x27;                            AS PART_SC,     --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;       SUBSTRING&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;, 1, 6&#x29; AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM &#x28;SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_EXTEND_DAY_01&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_EXTEND_DAY_02&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_EXTEND_DAY_03&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_EXTEND_DAY_04&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_EXTEND_DAY_05&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_EXTEND_DAY_06&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_EXTEND_DAY_07&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_EXTEND_DAY_08&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_STR_EXTEND_DAY_09&#xd;&#xa;       &#x29; TMP&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;           ON TMP.RGN_ID &#x3d; RGN.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;           ON TMP.BASE_ID &#x3d; BAS.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG STR&#xd;&#xa;           ON STR.ORG_TYPE &#x3d; &#x27;S&#x27;&#xd;&#xa;          AND TMP.STR_ID &#x3d; STR.STR_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;           ON TMP.BRAND_ID &#x3d; BRD.BRAND_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_MODEL MDL&#xd;&#xa;           ON TMP.MODEL_ID &#x3d; MDL.MODEL_ID&#xd;&#xa;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
