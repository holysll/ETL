      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_STR_EXTEND_DAY_07&#xd;&#xa;SELECT T.period_wid&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,STR_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,MODEL_ID&#xd;&#xa;      ,&#x27;&#x51fa;&#x5e93;&#x27; AS INDEX_NAME&#xd;&#xa;      ,sum&#x28;T.outstock_qty&#x29; AS OUT_STORE_QTY&#xd;&#xa;      ,SUM&#x28;T.outstock_plan_qty&#x29; AS OUT_STORE_PLAN_QTY&#xd;&#xa;  FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;T.period_wid,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;        &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;        GROUP BY T.period_wid&#xd;&#xa;                ,RGN_ID&#xd;&#xa;                ,BASE_ID&#xd;&#xa;                ,STR_ID&#xd;&#xa;                ,BRAND_ID&#xd;&#xa;                ,MODEL_ID&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.EARLY_PICKUP_DATE, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b;  86400, &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,SOURCE_LOCATION_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,MODEL_ID&#xd;&#xa;      ,&#x27;&#x51fa;&#x5e93;&#x27; AS INDEX_NAME&#xd;&#xa;      ,SUM&#x28;CASE WHEN SUBSTR&#x28;FACTORY_DAY,1,10&#x29; &#x3d; &#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27; THEN 1 ELSE 0 END&#x29; AS OUT_STORE_QTY&#xd;&#xa;      ,SUM&#x28;1&#x29; AS OUT_STORE_PLAN_QTY&#xd;&#xa;  FROM DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;    WHERE FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.EARLY_PICKUP_DATE, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b;  86400, &#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;        &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;        GROUP BY FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.EARLY_PICKUP_DATE, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x2b;  86400, &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;                ,RGN_ID&#xd;&#xa;                ,BASE_ID&#xd;&#xa;                ,SOURCE_LOCATION_ID&#xd;&#xa;                ,BRAND_ID&#xd;&#xa;                ,MODEL_ID&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2a;&#x2f;</sql>
