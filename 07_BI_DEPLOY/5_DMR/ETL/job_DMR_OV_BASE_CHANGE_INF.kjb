      <name>TO_DMR_BAK</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_BASE_CHANGE_INF PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;&#xd;&#xa;SELECT       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;   AS DATE_ID,        --&#x671f;&#x95f4;ID  8&#x4f4d;&#x65e5;&#x671f;  ETL&#x53c2;&#x6570;&#xd;&#xa;             T2.RGN_NAME                                     AS ST_RGN,              --&#x59cb;&#x53d1;&#x5730;&#x6240;&#x5c5e;&#x5927;&#x533a;&#xd;&#xa;             NVL&#x28;T2.BASE_NAME,T1.BASE_ID&#x29;                    AS ST_BASE_NAME,        --&#x59cb;&#x53d1;&#x57fa;&#x5730;&#x540d;&#x79f0;&#xff0c;&#x540c;&#x4e00;&#x57ce;&#x5e02;&#x540d;&#x79f0;&#x4f18;&#x5148;&#x987a;&#x5e8f; &#x57fa;&#x5730;&#x540d;&#x3001;&#x4e2d;&#x8f6c;&#x7ad9;&#x540d;&#x3001;&#x914d;&#x9001;&#x4e2d;&#x5fc3;&#x540d;&#xd;&#xa;             T1.SOURCE_CTY_ID                                AS SOURCE_CTY_ID,       --&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;&#xd;&#xa;             &#x27;&#x27;                                              AS DEST_ORG_NAME,       --&#x76ee;&#x7684;&#x7ec4;&#x7ec7;&#x540d;&#x79f0;&#xd;&#xa;--            &#x28; CASE WHEN T1.DEST_LOCATION_TYPE &#x3d;&#x27;DLR&#x27; THEN T3.CITY_ID ELSE T1.DEST_CTY_ID  END&#x29;   AS DEST_CTY_ID,      --&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;&#x2f;&#x7ecf;&#x9500;&#x5546;&#x6240;&#x5728;&#x8eab;&#x4efd;&#x7701;&#x4f1a;&#x57ce;&#x5e02;                                                                                    &#xd;&#xa;             T3.CITY_ID AS DEST_CTY_ID,&#xd;&#xa;             SUM&#x28;T1.SHIPPING_NUM&#x29;                            AS CUR_M_SHIP_QTY,      --&#x53d1;&#x8fd0;&#x91cf;&#x6b21;&#xd;&#xa;             T2.BASE_TYPE_IND                                AS ST_BASE_TYPE         -- &#xff08;1-&#x751f;&#x4ea7;&#x57fa;&#x5730;&#xff0c;2-&#x4e2d;&#x8f6c;&#x7ad9;&#x3001;3-&#x57ce;&#x5e02;&#x914d;&#x9001;&#x4e2d;&#x5fc3;&#xff09;&#xd;&#xa;             ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;             ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;    AS PART_DT&#xd;&#xa;             &#xd;&#xa;FROM        DMK.DMK_TP_OPERATE_D T1 --&#x8fd0;&#x8f93;&#x8fd0;&#x8425;&#x91cf;&#x7edf;&#x8ba1;&#xd;&#xa;&#xd;&#xa;LEFT JOIN  &#x28;SELECT BASE_ID,&#xd;&#xa;                   RGN_NAME,&#xd;&#xa;                   BASE_NAME,&#xd;&#xa;                   CITY_ID,&#xd;&#xa;                   CITY_NAME,&#xd;&#xa;                   CASE WHEN  BASE_TYPE&#x3d;&#x27;VDC&#x27; THEN 1&#xd;&#xa;                        WHEN  BASE_TYPE&#x3d;&#x27;VSC&#x27; THEN 2&#xd;&#xa;                        WHEN  BASE_TYPE&#x3d;&#x27;VTC&#x27; THEN 3&#xd;&#xa;                   END AS BASE_TYPE_IND,&#xd;&#xa;                   ROW_NUMBER&#x28;&#x29; OVER &#x28; PARTITION BY CITY_ID ORDER BY &#x28; CASE WHEN  BASE_TYPE&#x3d;&#x27;VDC&#x27; THEN 1&#xd;&#xa;                                                                            WHEN  BASE_TYPE&#x3d;&#x27;VSC&#x27; THEN 2&#xd;&#xa;                                                                            WHEN  BASE_TYPE&#x3d;&#x27;VTC&#x27; THEN 3&#xd;&#xa;                                                                            END&#x29; ASC&#x29;  AS NUM&#xd;&#xa;            FROM  DIM.DIM_ORG&#xd;&#xa;            WHERE ORG_TYPE&#x3d;&#x27;S&#x27;&#xd;&#xa;              AND RGN_NAME &#x3c;&#x3e; &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;              AND STR_TYPE NOT IN &#x28;&#x27;&#x5728;&#x9014;&#x5e93;&#x27;,&#x27;&#x865a;&#x62df;&#x5e93;&#x27;&#x29; &#x29; T2&#xd;&#xa;       ON  T1.SOURCE_CTY_ID &#x3d; T2.CITY_ID&#xd;&#xa;      AND  T2.NUM &#x3d; 1&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;LEFT JOIN  &#x28; SELECT PRVN_ID,PRVN_NAME,CITY_ID,CITY_NAME FROM DIM.DIM_AREA  WHERE AREA_TYPE &#x3d; &#x27;PRVN&#x27; &#x29; T3&#xd;&#xa;       ON  T1.DEST_PRVN_ID &#x3d; T3.PRVN_ID&#xd;&#xa;&#xd;&#xa;WHERE       SUBSTR&#x28;PERIOD_WID,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;      AND   NVL&#x28;T2.BASE_NAME,T1.BASE_ID&#x29; &#x3c;&#x3e; &#x27;-1&#x27;&#xd;&#xa;      AND   T1.SOURCE_CTY_ID IS NOT NULL&#xd;&#xa;      AND   T1.SOURCE_CTY_ID NOT IN &#x28;&#x27;-1&#x27;,&#x27;N&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;GROUP BY    T2.RGN_NAME&#xd;&#xa;           ,NVL&#x28;T2.BASE_NAME,T1.BASE_ID&#x29;&#xd;&#xa;           ,T1.SOURCE_CTY_ID&#xd;&#xa;--           ,&#x28; CASE WHEN T1.DEST_LOCATION_TYPE &#x3d;&#x27;DLR&#x27; THEN T3.CITY_ID ELSE T1.DEST_CTY_ID  END&#x29;&#xd;&#xa;            ,T3.CITY_ID&#xd;&#xa;           ,T2.BASE_TYPE_IND&#xd;&#xa;&#x3b;</sql>
