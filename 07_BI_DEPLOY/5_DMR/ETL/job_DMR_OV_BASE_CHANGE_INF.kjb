    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_BASE_CHANGE_INF PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT A.DATE_ID&#xd;&#xa;      ,A.ST_RGN&#xd;&#xa;      ,A.ST_BASE_NAME&#xd;&#xa;      ,A.ST_CITY_NAME&#xd;&#xa;      ,A.DEST_ORG_NAME&#xd;&#xa;      ,A.DEST_CITY_NAME&#xd;&#xa;      ,MAX&#x28;NVL&#x28;T.CUR_M_SHIP_QTY,0&#x29;&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;A.DATE_ID, 1, 6&#x29;, A.ST_RGN, A.ST_BASE_NAME, A.ST_CITY_NAME, A.DEST_ORG_NAME, A.DEST_CITY_NAME, A.ST_BASE_TYPE ORDER BY A.DATE_ID&#x29; AS CUR_M_SHIP_QTY&#xd;&#xa;      ,A.ST_BASE_TYPE&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM TEMP.TEMP_DMR_OV_BASE_CHANGE_INF_DKE A&#xd;&#xa;  LEFT JOIN TEMP.TEMP_DMR_OV_BASE_CHANGE_INF T&#xd;&#xa;    ON A.DATE_ID  &#x3d; T.DATE_ID&#xd;&#xa;   AND NVL&#x28;A.ST_RGN,&#x27;-1&#x27;&#x29; &#x3d; NVL&#x28;T.ST_RGN,&#x27;-1&#x27;&#x29;&#xd;&#xa;   AND NVL&#x28;A.ST_BASE_NAME,&#x27;-1&#x27;&#x29; &#x3d; NVL&#x28;T.ST_BASE_NAME,&#x27;-1&#x27;&#x29;&#xd;&#xa;   AND NVL&#x28;A.ST_CITY_NAME,&#x27;-1&#x27;&#x29; &#x3d; NVL&#x28;T.ST_CITY_NAME,&#x27;-1&#x27;&#x29;&#xd;&#xa;   AND NVL&#x28;A.DEST_ORG_NAME,&#x27;-1&#x27;&#x29; &#x3d; NVL&#x28;T.DEST_ORG_NAME,&#x27;-1&#x27;&#x29;&#xd;&#xa;   AND NVL&#x28;A.DEST_CITY_NAME,&#x27;-1&#x27;&#x29; &#x3d; NVL&#x28;T.DEST_CITY_NAME,&#x27;-1&#x27;&#x29;&#xd;&#xa;   AND NVL&#x28;A.ST_BASE_TYPE,&#x27;-1&#x27;&#x29; &#x3d; NVL&#x28;T.ST_BASE_TYPE,&#x27;-1&#x27;&#x29; &#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
