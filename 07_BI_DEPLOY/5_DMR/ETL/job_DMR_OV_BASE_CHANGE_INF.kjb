    <entry>
      <name>CREATE_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_OV_BASE_CHANGE_INF&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_OV_BASE_CHANGE_INF&#x28;&#xd;&#xa;DATE_ID                       STRING                             COMMENT  &#x27;&#x65e5;&#x671f;ID&#x27;,&#xd;&#xa;ST_RGN                        STRING                             COMMENT  &#x27;&#x59cb;&#x53d1;&#x5730;&#x6240;&#x5c5e;&#x5927;&#x533a;&#x27;,&#xd;&#xa;ST_BASE_NAME                  STRING                             COMMENT  &#x27;&#x59cb;&#x53d1;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ST_CITY_NAME                  STRING                             COMMENT  &#x27;&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;&#x27;,&#xd;&#xa;DEST_ORG_NAME                 STRING                             COMMENT  &#x27;&#x76ee;&#x7684;&#x7ec4;&#x7ec7;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;DEST_CITY_NAME                STRING                             COMMENT  &#x27;&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;&#x27;,&#xd;&#xa;CUR_M_SHIP_QTY                BIGINT                             COMMENT  &#x27;&#x5f53;&#x6708;&#x53d1;&#x8fd0;&#x91cf;&#x27;,&#xd;&#xa;ST_BASE_TYPE                  STRING                             COMMENT  &#x27;&#xff08;1-&#x751f;&#x4ea7;&#x57fa;&#x5730;&#xff0c;2-&#x4e2d;&#x8f6c;&#x7ad9;&#x3001;3-&#x57ce;&#x5e02;&#x914d;&#x9001;&#x4e2d;&#x5fc3;&#xff09;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;DMR&#x5927;&#x5c4f;&#x7aef;&#x57fa;&#x5730;&#x8fc1;&#x5f99;&#x4fe1;&#x606f;&#x27; &#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_OV_BASE_CHANGE_INF_DKE&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_OV_BASE_CHANGE_INF_DKE&#x28;&#xd;&#xa;DATE_ID                       STRING                             COMMENT  &#x27;&#x65e5;&#x671f;ID&#x27;,&#xd;&#xa;ST_RGN                        STRING                             COMMENT  &#x27;&#x59cb;&#x53d1;&#x5730;&#x6240;&#x5c5e;&#x5927;&#x533a;&#x27;,&#xd;&#xa;ST_BASE_NAME                  STRING                             COMMENT  &#x27;&#x59cb;&#x53d1;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ST_CITY_NAME                  STRING                             COMMENT  &#x27;&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;&#x27;,&#xd;&#xa;DEST_ORG_NAME                 STRING                             COMMENT  &#x27;&#x76ee;&#x7684;&#x7ec4;&#x7ec7;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;DEST_CITY_NAME                STRING                             COMMENT  &#x27;&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;&#x27;,&#xd;&#xa;ST_BASE_TYPE                  STRING                             COMMENT  &#x27;&#xff08;1-&#x751f;&#x4ea7;&#x57fa;&#x5730;&#xff0c;2-&#x4e2d;&#x8f6c;&#x7ad9;&#x3001;3-&#x57ce;&#x5e02;&#x914d;&#x9001;&#x4e2d;&#x5fc3;&#xff09;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;DMR&#x5927;&#x5c4f;&#x7aef;&#x57fa;&#x5730;&#x8fc1;&#x5f99;&#x4fe1;&#x606f;&#x27; &#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
