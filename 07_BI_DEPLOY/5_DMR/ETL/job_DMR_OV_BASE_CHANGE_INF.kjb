      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_BASE_CHANGE_INF_DKE &#xd;&#xa;SELECT D.PERIOD_WID AS DATE_ID&#xd;&#xa;      ,T.ST_RGN&#xd;&#xa;      ,T.ST_BASE_NAME&#xd;&#xa;      ,T.ST_CITY_NAME &#xd;&#xa;      ,T.DEST_ORG_NAME&#xd;&#xa;      ,T.DEST_CITY_NAME&#xd;&#xa;      ,T.ST_BASE_TYPE&#xd;&#xa;  FROM DIM.DIM_PERIOD_D D&#xd;&#xa;  JOIN TEMP.TEMP_DMR_OV_BASE_CHANGE_INF T&#xd;&#xa;    ON 1 &#x3d; 1&#xd;&#xa; WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;   AND D.PERIOD_WID &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; &#xd;&#xa; GROUP BY D.PERIOD_WID &#xd;&#xa;      ,T.ST_RGN&#xd;&#xa;      ,T.ST_BASE_NAME&#xd;&#xa;      ,T.ST_CITY_NAME &#xd;&#xa;      ,T.DEST_ORG_NAME&#xd;&#xa;      ,T.DEST_CITY_NAME&#xd;&#xa;      ,T.ST_BASE_TYPE&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
