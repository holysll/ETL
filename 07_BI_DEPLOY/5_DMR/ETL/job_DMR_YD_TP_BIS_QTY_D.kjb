LDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_TP_BIS_QTY_D_04&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_TP_BIS_QTY_D_04&#x28;&#xd;&#xa;RGN_ID           STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;RGN_NAME         STRING        COMMENT  &#x27;&#x5927;&#x533a;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;BASE_ID          STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BASE_NAME        STRING        COMMENT  &#x27;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;BRAND_ID         STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;BRAND_NAME       STRING        COMMENT  &#x27;&#x54c1;&#x724c;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;TP_PROCESS       STRING        COMMENT  &#x27;&#x8fd0;&#x8f93;&#x6b65;&#x9aa4;&#x27;,&#xd;&#xa;INDEX_NAME1      STRING        COMMENT  &#x27;&#x6307;&#x6807;1&#x540d;&#x79f0;&#x27;,&#xd;&#xa;INDEX_VALUE1     BIGINT        COMMENT  &#x27;&#x6307;&#x6807;1&#x6570;&#x503c;&#x27;,&#xd;&#xa;INDEX_NAME2      STRING        COMMENT  &#x27;&#x6307;&#x6807;2&#x540d;&#x79f0;&#x27;,&#xd;&#xa;INDEX_VALUE2     BIGINT        COMMENT  &#x27;&#x6307;&#x6807;2&#x6570;&#x503c;&#x27;,&#xd;&#xa;INDEX_NAME3      STRING        COMMENT  &#x27;&#x6307;&#x6807;3&#x540d;&#x79f0;&#x27;,&#xd;&#xa;INDEX_VALUE3     BIGINT        COMMENT  &#x27;&#x6307;&#x6807;3&#x6570;&#x503c;&#x27;,&#xd;&#xa;INDEX_NAME4      STRING        COMMENT  &#x27;&#x6307;&#x6807;4&#x540d;&#x79f0;&#x27;,&#xd;&#xa;INDEX_VALUE4     DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x6307;&#x6807;4&#x6570;&#x503c;&#x27;,&#xd;&#xa;INDEX_NAME5      STRING        COMMENT  &#x27;&#x6307;&#x6807;5&#x540d;&#x79f0;&#x27;,&#xd;&#xa;INDEX_VALUE5     DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x6307;&#x6807;5&#x6570;&#x503c;&#x27;,&#xd;&#xa;COLOR_FLAG1      STRING        COMMENT  &#x27;&#x9884;&#x8b66;&#x989c;&#x8272;&#x6807;&#x8bb0;1&#x27;,&#xd;&#xa;COLOR_FLAG2      STRING        COMMENT  &#x27;&#x