    <entry>
      <name>INTRANSIT_QP</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_TP_BIS_QTY_D_QP&#xd;&#xa;SELECT X.PERIOD_WID&#xd;&#xa;      ,X.RGN_ID&#xd;&#xa;      ,X.BASE_ID&#xd;&#xa;      ,X.BRAND_ID&#xd;&#xa;      ,X.LST_D_QP_QTY&#xd;&#xa;      ,X.CUR_M_QP_QTY&#xd;&#xa;      ,B.CUR_M_TGT_QP_QTY&#xd;&#xa;      ,X.CUR_Y_QP_QTY&#xd;&#xa;      ,B.CUR_Y_TGT_QP_QTY&#xd;&#xa;  FROM &#x28;SELECT X1.PERIOD_WID&#xd;&#xa;              ,X1.RGN_ID&#xd;&#xa;              ,X1.BASE_ID&#xd;&#xa;              ,X1.BRAND_ID&#xd;&#xa;              ,SUM&#x28;QP_QTY&#x29; OVER&#x28;PARTITION BY X1.PERIOD_WID, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS LST_D_QP_QTY&#xd;&#xa;              ,SUM&#x28;QP_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 6&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_M_QP_QTY&#xd;&#xa;              ,SUM&#x28;QP_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 4&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_Y_QP_QTY&#xd;&#xa;          FROM &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                      ,SUM&#x28;T.TICKET_QTY&#x29; AS QP_QTY&#xd;&#xa;                  FROM DMK.DMK_TP_ORDERQTY_D T&#xd;&#xa;                 WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 4&#x29;&#xd;&#xa;                   AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                 GROUP BY T.PERIOD_WID&#xd;&#xa;                         ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS X1&#x29; X&#xd;&#xa;  LEFT JOIN &#x28;SELECT NVL&#x28;COALESCE&#x28;T1.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                   ,NVL&#x28;COALESCE&#x28;T2.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                   ,NVL&#x28;COALESCE&#x28;T3.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN SUBSTR&#x28;T.MONTH_ID, 1, 6&#x29; &#x3d;&#xd;&#xa;                               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      6&#x29; THEN&#xd;&#xa;                           T.REV_PLAN&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS CUR_M_TGT_QP_QTY --&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN SUBSTR&#x28;T.MONTH_ID, 1, 4&#x29; &#x3d;&#xd;&#xa;                               SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29; THEN&#xd;&#xa;                           T.REV_PLAN&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS CUR_Y_TGT_QP_QTY --&#xd;&#xa;               FROM ODS_BLPT.T_HAND_REV_TICKET_PLAN T&#xd;&#xa;               LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                               ,RGN_NAME&#xd;&#xa;                           FROM DIM.DIM_ORG&#xd;&#xa;                          WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                          GROUP BY RGN_ID&#xd;&#xa;                                  ,RGN_NAME&#x29; T1&#xd;&#xa;                 ON T.RGN_NAME &#x3d; T1.RGN_NAME&#xd;&#xa;               LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                               ,BASE_NAME&#xd;&#xa;                           FROM DIM.DIM_ORG&#xd;&#xa;                          WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                          GROUP BY BASE_ID&#xd;&#xa;                                  ,BASE_NAME&#x29; T2&#xd;&#xa;                 ON T.BASE_NAME &#x3d; T2.BASE_NAME&#xd;&#xa;               LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                               ,BRAND_NAME&#xd;&#xa;                           FROM DIM.DIM_BRAND&#xd;&#xa;                          GROUP BY BRAND_ID&#xd;&#xa;                                  ,BRAND_NAME&#x29; T3&#xd;&#xa;                 ON T.BRAND_NAME &#x3d; T3.BRAND_NAME&#xd;&#xa;               LEFT JOIN &#x28;SELECT PROJ_ID&#xd;&#xa;                               ,PROJ_NAME&#xd;&#xa;                           FROM DIM.DIM_PROJ&#xd;&#xa;                          GROUP BY PROJ_ID&#xd;&#xa;                                  ,PROJ_NAME&#x29; T4&#xd;&#xa;                 ON T.PRO_NAME &#x3d; T4.PROJ_NAME&#xd;&#xa;              WHERE SUBSTR&#x28;T.MONTH_ID, 1, 4&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;&#xd;&#xa;                AND SUBSTR&#x28;T.MONTH_ID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                    SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;              GROUP BY COALESCE&#x28;T1.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                      ,COALESCE&#x28;T2.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                      ,COALESCE&#x28;T3.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS B&#xd;&#xa;    ON X.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;   AND X.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;   AND X.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa; WHERE SUBSTR&#x28;X.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;   AND SUBSTR&#x28;X.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
