      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_TP_BIS_QTY_D PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.DATE_ID&#xd;&#xa;      ,T.RGN_ID AS RNG_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T1.RGN_NAME&#xd;&#xa;       END AS RNG_NAME&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.BASE_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.BASE_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T2.BASE_NAME&#xd;&#xa;       END AS BASE_NAME&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T3.BRAND_NAME&#xd;&#xa;       END AS BRAND_NAME&#xd;&#xa;      ,T.TP_PROCESS --&#x8fd0;&#x8f93;&#x6b65;&#x9aa4;&#x28;&#x542f;&#x7968;&#xff0c;&#x5728;&#x9014;&#xff0c;&#x4ea4;&#x4ed8;&#x29;&#xd;&#xa;      ,T.SHOW_PART_ONE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;1&#x540d;&#x79f0;&#xd;&#xa;      ,T.SHOW_PART_ONE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;1&#x503c;&#xd;&#xa;      ,T.SHOW_PART_TWO_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;2&#x540d;&#x79f0;&#xd;&#xa;      ,T.SHOW_PART_TWO_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;2&#x503c;&#xd;&#xa;      ,T.SHOW_PART_THREE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;3&#x540d;&#x79f0;&#xd;&#xa;      ,T.SHOW_PART_THREE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;3&#x503c;&#xd;&#xa;      ,T.SHOW_PART_FOUR_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;4&#x540d;&#x79f0;&#xd;&#xa;      ,T.SHOW_PART_FOUR_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;4&#x503c;&#xd;&#xa;      ,T.SHOW_PART_FIVE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;5&#x540d;&#x79f0;&#xd;&#xa;      ,T.SHOW_PART_FIVE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;5&#x503c;&#xd;&#xa;      ,T.PART_FOUR_WRN_FLAG --&#x6a21;&#x5757;4&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,T.PART_FIVE_WRN_FLAG --&#x6a21;&#x5757;5&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;T.DATE_ID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT T.PERIOD_WID AS DATE_ID&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,T.BASE_ID&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,&#x27;&#x542f;&#x7968;&#x27; AS TP_PROCESS --&#x8fd0;&#x8f93;&#x6b65;&#x9aa4;&#x28;&#x542f;&#x7968;&#xff0c;&#x5728;&#x9014;&#xff0c;&#x4ea4;&#x4ed8;&#x29;&#xd;&#xa;              ,&#x27;&#x4eca;&#x65e5;&#x542f;&#x7968;&#x27; AS SHOW_PART_ONE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;1&#x540d;&#x79f0;&#xd;&#xa;              ,T.LST_D_QP_QTY AS SHOW_PART_ONE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;1&#x503c;&#xd;&#xa;              ,&#x27;&#x6708;&#x7d2f;&#x8ba1;&#x542f;&#x7968;&#x27; AS SHOW_PART_TWO_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;2&#x540d;&#x79f0;&#xd;&#xa;              ,T.CUR_M_QP_QTY AS SHOW_PART_TWO_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;2&#x503c;&#xd;&#xa;              ,&#x27;&#x5e74;&#x7d2f;&#x8ba1;&#x542f;&#x7968;&#x27; AS SHOW_PART_THREE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;3&#x540d;&#x79f0;&#xd;&#xa;              ,T.CUR_Y_QP_QTY AS SHOW_PART_THREE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;3&#x503c;&#xd;&#xa;              ,&#x27;&#x6708;&#x542f;&#x7968;&#x5b8c;&#x6210;&#x7387;&#x27; AS SHOW_PART_FOUR_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;4&#x540d;&#x79f0;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;T.CUR_M_TGT_QP_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  T.CUR_M_QP_QTY &#x2f; T.CUR_M_TGT_QP_QTY &#x2a; 100&#xd;&#xa;               END AS SHOW_PART_FOUR_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;4&#x503c;&#xd;&#xa;              ,&#x27;&#x5e74;&#x542f;&#x7968;&#x5b8c;&#x6210;&#x7387;&#x27; AS SHOW_PART_FIVE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;5&#x540d;&#x79f0;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;T.CUR_Y_TGT_QP_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  T.CUR_Y_QP_QTY &#x2f; T.CUR_Y_TGT_QP_QTY &#x2a; 100&#xd;&#xa;               END AS SHOW_PART_FIVE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;5&#x503c;&#xd;&#xa;              ,NULL AS PART_FOUR_WRN_FLAG --&#x6a21;&#x5757;4&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;              ,NULL AS PART_FIVE_WRN_FLAG --&#x6a21;&#x5757;5&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;          FROM TEMP.TEMP_DMR_YD_TP_BIS_QTY_D_QP T&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT T.PERIOD_WID AS DATE_ID&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,T.BASE_ID&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,&#x27;&#x5728;&#x9014;&#x27; AS TP_PROCESS --&#x8fd0;&#x8f93;&#x6b65;&#x9aa4;&#x28;&#x542f;&#x7968;&#xff0c;&#x5728;&#x9014;&#xff0c;&#x4ea4;&#x4ed8;&#x29;&#xd;&#xa;              ,&#x27;&#x5f85;&#x53d1;&#x6570;&#x27; AS SHOW_PART_ONE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;1&#x540d;&#x79f0;&#xd;&#xa;              ,T.CUR_D_WAITE_QTY AS SHOW_PART_ONE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;1&#x503c;&#xd;&#xa;              ,&#x27;&#x5728;&#x9014;&#x6570;&#x27; AS SHOW_PART_TWO_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;2&#x540d;&#x79f0;&#xd;&#xa;              ,T.CUR_D_ONWAY_QTY AS SHOW_PART_TWO_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;2&#x503c;&#xd;&#xa;              ,&#x27;&#x5728;&#x9014;&#x8d85;&#x65f6;&#x6570;&#x27; AS SHOW_PART_THREE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;3&#x540d;&#x79f0;&#xd;&#xa;              ,T.CUR_D_OUTTIME_QTY AS SHOW_PART_THREE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;3&#x503c;&#xd;&#xa;              ,&#x27;&#x5728;&#x9014;&#x8d85;&#x65f6;&#x7387;&#x27; AS SHOW_PART_FOUR_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;4&#x540d;&#x79f0;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;T.CUR_D_ONWAY_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  T.CUR_D_OUTTIME_QTY &#x2f; T.CUR_D_ONWAY_QTY &#x2a; 100&#xd;&#xa;               END AS SHOW_PART_FOUR_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;4&#x503c;&#xd;&#xa;              ,NULL AS SHOW_PART_FIVE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;5&#x540d;&#x79f0;&#xd;&#xa;              ,NULL AS SHOW_PART_FIVE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;5&#x503c;&#xd;&#xa;              ,NULL AS PART_FOUR_WRN_FLAG --&#x6a21;&#x5757;4&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;              ,NULL AS PART_FIVE_WRN_FLAG --&#x6a21;&#x5757;5&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;          FROM TEMP.TEMP_DMR_YD_TP_BIS_QTY_D_ONWAY T&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT T.PERIOD_WID AS DATE_ID&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,T.BASE_ID&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,&#x27;&#x4ea4;&#x4ed8;&#x27; AS TP_PROCESS --&#x8fd0;&#x8f93;&#x6b65;&#x9aa4;&#x28;&#x542f;&#x7968;&#xff0c;&#x5728;&#x9014;&#xff0c;&#x4ea4;&#x4ed8;&#x29;&#xd;&#xa;              ,&#x27;&#x4eca;&#x65e5;&#x4ea4;&#x4ed8;&#x27; AS SHOW_PART_ONE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;1&#x540d;&#x79f0;&#xd;&#xa;              ,T.LST_D_DELY_QTY AS SHOW_PART_ONE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;1&#x503c;&#xd;&#xa;              ,&#x27;&#x6708;&#x7d2f;&#x8ba1;&#x4ea4;&#x4ed8;&#x27; AS SHOW_PART_TWO_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;2&#x540d;&#x79f0;&#xd;&#xa;              ,T.CUR_M_DELY_QTY AS SHOW_PART_TWO_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;2&#x503c;&#xd;&#xa;              ,&#x27;&#x5e74;&#x7d2f;&#x8ba1;&#x4ea4;&#x4ed8;&#x27; AS SHOW_PART_THREE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;3&#x540d;&#x79f0;&#xd;&#xa;              ,T.CUR_Y_DELY_QTY AS SHOW_PART_THREE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;3&#x503c;&#xd;&#xa;              ,&#x27;&#x6708;&#x51c6;&#x65f6;&#x5230;&#x8fbe;&#x7387;&#x27; AS SHOW_PART_FOUR_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;4&#x540d;&#x79f0;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;T.CUR_M_TGT_DELY_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  T.CUR_M_DELY_OT_QTY &#x2f; T.CUR_M_TGT_DELY_QTY &#x2a; 100&#xd;&#xa;               END AS SHOW_PART_FOUR_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;4&#x503c;&#xd;&#xa;              ,&#x27;&#x5e74;&#x51c6;&#x65f6;&#x5230;&#x8fbe;&#x7387;&#x27; AS SHOW_PART_FIVE_NAME --&#x663e;&#x793a;&#x6a21;&#x5757;5&#x540d;&#x79f0;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;T.CUR_Y_TGT_DELY_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  T.CUR_Y_DELY_OT_QTY &#x2f; T.CUR_Y_TGT_DELY_QTY &#x2a; 100&#xd;&#xa;               END AS SHOW_PART_FIVE_VALUE --&#x663e;&#x793a;&#x6a21;&#x5757;5&#x503c;&#xd;&#xa;              ,NULL AS PART_FOUR_WRN_FLAG --&#x6a21;&#x5757;4&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;              ,NULL AS PART_FIVE_WRN_FLAG --&#x6a21;&#x5757;5&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;          FROM TEMP.TEMP_DMR_YD_TP_BIS_QTY_D_DELY T&#x29; AS T&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; T1&#xd;&#xa;    ON T.RGN_ID &#x3d; T1.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; T2&#xd;&#xa;    ON T.BASE_ID &#x3d; T2.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; T3&#xd;&#xa;    ON T.BRAND_ID &#x3d; T3.BRAND_ID&#xd;&#xa; WHERE 1&#x3d;1&#xd;&#xa;   AND SUBSTR&#x28;T.DATE_ID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#x3b;&#xd;&#xa;</sql>
