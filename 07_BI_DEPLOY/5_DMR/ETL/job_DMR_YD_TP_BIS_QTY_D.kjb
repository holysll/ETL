    <entry>
      <name>DELIVER_ONWAY</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_TP_BIS_QTY_D_ONWAY&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;A.RGN_ID, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,COALESCE&#x28;A.BASE_ID, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,COALESCE&#x28;A.BRAND_ID, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,SUM&#x28;CUR_D_WAITE_QTY&#x29; AS CUR_D_WAITE_QTY --&#x5f85;&#x53d1;&#x6570;&#xd;&#xa;      ,SUM&#x28;CUR_D_ONWAY_QTY&#x29; AS CUR_D_ONWAY_QTY --&#x5728;&#x9014;&#x6570;&#xd;&#xa;      ,SUM&#x28;CUR_D_OUTTIME_QTY&#x29; AS CUR_D_OUTTIME_QTY --&#x5728;&#x9014;&#x8d85;&#x65f6;&#x6570;&#xd;&#xa;  FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;              ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;              ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;              ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;              ,COUNT&#x28;DISTINCT T.VIN&#x29; AS CUR_D_WAITE_QTY&#xd;&#xa;              ,0 AS CUR_D_ONWAY_QTY&#xd;&#xa;              ,0 AS CUR_D_OUTTIME_QTY&#xd;&#xa;          FROM DIM.DIM_PERIOD_D D&#xd;&#xa;          JOIN DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;            ON 1 &#x3d; 1&#xd;&#xa;         WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;EARLY_PICKUP_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;               D.PERIOD_WID&#xd;&#xa;           AND NVL&#x28;SUBSTR&#x28;REGEXP_REPLACE&#x28;FACTORY_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;,&#x27;29991231&#x27;&#x29; &#x3e; D.PERIOD_WID&#xd;&#xa;           AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;           AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;           AND SUBSTR&#x28;D.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;               FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;         GROUP BY D.PERIOD_WID&#xd;&#xa;                 ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                 ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                 ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT D.PERIOD_WID&#xd;&#xa;              ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;              ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;              ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;              ,0 AS CUR_D_WAITE_QTY&#xd;&#xa;              ,COUNT&#x28;T.order_release_gid&#x29; AS CUR_D_ONWAY_QTY&#xd;&#xa;              ,COUNT&#x28;CASE WHEN unix_timestamp&#x28;concat&#x28;D.period_date,&#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;T.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;T.on_passage_days,0&#x29;&#xd;&#xa;               THEN T.order_release_gid &#xd;&#xa;                       ELSE&#xd;&#xa;                        NULL&#xd;&#xa;                     END&#x29; AS CUR_D_OUTTIME_QTY&#xd;&#xa;          FROM DIM.DIM_PERIOD_D D&#xd;&#xa;          JOIN DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;            ON 1 &#x3d; 1&#xd;&#xa;         WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;FACTORY_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;               D.PERIOD_WID&#xd;&#xa;           AND NVL&#x28;SUBSTR&#x28;REGEXP_REPLACE&#x28;NVL&#x28;DLVR_DAY,ARRIVAL_DAY&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;,&#xd;&#xa;                   &#x27;29991231&#x27;&#x29; &#x3e; D.PERIOD_WID&#xd;&#xa;           AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;           AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;           AND SUBSTR&#x28;D.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;               FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;         GROUP BY D.PERIOD_WID&#xd;&#xa;                 ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                 ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                 ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;&#x29; A&#xd;&#xa; GROUP BY A.PERIOD_WID&#xd;&#xa;         ,A.RGN_ID&#xd;&#xa;         ,A.BASE_ID&#xd;&#xa;         ,A.BRAND_ID WITH CUBE&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>256</xloc>
      <yloc>272</yloc>
      </entry>
    <entry>
      <name>DEVTICKET_DELY</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_TP_BIS_QTY_D_DELY&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;      ,A.RGN_ID&#xd;&#xa;      ,A.BASE_ID&#xd;&#xa;      ,A.BRAND_ID&#xd;&#xa;      ,SUM&#x28;CUR_D_DEVER_QTY&#x29; AS CUR_D_DEVER_QTY --&#x6628;&#x65e5;&#x4ea4;&#x91cf;&#xd;&#xa;      ,SUM&#x28;CUR_M_DEVER_QTY&#x29; AS CUR_M_DELY_QTY --&#x5f53;&#x6708;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;      ,SUM&#x28;CUR_M_ARRIVE_QTY&#x29; AS CUR_M_DELY_OT_QTY --&#xd;&#xa;      ,SUM&#x28;CUR_M_TOTAL_QTY&#x29; AS CUR_M_TGT_DELY_QTY --&#x5f53;&#x6708;&#x5e94;&#x5f53;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;      ,SUM&#x28;CUR_Y_DEVER_QTY&#x29; AS CUR_Y_DELY_QTY --&#x5f53;&#x5e74;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;      ,SUM&#x28;CUR_Y_ARRIVE_QTY&#x29; AS CUR_Y_DELY_OT_QTY --&#xd;&#xa;      ,SUM&#x28;CUR_Y_TOTAL_QTY&#x29; AS CUR_Y_TGT_DELY_QTY --&#x5f53;&#x5e74;&#x5e94;&#x5f53;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;  FROM &#x28;SELECT X.PERIOD_WID&#xd;&#xa;              ,X.RGN_ID&#xd;&#xa;              ,X.BASE_ID&#xd;&#xa;              ,X.BRAND_ID&#xd;&#xa;              ,X.CUR_D_DEVER_QTY&#xd;&#xa;              ,X.CUR_M_DEVER_QTY&#xd;&#xa;              ,X.CUR_M_ARRIVE_QTY&#xd;&#xa;              ,X.CUR_M_TOTAL_QTY&#xd;&#xa;              ,X.CUR_Y_DEVER_QTY&#xd;&#xa;              ,X.CUR_Y_ARRIVE_QTY&#xd;&#xa;              ,X.CUR_Y_TOTAL_QTY&#xd;&#xa;          FROM &#x28;SELECT X1.PERIOD_WID&#xd;&#xa;                      ,X1.RGN_ID&#xd;&#xa;                      ,X1.BASE_ID&#xd;&#xa;                      ,X1.BRAND_ID&#xd;&#xa;                      ,X2.DLVR_QTY AS CUR_D_DEVER_QTY --&#x6628;&#x65e5;&#x4ea4;&#x91cf;&#xd;&#xa;                      ,SUM&#x28;X2.DLVR_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 6&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_M_DEVER_QTY --&#x5f53;&#x6708;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;                      ,0 AS CUR_M_ARRIVE_QTY --&#xd;&#xa;                      ,0 AS CUR_M_TOTAL_QTY --&#x5f53;&#x6708;&#x5e94;&#x5f53;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;                      ,SUM&#x28;X2.DLVR_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 4&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_Y_DEVER_QTY --&#x5f53;&#x5e74;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;                      ,0 AS CUR_Y_ARRIVE_QTY --&#xd;&#xa;                      ,0 AS CUR_Y_TOTAL_QTY --&#x5f53;&#x5e74;&#x5e94;&#x5f53;&#x4ea4;&#x4ed8;&#x91cf; &#xd;&#xa;                  FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                              ,T.RGN_ID&#xd;&#xa;                              ,T.BASE_ID&#xd;&#xa;                              ,T.BRAND_ID&#xd;&#xa;                          FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                          JOIN &#x28;SELECT NVL&#x28;NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                     ,NVL&#x28;NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                     ,NVL&#x28;NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                 FROM DMK.dmk_tp_shipqty_d T&#xd;&#xa;                                WHERE SUBSTR&#x28;T.period_wid,&#xd;&#xa;                                             1,&#xd;&#xa;                                             4&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                        &#x27;-&#x27;,&#xd;&#xa;                                                                        &#x27;&#x27;&#x29;,&#xd;&#xa;                                                         1,&#xd;&#xa;                                                         4&#x29;&#xd;&#xa;                                  AND SUBSTR&#x28;T.period_wid,&#xd;&#xa;                                             1,&#xd;&#xa;                                             6&#x29; &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                         &#x27;-&#x27;,&#xd;&#xa;                                                                         &#x27;&#x27;&#x29;,&#xd;&#xa;                                                          1,&#xd;&#xa;                                                          6&#x29;&#xd;&#xa;                                GROUP BY NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                        ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                        ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS T&#xd;&#xa;                            ON 1 &#x3d; 1&#xd;&#xa;                         WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                           AND SUBSTR&#x28;D.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      4&#x29;&#xd;&#xa;                           AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      6&#x29;&#x29; AS X1&#xd;&#xa;                  LEFT JOIN &#x28;SELECT SUBSTR&#x28;T.period_wid,&#xd;&#xa;                                          1,&#xd;&#xa;                                          8&#x29; AS PERIOD_WID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                  ,sum&#x28;T.dlvr_qty&#x29; AS DLVR_QTY&#xd;&#xa;                              FROM DMK.dmk_tp_shipqty_d T&#xd;&#xa;                             WHERE SUBSTR&#x28;T.period_wid,&#xd;&#xa;                                          1,&#xd;&#xa;                                          4&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                     &#x27;-&#x27;,&#xd;&#xa;                                                                     &#x27;&#x27;&#x29;,&#xd;&#xa;                                                      1,&#xd;&#xa;                                                      4&#x29;&#xd;&#xa;                               AND SUBSTR&#x28;T.period_wid,&#xd;&#xa;                                          1,&#xd;&#xa;                                          6&#x29; &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                      &#x27;-&#x27;,&#xd;&#xa;                                                                      &#x27;&#x27;&#x29;,&#xd;&#xa;                                                       1,&#xd;&#xa;                                                       6&#x29;&#xd;&#xa;                               AND substr&#x28;T.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;                             GROUP BY SUBSTR&#x28;T.period_wid,&#xd;&#xa;                                             1,&#xd;&#xa;                                             8&#x29;&#xd;&#xa;                                     ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                     ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                     ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS X2&#xd;&#xa;                    ON X1.PERIOD_WID &#x3d; X2.PERIOD_WID&#xd;&#xa;                   AND X1.RGN_ID &#x3d; X2.RGN_ID&#xd;&#xa;                   AND X1.BASE_ID &#x3d; X2.BASE_ID&#xd;&#xa;                   AND X1.BRAND_ID &#x3d; X2.BRAND_ID&#x29; AS X&#xd;&#xa;         WHERE SUBSTR&#x28;X.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT X.PERIOD_WID&#xd;&#xa;              ,X.RGN_ID&#xd;&#xa;              ,X.BASE_ID&#xd;&#xa;              ,X.BRAND_ID&#xd;&#xa;              ,X.CUR_D_DEVER_QTY&#xd;&#xa;              ,X.CUR_M_DEVER_QTY&#xd;&#xa;              ,X.CUR_M_ARRIVE_QTY&#xd;&#xa;              ,X.CUR_M_TOTAL_QTY&#xd;&#xa;              ,X.CUR_Y_DEVER_QTY&#xd;&#xa;              ,X.CUR_Y_ARRIVE_QTY&#xd;&#xa;              ,X.CUR_Y_TOTAL_QTY&#xd;&#xa;          FROM &#x28;SELECT X1.PERIOD_WID&#xd;&#xa;                      ,X1.RGN_ID&#xd;&#xa;                      ,X1.BASE_ID&#xd;&#xa;                      ,X1.BRAND_ID&#xd;&#xa;                      ,0 AS CUR_D_DEVER_QTY --&#x6628;&#x65e5;&#x4ea4;&#x91cf;&#xd;&#xa;                      ,0 AS CUR_M_DEVER_QTY --&#x5f53;&#x6708;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;                      ,SUM&#x28;X2.ARRIVE_ONTIME_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 6&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_M_ARRIVE_QTY --&#xd;&#xa;                      ,SUM&#x28;X2.ARRIVE_PLAN_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 6&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_M_TOTAL_QTY --&#x5f53;&#x6708;&#x5e94;&#x5f53;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;                      ,0 AS CUR_Y_DEVER_QTY --&#x5f53;&#x5e74;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;                      ,SUM&#x28;X2.ARRIVE_ONTIME_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 4&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_Y_ARRIVE_QTY --&#xd;&#xa;                      ,SUM&#x28;X2.ARRIVE_PLAN_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 4&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_Y_TOTAL_QTY --&#x5f53;&#x5e74;&#x5e94;&#x5f53;&#x4ea4;&#x4ed8;&#x91cf; &#xd;&#xa;                  FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                              ,T.RGN_ID&#xd;&#xa;                              ,T.BASE_ID&#xd;&#xa;                              ,T.BRAND_ID&#xd;&#xa;                          FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                          JOIN &#x28;SELECT NVL&#x28;NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                     ,NVL&#x28;NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                     ,NVL&#x28;NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                 FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                                WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                                      SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                            &#x27;-&#x27;,&#xd;&#xa;                                                            &#x27;&#x27;&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             4&#x29;&#xd;&#xa;                                  AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                                      SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                            &#x27;-&#x27;,&#xd;&#xa;                                                            &#x27;&#x27;&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             6&#x29;&#xd;&#xa;                                GROUP BY NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                        ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                        ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS T&#xd;&#xa;                            ON 1 &#x3d; 1&#xd;&#xa;                         WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                           AND SUBSTR&#x28;D.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      4&#x29;&#xd;&#xa;                           AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      6&#x29;&#x29; AS X1&#xd;&#xa;                  LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                  ,SUM&#x28;T.ARRIVE_ONTIME_QTY&#x29; AS ARRIVE_ONTIME_QTY&#xd;&#xa;                                  ,SUM&#x28;T.ARRIVE_PLAN_QTY&#x29; AS ARRIVE_PLAN_QTY&#xd;&#xa;                                  ,SUM&#x28;T.ARRIVE_QTY&#x29; AS ARRIVE_QTY&#xd;&#xa;                              FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                             WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                                   SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                         &#x27;-&#x27;,&#xd;&#xa;                                                         &#x27;&#x27;&#x29;,&#xd;&#xa;                                          1,&#xd;&#xa;                                          4&#x29;&#xd;&#xa;                               AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                                   SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                         &#x27;-&#x27;,&#xd;&#xa;                                                         &#x27;&#x27;&#x29;,&#xd;&#xa;                                          1,&#xd;&#xa;                                          6&#x29;&#xd;&#xa;                             GROUP BY T.PERIOD_WID&#xd;&#xa;                                     ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                     ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                     ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS X2&#xd;&#xa;                    ON X1.PERIOD_WID &#x3d; X2.PERIOD_WID&#xd;&#xa;                   AND X1.RGN_ID &#x3d; X2.RGN_ID&#xd;&#xa;                   AND X1.BASE_ID &#x3d; X2.BASE_ID&#xd;&#xa;                   AND X1.BRAND_ID &#x3d; X2.BRAND_ID&#x29; AS X&#xd;&#xa;         WHERE SUBSTR&#x28;X.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#x29; A&#xd;&#xa; GROUP BY A.PERIOD_WID&#xd;&#xa;         ,A.RGN_ID&#xd;&#xa;         ,A.BASE_ID&#xd;&#xa;         ,A.BRAND_ID&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
