    <entry>
      <name>TO_TEMP_3</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_3&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_3&#xd;&#xa;SELECT coalesce&#x28;A.month_id,B.month_id,C.month_id,D.month_id,&#x27;-1&#x27;&#x29; AS MONTH_ID&#xd;&#xa;    ,coalesce&#x28;A.rgn_id,B.rgn_id,C.rgn_id,D.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID &#xd;&#xa;    ,coalesce&#x28;A.base_id,B.base_id,C.base_id,D.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;A.brand_id,B.brand_id,C.brand_id,D.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;A.proj_id,B.proj_id,C.proj_id,D.proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,coalesce&#x28;A.supplier_id,B.supplier_id,C.supplier_id,D.supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;    ,coalesce&#x28;A.tpmode_id,B.tpmode_id,C.tpmode_id,D.tpmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;    ,A.REPLACE_QTY&#xd;&#xa;    ,A.NON_REPLACE_QTY&#xd;&#xa;    ,A.TGT_REPLACE_QTY&#xd;&#xa;    ,D.NON_REP_PROD_COST&#xd;&#xa;    ,B.REPLACE_CYCLE_TIME&#x2f;&#x28;3600&#x2a;24&#x29; AS REPLACE_CYCLE_TIME&#xd;&#xa;    ,C.NON_REPLACE_CYCLE_TIME&#x2f;&#x28;3600&#x2a;24&#x29; AS NON_REPLACE_CYCLE_TIME&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT month_id&#xd;&#xa;        ,rgn_id&#xd;&#xa;        ,base_id&#xd;&#xa;        ,brand_id&#xd;&#xa;        ,proj_id&#xd;&#xa;        ,supplier_id&#xd;&#xa;        ,tpmode_id&#xd;&#xa;        ,SUM&#x28;1&#x29; AS TGT_REPLACE_QTY&#xd;&#xa;        ,SUM&#x28;CASE WHEN replace_date IS NOT NULL THEN 1 ELSE 0 END&#x29; AS REPLACE_QTY&#xd;&#xa;        ,SUM&#x28;CASE WHEN replace_date IS NULL THEN 1 ELSE 0 END&#x29; AS NON_REPLACE_QTY&#xd;&#xa;    FROM TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_2 &#xd;&#xa;    GROUP BY month_id&#xd;&#xa;        ,rgn_id&#xd;&#xa;        ,base_id&#xd;&#xa;        ,brand_id&#xd;&#xa;        ,proj_id&#xd;&#xa;        ,supplier_id&#xd;&#xa;        ,tpmode_id&#xd;&#xa;    &#x29; A&#xd;&#xa;FULL JOIN &#x28;&#xd;&#xa;    SELECT month_id&#xd;&#xa;        ,rgn_id&#xd;&#xa;        ,base_id&#xd;&#xa;        ,brand_id&#xd;&#xa;        ,proj_id&#xd;&#xa;        ,supplier_id&#xd;&#xa;        ,tpmode_id&#xd;&#xa;        ,AVG&#x28;unix_timestamp&#x28;replace_date,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS REPLACE_CYCLE_TIME&#xd;&#xa;    FROM TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_2&#xd;&#xa;    WHERE replace_date IS NOT NULL&#xd;&#xa;    GROUP BY month_id&#xd;&#xa;        ,rgn_id&#xd;&#xa;        ,base_id&#xd;&#xa;        ,brand_id&#xd;&#xa;        ,proj_id&#xd;&#xa;        ,supplier_id&#xd;&#xa;        ,tpmode_id&#xd;&#xa;    &#x29; B&#xd;&#xa;ON A.MONTH_ID &#x3d; B.MONTH_ID AND A.RGN_ID &#x3d; B.RGN_ID AND A.BASE_ID &#x3d; B.BASE_ID AND A.BRAND_ID &#x3d; B.BRAND_ID AND A.PROJ_ID &#x3d; B.PROJ_ID AND A.SUPPLIER_ID &#x3d; B.SUPPLIER_ID AND A.TPMODE_ID &#x3d; B.TPMODE_ID&#xd;&#xa;FULL JOIN&#x28;&#xd;&#xa;    SELECT month_id&#xd;&#xa;        ,rgn_id&#xd;&#xa;        ,base_id&#xd;&#xa;        ,brand_id&#xd;&#xa;        ,proj_id&#xd;&#xa;        ,supplier_id&#xd;&#xa;        ,tpmode_id&#xd;&#xa;        ,AVG&#x28;unix_timestamp&#x28;current_timestamp,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29; AS NON_REPLACE_CYCLE_TIME&#xd;&#xa;    FROM TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_2&#xd;&#xa;    WHERE replace_date IS NULL&#xd;&#xa;    GROUP BY month_id&#xd;&#xa;        ,rgn_id&#xd;&#xa;        ,base_id&#xd;&#xa;        ,brand_id&#xd;&#xa;        ,proj_id&#xd;&#xa;        ,supplier_id&#xd;&#xa;        ,tpmode_id&#xd;&#xa;    &#x29; C&#xd;&#xa;ON A.MONTH_ID &#x3d; C.MONTH_ID AND A.RGN_ID &#x3d; C.RGN_ID AND A.BASE_ID &#x3d; C.BASE_ID AND A.BRAND_ID &#x3d; C.BRAND_ID AND A.PROJ_ID &#x3d; C.PROJ_ID AND A.SUPPLIER_ID &#x3d; C.SUPPLIER_ID AND A.TPMODE_ID &#x3d; C.TPMODE_ID&#xd;&#xa;FULL JOIN&#x28;&#xd;&#xa;    SELECT A.month_id&#xd;&#xa;        ,A.rgn_id&#xd;&#xa;        ,A.base_id&#xd;&#xa;        ,A.brand_id&#xd;&#xa;        ,A.proj_id&#xd;&#xa;        ,A.supplier_id&#xd;&#xa;        ,A.tpmode_id&#xd;&#xa;        ,SUM&#x28;B.AMOUNT_SEQ_COST&#x29; AS NON_REP_PROD_COST&#xd;&#xa;    FROM&#x28;&#xd;&#xa;        SELECT month_id&#xd;&#xa;            ,rgn_id&#xd;&#xa;            ,base_id&#xd;&#xa;            ,brand_id&#xd;&#xa;            ,proj_id&#xd;&#xa;            ,supplier_id&#xd;&#xa;            ,tpmode_id&#xd;&#xa;            ,order_release_gid&#xd;&#xa;        FROM TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_2&#xd;&#xa;        WHERE replace_date IS NULL &#xd;&#xa;        &#x29; A&#xd;&#xa;    LEFT JOIN&#x28;&#xd;&#xa;        SELECT order_release_gid&#xd;&#xa;            ,AMOUNT_SEQ_COST&#xd;&#xa;        FROM DW.DW_TP_ORDERMOVEMENT&#xd;&#xa;        &#x29;B&#xd;&#xa;    ON A.order_release_gid &#x3d; B.order_release_gid&#xd;&#xa;    GROUP BY A.month_id &#xd;&#xa;        ,A.rgn_id&#xd;&#xa;        ,A.base_id&#xd;&#xa;        ,A.brand_id&#xd;&#xa;        ,A.proj_id&#xd;&#xa;        ,A.supplier_id&#xd;&#xa;        ,A.tpmode_id&#xd;&#xa;    &#x29; D&#xd;&#xa;ON A.MONTH_ID &#x3d; D.MONTH_ID AND A.RGN_ID &#x3d; D.RGN_ID AND A.BASE_ID &#x3d; D.BASE_ID AND A.BRAND_ID &#x3d; D.BRAND_ID AND A.PROJ_ID &#x3d; D.PROJ_ID AND A.SUPPLIER_ID &#x3d; D.SUPPLIER_ID AND A.TPMODE_ID &#x3d; D.TPMODE_ID&#xd;&#xa;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
