      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_3&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_4&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.SUPPLIER_ID&#xd;&#xa;    ,T.TPMODE_ID&#xd;&#xa;    ,sum&#x28;T.GL_ALLOT_ABNOMAL_QTY&#x29; AS GL_ALLOT_ABNOMAL_QTY&#xd;&#xa;    ,sum&#x28;T.GL_DLRY_QTY&#x29; AS GL_DLRY_QTY&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT A.month_id AS PERIOD_WID&#xd;&#xa;        ,coalesce&#x28;B.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;C.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;D.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;E.servprov_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,&#x27;GL&#x27; AS TPMODE_ID&#xd;&#xa;        ,sum&#x28;A.total_cnt&#x29; AS GL_ALLOT_ABNOMAL_QTY&#xd;&#xa;        ,0 AS GL_DLRY_QTY&#xd;&#xa;    FROM ODS_BLPT.T_HAND_ROAD_ABN_ALLO_RATE A&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT rgn_id,rgn_name&#xd;&#xa;        FROM DIM.dim_org &#xd;&#xa;        WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;        GROUP BY rgn_id,rgn_name&#xd;&#xa;        &#x29; B&#xd;&#xa;    ON A.rgn_name &#x3d; B.rgn_name&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT base_id,base_name&#xd;&#xa;        FROM DIM.dim_org&#xd;&#xa;        WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;        GROUP BY base_id,base_name&#xd;&#xa;        &#x29; C&#xd;&#xa;    ON A.base_name &#x3d; C.base_name&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT brand_id,brand_name&#xd;&#xa;        FROM DIM.dim_brand&#xd;&#xa;        GROUP BY brand_id,brand_name&#xd;&#xa;        &#x29; D&#xd;&#xa;    ON A.brand_nanme &#x3d; D.brand_name&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT servprov_id,servprov_name&#xd;&#xa;        FROM DIM.dim_servprov&#xd;&#xa;        GROUP BY servprov_id,servprov_name&#xd;&#xa;        &#x29; E   &#xd;&#xa;    ON A.servprov_name &#x3d; E.servprov_name&#xd;&#xa;    GROUP BY A.month_id&#xd;&#xa;        ,coalesce&#x28;B.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;C.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;D.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;E.servprov_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT substr&#x28;T.period_wid,1,6&#x29; AS PERIOD_WID&#xd;&#xa;        ,coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;T.supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,&#x27;GL&#x27; AS TPMODE_ID&#xd;&#xa;        ,0 AS GL_ALLOT_ABNOMAL_QTY&#xd;&#xa;        ,sum&#x28;NVL&#x28;T.shipping_num,0&#x29;&#x29; AS GL_DLRY_QTY&#xd;&#xa;    FROM DMK.dmk_tp_operate_d T&#xd;&#xa;    INNER JOIN TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_1 T1&#xd;&#xa;    ON substr&#x28;T.period_wid,1,6&#x29; &#x3d; T1.part_dt&#xd;&#xa;    WHERE T.transportmode_id &#x3d; &#x27;GL&#x27;&#xd;&#xa;    GROUP BY substr&#x28;T.period_wid,1,6&#x29;&#xd;&#xa;        ,coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.PERIOD_WID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.SUPPLIER_ID&#xd;&#xa;    ,T.TPMODE_ID&#xd;&#xa;&#x3b;   </sql>
