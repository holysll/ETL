      <name>TO_TEMP_KPI</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_KPI&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_KPI&#xd;&#xa;SELECT CONCAT&#x28;YEAR_ID, SPLIT&#x28;COL1, &#x27;&#x2f;&#x27;&#x29; &#x5b;0&#x5d;&#x29; AS PERIOD_WID&#xd;&#xa;      ,RGN_NAME&#xd;&#xa;      ,BASE_NAME&#xd;&#xa;      ,PROJ_NAME&#xd;&#xa;      ,SERVPROV_NAME AS SUPPLIER_NAME &#xd;&#xa;      ,TPMODE_NAME&#xd;&#xa;      ,SPLIT&#x28;COL1, &#x27;&#x2f;&#x27;&#x29; &#x5b;1&#x5d; AS KPI&#xd;&#xa;  FROM &#x28;SELECT ODS.YEAR_ID&#xd;&#xa;              ,ODS.RGN_NAME&#xd;&#xa;              ,ODS.base_name&#xd;&#xa;              ,ODS.proj_name&#xd;&#xa;              ,ODS.SERVPROV_NAME&#xd;&#xa;              ,ODS.tpmode_name&#xd;&#xa;              ,CONCAT_WS&#x28;&#x27;,&#x27;,&#xd;&#xa;                         CONCAT&#x28;&#x27;01&#x2f;&#x27;, ODS.JAN_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;02&#x2f;&#x27;, ODS.FEB_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;03&#x2f;&#x27;, ODS.MAR_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;04&#x2f;&#x27;, ODS.APR_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;05&#x2f;&#x27;, ODS.MAY_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;06&#x2f;&#x27;, ODS.JUNE_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;07&#x2f;&#x27;, ODS.JULY_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;08&#x2f;&#x27;, ODS.AUG_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;09&#x2f;&#x27;, ODS.SEPT_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;10&#x2f;&#x27;, ODS.OCT_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;11&#x2f;&#x27;, ODS.NOV_AMT&#x29;,&#xd;&#xa;                         CONCAT&#x28;&#x27;12&#x2f;&#x27;, ODS.DEC_AMT&#x29;&#x29; AS COL&#xd;&#xa;          FROM ODS_BLPT.T_HAND_SERVPROV_KPI ODS&#x29; T LATERAL VIEW EXPLODE&#x28;SPLIT&#x28;COL, &#x27;,&#x27;&#x29;&#x29; T AS COL1  &#xd;&#xa;&#x3b;</sql>
