      <name>TO_TEMP_2</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_2&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_2&#xd;&#xa;SELECT B.PART_DT AS MONTH_ID&#xd;&#xa;    ,A.rgn_id&#xd;&#xa;    ,A.base_id&#xd;&#xa;    ,A.brand_id&#xd;&#xa;    ,A.proj_id&#xd;&#xa;    ,A.supplier_id&#xd;&#xa;    ,A.transportmode_id AS TPMODE_ID&#xd;&#xa;    ,A.order_release_gid&#xd;&#xa;    ,A.factory_day&#xd;&#xa;    ,A.receipt_date&#xd;&#xa;FROM DMK.DMK_TP_DTD_PROG_TIME_D A  &#xd;&#xa;INNER JOIN TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_1 B  &#xd;&#xa;ON substr&#x28;regexp_replace&#x28;A.factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; B.PART_DT</sql>
