    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMR&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_SUPPL_MANAGE_M PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT T1.month_id&#xd;&#xa;    ,T1.rgn_id&#xd;&#xa;    ,T1.RGN_NAME&#xd;&#xa;    ,T1.base_id&#xd;&#xa;    ,T1.BASE_NAME&#xd;&#xa;    ,T1.brand_id&#xd;&#xa;    ,T1.brand_name&#xd;&#xa;    ,T1.proj_id&#xd;&#xa;    ,T1.proj_name&#xd;&#xa;    ,T1.supplier_id&#xd;&#xa;    ,T1.SUPPLIER_NAME&#xd;&#xa;    ,T1.tpmode_id&#xd;&#xa;    ,T1.tpmode_name&#xd;&#xa;    ,T1.replace_qty&#xd;&#xa;    ,T1.non_replace_qty&#xd;&#xa;    ,T1.tgt_replace_qty&#xd;&#xa;    ,T1.non_rep_prod_cost&#xd;&#xa;    ,T1.replace_cycle_time&#xd;&#xa;    ,T1.non_replace_cycle_time&#xd;&#xa;    ,T2.kpi AS KPI_GRADES&#xd;&#xa;    ,0 AS SATISFACTION_GRADES&#xd;&#xa;	,T4.gl_allot_abnomal_qty&#xd;&#xa;	,T4.gl_dlry_qty&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DATE  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;    ,T1.month_id AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT A.month_id&#xd;&#xa;        ,A.rgn_id&#xd;&#xa;        ,B.RGN_NAME&#xd;&#xa;        ,A.base_id&#xd;&#xa;        ,C.BASE_NAME&#xd;&#xa;        ,A.brand_id&#xd;&#xa;        ,D.brand_name&#xd;&#xa;        ,A.proj_id&#xd;&#xa;        ,E.proj_name&#xd;&#xa;        ,A.supplier_id&#xd;&#xa;        ,F.servprov_name AS SUPPLIER_NAME&#xd;&#xa;        ,A.tpmode_id&#xd;&#xa;        ,G.tpmode_name&#xd;&#xa;        ,A.replace_qty&#xd;&#xa;        ,A.non_replace_qty&#xd;&#xa;        ,A.tgt_replace_qty&#xd;&#xa;        ,A.non_rep_prod_cost&#xd;&#xa;        ,A.replace_cycle_time&#xd;&#xa;        ,A.non_replace_cycle_time&#xd;&#xa;    FROM TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_3 A&#xd;&#xa;    LEFT JOIN&#x28;&#xd;&#xa;        SELECT RGN_ID,RGN_NAME&#xd;&#xa;        FROM DIM.DIM_ORG&#xd;&#xa;        WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;        GROUP BY RGN_ID,RGN_NAME&#xd;&#xa;        &#x29; B&#xd;&#xa;    ON A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;    LEFT JOIN&#x28;&#xd;&#xa;        SELECT BASE_ID,BASE_NAME&#xd;&#xa;        FROM DIM.DIM_ORG&#xd;&#xa;        WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;        GROUP BY BASE_ID,BASE_NAME&#xd;&#xa;        &#x29; C&#xd;&#xa;    ON A.BASE_ID &#x3d; C.BASE_ID&#xd;&#xa;    LEFT JOIN&#x28;&#xd;&#xa;        SELECT brand_id,brand_name&#xd;&#xa;        FROM DIM.dim_brand&#xd;&#xa;        GROUP BY brand_id,brand_name&#xd;&#xa;        &#x29;D&#xd;&#xa;    ON A.brand_id &#x3d; D.brand_id&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT proj_id,proj_name&#xd;&#xa;        FROM DIM.dim_proj&#xd;&#xa;        GROUP BY proj_id,proj_name&#xd;&#xa;        &#x29; E&#xd;&#xa;    ON A.PROJ_ID &#x3d; E.PROJ_ID&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT servprov_id,servprov_name &#xd;&#xa;        FROM DIM.dim_servprov&#xd;&#xa;        GROUP BY servprov_id,servprov_name &#xd;&#xa;        &#x29; F&#xd;&#xa;    ON A.supplier_id &#x3d; F.servprov_id&#xd;&#xa;    LEFT JOIN&#x28;&#xd;&#xa;        SELECT tpmode_id,tpmode_name&#xd;&#xa;        FROM DIM.dim_transport_mode&#xd;&#xa;        GROUP BY tpmode_id,tpmode_name&#xd;&#xa;        &#x29; G&#xd;&#xa;    ON A.tpmode_id &#x3d; G.tpmode_id&#xd;&#xa;    &#x29; T1&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT A.period_wid&#xd;&#xa;        ,B.RGN_ID&#xd;&#xa;        ,A.rgn_name&#xd;&#xa;        ,C.BASE_ID&#xd;&#xa;        ,A.base_name&#xd;&#xa;        ,D.proj_id&#xd;&#xa;        ,A.proj_name&#xd;&#xa;        ,E.servprov_id AS SUPPLIER_ID&#xd;&#xa;        ,A.supplier_name&#xd;&#xa;        ,F.tpmode_id&#xd;&#xa;        ,A.tpmode_name&#xd;&#xa;        ,A.kpi&#xd;&#xa;    FROM TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_KPI A&#xd;&#xa;    LEFT JOIN&#x28;&#xd;&#xa;        SELECT RGN_ID,RGN_NAME&#xd;&#xa;        FROM DIM.DIM_ORG&#xd;&#xa;        WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;        GROUP BY RGN_ID,RGN_NAME&#xd;&#xa;        &#x29; B&#xd;&#xa;    ON A.rgn_name &#x3d; B.RGN_NAME&#xd;&#xa;    LEFT JOIN&#x28;&#xd;&#xa;        SELECT BASE_ID,BASE_NAME&#xd;&#xa;        FROM DIM.DIM_ORG&#xd;&#xa;        WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;        GROUP BY BASE_ID,BASE_NAME&#xd;&#xa;        &#x29; C&#xd;&#xa;    ON A.base_name &#x3d; C.BASE_NAME&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT proj_id,proj_name&#xd;&#xa;        FROM DIM.dim_proj&#xd;&#xa;        GROUP BY proj_id,proj_name&#xd;&#xa;        &#x29; D&#xd;&#xa;    ON A.proj_name &#x3d; D.proj_name&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT servprov_id,servprov_name &#xd;&#xa;        FROM DIM.dim_servprov&#xd;&#xa;        GROUP BY servprov_id,servprov_name &#xd;&#xa;        &#x29; E&#xd;&#xa;    ON A.supplier_name &#x3d; E.servprov_name&#xd;&#xa;    LEFT JOIN&#x28;&#xd;&#xa;        SELECT tpmode_id,tpmode_name&#xd;&#xa;        FROM DIM.dim_transport_mode&#xd;&#xa;        GROUP BY tpmode_id,tpmode_name&#xd;&#xa;        &#x29; F&#xd;&#xa;    ON A.TPMODE_NAME &#x3d; F.TPMODE_NAME&#xd;&#xa;    WHERE A.period_wid IS NOT NULL&#xd;&#xa;    &#x29; T2&#xd;&#xa;ON T1.month_id &#x3d; T2.PERIOD_WID AND T1.rgn_id &#x3d; T2.RGN_ID AND T1.base_id &#x3d; T2.BASE_ID AND T1.proj_id &#x3d; T2.PROJ_ID AND T1.supplier_id &#x3d; T2.SUPPLIER_ID&#xd;&#xa;LEFT JOIN TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_4 T4&#xd;&#xa;ON T1.month_id &#x3d; T4.PERIOD_WID AND T1.rgn_id &#x3d; T4.RGN_ID AND T1.base_id &#x3d; T4.BASE_ID AND T1.brand_id &#x3d; T4.BRAND_ID AND T1.supplier_id &#x3d; T4.SUPPLIER_ID&#xd;&#xa;&#x3b;</sql>
