      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_1&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_1&#x28;&#xd;&#xa;PART_DT		STRING	COMMENT&#x27;&#x52a8;&#x6001;&#x5206;&#x533a;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x52a8;&#x6001;&#x5206;&#x533a;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_2&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_2&#x28;&#xd;&#xa;MONTH_ID	STRING	COMMENT&#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID		STRING	COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID	STRING	COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	STRING	COMMENT&#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,PROJ_ID	STRING	COMMENT&#x27;&#x9879;&#x76ee;ID&#x27;&#xd;&#xa;,SUPPLIER_ID	STRING	COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID		STRING	COMMENT&#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;&#xd;&#xa;,ORDER_RELEASE_GID	STRING	COMMENT&#x27;OTM&#x5355;&#x53f7;&#x27;&#xd;&#xa;,FACTORY_DAY	STRING	COMMENT&#x27;&#x53d1;&#x8fd0;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,REPLACE_DATE	STRING	COMMENT&#x27;&#x56de;&#x5355;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x660e;&#x7ec6;&#x6570;&#x636e;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_3&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_3&#x28;&#xd;&#xa;MONTH_ID	STRING	COMMENT&#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID		STRING	COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID	STRING	COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	STRING	COMMENT&#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,PROJ_ID	STRING	COMMENT&#x27;&#x9879;&#x76ee;ID&#x27;&#xd;&#xa;,SUPPLIER_ID	STRING	COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID		STRING	COMMENT&#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;&#xd;&#xa;,REPLACE_QTY	BIGINT	COMMENT&#x27;&#x56de;&#x5355;&#x91cf;&#x27;&#xd;&#xa;,NON_REPLACE_QTY	BIGINT	COMMENT&#x27;&#x672a;&#x56de;&#x5355;&#x91cf;&#x27;&#xd;&#xa;,TGT_REPLACE_QTY	BIGINT	COMMENT&#x27;&#x76ee;&#x6807;&#x56de;&#x5355;&#x91cf;&#x27;&#xd;&#xa;,NON_REP_PROD_COST	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x672a;&#x56de;&#x5355;&#x91cf;&#x4ea7;&#x503c;&#x6210;&#x672c;&#x27;&#xd;&#xa;,REPLACE_CYCLE_TIME	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x56de;&#x5355;&#x5468;&#x671f;&#x27;&#xd;&#xa;,NON_REPLACE_CYCLE_TIME	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x672a;&#x56de;&#x5355;&#x5468;&#x671f;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x6c47;&#x603b;1&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_4&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_SUPPL_MANAGE_M_4&#x28;&#xd;&#xa;PERIOD_WID	STRING	COMMENT&#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID		STRING	COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID	STRING	COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	STRING	COMMENT&#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID	STRING	COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID		STRING	COMMENT&#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;&#xd;&#xa;,GL_ALLOT_ABNOMAL_QTY	BIGINT	COMMENT&#x27;&#x516c;&#x8def;&#x5f02;&#x5e38;&#x5206;&#x914d;&#x6570;&#x27;&#xd;&#xa;,GL_DLRY_QTY	BIGINT	COMMENT&#x27;&#x516c;&#x8def;&#x53d1;&#x