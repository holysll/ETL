      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_FIN_RECEIVABLES_M_1&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_FIN_RECEIVABLES_M_1&#x28;&#xd;&#xa;RGN_ID		STRING		COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID	STRING		COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	STRING		COMMENT&#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,CUR_M_RECEIVABLE	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#x27;&#xd;&#xa;,CUR_Y_RECEIVABLE	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#x27;&#xd;&#xa;,CUR_M_TGT_RECEIVABLE	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#x27;&#xd;&#xa;,CUR_Y_TGT_RECEIVABLE	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#x27;&#xd;&#xa;,CUR_M_MANAGE_NT_CASH_FW	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x672c;&#x6708;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#x27;&#xd;&#xa;,CUR_Y_MANAGE_NT_CASH_FW	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x672c;&#x5e74;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x4e34;&#x65f6;&#x8868;1&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_FIN_RECEIVABLES_M_SUM&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_FIN_RECEIVABLES_M_SUM&#x28;&#xd;&#xa;RGN_ID		STRING		COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,RGN_NAME		STRING		COMMENT&#x27;&#x5927;&#x533a;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,BASE_ID	STRING		COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BASE_NAME	STRING		COMMENT&#x27;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,BRAND_ID	STRING		COMMENT&#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,BRAND_NAME STRING		COMMENT&#x27;&#x54c1;&#x724c;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,CUR_M_MANAGE_NT_CASH_FW	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x672c;&#x6708;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#x27;&#xd;&#xa;,CUR_Y_MANAGE_NT_CASH_FW	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x672c;&#x5e74;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#x27;&#xd;&#xa;,CUR_M_RECEIVABLE	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#x27;&#xd;&#xa;,CUR_Y_RECEIVABLE	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#x27;&#xd;&#xa;,CUR_M_TGT_RECEIVABLE	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#x27;&#xd;&#xa;,CUR_Y_TGT_RECEIVABLE	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#x27;&#xd;&#xa;,RECEIVABLE_M_CMP_RATE 	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#x27;&#xd;&#xa;,RECEIVABLE_Y_CMP_RATE 	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#x27;&#xd;&#xa;,WRN_M_FLAG	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x56de;&#x6b3e;&#x7387;&#x6708;&#x5ea6;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#x27;&#xd;&#xa;,WRN_Y_FLAG	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x56de;&#x6b3e;&#x7387;&#x6708;&#x5ea6;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x4e34;&#x65f6;&#x8868;1&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;</sql>
