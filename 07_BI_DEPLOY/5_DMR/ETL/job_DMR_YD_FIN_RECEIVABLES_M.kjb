    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_FIN_RECEIVABLES_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;    ,T.&#x2a;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4; &#xd;&#xa;    ,&#x27;DMK&#x27; AS PART_SC --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;FROM TEMP.TEMP_DMR_YD_FIN_RECEIVABLES_M_SUM T&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT M.PERIOD_WID AS MONTH_ID&#xd;&#xa;      ,M.RGN_ID&#xd;&#xa;      ,M.RGN_NAME&#xd;&#xa;      ,M.BASE_ID&#xd;&#xa;      ,M.BASE_NAME&#xd;&#xa;      ,M.BRAND_ID&#xd;&#xa;      ,M.BRAND_NAME&#xd;&#xa;      ,M.CUR_M_MANAGE_NT_CASH_FW&#xd;&#xa;      ,M.CUR_Y_MANAGE_NT_CASH_FW&#xd;&#xa;      ,M.CUR_M_RECEIVABLE&#xd;&#xa;      ,Y.CUR_Y_RECEIVABLE&#xd;&#xa;      ,M.CUR_M_TGT_RECEIVABLE&#xd;&#xa;      ,Y.CUR_Y_TGT_RECEIVABLE&#xd;&#xa;      ,M.RECEIVABLE_M_CMP_RATE&#xd;&#xa;      ,Y.RECEIVABLE_Y_CMP_RATE&#xd;&#xa;      ,NULL AS WRN_M_FLAG&#xd;&#xa;      ,NULL AS WRN_Y_FLAG&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4; &#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;      ,M.PERIOD_WID AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM &#x28;SELECT T.PERIOD_WID --&#x671f;&#x95f4;ID&#xd;&#xa;              ,T.RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;              ,O1.RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;              ,T.BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;              ,O2.BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;              ,T.BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;              ,B.BRAND_NAME --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;              ,NULL AS CUR_M_MANAGE_NT_CASH_FW -- &#x672c;&#x6708;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;              ,NULL AS CUR_Y_MANAGE_NT_CASH_FW -- &#x672c;&#x5e74;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;              ,SUM&#x28;T.AR_RETURN_AMT&#x29; AS CUR_M_RECEIVABLE --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;              ,NULL AS CUR_Y_RECEIVABLE --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;              ,SUM&#x28;T.AR_RETURN_TGT_AMT&#x29; AS CUR_M_TGT_RECEIVABLE --&#x5f53;&#x6708;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;              ,NULL AS CUR_Y_TGT_RECEIVABLE --&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN SUM&#x28;T.AR_RETURN_TGT_AMT&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  &#x28;SUM&#x28;T.AR_RETURN_AMT&#x29; &#x2f; SUM&#x28;T.AR_RETURN_TGT_AMT&#x29;&#x29;&#x2a;100&#xd;&#xa;               END AS RECEIVABLE_M_CMP_RATE --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,NULL AS RECEIVABLE_Y_CMP_RATE --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;          FROM DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;          LEFT JOIN DIM.DIM_ORG O1&#xd;&#xa;            ON T.RGN_ID &#x3d; O1.ORG_ID&#xd;&#xa;           AND O1.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;          LEFT JOIN DIM.DIM_ORG O2&#xd;&#xa;            ON T.BASE_ID &#x3d; O2.ORG_ID&#xd;&#xa;           AND O2.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;          LEFT JOIN DIM.DIM_BRAND B&#xd;&#xa;            ON T.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;          LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;            ON T.PROJ_ID &#x3d; P.PROJ_ID&#xd;&#xa;         WHERE T.PERIOD_WID &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;         GROUP BY T.PERIOD_WID&#xd;&#xa;                 ,T.RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                 ,O1.RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                 ,T.BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;                 ,O2.BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;                 ,T.BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;                 ,B.BRAND_NAME --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;        &#x29; AS M&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                   ,O1.RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                   ,T.BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;                   ,O2.BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;                   ,T.BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;                   ,B.BRAND_NAME --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;                   ,NULL AS CUR_M_MANAGE_NT_CASH_FW -- &#x672c;&#x6708;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;                   ,NULL AS CUR_Y_MANAGE_NT_CASH_FW -- &#x672c;&#x5e74;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;                   ,NULL AS CUR_M_RECEIVABLE --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.AR_RETURN_AMT&#x29; AS CUR_Y_RECEIVABLE --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;                   ,NULL AS CUR_M_TGT_RECEIVABLE --&#x5f53;&#x6708;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.AR_RETURN_TGT_AMT&#x29; AS CUR_Y_TGT_RECEIVABLE --&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;                   ,NULL AS RECEIVABLE_M_CMP_RATE --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;                   ,CASE&#xd;&#xa;                      WHEN SUM&#x28;T.AR_RETURN_TGT_AMT&#x29; &#x3d; 0 THEN&#xd;&#xa;                       0&#xd;&#xa;                      ELSE&#xd;&#xa;                       SUM&#x28;T.AR_RETURN_AMT&#x29; &#x2f; SUM&#x28;T.AR_RETURN_TGT_AMT&#x29;&#xd;&#xa;                    END AS RECEIVABLE_Y_CMP_RATE --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;               FROM DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;               LEFT JOIN DIM.DIM_ORG O1&#xd;&#xa;                 ON T.RGN_ID &#x3d; O1.ORG_ID&#xd;&#xa;                AND O1.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;               LEFT JOIN DIM.DIM_ORG O2&#xd;&#xa;                 ON T.BASE_ID &#x3d; O2.ORG_ID&#xd;&#xa;                AND O2.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;               LEFT JOIN DIM.DIM_BRAND B&#xd;&#xa;                 ON T.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;               LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;                 ON T.PROJ_ID &#x3d; P.PROJ_ID&#xd;&#xa;              WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T.RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                      ,O1.RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;                      ,O2.BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;                      ,B.BRAND_NAME --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;             &#x29; AS Y&#xd;&#xa;    ON M.PERIOD_WID &#x3d; Y.PERIOD_WID&#xd;&#xa;   AND M.RGN_ID &#x3d; Y.RGN_ID&#xd;&#xa;   AND M.RGN_NAME &#x3d; Y.RGN_NAME&#xd;&#xa;   AND M.BASE_ID &#x3d; Y.BASE_ID&#xd;&#xa;   AND M.BASE_NAME &#x3d; Y.BASE_NAME&#xd;&#xa;   AND M.BRAND_ID &#x3d; Y.BRAND_ID&#xd;&#xa;   AND M.BRAND_NAME &#x3d; Y.BRAND_NAME &#x3b;&#xd;&#xa;&#x2a;&#x2f;</sql>
