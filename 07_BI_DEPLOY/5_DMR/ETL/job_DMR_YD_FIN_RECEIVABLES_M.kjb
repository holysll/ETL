#x5b8c;&#x6210;&#x7387;&#xd;&#xa;    ,sum&#x28;A.cur_y_receivable&#x29;&#x2f;sum&#x28;A.cur_y_tgt_receivable&#x29;&#x2a;100 AS RECEIVABLE_Y_CMP_RATE  --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;    ,CASE WHEN sum&#x28;A.cur_m_receivable&#x29;&#x2f;sum&#x28;A.cur_m_tgt_receivable&#x29; &#x3e;&#x3d; 0.9 THEN 1 &#xd;&#xa;        WHEN sum&#x28;A.cur_m_receivable&#x29;&#x2f;sum&#x28;A.cur_m_tgt_receivable&#x29; &#x3c; 0.9 AND sum&#x28;A.cur_m_receivable&#x29;&#x2f;sum&#x28;A.cur_m_tgt_receivable&#x29; &#x3e;&#x3d; 0.8 THEN 0 &#xd;&#xa;        ELSE -1 END AS WRN_M_FLAG  --&#x56de;&#x6b3e;&#x7387;&#x6708;&#x5ea6;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;    ,CASE WHEN sum&#x28;A.cur_y_receivable&#x29;&#x2f;sum&#x28;A.cur_y_tgt_receivable&#x29; &#x3e;&#x3d; 0.9 THEN 1 &#xd;&#xa;        WHEN sum&#x28;A.cur_y_receivable&#x29;&#x2f;sum&#x28;A.cur_y_tgt_receivable&#x29; &#x3c; 0.9 AND sum&#x28;A.cur_y_receivable&#x29;&#x2f;sum&#x28;A.cur_y_tgt_receivable&#x29; &#x3e;&#x3d; 0.8 THEN 0 &#xd;&#xa;        ELSE -1 END AS WRN_Y_FLAG  --&#x56de;&#x6b3e;&#x7387;&#x6708;&#x5ea6;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;FROM TEMP.TEMP_DMR_YD_FIN_RECEIVABLES_M_1 A&#xd;&#xa;LEFT JOIN TEMP.TEMP_DMR_YD_FIN_RECEIVABLES_M_2 B  &#xd;&#xa;ON A.rgn_id &#x3d; B.rgn_id AND A.base_id &#x3d; B.base_id AND A.brand_id &#x3d; B.brand_id&#xd;&#xa;WHERE A.rgn_id &#x3d; &#x27;-1&#x27; AND A.base_id &#x3d; &#x27;-1&#x27; AND A.brand_id &#x3d;&#x27;-1&#x27;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT coalesce&#x28;A.rgn_id,B.rgn_id&#x29; AS RGN_ID&#xd;&#xa;    ,C.rgn_name AS RGN_NAME&#xd;&#xa;    ,&#x27;ALL&#x27; AS BASE_ID&#xd;&#xa;    ,&#x27;&#x5168;&#x90e8;&#x27; AS BASE_NAME&#xd;&#xa;    ,&#x27;ALL&#x27; AS BRAND_ID&#xd;&#xa;    ,&#x27;&#x5168;&#x90e8;&#x27; AS BRAND_NAME&#xd;&#xa;    ,sum&#x28;B.cur_m_manage_nt_cash_fw&#x29; AS CUR_M_MANAGE_NT_CASH_FW  --&#x672c;&#x6708;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;    ,sum&#x28;B.cur_y_manage_nt_cash_fw&#x29; AS CUR_Y_MANAGE_NT_CASH_FW  --&#x672c;&#x5e74;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;    ,sum&#x28;A.cur_m_receivable&#x29; AS CUR_M_RECEIVABLE  --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;A.cur_y_receivable&#x29; AS CUR_Y_RECEIVABLE  --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;A.cur_m_tgt_receivable&#x29; AS CUR_M_TGT_RECEIVABLE  --&#x5f53;&#x6708;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;A.cur_y_tgt_receivable&#x29; AS CUR_Y_TGT_RECEIVABLE  --&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;A.cur_m_receivable&#x29;&#x2f;sum&#x28;A.cur_m_tgt_receivable&#x29;&#x2a;100 AS RECEIVABLE_M_CMP_RATE  --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;    ,sum&#x28;A.cur_y_receivable&#x29;&#x2f;sum&#x28;A.cur_y_tgt_receivable&#x29;&#x2a;100 AS RECEIVABLE_Y_CMP_RATE  --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;    ,CASE WHEN sum&#x28;A.cur_m_receivable&#x29;&#x2f;sum&#x28;A.cur_m_tgt_receivable&#x29; &#x3e;&#x3d; 0.9 THEN 1 &#xd;&#xa;        WHEN sum&#x28;A.cur_m_receivable&#x29;&#x2f;sum&#x28;A.cur_m_tgt_receivable&#x29; &#x3c; 0.9 AND sum&#x28;A.cur_m_receivable&#x29;&#x2f;sum&#x28;A.cur_m_tgt_receivable&#x29; &#x3e;&#x3d; 0.8 THEN 0 &#xd;&#xa;        ELSE -1 END AS WRN_M_FLAG  --&#x56de;&#x6b3e;&#x7387;&#x6708;&#x5ea6;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;    ,CASE WHEN sum&#x28;A.cur_y_receivable&#x29;&#x2f;sum&#x28;A.cur_y_tgt_receivable&#x29; &#x3e;&#x3d; 0.9 THEN 1 &#xd;&#xa;        WHEN sum&#x28;A.cur_y_receivable&#x29;&#x2f;sum&#x28;A.cur_y_tgt_receivable&#x29; &#x3c; 0.9 AND sum&#x28;A.cur_y_receivable&#x29;&#x2f;sum&#x28;A.cur_y_tgt_receivable&#x29; &#x3e;&#x3d; 0.8 THEN 0 &#xd;&#xa;        ELSE -1 END AS WRN_Y_FLAG  --&#x56de;&#x6b3e;&#x7387;&#x6708;&#x5ea6;&#x988