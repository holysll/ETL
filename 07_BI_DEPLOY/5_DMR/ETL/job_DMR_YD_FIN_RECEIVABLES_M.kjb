      <name>TO_SUM</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_FIN_RECEIVABLES_M_SUM&#xd;&#xa;SELECT T.rgn_id  --&#x5927;&#x533a;ID&#xd;&#xa;    ,CASE WHEN T.rgn_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.rgn_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.rgn_name END AS RNG_NAME  --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;    ,T.base_id  --&#x57fa;&#x5730;ID&#xd;&#xa;    ,CASE WHEN T.base_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.base_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.base_name END AS BASE_NAME  --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;    ,T.brand_id  --&#x54c1;&#x724c;ID&#xd;&#xa;    ,CASE WHEN T.brand_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.brand_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.brand_name END AS BRAND_NAME  --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;    ,sum&#x28;T.cur_m_manage_nt_cash_fw&#x29; AS CUR_M_MANAGE_NT_CASH_FW  --&#x672c;&#x6708;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;    ,sum&#x28;T.cur_y_manage_nt_cash_fw&#x29; AS CUR_Y_MANAGE_NT_CASH_FW  --&#x672c;&#x5e74;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;    ,sum&#x28;T.cur_m_receivable&#x29; AS CUR_M_RECEIVABLE  --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;T.cur_y_receivable&#x29; AS CUR_Y_RECEIVABLE  --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;T.cur_m_tgt_receivable&#x29; AS CUR_M_TGT_RECEIVABLE  --&#x5f53;&#x6708;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;T.cur_y_tgt_receivable&#x29; AS CUR_Y_TGT_RECEIVABLE  --&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;T.cur_m_receivable&#x29;&#x2f;sum&#x28;T.cur_m_tgt_receivable&#x29;&#x2a;100 AS RECEIVABLE_M_CMP_RATE  --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;    ,sum&#x28;T.cur_y_receivable&#x29;&#x2f;sum&#x28;T.cur_y_tgt_receivable&#x29;&#x2a;100 AS RECEIVABLE_Y_CMP_RATE  --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;    ,CASE WHEN sum&#x28;T.cur_m_receivable&#x29;&#x2f;sum&#x28;T.cur_m_tgt_receivable&#x29; &#x3e;&#x3d; 0.9 THEN 1 &#xd;&#xa;        WHEN sum&#x28;T.cur_m_receivable&#x29;&#x2f;sum&#x28;T.cur_m_tgt_receivable&#x29; &#x3c; 0.9 AND sum&#x28;T.cur_m_receivable&#x29;&#x2f;sum&#x28;T.cur_m_tgt_receivable&#x29; &#x3e;&#x3d; 0.8 THEN 0 &#xd;&#xa;        ELSE -1 END AS WRN_M_FLAG  --&#x56de;&#x6b3e;&#x7387;&#x6708;&#x5ea6;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;    ,CASE WHEN sum&#x28;T.cur_y_receivable&#x29;&#x2f;sum&#x28;T.cur_y_tgt_receivable&#x29; &#x3e;&#x3d; 0.9 THEN 1 &#xd;&#xa;        WHEN sum&#x28;T.cur_y_receivable&#x29;&#x2f;sum&#x28;T.cur_y_tgt_receivable&#x29; &#x3c; 0.9 AND sum&#x28;T.cur_y_receivable&#x29;&#x2f;sum&#x28;T.cur_y_tgt_receivable&#x29; &#x3e;&#x3d; 0.8 THEN 0 &#xd;&#xa;        ELSE -1 END AS WRN_Y_FLAG  --&#x56de;&#x6b3e;&#x7387;&#x6708;&#x5ea6;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;FROM TEMP.TEMP_DMR_YD_FIN_RECEIVABLES_M_1 T&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#x