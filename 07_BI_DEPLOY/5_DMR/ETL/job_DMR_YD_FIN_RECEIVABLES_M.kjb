    <entry>
      <name>TO_DETAIL</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_FIN_RECEIVABLES_M_1&#xd;&#xa;SELECT coalesce&#x28;A.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;A.BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;A.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,sum&#x28;A.CUR_M_RECEIVABLE&#x29; AS CUR_M_RECEIVABLE  --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;A.CUR_Y_RECEIVABLE&#x29; AS CUR_Y_RECEIVABLE  --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;A.CUR_M_TGT_RECEIVABLE&#x29; AS CUR_M_TGT_RECEIVABLE  --&#x5f53;&#x6708;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;A.CUR_Y_TGT_RECEIVABLE&#x29; AS CUR_Y_TGT_RECEIVABLE  --&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    ,sum&#x28;A.CUR_M_MANAGE_NT_CASH_FW&#x29; AS CUR_M_MANAGE_NT_CASH_FW --&#x672c;&#x6708;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;    ,sum&#x28;A.CUR_Y_MANAGE_NT_CASH_FW&#x29; AS CUR_Y_MANAGE_NT_CASH_FW  --&#x672c;&#x5e74;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,sum&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AND TYPE &#x3d; &#x27;&#x5b9e;&#x9645;&#x6536;&#x6b3e;&#x27; THEN ar_amt ELSE 0 END&#x29; AS CUR_M_RECEIVABLE  --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;part_dt,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND TYPE &#x3d; &#x27;&#x5b9e;&#x9645;&#x6536;&#x6b3e;&#x27; THEN ar_amt ELSE 0 END&#x29; AS CUR_Y_RECEIVABLE  --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;        ,SUM&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AND TYPE &#x3d;&#x27;&#x6536;&#x6b3e;&#x8ba1;&#x5212;&#x27; THEN ar_amt ELSE 0 END&#x29; AS CUR_M_TGT_RECEIVABLE  --&#x5f53;&#x6708;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;        ,sum&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AND TYPE &#x3d;&#x27;&#x6536;&#x6b3e;&#x8ba1;&#x5212;&#x27; THEN ar_amt &#xd;&#xa;                    WHEN part_dt &#x3c;&#x3e; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AND TYPE &#x3d; &#x27;&#x5b9e;&#x9645;&#x6536;&#x6b3e;&#x27; THEN ar_amt ELSE 0 END&#x29; AS CUR_Y_TGT_RECEIVABLE  --&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    	,0 AS CUR_M_MANAGE_NT_CASH_FW --&#x672c;&#x6708;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;    	,0 AS CUR_Y_MANAGE_NT_CASH_FW  --&#x672c;&#x5e74;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;    FROM DW.dw_fin_ar&#xd;&#xa;    WHERE substr&#x28;part_dt,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND part_dt &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;		AND type IN &#x28;&#x27;&#x6536;&#x6b3e;&#x8ba1;&#x5212;&#x27;,&#x27;&#x5b9e;&#x9645;&#x6536;&#x6b3e;&#x27;&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    	,0 AS CUR_M_RECEIVABLE  --&#x5f53;&#x6708;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    	,0 AS CUR_Y_RECEIVABLE  --&#x5f53;&#x5e74;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    	,0 AS CUR_M_TGT_RECEIVABLE  --&#x5f53;&#x6708;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;    	,0 AS CUR_Y_TGT_RECEIVABLE  --&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x5e94;&#x6536;&#x8d26;&#x6b3e;&#x91d1;&#x989d;&#xd;&#xa;        ,sum&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AND type &#x3d; &#x27;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#x5b9e;&#x9645;&#x27; THEN ar_amt ELSE 0 END&#x29; AS CUR_M_MANAGE_NT_CASH_FW --&#x672c;&#x6708;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;part_dt,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND type &#x3d; &#x27;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#x5b9e;&#x9645;&#x27; THEN ar_amt ELSE 0 END&#x29; AS CUR_Y_MANAGE_NT_CASH_FW  --&#x672c;&#x5e74;&#x7ecf;&#x8425;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#xd;&#xa;    FROM DW.dw_fin_ar &#xd;&#xa;    WHERE substr&#x28;part_dt,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND part_dt &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND type IN &#x28;&#x27;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#x8ba1;&#x5212;&#x27;,&#x27;&#x73b0;&#x91d1;&#x51c0;&#x6d41;&#x91cf;&#x5b9e;&#x9645;&#x27;&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; A&#xd;&#xa;GROUP BY A.RGN_ID,A.BASE_ID,A.BRAND_ID WITH CUBE</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
