      <name>CREATE_TEMP</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_DTD_PROG_DETAIL&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_DTD_PROG_DETAIL&#x28;&#xd;&#xa;PROJ_NAME STRING   COMMENT&#x27;&#x9879;&#x76ee;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,VIN STRING   COMMENT&#x27;VIN&#x27;&#xd;&#xa;,BRAND_ID STRING   COMMENT&#x27;&#x54c1;&#x724c;&#x27;&#xd;&#xa;,ORDER_RELEASE_NAME STRING   COMMENT&#x27;&#x8ba2;&#x5355;&#x53f7;&#x27;&#xd;&#xa;,SEQ_NO STRING   COMMENT&#x27;&#x8fd0;&#x8f93;&#x6bb5;&#x27;&#xd;&#xa;,ORDER_MOVEMENT_GID STRING COMMENT&#x27;&#x5206;&#x6bb5;&#x8def;&#x7531;GID&#x27;&#xd;&#xa;,TRANSPORTMODE_ID STRING   COMMENT&#x27;&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,EARLY_PICKUP_DATE STRING   COMMENT&#x27;&#x63a5;&#x5355;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,ALLOT_PLN_DAY STRING   COMMENT&#x27;&#x5206;&#x914d;&#x65f6;&#x95f4; &#x82e5;&#x6ca1;&#x6709;&#x5219;&#x53d6;&#x627f;&#x8fd0;&#x5546;&#x6700;&#x65b0;&#x786e;&#x8ba4;&#x65f6;&#x95f4;  &#x27;&#xd;&#xa;,OUT_STORE_DAY STRING   COMMENT&#x27;&#x51fa;&#x5e93;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,FACTORY_DAY STRING   COMMENT&#x27;&#x6253;&#x5355;&#x65f6;&#x95f4;  &#x51fa;&#x573a;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,ARRIVAL_DAY STRING   COMMENT&#x27;&#x4ea4;&#x4ed8;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,RECEIPT_DATE STRING   COMMENT&#x27;&#x56de;&#x5355;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,BILL_DATE STRING   COMMENT&#x27;&#x7ed3;&#x7b97;&#x65f6;&#x95f4;&#x28;&#x4e0e;&#x627f;&#x8fd0;&#x5546;&#xff09;&#x27;&#xd;&#xa;,DETAIL_FLAG STRING   COMMENT&#x27;&#x6563;&#x5355;&#x6807;&#x5fd7;&#x28;&#x6563;&#x5355;&#x7684;&#x5224;&#x65ad;&#x903b;&#x8f91;&#x29;&#x27;&#xd;&#xa;,ON_PASSAGE_DAYS	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x6807;&#x51c6;&#x5728;&#x9014;&#x5929;&#x6570;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;DTD&#x603b;&#x65f6;&#x957f;TEMP&#x8868;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_DTD_PROG_DETAIL_STAND&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_DTD_PROG_DETAIL_STAND&#x28;&#xd;&#xa;PROJ_ID		STRING	COMMENT&#x27;&#x9879;&#x76ee;ID&#x27;&#xd;&#xa;,PROJ_NAME	STRING	COMMENT&#x27;&#x9879;&#x76ee;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,TPMODE_CODE		STRING	COMMENT&#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x4ee3;&#x7801;&#x27;&#xd;&#xa;,CATD_NAME		STRING	COMMENT&#x27;&#x8fd0;&#x8f93;&#x73af;&#x8282;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,STAND_VALUE	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x8fd0;&#x8f93;&#x73af;&#x8282;&#x6807;&#x51c6;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,DAY_VALUE		STRING	COMMENT&#x27;&#x5355;&#x4f4d;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;DTD&#x5404;&#x73af;&#x8282;&#x6807;&#x51c6;&#x65f6;&#x957f;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_DTD_PROG_DETAIL_BIZ&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_DTD_PROG_DETAIL_BIZ&#x28;&#xd;&#xa;SEGMETN_TRANSPORTMODE	STRING	COMMENT&#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CARD_ID	STRING	COMMENT&#x27;&#x5361;&#x7247;ID&#x27;&#xd;&#xa;,CARD_NAME	STRING	COMMENT&#x27;&#x5361;&#x7247;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,PROJ_NAME	STRING	COMMENT&#x27;&#x9879;&#x76ee;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,BIZ_CNT	BIGINT	COMMENT&#x27;&#x603b;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;,TOTAL_BIZ_COMP_CNT	BIGINT	COMMENT&#x27;&#x6574;&#x4f53;&#x4e1a;&#x52a1;&#x5b8c;&#x6210;&#x91cf;&#x27;&#xd;&#xa;,THROWAWAY_CNT	STRING	COMMENT&#x27;&#x6563;&#x5355;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;DTD&#x603b;&#x65f6;&#x957f;TEMP&#x8868;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_DTD_PROG_DETAIL_CYCLE&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_DTD_PROG_DETAIL_CYCLE&#x28;&#xd;&#xa;SEGMETN_TRANSPORTMODE	STRING	COMMENT&#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CARD_ID	STRING	COMMENT&#x27;&#x5361;&#x7247;ID&#x27;&#xd;&#xa;,CARD_NAME	STRING	COMMENT&#x27;&#x5361;&#x7247;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,PROJ_NAME	STRING	COMMENT&#x27;&#x9879;&#x76ee;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,T_AVG_TIME	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x65f6;&#x95f4;&#x5468;&#x671f;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;DTD&#x603b;&#x65f6;&#x957f;TEMP&#x8868;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_DTD_PROG_DETAIL_OUTTIME&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_DTD_PROG_DETAIL_OUTTIME&#x28;&#xd;&#xa;SEGMETN_TRANSPORTMODE	STRING	COMMENT&#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CARD_ID	STRING	COMMENT&#x27;&#x5361;&#x7247;ID&#x27;&#xd;&#xa;,CARD_NAME	STRING	COMMENT&#x27;&#x5361;&#x7247;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,PROJ_NAME	STRING	COMMENT&#x27;&#x9879;&#x76ee;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,OVERTIME_BILL_CNT   BIGINT	COMMENT&#x27;&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;DTD&#x603b;&#x65f6;&#x957f;TEMP&#x8868;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
