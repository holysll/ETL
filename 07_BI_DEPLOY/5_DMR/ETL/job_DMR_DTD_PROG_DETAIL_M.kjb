      <name>TO_TEMP_ALL_BIZ</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_DTD_PROG_DETAIL_BIZ&#xd;&#xa;SELECT transportmode_id AS SEGMETN_TRANSPORTMODE&#xd;&#xa;	,&#x27;CARD001&#x27; AS CARD_ID&#xd;&#xa;	,&#x27;&#x63a5;&#x5355;-&#x5206;&#x914d;&#x27; AS CARD_NAME&#xd;&#xa;	,PROJ_NAME&#xd;&#xa;	,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN vin ELSE NULL END&#x29; AS BIZ_CNT&#xd;&#xa;	,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VIN ELSE NULL END&#x29; AS TOTAL_BIZ_COMP_CNT&#xd;&#xa;	,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AND detail_flag &#x3d; 1 THEN VIN ELSE NULL END&#x29; AS THROWAWAY_CNT&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL&#xd;&#xa;WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;	OR SUBSTR&#x28;REGEXP_REPLACE&#x28;ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY transportmode_id,proj_name&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT transportmode_id AS SEGMETN_TRANSPORTMODE&#xd;&#xa;	,&#x27;CARD002&#x27; AS CARD_ID&#xd;&#xa;	,&#x27;&#x5206;&#x914d;-&#x51fa;&#x5e93;&#x27; AS CARD_NAME&#xd;&#xa;	,PROJ_NAME&#xd;&#xa;	,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN vin ELSE NULL END&#x29; AS BIZ_CNT&#xd;&#xa;	,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;OUT_STORE_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VIN ELSE NULL END&#x29; AS TOTAL_BIZ_COMP_CNT&#xd;&#xa;	,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AND detail_flag &#x3d; 1 THEN VIN ELSE NULL END&#x29; AS THROWAWAY_CNT&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL&#xd;&#xa;WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;	OR SUBSTR&#x28;REGEXP_REPLACE&#x28;OUT_STORE_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY transportmode_id,proj_name&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT transportmode_id AS SEGMETN_TRANSPORTMODE&#xd;&#xa;	,&#x27;CARD003&#x27; AS CARD_ID&#xd;&#xa;	,&#x27;&#x63a5;&#x8f66;-&#x6253;&#x5355;&#x27; AS CARD_NAME&#xd;&#xa;	,PROJ_NAME&#xd;&#xa;	,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;OUT_STORE_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;