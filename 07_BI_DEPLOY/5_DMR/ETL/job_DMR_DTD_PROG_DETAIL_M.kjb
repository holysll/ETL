      <name>TO_TEMP_OUTTIME</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_DTD_PROG_DETAIL_OUTTIME&#xd;&#xa;SELECT A.transportmode_id AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;       ,&#x27;CARD001&#x27; AS CARD_ID  --&#x5361;&#x7247;ID&#xd;&#xa;       ,&#x27;&#x63a5;&#x5355;-&#x5206;&#x914d;&#x27; AS CARD_NAME  --&#x5361;&#x7247;&#x540d;&#x79f0;&#xd;&#xa;       ,A.proj_name AS  PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;       ,COUNT&#x28;CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;A.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;A.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; B.stand_value &#xd;&#xa;                    THEN A.VIN ELSE NULL END&#x29; AS OVERTIME_BILL_CNT   --&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL A&#xd;&#xa;LEFT JOIN TEMP.TEMP_DMR_DTD_PROG_DETAIL_STAND B&#xd;&#xa;ON A.transportmode_id &#x3d; B.tpmode_code AND A.proj_name &#x3d; B.proj_name AND B.catd_name &#x3d; &#x27;&#x63a5;&#x5355;-&#x5206;&#x914d;&#x27;&#xd;&#xa;WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;A.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY A.transportmode_id,A.proj_name&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT A.transportmode_id AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;       ,&#x27;CARD002&#x27; AS CARD_ID  --&#x5361;&#x7247;ID&#xd;&#xa;       ,&#x27;&#x5206;&#x914d;-&#x51fa;&#x5e93;&#x27; AS CARD_NAME  --&#x5361;&#x7247;&#x540d;&#x79f0;&#xd;&#xa;       ,A.proj_name AS  PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;       ,COUNT&#x28;CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;A.OUT_STORE_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;A.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; B.stand_value &#xd;&#xa;                    THEN A.VIN ELSE NULL END&#x29; AS OVERTIME_BILL_CNT   --&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL A&#xd;&#xa;LEFT JOIN TEMP.TEMP_DMR_DTD_PROG_DETAIL_STAND B&#xd;&#xa;ON A.transportmode_id &#x3d; B.tpmode_code AND A.proj_name &#x3d; B.proj_name AND B.catd_name &#x3d; &#x27;&#x5206;&#x914d;-&#x51fa;&#x5e93;&#x27;&#xd;&#xa;WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;A.OUT_STORE_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY A.transportmode_id,A.proj_name&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT A.transportmode_id AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;       ,&#x27;CARD003&#x27; AS CARD_ID  --&#x5361;&#x7247;ID&#xd;&#xa;       ,&#x27;&#x63a5;&#x8f66;-&#x6253;&#x5355;&#x27; AS CARD_NAME  --&#x5361;&#x7247;&#x540d;&#x79f0;&#xd;&#xa;       ,A.proj_name AS  PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;       ,COUNT&#x28;CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;A.FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;A.OUT_STORE_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; B.stand_value &#xd;&#xa;                    THEN A.VIN ELSE NULL END&#x29; AS OVERTIME_BILL_CNT   --&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL A&#xd;&#xa;LEFT JOIN TEMP.TEMP_DMR_DTD_PROG_DETAIL_STAND B&#xd;&#xa;ON A.transportmode_id &#x3d; B.tpmode_code AND A.proj_name &#x3d; B.proj_name AND B.catd_name &#x3d; &#x27;&#x63a5;&#x8f66;-&#x6253;&#x5355;&#x27;&#xd;&#xa;WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;A.FACTORY_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY A.transportmode_id,A.proj_name&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT transportmode_id AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;       ,&#x27;CARD003&#x27; AS CARD_ID  --&#x5361;&#x7247;ID&#xd;&#xa;       ,&#x27;&#x53d1;&#x8fd0;-&#x4ea4;&#x4ed8;&#x27; AS CARD_NAME  --&#x5361;&#x7247;&#x540d;&#x79f0;&#xd;&#xa;       ,proj_name AS  PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;	   ,COUNT&#x28;CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; &#x28;24&#x2a;3600&#x2a;on_passage_days&#x29;&#xd;&#xa;				THEN VIN ELSE NULL END&#x29; AS OVERTIME_BILL_CNT   --&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL &#xd;&#xa;WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY transportmode_id,proj_name&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT A.transportmode_id AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;       ,&#x27;CARD005&#x27; AS CARD_ID  --&#x5361;&#x7247;ID&#xd;&#xa;       ,&#x27;&#x4ea4;&#x4ed8;-&#x56de;&#x5355;&#x27; AS CARD_NAME  --&#x5361;&#x7247;&#x540d;&#x79f0;&#xd;&#xa;       ,A.proj_name AS  PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;       ,COUNT&#x28;CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;A.RECEIPT_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;A.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; B.stand_value &#xd;&#xa;                    THEN A.VIN ELSE NULL END&#x29; AS OVERTIME_BILL_CNT   --&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL A&#xd;&#xa;LEFT JOIN TEMP.TEMP_DMR_DTD_PROG_DETAIL_STAND B&#xd;&#xa;ON A.transportmode_id &#x3d; B.tpmode_code AND A.proj_name &#x3d; B.proj_name AND B.catd_name &#x3d; &#x27;&#x4ea4;&#x4ed8;-&#x56de;&#x5355;&#x27;&#xd;&#xa;WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;A.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY A.transportmode_id,A.proj_name</sql>
