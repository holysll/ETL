#x6253;&#x5355;&#x27; AS CARD_NAME&#xd;&#xa;	,PROJ_NAME&#xd;&#xa;	,AVG&#x28;&#x28;UNIX_TIMESTAMP&#x28;FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;OUT_STORE_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600&#x29; AS T_AVG_TIME&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL&#xd;&#xa;WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;FACTORY_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY transportmode_id,PROJ_NAME&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT transportmode_id AS SEGMETN_TRANSPORTMODE&#xd;&#xa;	,&#x27;CARD004&#x27; AS CARD_ID&#xd;&#xa;	,&#x27;&#x53d1;&#x8fd0;-&#x4ea4;&#x4ed8;&#x27; AS CARD_NAME&#xd;&#xa;	,PROJ_NAME&#xd;&#xa;	,AVG&#x28;&#x28;UNIX_TIMESTAMP&#x28;ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600&#x29; AS T_AVG_TIME&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL&#xd;&#xa;WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY transportmode_id,PROJ_NAME&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT transportmode_id AS SEGMETN_TRANSPORTMODE&#xd;&#xa;	,&#x27;CARD005&#x27; AS CARD_ID&#xd;&#xa;	,&#x27;&#x4ea4;&#x4ed8;-&#x56de;&#x5355;&#x27; AS CARD_NAME&#xd;&#xa;	,PROJ_NAME&#xd;&#xa;	,AVG&#x28;&#x28;UNIX_TIMESTAMP&#x28;RECEIPT_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600&#x29; AS T_AVG_TIME&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL&#xd;&#xa;WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&