    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_DTD_PROG_DETAIL	DMR&#x5927;&#x5c4f;&#x7aef;DTD&#x9879;&#x76ee;&#x660e;&#x7ec6;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;06_DMR&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;64&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set hive.execution.engine&#x3d;mr&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_DTD_PROG_DETAIL PARTITION &#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;    ,A.segmetn_transportmode --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;    ,A.card_id --&#x5361;&#x7247;ID&#xd;&#xa;    ,A.card_name  --&#x5361;&#x7247;&#x540d;&#x79f0;&#xd;&#xa;    ,A.proj_name  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;    ,A.biz_cnt  --&#x4e1a;&#x52a1;&#x91cf;v&#xd;&#xa;    ,A.throwaway_cnt    --&#x6563;&#x5355;&#x6570;&#x91cf;v&#xd;&#xa;    ,A.total_biz_comp_cnt   --&#x6574;&#x4f53;&#x4e1a;&#x52a1;&#x5b8c;&#x6210;&#x91cf;&#xd;&#xa;    ,CASE WHEN A.CARD_ID IN &#x28;&#x27;CARD001&#x27;,&#x27;CARD002&#x27;,&#x27;CARD003&#x27;&#x29; THEN B.t_avg_time&#xd;&#xa;        WHEN A.CARD_ID IN&#x28;&#x27;CARD004&#x27;,&#x27;CARD005&#x27;&#x29; THEN B.t_avg_time&#x2f;24 END AS T_AVG_TIME  --&#x65f6;&#x95f4;&#x5468;&#x671f;&#xd;&#xa;    ,C.overtime_bill_cnt  --&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;    ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT  --&#x5206;&#x533a;&#x65f6;&#x95f4;  &#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL_BIZ A&#xd;&#xa;LEFT JOIN TEMP.TEMP_DMR_DTD_PROG_DETAIL_CYCLE B&#xd;&#xa;ON A.segmetn_transportmode &#x3d; B.segmetn_transportmode AND A.proj_name &#x3d; B.proj_name AND A.card_id &#x3d; B.card_id&#xd;&#xa;LEFT JOIN TEMP.TEMP_DMR_DTD_PROG_DETAIL_OUTTIME C&#xd;&#xa;ON A.segmetn_transportmode &#x3d; C.segmetn_transportmode AND A.proj_name &#x3d; C.proj_name AND A.card_id &#x3d; C.card_id&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;--&#x63a5;&#x5355;-&#x5206;&#x914d;&#xd;&#xa;SELECT  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID  --&#x6708;&#x4efd;ID&#xd;&#xa;       ,A.SEGMETN_TRANSPORTMODE AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;       ,&#x27;CARD001&#x27; AS CARD_ID  --&#x5361;&#x7247;ID&#xd;&#xa;       ,&#x27;&#x63a5;&#x5355;-&#x5206;&#x914d;&#x27; AS CARD_NAME  --&#x5361;&#x7247;&#x540d;&#x79f0;&#xd;&#xa;       ,COALESCE&#x28;A.PROJ_NAME,-1&#x29; AS PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;       ,SUM&#x28;1&#x29; AS BIZ_CNT  --&#x4e1a;&#x52a1;&#x91cf;v&#xd;&#xa;       ,SUM&#x28;CASE WHEN A.DETAIL_FLAG &#x3d; 1 THEN 1 ELSE 0 END&#x29; AS THROWAWAY_CNT  --&#x6563;&#x5355;&#x6570;&#x91cf;v&#xd;&#xa;       ,SUM&#x28;CASE WHEN B.ORDER_MOVEMENT_GID IS NOT NULL THEN 1 ELSE 0 END &#x29; AS TOTAL_BIZ_COMP_CNT  --&#x6574;&#x4f53;&#x4e1a;&#x52a1;&#x5b8c;&#x6210;&#x91cf;&#xd;&#xa;       ,C.T_AVG_TIME AS TIME_CYCLE  --&#x65f6;&#x95f4;&#x5468;&#x671f;&#xd;&#xa;       ,SUM&#x28;CASE WHEN B.TP_TIME IS NOT NULL AND B.TP_TIME &#x3e; C.T_AVG_TIME THEN 1 ELSE 0 END&#x29; AS OVERTIME_BILL_CNT  --&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#xd;&#xa;       ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;       ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;       ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;       ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT  --&#x5206;&#x533a;&#x65f6;&#x95f4;  &#xd;&#xa;FROM&#x28; &#xd;&#xa;    --&#x603b;&#x4e1a;&#x52a1;&#x91cf;&#x28;&#x63a5;&#x5355;&#x29;&#xd;&#xa;    SELECT  SD.PROJ_NAME AS PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;           ,SD.ORDER_RELEASE_NAME AS ORDER_RELEASE_NAME  --&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;           ,SD.ORDER_MOVEMENT_GID AS ORDER_MOVEMENT_GID  --&#x8def;&#x7531;&#x7f16;&#x53f7;&#xd;&#xa;           ,SD.TRANSPORTMODE_ID AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;           ,SD.DETAIL_FLAG AS DETAIL_FLAG  --&#x6563;&#x5355;&#x6807;&#x5fd7;&#xd;&#xa;    FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL SD&#xd;&#xa;    WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;&#x29; A&#xd;&#xa;&#xd;&#xa;LEFT JOIN &#x28;&#xd;&#xa;    --&#x4e1a;&#x52a1;&#x5b8c;&#x6210;&#x91cf;&#x28;&#x5206;&#x914d;&#x29;&#xd;&#xa;    SELECT  SD.PROJ_NAME AS PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;           ,SD.ORDER_RELEASE_NAME AS ORDER_RELEASE_NAME --&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;           ,SD.ORDER_MOVEMENT_GID AS ORDER_MOVEMENT_GID  --&#x8def;&#x7531;&#x7f16;&#x53f7;&#xd;&#xa;           ,SD.TRANSPORTMODE_ID AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;           ,&#x28;UNIX_TIMESTAMP&#x28;SD.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 AS TP_TIME  --&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;    FROM TEMP.TEMP_DMR_DTD_PROG_DETAIL SD&#xd;&#xa;    WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;&#x29; B&#xd;&#xa;ON A.PROJ_NAME &#x3d; B.PROJ_NAME  AND A.ORDER_MOVEMENT_GID &#x3d; B.ORDER_MOVEMENT_GID AND A.SEGMETN_TRANSPORTMODE &#x3d; B.SEGMETN_TRANSPORTMODE&#xd;&#xa;&#xd;&#xa;LEFT JOIN&#x28; &#xd;&#xa;--&#x65f6;&#x95f4;&#x5468;&#x671f;&#x28;&#x63a5;&#x5355;-&#x5206;&#x914d;&#x29;&#xd;&#xa;     SELECT  T1.SEGMETN_TRANSPORTMODE AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;            ,T1.PROJ_NAME AS PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;            ,AVG&#x28;T1.T_D_TP_TIME&#x29; AS T_AVG_TIME  --&#x672c;&#x671f;&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;        FROM &#x28;&#xd;&#xa;            SELECT SD.ORDER_MOVEMENT_GID AS ORDER_MOVEMENT_GID  --&#x8def;&#x7531;&#x7f16;&#x53f7;&#xd;&#xa;                   ,SD.TRANSPORTMODE_ID AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;                   ,SD.PROJ_NAME AS PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;                   ,&#x28;UNIX_TIMESTAMP&#x28;SD.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&