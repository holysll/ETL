    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_FIN_STR_COST_DTL PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID  --&#x6708;&#x4efd;ID&#xd;&#xa;    ,T.rgn_id  --&#x5927;&#x533a;ID&#xd;&#xa;    ,CASE WHEN T.rgn_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.rgn_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.rgn_name END AS RNG_NAME  --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;    ,T.base_id  --&#x57fa;&#x5730;ID&#xd;&#xa;    ,CASE WHEN T.base_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.base_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.base_name END AS BASE_NAME  --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;    ,T.brand_id  --&#x54c1;&#x724c;ID&#xd;&#xa;    ,CASE WHEN T.brand_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.brand_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.brand_name END AS BRAND_NAME  --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;    ,&#x27;STR&#x27; AS BIZ_TYPE_ID  --&#x4e1a;&#x52a1;&#x5c5e;&#x6027;ID&#xd;&#xa;    ,&#x27;&#x4ed3;&#x50a8;&#x27; AS BIZ_TYPE_NAME  --&#x4e1a;&#x52a1;&#x5c5e;&#x6027;&#x540d;&#x79f0;&#xd;&#xa;    ,T.proj_id  --&#x9879;&#x76ee;ID&#xd;&#xa;    ,CASE WHEN T.proj_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x4f53;&#x27;&#xd;&#xa;        WHEN T.proj_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T4.proj_name END AS PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;    ,sum&#x28;T.CUR_M_HUMAN_COST&#x29; AS CUR_M_HUMAN_COST  --&#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;    ,sum&#x28;T.LST_M_HUMAN_COST&#x29; AS LST_M_HUMAN_COST  --&#x4e0a;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;    ,sum&#x28;T.LST_Y_M_HUMAN_COST&#x29; AS LST_Y_M_HUMAN_COST  --&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;    ,sum&#x28;T.CUR_M_FOUR_ITEM_AMT&#x29; AS CUR_M_FOUR_ITEM_AMT  --&#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;    ,sum&#x28;T.LST_M_FOUR_ITEM_AMT&#x29; AS LST_M_FOUR_ITEM_AMT  --&#x4e0a;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;    ,sum&#x28;T.LST_Y_M_FOUR_ITEM_AMT&#x29; AS LST_Y_M_FOUR_ITEM_AMT  --&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;    ,&#x28;sum&#x28;T.CUR_M_HUMAN_COST&#x29;-sum&#x28;T.LST_M_HUMAN_COST&#x29;&#x29;&#x2f;sum&#x28;T.LST_M_HUMAN_COST&#x29; AS HUMAN_COST_M_MOM  --&#x4eba;&#x529b;&#x6210;&#x672c;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;sum&#x28;T.CUR_M_HUMAN_COST&#x29;-sum&#x28;T.LST_Y_M_HUMAN_COST&#x29;&#x29;&#x2f;sum&#x28;T.LST_Y_M_HUMAN_COST&#x29; AS HUMAN_COST_M_YOY  --&#x4eba;&#x529b;&#x6210;&#x672c;&#x540c;&#x6bd4;&#xd;&#xa;    ,&#x28;sum&#x28;T.CUR_M_FOUR_ITEM_AMT&#x29;-sum&#x28;T.LST_M_FOUR_ITEM_AMT&#x29;&#x29;&#x2f;sum&#x28;T.LST_M_FOUR_ITEM_AMT&#x29; AS FOUR_ITEM_M_MOM  --&#x56db;&#x9879;&#x8d39;&#x7528;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;sum&#x28;T.CUR_M_FOUR_ITEM_AMT&#x29;-sum&#x28;T.LST_Y_M_FOUR_ITEM_AMT&#x29;&#x29;&#x2f;sum&#x28;T.LST_Y_M_FOUR_ITEM_AMT&#x29; AS FOUR_ITEM_M_YOY  --&#x56db;&#x9879;&#x8d39;&#x7528;&#x540c;&#x6bd4;&#xd;&#xa;    ,NULL AS QAT_WRN_Y_FLAG  --&#x5f53;&#x5e74;&#x5728;&#x5e93;&#x8d28;&#x635f;&#x7387;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;A.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;A.BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;A.BRNAD_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;A.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,sum&#x28;A.CUR_M_HUMAN_COST&#x29; AS CUR_M_HUMAN_COST&#xd;&#xa;        ,sum&#x28;A.LST_M_HUMAN_COST&#x29; AS LST_M_HUMAN_COST&#xd;&#xa;        ,sum&#x28;A.LST_Y_M_HUMAN_COST&#x29; AS LST_Y_M_HUMAN_COST&#xd;&#xa;        ,sum&#x28;A.CUR_M_FOUR_ITEM_AMT&#x29; AS CUR_M_FOUR_ITEM_AMT&#xd;&#xa;        ,sum&#x28;A.LST_M_FOUR_ITEM_AMT&#x29; AS LST_M_FOUR_ITEM_AMT&#xd;&#xa;        ,sum&#x28;A.LST_Y_M_FOUR_ITEM_AMT&#x29; AS LST_Y_M_FOUR_ITEM_AMT&#xd;&#xa;    FROM&#x28;&#xd;&#xa;        SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRNAD_ID&#xd;&#xa;            ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;            ,sum&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN manpower_cost ELSE 0 END&#x29; AS CUR_M_HUMAN_COST  --&#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;            ,sum&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN manpower_cost ELSE 0 END&#x29; AS LST_M_HUMAN_COST  --&#x4e0a;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;            ,sum&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN manpower_cost ELSE 0 END&#x29; AS LST_Y_M_HUMAN_COST  --&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;            ,sum&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN four_itm_fee ELSE 0 END&#x29; AS CUR_M_FOUR_ITEM_AMT  --&#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;            ,sum&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN four_itm_fee ELSE 0 END&#x29; AS LST_M_FOUR_ITEM_AMT  --&#x4e0a;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;            ,sum&#x28;CASE WHEN part_dt &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN four_itm_fee ELSE 0 END&#x29; AS LST_Y_M_FOUR_ITEM_AMT  --&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;        FROM DW.DW_FIN_MANAGE_FEE&#xd;&#xa;        WHERE part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            OR part_dt &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            OR part_dt &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        &#x29; A&#xd;&#xa;    GROUP BY A.RGN_ID,A.BASE_ID,A.BRNAD_ID,A.PROJ_ID WITH CUBE&#xd;&#xa;    &#x29; T&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT base_id,base_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY base_id,base_name&#xd;&#xa;    &#x29; T2&#xd;&#xa;ON T.base_id &#x3d; T2.base_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.dim_brand &#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29; T3&#xd;&#xa;ON T.brand_id &#x3d; T3.brand_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT proj_id,proj_name&#xd;&#xa;    FROM DIM.dim_proj &#xd;&#xa;    GROUP BY proj_id,proj_name&#xd;&#xa;    &#x29; T4&#xd;&#xa;ON T.proj_id &#x3d; T4.proj_id&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;GROUP BY T.rgn_id,T1.rgn_name,T.base_id,T2.base_name,T.brand_id,T3.brand_name,T.proj_id,T4.proj_name&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT T.PERIOD_WID AS MONTH_ID&#xd;&#xa;      ,T.RNG_ID&#xd;&#xa;      ,T.RNG_NAME&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,T.BASE_NAME&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,T.BRAND_NAME&#xd;&#xa;      ,T.BIZ_TYPE_ID&#xd;&#xa;      ,T.BIZ_TYPE_NAME&#xd;&#xa;      ,T.PROJ_ID&#xd;&#xa;      ,T.PROJ_NAME&#xd;&#xa;      ,T.CUR_M_HUMAN_COST --  &#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;      ,LST_M.LST_M_HUMAN_COST --  &#x4e0a;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;      ,LST_Y.LST_Y_M_HUMAN_COST --  &#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;      ,T.CUR_M_FOUR_ITEM_AMT --  &#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;      ,LST_M.LST_M_FOUR_ITEM_AMT --  &#x4e0a;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;      ,LST_Y.LST_Y_M_FOUR_ITEM_AMT --  &#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;LST_M.LST_M_HUMAN_COST, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;T.CUR_M_HUMAN_COST - LST_M.LST_M_HUMAN_COST&#x29; &#x2f;&#xd;&#xa;          LST_M.LST_M_HUMAN_COST&#xd;&#xa;       END AS HUMAN_COST_M_MOM --  &#x4eba;&#x529b;&#x6210;&#x672c;&#x73af;&#x6bd4;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;LST_Y.LST_Y_M_HUMAN_COST, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;T.CUR_M_HUMAN_COST - LST_Y.LST_Y_M_HUMAN_COST&#x29; &#x2f;&#xd;&#xa;          LST_Y.LST_Y_M_HUMAN_COST&#xd;&#xa;       END AS HUMAN_COST_M_YOY --  &#x4eba;&#x529b;&#x6210;&#x672c;&#x540c;&#x6bd4;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;LST_M.LST_M_FOUR_ITEM_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;T.CUR_M_FOUR_ITEM_AMT - LST_M.LST_M_FOUR_ITEM_AMT&#x29; &#x2f;&#xd;&#xa;          LST_M.LST_M_FOUR_ITEM_AMT&#xd;&#xa;       END AS FOUR_ITEM_M_MOM --  &#x56db;&#x9879;&#x8d39;&#x7528;&#x73af;&#x6bd4;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;LST_Y.LST_Y_M_FOUR_ITEM_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;T.CUR_M_HUMAN_COST - LST_Y.LST_Y_M_FOUR_ITEM_AMT&#x29; &#x2f;&#xd;&#xa;          LST_Y.LST_Y_M_FOUR_ITEM_AMT&#xd;&#xa;       END AS FOUR_ITEM_M_YOY --  &#x56db;&#x9879;&#x8d39;&#x7528;&#x540c;&#x6bd4;&#xd;&#xa;      ,NULL AS QAT_WRN_Y_FLAG --  &#x5f53;&#x5e74;&#x5728;&#x5e93;&#x8d28;&#x635f;&#x7387;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4; &#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;      ,T.PERIOD_WID AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,T.RGN_ID AS RNG_ID --  &#x5927;&#x533a;ID&#xd;&#xa;              ,T.RGN_NAME AS RNG_NAME --  &#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;              ,T.BASE_ID AS BASE_ID --  &#x57fa;&#x5730;ID&#xd;&#xa;              ,T.BASE_NAME AS BASE_NAME --  &#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;              ,T.BRAND_ID AS BRAND_ID --  &#x54c1;&#x724c;ID&#xd;&#xa;              ,T.BRAND_NAME AS BRAND_NAME --  &#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;              ,&#x27;STR&#x27; AS BIZ_TYPE_ID --  &#x4e1a;&#x52a1;&#x5c5e;&#x6027;ID&#xd;&#xa;              ,&#x27;&#x4ed3;&#x50a8;&#x27; AS BIZ_TYPE_NAME --  &#x4e1a;&#x52a1;&#x5c5e;&#x6027;&#x540d;&#x79f0;&#xd;&#xa;              ,T.PROJ_ID AS PROJ_ID --  &#x9879;&#x76ee;ID&#xd;&#xa;              ,T.PROJ_NAME AS PROJ_NAME --  &#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;              ,SUM&#x28;T.MANPOWER_COST&#x29; AS CUR_M_HUMAN_COST --  &#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;              ,NULL AS LST_M_HUMAN_COST --  &#x4e0a;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;              ,NULL AS LST_Y_M_HUMAN_COST --  &#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;              ,SUM&#x28;T.FOUR_ITM_FEE&#x29; AS CUR_M_FOUR_ITEM_AMT --  &#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;              ,NULL AS LST_M_FOUR_ITEM_AMT --  &#x4e0a;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;              ,NULL AS LST_Y_M_FOUR_ITEM_AMT --  &#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;              ,NULL AS HUMAN_COST_M_MOM --  &#x4eba;&#x529b;&#x6210;&#x672c;&#x73af;&#x6bd4;&#xd;&#xa;              ,NULL AS HUMAN_COST_M_YOY --  &#x4eba;&#x529b;&#x6210;&#x672c;&#x540c;&#x6bd4;&#xd;&#xa;              ,NULL AS FOUR_ITEM_M_MOM --  &#x56db;&#x9879;&#x8d39;&#x7528;&#x73af;&#x6bd4;&#xd;&#xa;              ,NULL AS FOUR_ITEM_M_YOY --  &#x56db;&#x9879;&#x8d39;&#x7528;&#x540c;&#x6bd4;&#xd;&#xa;              ,NULL AS QAT_WRN_Y_FLAG --  &#x5f53;&#x5e74;&#x5728;&#x5e93;&#x8d28;&#x635f;&#x7387;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;          FROM DW.DW_FIN_MANAGE_FEE T&#xd;&#xa;         WHERE T.PERIOD_WID &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;         GROUP BY T.RGN_ID&#xd;&#xa;                 ,T.RGN_NAME&#xd;&#xa;                 ,T.BASE_ID&#xd;&#xa;                 ,T.BASE_NAME&#xd;&#xa;                 ,T.BRAND_ID&#xd;&#xa;                 ,T.BRAND_NAME&#xd;&#xa;                 ,T.PROJ_ID&#xd;&#xa;                 ,T.PROJ_NAME&#x29; AS T&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID --  &#x6708;&#x4efd;ID&#xd;&#xa;                   ,T.RGN_ID AS RNG_ID --  &#x5927;&#x533a;ID&#xd;&#xa;                   ,T.RGN_NAME AS RNG_NAME --  &#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                   ,T.BASE_ID AS BASE_ID --  &#x57fa;&#x5730;ID&#xd;&#xa;                   ,T.BASE_NAME AS BASE_NAME --  &#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;                   ,T.BRAND_ID AS BRAND_ID --  &#x54c1;&#x724c;ID&#xd;&#xa;                   ,T.BRAND_NAME AS BRAND_NAME --  &#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;                   ,&#x27;STR&#x27; AS BIZ_TYPE_ID --  &#x4e1a;&#x52a1;&#x5c5e;&#x6027;ID&#xd;&#xa;                   ,&#x27;&#x4ed3;&#x50a8;&#x27; AS BIZ_TYPE_NAME --  &#x4e1a;&#x52a1;&#x5c5e;&#x6027;&#x540d;&#x79f0;&#xd;&#xa;                   ,T.PROJ_ID AS PROJ_ID --  &#x9879;&#x76ee;ID&#xd;&#xa;                   ,T.PROJ_NAME AS PROJ_NAME --  &#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;                   ,NULL AS CUR_M_HUMAN_COST --  &#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;                   ,SUM&#x28;T.MANPOWER_COST&#x29; AS LST_M_HUMAN_COST --  &#x4e0a;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;                   ,NULL AS LST_Y_M_HUMAN_COST --  &#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;                   ,NULL AS CUR_M_FOUR_ITEM_AMT --  &#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;                   ,SUM&#x28;T.FOUR_ITM_FEE&#x29; AS LST_M_FOUR_ITEM_AMT --  &#x4e0a;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;                   ,NULL AS LST_Y_M_FOUR_ITEM_AMT --  &#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;               FROM DW.DW_FIN_MANAGE_FEE T&#xd;&#xa;              WHERE T.PERIOD_WID &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T.RGN_ID&#xd;&#xa;                      ,T.RGN_NAME&#xd;&#xa;                      ,T.BASE_ID&#xd;&#xa;                      ,T.BASE_NAME&#xd;&#xa;                      ,T.BRAND_ID&#xd;&#xa;                      ,T.BRAND_NAME&#xd;&#xa;                      ,T.PROJ_ID&#xd;&#xa;                      ,T.PROJ_NAME&#x29; AS LST_M&#xd;&#xa;    ON T.PERIOD_WID &#x3d; LST_M.PERIOD_WID&#xd;&#xa;   AND T.RNG_ID &#x3d; LST_M.RNG_ID&#xd;&#xa;   AND T.RNG_NAME &#x3d; LST_M.RNG_NAME&#xd;&#xa;   AND T.BASE_ID &#x3d; LST_M.BASE_ID&#xd;&#xa;   AND T.BASE_NAME &#x3d; LST_M.BASE_NAME&#xd;&#xa;   AND T.BRAND_ID &#x3d; LST_M.BRAND_ID&#xd;&#xa;   AND T.BRAND_NAME &#x3d; LST_M.BRAND_NAME&#xd;&#xa;   AND T.PROJ_ID &#x3d; LST_M.PROJ_ID&#xd;&#xa;   AND T.PROJ_NAME &#x3d; LST_M.PROJ_NAME&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID --  &#x6708;&#x4efd;ID&#xd;&#xa;                   ,T.RGN_ID AS RNG_ID --  &#x5927;&#x533a;ID&#xd;&#xa;                   ,T.RGN_NAME AS RNG_NAME --  &#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                   ,T.BASE_ID AS BASE_ID --  &#x57fa;&#x5730;ID&#xd;&#xa;                   ,T.BASE_NAME AS BASE_NAME --  &#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;                   ,T.BRAND_ID AS BRAND_ID --  &#x54c1;&#x724c;ID&#xd;&#xa;                   ,T.BRAND_NAME AS BRAND_NAME --  &#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;                   ,&#x27;STR&#x27; AS BIZ_TYPE_ID --  &#x4e1a;&#x52a1;&#x5c5e;&#x6027;ID&#xd;&#xa;                   ,&#x27;&#x4ed3;&#x50a8;&#x27; AS BIZ_TYPE_NAME --  &#x4e1a;&#x52a1;&#x5c5e;&#x6027;&#x540d;&#x79f0;&#xd;&#xa;                   ,T.PROJ_ID AS PROJ_ID --  &#x9879;&#x76ee;ID&#xd;&#xa;                   ,T.PROJ_NAME AS PROJ_NAME --  &#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;                   ,NULL AS CUR_M_HUMAN_COST --  &#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;                   ,NULL AS LST_M_HUMAN_COST --  &#x4e0a;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;                   ,SUM&#x28;T.MANPOWER_COST&#x29; AS LST_Y_M_HUMAN_COST --  &#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;                   ,NULL AS CUR_M_FOUR_ITEM_AMT --  &#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;                   ,NULL AS LST_M_FOUR_ITEM_AMT --  &#x4e0a;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;                   ,SUM&#x28;T.FOUR_ITM_FEE&#x29; AS LST_Y_M_FOUR_ITEM_AMT --  &#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;               FROM DW.DW_FIN_MANAGE_FEE T&#xd;&#xa;              WHERE T.PERIOD_WID &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T.RGN_ID&#xd;&#xa;                      ,T.RGN_NAME&#xd;&#xa;                      ,T.BASE_ID&#xd;&#xa;                      ,T.BASE_NAME&#xd;&#xa;                      ,T.BRAND_ID&#xd;&#xa;                      ,T.BRAND_NAME&#xd;&#xa;                      ,T.PROJ_ID&#xd;&#xa;                      ,T.PROJ_NAME&#x29; AS LST_Y&#xd;&#xa;    ON T.PERIOD_WID &#x3d; LST_Y.PERIOD_WID&#xd;&#xa;   AND T.RNG_ID &#x3d; LST_Y.RNG_ID&#xd;&#xa;   AND T.RNG_NAME &#x3d; LST_Y.RNG_NAME&#xd;&#xa;   AND T.BASE_ID &#x3d; LST_Y.BASE_ID&#xd;&#xa;   AND T.BASE_NAME &#x3d; LST_Y.BASE_NAME&#xd;&#xa;   AND T.BRAND_ID &#x3d; LST_Y.BRAND_ID&#xd;&#xa;   AND T.BRAND_NAME &#x3d; LST_Y.BRAND_NAME&#xd;&#xa;   AND T.PROJ_ID &#x3d; LST_Y.PROJ_ID&#xd;&#xa;   AND T.PROJ_NAME &#x3d; LST_Y.PROJ_NAME  &#x3b;&#xd;&#xa;&#x2a;&#x2f;</sql>
