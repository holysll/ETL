    <entry>
      <name>TO_TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_OVR_M_1&#xd;&#xa;SELECT rgn_id&#xd;&#xa;    ,base_id&#xd;&#xa;    ,brand_id&#xd;&#xa;    ,supplier_id&#xd;&#xa;    ,transportmode_id AS TPMODE_ID&#xd;&#xa;    ,order_release_gid&#xd;&#xa;    ,vin&#xd;&#xa;    ,factory_day&#xd;&#xa;    ,arrival_day&#xd;&#xa;    ,on_passage_days&#xd;&#xa;    ,&#x27;Y&#x27; AS IS_ARRIVE&#xd;&#xa;    ,CASE WHEN unix_timestamp&#x28;NVL&#x28;dlvr_day,arrival_day&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;on_passage_days,0&#x29;&#xd;&#xa;            THEN &#x27;Y&#x27; &#xd;&#xa;            ELSE &#x27;N&#x27; END AS IS_OUTTIME&#xd;&#xa;    ,CASE WHEN unix_timestamp&#x28;NVL&#x28;dlvr_day,arrival_day&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;on_passage_days,0&#x29;&#xd;&#xa;            THEN unix_timestamp&#x28;NVL&#x28;dlvr_day,arrival_day&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-24&#x2a;3600&#x2a;NVL&#x28;on_passage_days,0&#x29;&#xd;&#xa;            ELSE 0 END AS OUTTIME_TIME&#xd;&#xa;FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;WHERE substr&#x28;regexp_replace&#x28;NVL&#x28;dlvr_day,arrival_day&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;	AND substr&#x28;regexp_replace&#x28;NVL&#x28;dlvr_day,arrival_day&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT rgn_id&#xd;&#xa;    ,base_id&#xd;&#xa;    ,brand_id&#xd;&#xa;    ,supplier_id&#xd;&#xa;    ,transportmode_id AS TPMODE_ID&#xd;&#xa;    ,order_release_gid&#xd;&#xa;    ,vin&#xd;&#xa;    ,factory_day&#xd;&#xa;    ,arrival_day&#xd;&#xa;    ,on_passage_days&#xd;&#xa;    ,&#x27;N&#x27; AS IS_ARRIVE&#xd;&#xa;    ,CASE WHEN unix_timestamp&#x28;concat&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;                                                        THEN &#xd;&#xa;                                                        from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#xd;&#xa;                                                        ELSE &#xd;&#xa;                                                        last_day&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                                                        END,&#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;on_passage_days,0&#x29;&#xd;&#xa;            THEN &#xd;&#xa;            &#x27;Y&#x27; &#xd;&#xa;            ELSE &#xd;&#xa;            &#x27;N&#x27; END AS IS_OUTTIME&#xd;&#xa;    ,CASE WHEN unix_timestamp&#x28;concat&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;                                                        THEN &#xd;&#xa;                                                        from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#xd;&#xa;                                                        ELSE &#xd;&#xa;                                                        last_day&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                                                        END,&#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;on_passage_days,0&#x29;&#xd;&#xa;            THEN &#xd;&#xa;            unix_timestamp&#x28;concat&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;                                                        THEN &#xd;&#xa;                                                        from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#xd;&#xa;                                                        ELSE &#xd;&#xa;                                                        last_day&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                                                        END,&#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-24&#x2a;3600&#x2a;NVL&#x28;on_passage_days,0&#x29;&#xd;&#xa;            ELSE &#xd;&#xa;            0 END AS OUTTIME_TIME&#xd;&#xa;FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;WHERE substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; &#xd;&#xa;    CASE WHEN substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;    THEN &#xd;&#xa;    from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    ELSE &#xd;&#xa;    substr&#x28;regexp_replace&#x28;last_day&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;    END&#xd;&#xa;    AND NVL&#x28;substr&#x28;regexp_replace&#x28;NVL&#x28;dlvr_day,arrival_day&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;,&#x27;29991231&#x27;&#x29; &#x3e; &#xd;&#xa;    CASE WHEN substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;    THEN &#xd;&#xa;    from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    ELSE &#xd;&#xa;    substr&#x28;regexp_replace&#x28;last_day&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;    END&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT B.part_dt&#xd;&#xa;    ,A.rgn_id&#xd;&#xa;    ,A.base_id&#xd;&#xa;    ,A.brand_id&#xd;&#xa;    ,A.supplier_id&#xd;&#xa;    ,A.transportmode_id AS TPMODE_ID&#xd;&#xa;    ,A.order_release_gid&#xd;&#xa;    ,A.vin&#xd;&#xa;    ,A.factory_day&#xd;&#xa;    ,A.arrival_day&#xd;&#xa;    ,A.on_passage_days&#xd;&#xa;    ,CASE WHEN A.arrival_day IS NOT NULL THEN &#x27;Y&#x27; ELSE &#x27;N&#x27; END AS IS_ARRIVE&#xd;&#xa;    ,CASE WHEN unix_timestamp&#x28;NVL&#x28;A.arrival_day,&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - unix_timestamp&#x28;A.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;A.on_passage_days &#xd;&#xa;            THEN &#x27;Y&#x27; &#xd;&#xa;            ELSE &#x27;N&#x27; END AS IS_OUTTIME&#xd;&#xa;    ,CASE WHEN unix_timestamp&#x28;NVL&#x28;A.arrival_day,&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - unix_timestamp&#x28;A.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;A.on_passage_days&#xd;&#xa;            THEN unix_timestamp&#x28;NVL&#x28;A.arrival_day,&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - unix_timestamp&#x28;A.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-24&#x2a;3600&#x2a;A.on_passage_days&#xd;&#xa;            ELSE 0 END AS OUTTIME_TIME&#xd;&#xa;FROM DMK.DMK_TP_DTD_PROG_TIME_D A&#xd;&#xa;INNER JOIN TEMP.TEMP_DMR_PC_TP_ORD_OVR_M_PART B&#xd;&#xa;ON substr&#x28;regexp_replace&#x28;date_add&#x28;to_date&#x28;A.factory_day&#x29;,cast&#x28;A.on_passage_days AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; B.part_dt&#xd;&#xa;WHERE A.on_passage_days IS NOT NULL&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;SELECT A.period_wid&#xd;&#xa;    ,COALESCE&#x28;B.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,COALESCE&#x28;B.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,COALESCE&#x28;B.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,COALESCE&#x28;B.supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;    ,B.segmetn_transportmode AS TPMODE_ID&#xd;&#xa;    ,B.order_release_gid&#xd;&#xa;    ,CASE WHEN FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;B.arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; &#x3c;&#x3d; A.period_wid THEN &#x27;Y&#x27; ELSE &#x27;N&#x27; END AS IS_ARRIVE&#xd;&#xa;    ,CASE WHEN &#x28;B.arrival_day IS NOT NULL AND UNIX_TIMESTAMP&#x28;B.arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x3e; 3600&#x2a;24&#x2a;B.on_passage_days&#x29; &#xd;&#xa;        OR &#x28;B.arrival_day IS NULL AND UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#x3c; unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29; THEN &#x27;Y&#x27; ELSE &#x27;N&#x27; END AS IS_OUTTIME&#xd;&#xa;    ,CASE WHEN &#x28;B.arrival_day IS NOT NULL AND UNIX_TIMESTAMP&#x28;B.arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x3e; 3600&#x2a;24&#x2a;B.on_passage_days&#x29;&#xd;&#xa;            THEN UNIX_TIMESTAMP&#x28;B.arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-3600&#x2a;24&#x2a;B.on_passage_days&#xd;&#xa;          WHEN &#x28;B.arrival_day IS NULL AND UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#x3c; unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;            THEN unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29; - UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#xd;&#xa;          ELSE 0 END AS OUTTIME_TIME&#xd;&#xa;FROM DIM.DIM_PERIOD_D A  &#xd;&#xa;LEFT JOIN DW.DW_TP_ORDERMOVEMENT B&#xd;&#xa;ON 1&#x3d;1&#xd;&#xa;LEFT JOIN DW.DW_TP_ORDERRELEASE C&#xd;&#xa;ON 1&#x3d;1&#xd;&#xa;WHERE 1&#x3d;1 &#xd;&#xa;    AND substr&#x28;regexp_replace&#x28;A.period_wid,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;    AND A.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;    AND C.or_plan_type &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27; &#xd;&#xa;    AND B.brand_id &#x3d; &#x27;CA&#x27;&#xd;&#xa;    AND A.period_wid &#x3e;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;B.out_store_day, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    AND A.period_wid &#x3c; NVL&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;B.arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;,&#x27;29991231&#x27;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT A.period_wid&#xd;&#xa;    ,COALESCE&#x28;B.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,COALESCE&#x28;B.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,COALESCE&#x28;B.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,COALESCE&#x28;C.supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;    ,C.segmetn_transportmode AS TPMODE_ID&#xd;&#xa;    ,B.order_release_gid&#xd;&#xa;    ,CASE WHEN FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;B.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; &#x3c;&#x3d; A.period_wid THEN &#x27;Y&#x27; ELSE &#x27;N&#x27; END AS IS_ARRIVE&#xd;&#xa;    ,CASE WHEN &#x28;B.dlvr_day IS NOT NULL AND UNIX_TIMESTAMP&#x28;B.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x3e; 3600&#x2a;24&#x2a;B.on_passage_days&#x29; &#xd;&#xa;        OR &#x28;B.dlvr_day IS NULL AND UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#x3c; unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29; THEN &#x27;Y&#x27; ELSE &#x27;N&#x27; END AS IS_OUTTIME&#xd;&#xa;    ,CASE WHEN &#x28;B.dlvr_day IS NOT NULL AND UNIX_TIMESTAMP&#x28;B.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x3e; 3600&#x2a;24&#x2a;B.on_passage_days&#x29;&#xd;&#xa;            THEN UNIX_TIMESTAMP&#x28;B.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-3600&#x2a;24&#x2a;B.on_passage_days&#xd;&#xa;          WHEN &#x28;B.dlvr_day IS NULL AND UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#x3c; unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;            THEN unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29; - UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#xd;&#xa;          ELSE 0 END AS OUTTIME_TIME&#xd;&#xa;FROM DIM.DIM_PERIOD_D A  &#xd;&#xa;JOIN DW.DW_TP_ORDERRELEASE B&#xd;&#xa;ON 1&#x3d;1&#xd;&#xa;JOIN DW.DW_TP_ORDERMOVEMENT C&#xd;&#xa;ON 1&#x3d;1&#xd;&#xa;WHERE 1&#x3d;1 &#xd;&#xa;    AND substr&#x28;regexp_replace&#x28;A.period_wid,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;    AND A.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;    AND B.or_plan_type &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27; &#xd;&#xa;    AND B.brand_id &#x3c;&#x3e; &#x27;CA&#x27;&#xd;&#xa;    AND A.period_wid &#x3e;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;B.factory_day, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    AND A.period_wid &#x3c; NVL&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;B.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;,&#x27;29991231&#x27;&#x29;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;</sql>
