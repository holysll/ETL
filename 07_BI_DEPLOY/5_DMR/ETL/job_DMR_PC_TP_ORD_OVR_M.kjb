      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMR&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_ORD_OVR_M PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;    ,M.RGN_ID&#xd;&#xa;    ,coalesce&#x28;M1.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;    ,M.BASE_ID&#xd;&#xa;    ,coalesce&#x28;M2.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,M.BRAND_ID&#xd;&#xa;    ,coalesce&#x28;M3.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME&#xd;&#xa;    ,M.SUPPLIER_ID&#xd;&#xa;    ,coalesce&#x28;M4.servprov_name,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS SUPPLIER_NAME&#xd;&#xa;    ,M.TPMODE_ID&#xd;&#xa;    ,coalesce&#x28;M5.TPMODE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS TPMODE_NAME&#xd;&#xa;    ,M.OVRTM_TYPE_ID&#xd;&#xa;    ,M.OVRTM_TYPE_NAME&#xd;&#xa;    ,M.OVRTM_ORD_QTY&#xd;&#xa;    ,M.ORD_QTY&#xd;&#xa;    ,M.AGE_ONE_QTY&#xd;&#xa;    ,M.AGE_TWO_QTY&#xd;&#xa;    ,M.AGE_THREE_QTY&#xd;&#xa;    ,M.AGE_FOUR_QTY&#xd;&#xa;    ,M.AGE_FIVE_QTY&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;T.RGN_ID,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;T.TPMODE_ID,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,&#x27;OT_TYPE_01&#x27; AS OVRTM_TYPE_ID&#xd;&#xa;        ,&#x27;&#x672a;&#x5230;&#x8fbe;&#x27; AS OVRTM_TYPE_NAME&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; THEN T.order_release_gid END&#x29; AS OVRTM_ORD_QTY&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; THEN T.order_release_gid END&#x29; AS ORD_QTY&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND T.OUTTIME_TIME &#x3e; 0 AND T.OUTTIME_TIME &#x3c;&#x3d; 3600&#x2a;24&#x2a;3 THEN T.order_release_gid END&#x29; AS AGE_ONE_QTY&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND T.OUTTIME_TIME &#x3e; 3600&#x2a;24&#x2a;3 AND T.OUTTIME_TIME &#x3c;&#x3d; 3600&#x2a;24&#x2a;5 THEN T.order_release_gid END&#x29; AS AGE_TWO_QTY&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND T.OUTTIME_TIME &#x3e; 3600&#x2a;24&#x2a;5 AND T.OUTTIME_TIME &#x3c;&#x3d; 3600&#x2a;24&#x2a;10 THEN T.order_release_gid END&#x29; AS AGE_THREE_QTY    &#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND T.OUTTIME_TIME &#x3e; 3600&#x2a;24&#x2a;10 AND T.OUTTIME_TIME &#x3c;&#x3d; 3600&#x2a;24&#x2a;15 THEN T.order_release_gid END&#x29; AS AGE_FOUR_QTY&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND T.OUTTIME_TIME &#x3e; 3600&#x2a;24&#x2a;15 THEN T.order_release_gid END&#x29; AS AGE_FIVE_QTY&#xd;&#xa;    FROM TEMP.TEMP_DMR_PC_TP_ORD_OVR_M_1 T&#xd;&#xa;    GROUP BY coalesce&#x28;T.RGN_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.TPMODE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;T.RGN_ID,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;T.TPMODE_ID,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,&#x27;OT_TYPE_02&#x27; AS OVRTM_TYPE_ID&#xd;&#xa;        ,&#x27;&#x5df2;&#x5230;&#x8fbe;&#x27; AS OVRTM_TYPE_NAME&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; THEN T.order_release_gid END&#x29; AS OVRTM_ORD_QTY&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; THEN T.order_release_gid END&#x29; AS ORD_QTY&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND T.OUTTIME_TIME &#x3e; 0 AND T.OUTTIME_TIME &#x3c;&#x3d; 3600&#x2a;24&#x2a;3 THEN T.order_release_gid END&#x29; AS AGE_ONE_QTY&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND T.OUTTIME_TIME &#x3e; 3600&#x2a;24&#x2a;3 AND T.OUTTIME_TIME &#x3c;&#x3d; 3600&#x2a;24&#x2a;5 THEN T.order_release_gid END&#x29; AS AGE_TWO_QTY&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND T.OUTTIME_TIME &#x3e; 3600&#x2a;24&#x2a;5 AND T.OUTTIME_TIME &#x3c;&#x3d; 3600&#x2a;24&#x2a;10 THEN T.order_release_gid END&#x29; AS AGE_THREE_QTY    &#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND T.OUTTIME_TIME &#x3e; 3600&#x2a;24&#x2a;10 AND T.OUTTIME_TIME &#x3c;&#x3d; 3600&#x2a;24&#x2a;15 THEN T.order_release_gid END&#x29; AS AGE_FOUR_QTY&#xd;&#xa;        ,COUNT&#x28;CASE WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND T.OUTTIME_TIME &#x3e; 3600&#x2a;24&#x2a;15 THEN T.order_release_gid END&#x29; AS AGE_FIVE_QTY&#xd;&#xa;    FROM TEMP.TEMP_DMR_PC_TP_ORD_OVR_M_1 T&#xd;&#xa;    GROUP BY coalesce&#x28;T.RGN_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.TPMODE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; M&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29;M1&#xd;&#xa;ON M.RGN_ID &#x3d; M1.RGN_ID&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT base_id,base_name&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY base_id,base_name&#xd;&#xa;    &#x29;M2&#xd;&#xa;ON M.BASE_ID &#x3d; M2.BASE_ID&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.dim_brand&#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29;M3&#xd;&#xa;ON M.BRAND_ID &#x3d; M3.BRAND_ID&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;	SELECT servprov_id,servprov_name&#xd;&#xa;	FROM dim.dim_servprov&#xd;&#xa;	GROUP BY servprov_id,servprov_name&#xd;&#xa;    &#x29;M4&#xd;&#xa;ON M.SUPPLIER_ID &#x3d; M4.servprov_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;	SELECT tpmode_id,tpmode_name &#xd;&#xa;	FROM DIM.dim_transport_mode&#xd;&#xa;	GROUP BY tpmode_id,tpmode_name &#xd;&#xa;    &#x29;M5&#xd;&#xa;ON M.TPMODE_ID &#x3d; M5.tpmode_id</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
