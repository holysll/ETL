      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_PART&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_OVR_M_PART&#xd;&#xa;SELECT&#xd;&#xa;    A.PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;date_add&#x28;to_date&#x28;factory_day&#x29;,cast&#x28;on_passage_days AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;    FROM DMK.DMK_TP_DTD_PROG_TIME_D&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND factory_day IS NOT NULL&#xd;&#xa;        AND on_passage_days IS NOT NULL&#xd;&#xa;    GROUP BY substr&#x28;regexp_replace&#x28;date_add&#x28;to_date&#x28;factory_day&#x29;,cast&#x28;on_passage_days AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    UNION ALL &#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;    &#x29; A&#xd;&#xa;GROUP BY A.PART_DT</sql>
