      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_OVR_M_1&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_OVR_M_1&#x28;&#xd;&#xa;RGN_ID	STRING	COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID STRING    COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID STRING   COMMENT&#x27;&#x54c1;&#x724c;&#x27;&#xd;&#xa;,SUPPLIER_ID STRING		COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	STRING	COMMENT&#x27;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;ID&#x27;&#xd;&#xa;,ORDER_RELEASE_GID STRING   COMMENT&#x27;OTM&#x5355;&#x53f7;&#x27;&#xd;&#xa;,VIN	STRING	COMMENT&#x27;VIN&#x53f7;&#x27;&#xd;&#xa;,FACTORY_DAY	STRING	COMMENT&#x27;&#x53d1;&#x8fd0;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,ARRIVAL_DAY	STRING	COMMENT&#x27;&#x5230;&#x8fbe;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,ON_PASSAGE_DAYS	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x6807;&#x51c6;&#x5728;&#x9014;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,IS_ARRIVE	STRING	COMMENT&#x27;&#x662f;&#x5426;&#x5230;&#x8fbe;&#x27;&#xd;&#xa;,IS_OUTTIME	STRING	COMMENT&#x27;&#x662f;&#x5426;&#x8d85;&#x65f6;&#x27;&#xd;&#xa;,OUTTIME_TIME	BIGINT	COMMENT&#x27;&#x8d85;&#x65f6;&#x65f6;&#x957f;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x660e;&#x7ec6;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_OVR_M_PART&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_OVR_M_PART&#x28;&#xd;&#xa;PART_DT		STRING	COMMENT&#x27;&#x52a8;&#x6001;&#x5206;&#x533a;&#x27;&#xd;&#xa;&#x29; COMMENT