      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_FIN_DTL_RPT_01&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_FIN_DTL_RPT_01&#x28;&#xd;&#xa;PERIOD_WID               STRING         COMMENT  &#x27;&#x671f;&#x95f4;&#x27;,&#xd;&#xa;RGN_ID                   STRING         COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID                  STRING         COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BRAND_ID                 STRING         COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;PROJ_ID                  STRING         COMMENT  &#x27;&#x9879;&#x76ee;ID&#x27;,&#xd;&#xa;SUPPLIER_ID              STRING         COMMENT  &#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;,&#xd;&#xa;DEST_PRVN_ID			 STRING         COMMENT  &#x27;&#x76ee;&#x7684;&#x7701;&#x4efd;ID&#x27;,&#xd;&#xa;DEST_CTY_ID              STRING         COMMENT  &#x27;&#x76ee;&#x7684;&#x57ce;&#x5e02;ID&#x27;,&#xd;&#xa;TPMODE_ID                STRING         COMMENT  &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;,&#xd;&#xa;MILEGE                   DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x91cc;&#x7a0b;&#x27;,&#xd;&#xa;BIZ_CNT                  DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_NUM                  DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x4e1a;&#x52a1;&#x91cf;&#x6b21;&#x27;,&#xd;&#xa;TP_INCOME_AMT            DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x8fd0;&#x8f93;&#x6536;&#x5165;&#x27;,&#xd;&#xa;TP_COST_AMT              DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x8fd0;&#x8f93;&#x6210;&#x672c;&#x27;,&#xd;&#xa;STR_BIZ_INCOME_AMT       DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x4ed3;&#x50a8;&#x6536;&#x5165;&#x27;,&#xd;&#xa;SAL_INCOME_AMT           DECIMAL&#x28;32,8&#x29;  COMMENT  