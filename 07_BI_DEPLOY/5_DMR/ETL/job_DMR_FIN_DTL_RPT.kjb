      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_FIN_DTL_RPT_02&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; AS PERIOD_WID,&#xd;&#xa;       COALESCE&#x28;A.RGN_ID,B.RGN_ID&#x29;,&#xd;&#xa;       COALESCE&#x28;A.BASE_ID,B.BASE_ID&#x29;,&#xd;&#xa;       COALESCE&#x28;A.BRAND_ID,B.BRAND_ID&#x29;,&#xd;&#xa;       COALESCE&#x28;A.PROJ_ID,B.PROJ_ID&#x29;,&#xd;&#xa;       NULL AS SUPPLIER_ID,&#xd;&#xa;       NULL AS DEST_PRVN_ID,&#xd;&#xa;       NULL AS DEST_CTY_ID,&#xd;&#xa;       NULL AS TPMODE_ID,&#xd;&#xa;       0,&#xd;&#xa;       0,&#xd;&#xa;       0,&#xd;&#xa;       0,&#xd;&#xa;       0,&#xd;&#xa;       NVL&#x28;A.STR_BIZ_INCOME_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SAL_INCOME_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OUT_BIZ_INCOME_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SAL_COST_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OUT_BIZ_COST_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SECURE_CLEAN_COST_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_COST_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_COST_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SECURE_CLEAN_INCOME_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.VCL_MANAGER_INCOME_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_INCOME_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.VCL_RENT_INCOME_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;A.HUMAN_COST_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;B.FOUR_ITM_FEE_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;B.DEPRECIATION_AMT,0&#x29;,&#xd;&#xa;       NVL&#x28;B.OTHER_FEE_AMT,0&#x29;&#xd;&#xa;  FROM &#x28;&#xd;&#xa;SELECT RGN_ID,&#xd;&#xa;       BASE_ID,&#xd;&#xa;       BRAND_ID,&#xd;&#xa;       PROJ_ID,&#xd;&#xa;       SUM&#x28;STORE_BIZ_INCOME_AMT&#x29;     AS STR_BIZ_INCOME_AMT,       --&#x4ed3;&#x50a8;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;SAL_INCOME_AMT&#x29;           AS SAL_INCOME_AMT,           --&#x9500;&#x552e;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;OUT_BIZ_INCOME_AMT&#x29;       AS OUT_BIZ_INCOME_AMT,       --&#x8425;&#x4e1a;&#x5916;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;SAL_COST_AMT&#x29;             AS SAL_COST_AMT,             --&#x9500;&#x552e;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;OUT_BIZ_COST_AMT&#x29;         AS OUT_BIZ_COST_AMT,         --&#x8425;&#x4e1a;&#x5916;&#x652f;&#x51fa;&#xd;&#xa;       SUM&#x28;SECURE_CLEAN_COST_AMT&#x29;    AS SECURE_CLEAN_COST_AMT,    --&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;OTHER_VAT_COST_AMT&#x29;       AS OTHER_VAT_COST_AMT,       --&#x5176;&#x4ed6;&#x589e;&#x503c;&#x670d;&#x52a1;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;RENT_COST_AMT&#x29;            AS RENT_COST_AMT,            --&#x79df;&#x91d1;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;SECURE_CLEAN_INCOME_AMT&#x29;  AS SECURE_CLEAN_INCOME_AMT,  --&#x6574;&#x8f66;&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;VCL_MANAGER_INCOME_AMT&#x29;   AS VCL_MANAGER_INCOME_AMT,   --&#x6574;&#x8f66;&#x7ba1;&#x7406;&#x8d39;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;OTHER_VAT_INCOME_AMT&#x29;     AS OTHER_VAT_INCOME_AMT,     --&#x6574;&#x8f66;&#x5176;&#x4ed6;&#x589e;&#x503c;&#x670d;&#x52a1;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;VCL_RENT_INCOME_AMT&#x29;      AS VCL_RENT_INCOME_AMT,      --&#x6574;&#x8f66;&#x79df;&#x91d1;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;HUMAN_COST_AMT&#x29;           AS HUMAN_COST_AMT            --&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;  FROM DMK.DMK_FIN_INCOMECOST_M&#xd;&#xa;    WHERE PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;      GROUP BY RGN_ID,&#xd;&#xa;               BASE_ID,&#xd;&#xa;               BRAND_ID,&#xd;&#xa;               PROJ_ID&#xd;&#xa;&#x29; A&#xd;&#xa;FULL JOIN &#x28;&#xd;&#xa;SELECT RGN_ID,&#xd;&#xa;       BASE_ID,&#xd;&#xa;       BRAND_ID,&#xd;&#xa;       PROJ_ID,&#xd;&#xa;       SUM&#x28;FOUR_ITM_FEE&#x29; AS FOUR_ITM_FEE_AMT,  --&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;       SUM&#x28;DEPRECIATION&#x29; AS DEPRECIATION_AMT,  --&#x6298;&#x65e7;&#xd;&#xa;       SUM&#x28;OTHER_FEE&#x29;    AS OTHER_FEE_AMT      --&#x5176;&#x4ed6;&#x8d39;&#x7528;&#xd;&#xa;  FROM DW.DW_FIN_MANAGE_FEE&#xd;&#xa;    WHERE PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;      GROUP BY RGN_ID,&#xd;&#xa;               BASE_ID,&#xd;&#xa;               BRAND_ID,&#xd;&#xa;               PROJ_ID&#xd;&#xa;&#x29; B ON A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;   AND A.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;   AND A.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;   AND A.PROJ_ID &#x3d; B.PROJ_ID&#xd;&#xa;&#x3b;</sql>
