    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_FIN_DTL_RPT PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT TMP.PERIOD_WID,&#xd;&#xa;       COALESCE&#x28;RGN.RGN_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x5927;&#x533a;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;BAS.BASE_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x57fa;&#x5730;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;BRD.BRAND_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;BRD.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x54c1;&#x724c;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;PRJ.PROJ_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;PRJ.PROJ_NAME,&#x27;&#x672a;&#x77e5;&#x9879;&#x76ee;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;SRV.SERVPROV_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;SRV.SERVPROV_NAME,&#x27;&#x672a;&#x77e5;&#x627f;&#x8fd0;&#x5546;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;TMP.DEST_PRVN_ID,&#x27;&#x672a;&#x77e5;&#x7701;&#x4efd;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;TMP.DEST_CTY_ID,&#x27;&#x672a;&#x77e5;&#x57ce;&#x5e02;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;TMP.TPMODE_ID,&#x27;&#x672a;&#x77e5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#x29;,&#xd;&#xa;       SUM&#x28;TMP.BIZ_CNT&#x29;,                 --&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;       SUM&#x28;TMP.BIZ_NUM&#x29;,                 --&#x4e1a;&#x52a1;&#x91cf;&#x6b21;&#xd;&#xa;       SUM&#x28;TMP.TP_INCOME_AMT&#x29; &#x2b; &#xd;&#xa;       SUM&#x28;TMP.SECURE_CLEAN_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.VCL_MANAGER_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.OTHER_VAT_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.VCL_RENT_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.SAL_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.OUT_BIZ_INCOME_AMT&#x29; -&#xd;&#xa;       SUM&#x28;TMP.TP_COST_AMT&#x29; -&#xd;&#xa;       SUM&#x28;TMP.HUMAN_COST_AMT&#x29; -&#xd;&#xa;       SUM&#x28;TMP.DEPRECIATION_AMT&#x29; -&#xd;&#xa;       SUM&#x28;TMP.FOUR_ITM_FEE_AMT&#x29; -&#xd;&#xa;       SUM&#x28;TMP.OTHER_FEE_AMT&#x29; -&#xd;&#xa;       SUM&#x28;TMP.SAL_COST_AMT&#x29; -&#xd;&#xa;       SUM&#x28;TMP.OUT_BIZ_COST_AMT&#x29; -&#xd;&#xa;       SUM&#x28;TMP.SECURE_CLEAN_COST_AMT&#x29; -&#xd;&#xa;       SUM&#x28;TMP.OTHER_VAT_COST_AMT&#x29; -&#xd;&#xa;       SUM&#x28;TMP.RENT_COST_AMT&#x29;,           --&#x5229;&#x6da6;&#x603b;&#x989d;&#xd;&#xa;       SUM&#x28;TMP.TP_INCOME_AMT&#x29; &#x2b; &#xd;&#xa;       SUM&#x28;TMP.SECURE_CLEAN_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.VCL_MANAGER_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.OTHER_VAT_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.VCL_RENT_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.SAL_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.OUT_BIZ_INCOME_AMT&#x29;,      --&#x6536;&#x5165;&#x603b;&#x989d;&#xd;&#xa;       SUM&#x28;TMP.TP_COST_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.HUMAN_COST_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.DEPRECIATION_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.FOUR_ITM_FEE_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.OTHER_FEE_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.SAL_COST_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.OUT_BIZ_COST_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.SECURE_CLEAN_COST_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.OTHER_VAT_COST_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.RENT_COST_AMT&#x29;,           --&#x6210;&#x672c;&#x603b;&#x989d;&#xd;&#xa;       SUM&#x28;TMP.TP_INCOME_AMT&#x29;,           --&#x8fd0;&#x8f93;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;TMP.SECURE_CLEAN_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.VCL_MANAGER_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.OTHER_VAT_INCOME_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.VCL_RENT_INCOME_AMT&#x29;,     --&#x4ed3;&#x50a8;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;TMP.SECURE_CLEAN_INCOME_AMT&#x29;, --&#x6574;&#x8f66;&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;TMP.VCL_MANAGER_INCOME_AMT&#x29;,  --&#x6574;&#x8f66;&#x7ba1;&#x7406;&#x8d39;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;TMP.OTHER_VAT_INCOME_AMT&#x29;,    --&#x6574;&#x8f66;&#x5176;&#x4ed6;&#x589e;&#x503c;&#x670d;&#x52a1;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;TMP.VCL_RENT_INCOME_AMT&#x29;,     --&#x6574;&#x8f66;&#x79df;&#x91d1;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;TMP.SAL_INCOME_AMT&#x29;,          --&#x9500;&#x552e;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;TMP.TP_COST_AMT&#x29;,             --&#x8fd0;&#x8f93;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;TMP.SECURE_CLEAN_COST_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.FOUR_ITM_FEE_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.DEPRECIATION_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.RENT_COST_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.HUMAN_COST_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.OTHER_FEE_AMT&#x29;,           --&#x4ed3;&#x50a8;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;TMP.SECURE_CLEAN_COST_AMT&#x29;,   --&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;TMP.OTHER_VAT_COST_AMT&#x29;,      --&#x5176;&#x4ed6;&#x589e;&#x503c;&#x670d;&#x52a1;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;TMP.RENT_COST_AMT&#x29;,           --&#x79df;&#x91d1;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;TMP.FOUR_ITM_FEE_AMT&#x29;,        --&#x56db;&#x9879;&#x8d39;&#x7528;&#xd;&#xa;       SUM&#x28;TMP.DEPRECIATION_AMT&#x29;,        --&#x6298;&#x65e7;&#xd;&#xa;       SUM&#x28;TMP.HUMAN_COST_AMT&#x29;,          --&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;TMP.OTHER_FEE_AMT&#x29;,           --&#x5176;&#x4ed6;&#x8d39;&#x7528;&#xd;&#xa;       SUM&#x28;TMP.HUMAN_COST_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.DEPRECIATION_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.FOUR_ITM_FEE_AMT&#x29; &#x2b;&#xd;&#xa;       SUM&#x28;TMP.OTHER_FEE_AMT&#x29;,           --&#x671f;&#x95f4;&#x8d39;&#x7528;&#xd;&#xa;       SUM&#x28;TMP.SAL_COST_AMT&#x29;,            --&#x9500;&#x552e;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;TMP.OUT_BIZ_INCOME_AMT&#x29;,      --&#x8425;&#x4e1a;&#x5916;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;TMP.OUT_BIZ_COST_AMT&#x29;,        --&#x8425;&#x4e1a;&#x5916;&#x652f;&#x51fa;&#xd;&#xa;       SUM&#x28;TMP.MILEGE&#x29;,                  --&#x603b;&#x91cc;&#x7a0b;&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;&#x29;  AS INSERT_TIME,&#xd;&#xa;       &#x27;DW&#x27;                             AS PART_SC,&#xd;&#xa;       TMP.PERIOD_WID                   AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_FIN_DTL_RPT_01&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_FIN_DTL_RPT_02&#xd;&#xa;       &#x29; TMP&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;		SELECT RGN_ID,RGN_NAME&#xd;&#xa;		FROM DIM.DIM_ORG&#xd;&#xa;		WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;		GROUP BY RGN_ID,RGN_NAME&#xd;&#xa;		&#x29; RGN&#xd;&#xa;           ON TMP.RGN_ID &#x3d; RGN.RGN_ID&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;		SELECT BASE_ID,BASE_NAME&#xd;&#xa;		FROM DIM.DIM_ORG&#xd;&#xa;		WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;		GROUP BY BASE_ID,BASE_NAME&#xd;&#xa;		&#x29; BAS&#xd;&#xa;           ON TMP.BASE_ID &#x3d; BAS.BASE_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;           ON TMP.BRAND_ID &#x3d; BRD.BRAND_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_PROJ PRJ&#xd;&#xa;           ON TMP.PROJ_ID &#x3d; PRJ.PROJ_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_SERVPROV SRV&#xd;&#xa;           ON TMP.SUPPLIER_ID &#x3d; SRV.SERVPROV_ID&#xd;&#xa;GROUP BY TMP.PERIOD_WID,&#xd;&#xa;       COALESCE&#x28;RGN.RGN_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x5927;&#x533a;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;BAS.BASE_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x57fa;&#x5730;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;BRD.BRAND_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;BRD.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x54c1;&#x724c;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;PRJ.PROJ_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;PRJ.PROJ_NAME,&#x27;&#x672a;&#x77e5;&#x9879;&#x76ee;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;SRV.SERVPROV_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;SRV.SERVPROV_NAME,&#x27;&#x672a;&#x77e5;&#x627f;&#x8fd0;&#x5546;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;TMP.DEST_PRVN_ID,&#x27;&#x672a;&#x77e5;&#x7701;&#x4efd;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;TMP.DEST_CTY_ID,&#x27;&#x672a;&#x77e5;&#x57ce;&#x5e02;&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;TMP.TPMODE_ID,&#x27;&#x672a;&#x77e5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#x29;&#xd;&#xa;&#x3b;</sql>
