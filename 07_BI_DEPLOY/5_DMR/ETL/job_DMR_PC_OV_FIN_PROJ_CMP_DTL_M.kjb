      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMR&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_OV_FIN_PROJ_CMP_DTL_M PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT A.month_id&#xd;&#xa;    ,&#x27;M&#x27; AS TIME_TYPE&#xd;&#xa;    ,NULL AS BIZ_TYPE_ID&#xd;&#xa;    ,NULL AS BIZ_TYPE_NAME&#xd;&#xa;    ,A.rgn_id&#xd;&#xa;    ,A.rgn_name&#xd;&#xa;    ,A.proj_id&#xd;&#xa;    ,A.proj_name&#xd;&#xa;    ,&#x27;BIZ&#x27; AS INDEX_ID&#xd;&#xa;    ,&#x27;&#x4e1a;&#x52a1;&#x91cf;&#x6b21;&#x28;&#x4e07;&#x8f86;&#x29;&#x27; AS INDEX_NAME&#xd;&#xa;    ,A.cur_m_tgt_biz_qty AS INDEX_M_PLAN    --&#x6708;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;    ,A.lst_m_tgt_biz_qty AS LST_INDEX_M_PLAN    --&#x4e0a;&#x671f;&#x6708;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;    ,A.lst_y_m_tgt_biz_qty AS LST_Y_INDEX_M_PLAN    --&#x540c;&#x671f;&#x6708;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;    ,B.cur_m_tp_biz_qty AS INDEX_M_ACT  --&#x6708;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;    ,B.lst_m_tp_biz_qty AS LST_INDEX_M_ACT  --&#x4e0a;&#x671f;&#x6708;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;    ,B.lst_y_m_tp_biz_qty AS LST_Y_INDEX_M_ACT  --&#x540c;&#x671f;&#x6708;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;    ,A.cur_y_tgt_biz_qty AS INDEX_Y_PLAN    --&#x5e74;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;    ,A.lst_y_tgt_biz_qty AS LST_Y_INDEX_Y_PLAN  --&#x540c;&#x671f;&#x5e74;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;    ,B.cur_y_tp_biz_qty AS INDEX_Y_ACT  --&#x5e74;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;    ,B.lst_y_tp_biz_qty AS LST_Y_INDEX_Y_ACT    --&#x540c;&#x671f;&#x5e74;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM TEMP.TEMP_DMR_PC_OV_FIN_PROJ_CMP_DTL_M_1 A&#xd;&#xa;LEFT JOIN TEMP.TEMP_DMR_PC_OV_FIN_PROJ_CMP_DTL_M_2 B&#xd;&#xa;ON A.month_id &#x3d; B.month_id AND A.rgn_id &#x3d; B.rgn_id AND A.proj_id &#x3d; B.proj_id&#xd;&#xa;UNION ALL &#xd;&#xa;SELECT T.month_id&#xd;&#xa;    ,T.TIME_TYPE&#xd;&#xa;    ,T.BIZ_TYPE_ID&#xd;&#xa;    ,T.BIZ_TYPE_NAME&#xd;&#xa;    ,T.rgn_id&#xd;&#xa;    ,T1.RGN_NAME&#xd;&#xa;    ,T.proj_id&#xd;&#xa;    ,T2.PROJ_NAME&#xd;&#xa;    ,T.INDEX_ID&#xd;&#xa;    ,T.INDEX_NAME&#xd;&#xa;    ,T.INDEX_M_PLAN&#xd;&#xa;    ,T.LST_INDEX_M_PLAN&#xd;&#xa;    ,T.LST_Y_INDEX_M_PLAN&#xd;&#xa;    ,T.INDEX_M_ACT&#xd;&#xa;    ,T.LST_INDEX_M_ACT&#xd;&#xa;    ,T.LST_Y_INDEX_M_ACT&#xd;&#xa;    ,T.INDEX_Y_PLAN&#xd;&#xa;    ,T.LST_Y_INDEX_Y_PLAN&#xd;&#xa;    ,T.INDEX_Y_ACT&#xd;&#xa;    ,T.LST_Y_INDEX_Y_ACT&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT C.month_id&#xd;&#xa;        ,&#x27;M&#x27; AS TIME_TYPE&#xd;&#xa;        ,NULL AS BIZ_TYPE_ID&#xd;&#xa;        ,NULL AS BIZ_TYPE_NAME&#xd;&#xa;        ,C.rgn_id&#xd;&#xa;        ,C.proj_id&#xd;&#xa;        ,&#x27;INCOME&#x27; AS INDEX_ID&#xd;&#xa;        ,&#x27;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x28;&#x4e07;&#x5143;&#x29;&#x27; AS INDEX_NAME&#xd;&#xa;        ,C.cur_m_tgt_income_amt AS INDEX_M_PLAN    --&#x6708;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;        ,C.lst_m_tgt_income_amt AS LST_INDEX_M_PLAN    --&#x4e0a;&#x671f;&#x6708;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;        ,C.lst_y_m_tgt_income_amt AS LST_Y_INDEX_M_PLAN    --&#x540c;&#x671f;&#x6708;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;        ,C.cur_m_income_amt AS INDEX_M_ACT  --&#x6708;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;        ,C.lst_m_income_amt AS LST_INDEX_M_ACT  --&#x4e0a;&#x671f;&#x6708;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;        ,C.lst_y_m_income_amt AS LST_Y_INDEX_M_ACT  --&#x540c;&#x671f;&#x6708;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;        ,C.cur_y_tgt_income_amt AS INDEX_Y_PLAN    --&#x5e74;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;        ,C.lst_y_tgt_income_amt AS LST_Y_INDEX_Y_PLAN  --&#x540c;&#x671f;&#x5e74;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;        ,C.cur_y_income_amt AS INDEX_Y_ACT  --&#x5e74;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;        ,C.lst_y_income_amt AS LST_Y_INDEX_Y_ACT    --&#x540c;&#x671f;&#x5e74;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;    FROM TEMP.TEMP_DMR_PC_OV_FIN_PROJ_CMP_DTL_M_3 C&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT C.month_id&#xd;&#xa;        ,&#x27;M&#x27; AS TIME_TYPE&#xd;&#xa;        ,NULL AS BIZ_TYPE_ID&#xd;&#xa;        ,NULL AS BIZ_TYPE_NAME&#xd;&#xa;        ,C.rgn_id&#xd;&#xa;        ,C.proj_id&#xd;&#xa;        ,&#x27;PROFIT&#x27; AS INDEX_ID&#xd;&#xa;        ,&#x27;&#x5229;&#x6da6;&#x603b;&#x989d;&#x28;&#x4e07;&#x5143;&#x29;&#x27; AS INDEX_NAME&#xd;&#xa;        ,C.cur_m_tgt_profit_amt AS INDEX_M_PLAN    --&#x6708;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;        ,C.lst_m_tgt_profit_amt AS LST_INDEX_M_PLAN    --&#x4e0a;&#x671f;&#x6708;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;        ,C.lst_y_m_tgt_profit_amt AS LST_Y_INDEX_M_PLAN    --&#x540c;&#x671f;&#x6708;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;        ,C.cur_m_profit_amt AS INDEX_M_ACT  --&#x6708;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;        ,C.lst_m_profit_amt AS LST_INDEX_M_ACT  --&#x4e0a;&#x671f;&#x6708;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;        ,C.lst_y_m_profit_amt AS LST_Y_INDEX_M_ACT  --&#x540c;&#x671f;&#x6708;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;        ,C.cur_y_tgt_profit_amt AS INDEX_Y_PLAN    --&#x5e74;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;        ,C.lst_y_tgt_profit_amt AS LST_Y_INDEX_Y_PLAN  --&#x540c;&#x671f;&#x5e74;&#x5ea6;&#x8ba1;&#x5212;&#xd;&#xa;        ,C.cur_y_profit_amt AS INDEX_Y_ACT  --&#x5e74;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;        ,C.lst_y_profit_amt AS LST_Y_INDEX_Y_ACT    --&#x540c;&#x671f;&#x5e74;&#x5ea6;&#x5b9e;&#x9645;&#xd;&#xa;    FROM TEMP.TEMP_DMR_PC_OV_FIN_PROJ_CMP_DTL_M_3 C&#xd;&#xa;    &#x29; T&#xd;&#xa;INNER JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29;T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.RGN_ID&#xd;&#xa;INNER JOIN&#x28;&#xd;&#xa;    SELECT proj_id,proj_name&#xd;&#xa;    FROM DIM.dim_proj&#xd;&#xa;    GROUP BY proj_id,proj_name&#xd;&#xa;    &#x29;T2&#xd;&#xa;ON T.PROJ_ID &#x3d; T2.PROJ_ID&#xd;&#xa;&#x3b;&#xd;&#xa;</sql>
