      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_2&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_FIN_PROJ_CMP_DTL_M_2&#xd;&#xa;--&#x5b9e;&#x9645;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;SELECT SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;	,RGN_ID&#xd;&#xa;    ,PROJ_ID&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN biz_qty ELSE 0 END&#x29; AS CUR_M_TP_BIZ_QTY&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN biz_qty ELSE 0 END&#x29; AS LST_M_TP_BIZ_QTY&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN biz_qty ELSE 0 END&#x29; AS LST_Y_M_TP_BIZ_QTY&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND substr&#x28;period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN biz_qty ELSE 0 END&#x29; AS CUR_Y_TP_BIZ_QTY&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,1,4&#x29; AND substr&#x28;period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN biz_qty ELSE 0 END&#x29; AS LST_Y_TP_BIZ_QTY&#xd;&#xa;FROM DMK.DMK_TP_PROJBIZQTY_M&#xd;&#xa;WHERE substr&#x28;period_wid,1,6&#x29; &#x3e;&#x3d; CONCAT&#x28;substr&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,1,4&#x29;,&#x27;01&#x27;&#x29;&#xd;&#xa;    AND substr&#x28;period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY rgn_id,proj_id&#xd;&#xa;&#xd;&#xa;&#x3b;</sql>
