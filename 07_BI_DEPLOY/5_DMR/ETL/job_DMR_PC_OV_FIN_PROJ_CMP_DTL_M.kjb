      <name>TO_TEMP_3</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_3&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_FIN_PROJ_CMP_DTL_M_3&#xd;&#xa;--&#x8d22;&#x52a1;&#x6536;&#x5165;&#xff0c;&#x5229;&#x6da6;&#xd;&#xa;SELECT SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;	,rgn_id&#xd;&#xa;    ,proj_id&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VCL_INCOME_AMT ELSE 0 END&#x29; AS CUR_M_INCOME_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VCL_INCOME_AMT ELSE 0 END&#x29; AS LST_M_INCOME_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VCL_INCOME_AMT ELSE 0 END&#x29; AS LST_Y_M_INCOME_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND period_wid &#x3c;&#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VCL_INCOME_AMT ELSE 0 END&#x29; AS CUR_Y_INCOME_QMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,1,4&#x29; THEN VCL_INCOME_AMT ELSE 0 END&#x29; AS LST_Y_INCOME_QMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN BIZ_INCOME_PLAN_AMT ELSE 0 END&#x29; AS CUR_M_TGT_INCOME_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN BIZ_INCOME_PLAN_AMT ELSE 0 END&#x29; AS LST_M_TGT_INCOME_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN BIZ_INCOME_PLAN_AMT ELSE 0 END&#x29; AS LST_Y_M_TGT_INCOME_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND period_wid &#x3c;&#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN BIZ_INCOME_PLAN_AMT ELSE 0 END&#x29; AS CUR_Y_TGT_INCOME_QMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,1,4&#x29;  THEN BIZ_INCOME_PLAN_AMT ELSE 0 END&#x29; AS LST_Y_TGT_INCOME_QMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN PROFIT_AMT ELSE 0 END&#x29; AS CUR_M_PROFIT_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN PROFIT_AMT ELSE 0 END&#x29; AS LST_M_PROFIT_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN PROFIT_AMT ELSE 0 END&#x29; AS LST_Y_M_PROFIT_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND period_wid &#x3c;&#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN PROFIT_AMT ELSE 0 END&#x29; AS CUR_Y_PROFIT_QMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,1,4&#x29;  THEN PROFIT_AMT ELSE 0 END&#x29; AS LST_Y_PROFIT_QMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN PROFIT_PLAN_AMT ELSE 0 END&#x29; AS CUR_M_TGT_PROFIT_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN PROFIT_PLAN_AMT ELSE 0 END&#x29; AS LST_M_TGT_PROFIT_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN PROFIT_PLAN_AMT ELSE 0 END&#x29; AS LST_Y_M_TGT_PROFIT_AMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND period_wid &#x3c;&#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN PROFIT_PLAN_AMT ELSE 0 END&#x29; AS CUR_Y_TGT_PROFIT_QMT&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,1,4&#x29;  THEN PROFIT_PLAN_AMT ELSE 0 END&#x29; AS LST_Y_TGT_PROFIT_QMT&#xd;&#xa;FROM DMK.DMK_FIN_INCOMECOST_M&#xd;&#xa;WHERE period_wid &#x3e;&#x3d; CONCAT&#x28;substr&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,1,4&#x29;,&#x27;01&#x27;&#x29;&#xd;&#xa;    AND period_wid &#x3c;&#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY rgn_id,proj_id&#xd;&#xa;&#x3b;</sql>
