    <entry>
      <name>TO_TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_FIN_PROJ_CMP_DTL_M_1&#xd;&#xa;--&#x8ba1;&#x5212;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;SELECT SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;	,A.rgn_id&#xd;&#xa;    ,A.rgn_name&#xd;&#xa;    ,NVL&#x28;B.proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,A.proj_name&#xd;&#xa;    ,SUM&#x28;CASE WHEN A.period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN ABS&#x28;A.biz_qty&#x29; ELSE 0 END&#x29; AS CUR_M_TGT_BIZ_QTY&#xd;&#xa;    ,SUM&#x28;CASE WHEN A.period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN ABS&#x28;A.biz_qty&#x29; ELSE 0 END&#x29; AS LST_M_TGT_BIZ_QTY&#xd;&#xa;    ,SUM&#x28;CASE WHEN A.period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN ABS&#x28;A.biz_qty&#x29; ELSE 0 END&#x29; AS LST_Y_M_TGT_BIZ_QTY&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;A.period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND A.period_wid &#x3c;&#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN ABS&#x28;A.biz_qty&#x29; ELSE 0 END&#x29; AS CUR_Y_TGT_BIZ_QTY&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;A.period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,1,4&#x29; AND A.period_wid &#x3c;&#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN ABS&#x28;A.biz_qty&#x29; ELSE 0 END&#x29; AS LST_Y_TGT_BIZ_QTY&#xd;&#xa;FROM DMK.DMK_TP_BIZQTYTGT_M A&#xd;&#xa;LEFT JOIN DIM.dim_proj B&#xd;&#xa;ON A.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;AND A.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;WHERE A.period_wid &#x3e;&#x3d; CONCAT&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,1,4&#x29;,&#x27;01&#x27;&#x29;&#xd;&#xa;    AND A.period_wid &#x3c;&#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    AND A.PROJ_NAME NOT IN &#x28;&#x27;&#x535a;&#x5b87;&#x516c;&#x53f8;&#xff1a;&#x4ece;&#x6574;&#x8f66;&#x5206;&#x5f97;&#x27;&#x29; &#xd;&#xa;GROUP BY A.rgn_id&#xd;&#xa;    ,A.rgn_name&#xd;&#xa;    ,NVL&#x28;B.proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,A.proj_name</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
