    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_OV_DTD_TIME_M PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;    ,T.rgn_id AS RNG_ID&#xd;&#xa;    ,CASE WHEN T.rgn_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.rgn_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.rgn_name END AS RNG_NAME&#xd;&#xa;    ,T.base_id&#xd;&#xa;    ,CASE WHEN T.base_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.base_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.base_name END AS BASE_NAME&#xd;&#xa;    ,T.brand_id&#xd;&#xa;    ,CASE WHEN T.brand_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.brand_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.brand_name END AS BRAND_NAME&#xd;&#xa;    ,T.TPMODE_ID AS TP_MODE_ID&#xd;&#xa;    ,CASE WHEN T.TPMODE_ID &#x3d; &#x27;LP&#x27; THEN &#x27;&#x4eba;&#x5de5;&#x27; ELSE T4.tpmode_name END AS TP_MODE_NAME&#xd;&#xa;    ,T.DTD_AVG_TIME AS AVG_DTD_TIME  --DTD&#x5e73;&#x5747;&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,avg&#x28;&#x28;unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;early_pickup_date,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;24&#x2f;3600&#x29; AS DTD_AVG_TIME&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_OV_DTD_TIME_M&#xd;&#xa;        GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,&#x27;ALL&#x27; AS BASE_ID&#xd;&#xa;        ,&#x27;ALL&#x27; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,avg&#x28;&#x28;unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;early_pickup_date,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;24&#x2f;3600&#x29; AS DTD_AVG_TIME&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_OV_DTD_TIME_M&#xd;&#xa;        GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,&#x27;ALL&#x27; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,avg&#x28;&#x28;unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;early_pickup_date,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;24&#x2f;3600&#x29; AS DTD_AVG_TIME&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_OV_DTD_TIME_M&#xd;&#xa;        GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT &#x27;ALL&#x27; AS RGN_ID&#xd;&#xa;        ,&#x27;ALL&#x27; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,avg&#x28;&#x28;unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;early_pickup_date,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;24&#x2f;3600&#x29; AS DTD_AVG_TIME&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_OV_DTD_TIME_M&#xd;&#xa;        GROUP BY coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT &#x27;ALL&#x27; AS RGN_ID&#xd;&#xa;        ,&#x27;ALL&#x27; AS BASE_ID&#xd;&#xa;        ,&#x27;ALL&#x27; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,avg&#x28;&#x28;unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;early_pickup_date,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;24&#x2f;3600&#x29; AS DTD_AVG_TIME&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_OV_DTD_TIME_M&#xd;&#xa;        GROUP BY coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;    &#xd;&#xa;    &#x29; T  &#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT base_id,base_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY base_id,base_name&#xd;&#xa;    &#x29; T2&#xd;&#xa;ON T.base_id &#x3d; T2.base_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.dim_brand &#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29; T3&#xd;&#xa;ON T.brand_id &#x3d; T3.brand_id&#xd;&#xa;LEFT JOIN &#x28;&#xd;&#xa;    SELECT tpmode_id,tpmode_name&#xd;&#xa;    FROM DIM.dim_transport_mode&#xd;&#xa;    GROUP BY tpmode_id,tpmode_name&#xd;&#xa;    &#x29; T4&#xd;&#xa;ON T.TPMODE_ID &#x3d; T4.tpmode_id&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>624</xloc>
      <yloc>208</yloc>
      </entry>
    <entry>
      <name>TO_TEMP</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_OV_DTD_TIME_M&#xd;&#xa;SELECT &#xd;&#xa;    T.RGN_ID AS RGN_ID  --&#x5927;&#x533a;ID&#xd;&#xa;    ,T.BASE_ID AS BASE_ID  --&#x57fa;&#x5730;ID&#xd;&#xa;    ,T.BRAND_ID AS BRAND_ID  --&#x54c1;&#x724c;&#xd;&#xa;    ,T.PROJ_NAME AS PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;    ,T.VIN AS VIN  --VIN&#xd;&#xa;    ,T.ORDER_RELEASE_NAME AS ORDER_RELEASE_NAME  --&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;    ,T.SEQ_NO AS SEQ_NO  --&#x8fd0;&#x8f93;&#x6bb5;&#xd;&#xa;    ,T.ORDER_MOVEMENT_GID  --&#x5206;&#x6bb5;&#x8def;&#x7531;GID&#xd;&#xa;    ,T.TRANSPORTMODE_ID AS TRANSPORTMODE_ID  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;    ,T.EARLY_PICKUP_DATE AS EARLY_PICKUP_DATE  --&#x63a5;&#x5355;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.ALLOT_PLN_DAY AS ALLOT_PLN_DAY  --&#x5206;&#x914d;&#x65f6;&#x95f4; &#x82e5;&#x6ca1;&#x6709;&#x5219;&#x53d6;&#x627f;&#x8fd0;&#x5546;&#x6700;&#x65b0;&#x786e;&#x8ba4;&#x65f6;&#x95f4;  &#xd;&#xa;    ,T.OUT_STORE_DAY AS OUT_STORE_DAY  --&#x51fa;&#x5e93;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.FACTORY_DAY AS FACTORY_DAY  --&#x6253;&#x5355;&#x65f6;&#x95f4;  &#x51fa;&#x573a;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.ARRIVAL_DAY AS ARRIVAL_DAY  --&#x4ea4;&#x4ed8;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.RECEIPT_DATE AS RECEIPT_DATE  --&#x56de;&#x5355;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.BILL_DATE AS BILL_DATE  --&#x7ed3;&#x7b97;&#x65f6;&#x95f4;&#x28;&#x4e0e;&#x627f;&#x8fd0;&#x5546;&#xff09;&#xd;&#xa;    ,T.DETAIL_FLAG AS DETAIL_FLAG  --&#x6563;&#x5355;&#x6807;&#x5fd7;&#x28;&#x6563;&#x5355;&#x7684;&#x5224;&#x65ad;&#x903b;&#x8f91;&#x29;&#xd;&#xa;FROM DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;WHERE FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ARRIVAL_DAY, &#x2