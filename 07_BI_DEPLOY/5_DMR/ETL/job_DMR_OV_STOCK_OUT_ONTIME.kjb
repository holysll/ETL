      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_STOCK_OUT_ONTIME PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID  --&#x671f;&#x95f4; &#xd;&#xa;    ,A.RGN_ID AS RGN_ID   --&#x5927;&#x533a;ID&#xd;&#xa;    ,B.rgn_name AS RGN_NAME  --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;    ,A.STOCK_OUT_QTY AS STOCK_OUT_QTY  --&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;    ,A.OUT_STORE_QTY AS ALL_STOCK_OUT_QTY  --&#x603b;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;    ,A.STOCK_OUT_QTY&#x2f;A.OUT_STORE_QTY&#x2a;100 AS ON_TIME_RATE  --&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x7387;&#xd;&#xa;    ,&#x27;0.98&#x27; AS TAR_RATE  --&#x76ee;&#x6807;&#x503c;&#xd;&#xa;    ,&#x27;OTM&#x27; AS PART_SC  --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;    ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT  --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;FROM&#x28;&#xd;&#xa;	SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,sum&#x28;outstock_ontime_qty&#x29; AS STOCK_OUT_QTY  --&#x51c6;&#x65f6;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;        ,sum&#x28;outstock_plan_qty&#x29; AS OUT_STORE_QTY --&#x5f53;&#x6708;&#x8ba1;&#x5212;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;    FROM DMK.dmk_str_operqty_d &#xd;&#xa;    WHERE substr&#x28;period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;   &#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; A  &#xd;&#xa;INNER JOIN &#x28;&#xd;&#xa;SELECT rgn_id,rgn_name&#xd;&#xa;FROM DIM.dim_org&#xd;&#xa;WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    AND org_id NOT IN &#x28;&#x27;CMAL&#x2f;ZC&#x2f;-1&#x27;,&#x27;CENTER&#x27;,&#x27;BOYU&#x27;&#x29;&#xd;&#xa;GROUP BY rgn_id,rgn_name&#xd;&#xa;&#x29; B&#xd;&#xa;ON A.rgn_id &#x3d; B.rgn_id &#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
