    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_TP_BIS_QAT_LOS_DTL PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;&#xd;&#xa;SELECT FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; AS PERIOD_WID,&#xd;&#xa;       TMP.&#x2a;,&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;&#x29; AS W_INSERT_DT, --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;       NULL                            AS W_UPDATE_DT, --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x4fee;&#x6539;&#x65f6;&#x95f4;&#xd;&#xa;       &#x27;DMK&#x27;                           AS PART_SC,     --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM TEMP.TEMP_DMR_YD_TP_BIS_QAT_LOS_DTL_02 TMP&#xd;&#xa;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>32</xloc>
      <yloc>432</yloc>
      </entry>
    <entry>
      <name>TO_TMP</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_TP_BIS_QAT_LOS_DTL_01&#xd;&#xa;SELECT COALESCE&#x28;TOT.RGN_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;TOT.BASE_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;TOT.BRAND_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;TOT.PROJ_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       SUM&#x28;LOS.LOSE_NUM_M&#x29;,&#xd;&#xa;       SUM&#x28;TOT.SHIPPING_NUM_M&#x29;,&#xd;&#xa;       SUM&#x28;LOS.LOSE_NUM_Y&#x29;,&#xd;&#xa;       SUM&#x28;TOT.SHIPPING_NUM_Y&#x29;&#xd;&#xa;  FROM &#x28;SELECT RGN_ID,&#xd;&#xa;               BASE_ID,&#xd;&#xa;               BRAND_ID,&#xd;&#xa;               PROJ_ID,&#xd;&#xa;               SUM&#x28;CASE WHEN SUBSTR&#x28;PERIOD_WID,5,2&#x29; &#x3d; MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; THEN SHIPPING_NUM ELSE 0 END&#x29; AS SHIPPING_NUM_M,&#xd;&#xa;               SUM&#x28;SHIPPING_NUM&#x29; AS SHIPPING_NUM_Y&#xd;&#xa;          FROM DMK.DMK_TP_OPERATE_D&#xd;&#xa;            WHERE SUBSTR&#x28;PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;              AND SUBSTR&#x28;PERIOD_WID,5,2&#x29; &#x3c;&#x3d; MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                GROUP BY RGN_ID,&#xd;&#xa;                         BASE_ID,&#xd;&#xa;                         BRAND_ID,&#xd;&#xa;                         PROJ_ID&#xd;&#xa;       &#x29; TOT&#xd;&#xa;    LEFT JOIN &#x28;SELECT RGN.RGN_ID,&#xd;&#xa;                      BAS.BASE_ID,&#xd;&#xa;                      BRD.BRAND_ID,&#xd;&#xa;                      PRJ.PROJ_ID,&#xd;&#xa;                      SUM&#x28;CASE WHEN SUBSTR&#x28;RECEIVING_DATE,6,2&#x29; &#x3d; MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; THEN 1 ELSE 0 END&#x29; LOSE_NUM_M,&#xd;&#xa;                      SUM&#x28;1&#x29; AS LOSE_NUM_Y&#xd;&#xa;                 FROM ODS_BLPT.T_HAND_IN_ROAD_LOSS TMP&#xd;&#xa;                   LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;                          ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                         AND TMP.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;                   LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;                          ON BAS.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                         AND TMP.BASE_NAME &#x3d; BAS.BASE_NAME&#xd;&#xa;                   LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;                          ON TMP.BRAND_NAME &#x3d; BRD.BRAND_NAME&#xd;&#xa;                   LEFT JOIN DIM.DIM_PROJ PRJ&#xd;&#xa;                          ON TMP.PRO_NAME &#x3d; PRJ.PROJ_NAME&#xd;&#xa;                     WHERE SUBSTR&#x28;RECEIVING_DATE,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                       AND SUBSTR&#x28;RECEIVING_DATE,6,2&#x29; &#x3c;&#x3d; MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                       GROUP BY RGN.RGN_ID,&#xd;&#xa;                                BAS.BASE_ID,&#xd;&#xa;                                BRD.BRAND_ID,&#xd;&#xa;                                PRJ.PROJ_ID&#xd;&#xa;              &#x29; LOS ON TOT.RGN_ID &#x3d; LOS.RGN_ID&#xd;&#xa;                   AND TOT.BASE_ID &#x3d; LOS.BASE_ID&#xd;&#xa;                   AND TOT.BRAND_ID &#x3d; LOS.BRAND_ID&#xd;&#xa;                   AND TOT.PROJ_ID &#x3d; LOS.PROJ_ID&#xd;&#xa;      GROUP BY TOT.RGN_ID,&#xd;&#xa;               TOT.BASE_ID,&#xd;&#xa;               TOT.BRAND_ID,&#xd;&#xa;               TOT.PROJ_ID&#xd;&#xa;        WITH CUBE&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_TP_BIS_QAT_LOS_DTL_02&#xd;&#xa;SELECT TMP.RGN_ID,&#xd;&#xa;       CASE WHEN TMP.RGN_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       TMP.BASE_ID,&#xd;&#xa;       CASE WHEN TMP.BASE_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       TMP.BRAND_ID,&#xd;&#xa;       CASE WHEN TMP.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;BRD.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       TMP.PROJ_ID,&#xd;&#xa;       CASE WHEN TMP.PROJ_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;PRJ.PROJ_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       ROUND&#x28;TMP.LOSE_NUM_M &#x2f; TMP.SHIPPING_NUM_M &#x2a; 100,2&#x29;,&#xd;&#xa;       TMP.LOSE_NUM_M,&#xd;&#xa;       ROUND&#x28;TMP.LOSE_NUM_Y &#x2f; TMP.SHIPPING_NUM_Y &#x2a; 100,2&#x29;,&#xd;&#xa;       TMP.LOSE_NUM_Y,&#xd;&#xa;       CASE WHEN ROUND&#x28;TMP.LOSE_NUM_M &#x2f; TMP.SHIPPING_NUM_M &#x2a; 100,2&#x29; &#x3e; &#xd;&#xa;            CASE MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; WHEN 1 THEN RAT.JAN_PLAN_AMT&#xd;&#xa;                                        WHEN 2 THEN RAT.FEB_PLAN_AMT&#xd;&#xa;                                        WHEN 3 THEN RAT.MAR_PLAN_AMT&#xd;&#xa;                                        WHEN 4 THEN RAT.APR_PLAN_AMT&#xd;&#xa;                                        WHEN 5 THEN RAT.MAY_PLAN_AMT &#xd;&#xa;                                        WHEN 6 THEN RAT.JUNE_PLAN_AMT&#xd;&#xa;                                        WHEN 7 THEN RAT.JULY_PLAN_AMT&#xd;&#xa;                                        WHEN 8 THEN RAT.AUG_PLAN_AMT&#xd;&#xa;                                        WHEN 9 THEN RAT.SEPT_PLAN_AMT&#xd;&#xa;                                        WHEN 10 THEN RAT.OCT_PLAN_AMT&#xd;&#xa;                                        WHEN 11 THEN RAT.NOV_PLAN_AMT&#xd;&#xa;                                        ELSE RAT.DEC_PLAN_AMT END&#xd;&#xa;            THEN 1 ELSE 0 END AS RATE_COLOR_FLAG_M,&#xd;&#xa;       CASE WHEN ROUND&#x28;TMP.LOSE_NUM_Y &#x2f; TMP.SHIPPING_NUM_Y &#x2a; 100,2&#x29; &#x3e; &#xd;&#xa;            CASE MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; WHEN 1 THEN RAT.JAN_PLAN_AMT&#xd;&#xa;                                        WHEN 2 THEN RAT.FEB_PLAN_AMT&#xd;&#xa;                                        WHEN 3 THEN RAT.MAR_PLAN_AMT&#xd;&#xa;                                        WHEN 4 THEN RAT.APR_PLAN_AMT&#xd;&#xa;                                        WHEN 5 THEN RAT.MAY_PLAN_AMT &#xd;&#xa;                                        WHEN 6 THEN RAT.JUNE_PLAN_AMT&#xd;&#xa;                                        WHEN 7 THEN RAT.JULY_PLAN_AMT&#xd;&#xa;                                        WHEN 8 THEN RAT.AUG_PLAN_AMT&#xd;&#xa;                                        WHEN 9 THEN RAT.SEPT_PLAN_AMT&#xd;&#xa;                                        WHEN 10 THEN RAT.OCT_PLAN_AMT&#xd;&#xa;                                        WHEN 11 THEN RAT.NOV_PLAN_AMT&#xd;&#xa;                                        ELSE RAT.DEC_PLAN_AMT END&#xd;&#xa;            THEN 1 ELSE 0 END AS RATE_COLOR_FLAG_Y,&#xd;&#xa;       CASE WHEN TMP.LOSE_NUM_M &#x3e; &#xd;&#xa;            CASE MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; WHEN 1 THEN QTY.JAN_PLAN_AMT&#xd;&#xa;                                        WHEN 2 THEN QTY.FEB_PLAN_AMT&#xd;&#xa;                                        WHEN 3 THEN QTY.MAR_PLAN_AMT&#xd;&#xa;                                        WHEN 4 THEN QTY.APR_PLAN_AMT&#xd;&#xa;                                        WHEN 5 THEN QTY.MAY_PLAN_AMT &#xd;&#xa;                                        WHEN 6 THEN QTY.JUNE_PLAN_AMT&#xd;&#xa;                                        WHEN 7 THEN QTY.JULY_PLAN_AMT&#xd;&#xa;                                        WHEN 8 THEN QTY.AUG_PLAN_AMT&#xd;&#xa;                                        WHEN 9 THEN QTY.SEPT_PLAN_AMT&#xd;&#xa;                                        WHEN 10 THEN QTY.OCT_PLAN_AMT&#xd;&#xa;                                        WHEN 11 THEN QTY.NOV_PLAN_AMT&#xd;&#xa;                                        ELSE QTY.DEC_PLAN_AMT END&#xd;&#xa;            THEN 1 ELSE 0 END AS QTY_COLOR_FLAG_M,&#xd;&#xa;       CASE WHEN TMP.LOSE_NUM_Y &#x3e; &#xd;&#xa;            CASE MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; WHEN 1 THEN QTY.JAN_PLAN_AMT&#xd;&#xa;                                        WHEN 2 THEN QTY.FEB_PLAN_AMT&#xd;&#xa;                                        WHEN 3 THEN QTY.MAR_PLAN_AMT&#xd;&#xa;                                        WHEN 4 THEN QTY.APR_PLAN_AMT&#xd;&#xa;                                        WHEN 5 THEN QTY.MAY_PLAN_AMT &#xd;&#xa;                                        WHEN 6 THEN QTY.JUNE_PLAN_AMT&#xd;&#xa;                                        WHEN 7 THEN QTY.JULY_PLAN_AMT&#xd;&#xa;                                        WHEN 8 THEN QTY.AUG_PLAN_AMT&#xd;&#xa;                                        WHEN 9 THEN QTY.SEPT_PLAN_AMT&#xd;&#xa;                                        WHEN 10 THEN QTY.OCT_PLAN_AMT&#xd;&#xa;                                        WHEN 11 THEN QTY.NOV_PLAN_AMT&#xd;&#xa;                                        ELSE QTY.DEC_PLAN_AMT END&#xd;&#xa;            THEN 1 ELSE 0 END AS QTY_COLOR_FLAG_Y&#xd;&#xa;  FROM TEMP.TEMP_DMR_YD_TP_BIS_QAT_LOS_DTL_01 TMP&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;           ON TMP.RGN_ID &#x3d; RGN.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;           ON TMP.BASE_ID &#x3d; BAS.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;           ON TMP.BRAND_ID &#x3d; BRD.BRAND_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_PROJ PRJ&#xd;&#xa;           ON TMP.PROJ_ID &#x3d; PRJ.PROJ_ID&#xd;&#xa;    LEFT JOIN ODS_BLPT.T_HAND_WARNING_TARGET RAT&#xd;&#xa;           ON RAT.YEAR_ID &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;          AND RAT.INDEX_NAME &#x3d; &#x27;&#x5728;&#x9014;&#x8d28;&#x635f;&#x7387;&#x27;&#xd;&#xa;    LEFT JOIN ODS_BLPT.T_HAND_WARNING_TARGET QTY&#xd;&#xa;           ON QTY.YEAR_ID &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;          AND QTY.INDEX_NAME &#x3d; &#x27;&#x5728;&#x9014;&#x8d28;&#x635f;&#x6570;&#x27;&#xd;&#xa;&#x3b;</sql>
