      <name>TO_DMR 2</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_TP_BIS_QAT_LOS_DTL PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID AS DATE_ID&#xd;&#xa;      ,T.RGN_ID AS RNG_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T1.RGN_NAME&#xd;&#xa;       END AS RNG_NAME&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.BASE_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.BASE_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T2.BASE_NAME&#xd;&#xa;       END AS BASE_NAME&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T3.BRAND_NAME&#xd;&#xa;       END AS BRAND_NAME&#xd;&#xa;      ,T.PROJ_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.PROJ_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.PROJ_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T4.PROJ_NAME&#xd;&#xa;       END AS PROJ_NAME&#xd;&#xa;      ,CUR_M_ONWAY_LOS_QTY &#x2f; CUR_M_PLAN_QTY &#x2a; 100 AS CUR_M_QAT_LOS_RATE --&#xd;&#xa;      ,CUR_M_ONWAY_LOS_QTY AS CUR_M_QAT_LOS_AMT --&#xd;&#xa;      ,CUR_Y_ONWAY_LOS_QTY &#x2f; CUR_Y_PLAN_QTY &#x2a; 100 AS CUR_Y_QAT_LOS_RATE --&#xd;&#xa;      ,CUR_Y_ONWAY_LOS_QTY AS CUR_Y_QAT_LOS_AMT --&#xd;&#xa;      ,NULL AS RATE_WRN_M_FLAG --&#xd;&#xa;      ,NULL AS RATE_WRN_Y_FLAG --&#xd;&#xa;      ,NULL AS QTY_WRN_M_FLAG --&#xd;&#xa;      ,NULL AS QTY_WRN_Y_FLAG --&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT X1.PERIOD_WID&#xd;&#xa;              ,X1.RGN_ID&#xd;&#xa;              ,X1.BASE_ID&#xd;&#xa;              ,X1.BRAND_ID&#xd;&#xa;              ,X1.PROJ_ID&#xd;&#xa;              ,SUM&#x28;CNT_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 6&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID, X1.PROJ_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_M_ONWAY_LOS_QTY&#xd;&#xa;              ,X2.CUR_M_PLAN_QTY AS CUR_M_PLAN_QTY --&#xd;&#xa;              ,SUM&#x28;CNT_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 4&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID, X1.PROJ_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_Y_ONWAY_LOS_QTY --&#xd;&#xa;              ,X2.CUR_Y_PLAN_QTY AS CUR_Y_PLAN_QTY --&#xd;&#xa;          FROM &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;RECEIVING_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS PERIOD_WID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;PROJ_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                      ,COUNT&#x28;1&#x29; AS CNT_QTY&#xd;&#xa;                  FROM DW.DW_TP_INROAD_LOSS&#xd;&#xa;                 WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;RECEIVING_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 4&#x29; &#x3d;&#xd;&#xa;                       SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;&#xd;&#xa;                   AND SUBSTR&#x28;REGEXP_REPLACE&#x28;RECEIVING_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                 GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;RECEIVING_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                 1,&#xd;&#xa;                                 8&#x29;&#xd;&#xa;                         ,NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;PROJ_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS X1&#xd;&#xa;          LEFT JOIN &#x28;SELECT NVL&#x28;NVL&#x28;RGN.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;BASE.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;BRAND.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;P.PROJ_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                          ,SUM&#x28;CASE&#xd;&#xa;                                 WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;PLN.MONTH_ID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                        &#x27;-&#x27;,&#xd;&#xa;                                                                        &#x27;&#x27;&#x29;,&#xd;&#xa;                                                         1,&#xd;&#xa;                                                         6&#x29; THEN&#xd;&#xa;                                  PLN.REV_PLAN&#xd;&#xa;                                 ELSE&#xd;&#xa;                                  0&#xd;&#xa;                               END&#x29; AS CUR_M_PLAN_QTY&#xd;&#xa;                          ,SUM&#x28;CASE&#xd;&#xa;                                 WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;PLN.MONTH_ID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             4&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                        &#x27;-&#x27;,&#xd;&#xa;                                                                        &#x27;&#x27;&#x29;,&#xd;&#xa;                                                         1,&#xd;&#xa;                                                         4&#x29; AND&#xd;&#xa;                                      SUBSTR&#x28;REGEXP_REPLACE&#x28;PLN.MONTH_ID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             6&#x29; &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                         &#x27;-&#x27;,&#xd;&#xa;                                                                         &#x27;&#x27;&#x29;,&#xd;&#xa;                                                          1,&#xd;&#xa;                                                          6&#x29; THEN&#xd;&#xa;                                  PLN.REV_PLAN&#xd;&#xa;                                 ELSE&#xd;&#xa;                                  0&#xd;&#xa;                               END&#x29; AS CUR_Y_PLAN_QTY&#xd;&#xa;                      FROM ODS_BLPT.T_HAND_REV_TICKET_PLAN PLN&#xd;&#xa;                      LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;                        ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                       AND PLN.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;                      LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                                      ,BASE_NAME&#xd;&#xa;                                  FROM DIM.DIM_ORG&#xd;&#xa;                                 WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                                 GROUP BY BASE_ID&#xd;&#xa;                                         ,BASE_NAME&#x29; BASE&#xd;&#xa;                        ON PLN.BASE_NAME &#x3d; BASE.BASE_NAME&#xd;&#xa;                      LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                                      ,BRAND_NAME&#xd;&#xa;                                  FROM DIM.DIM_BRAND&#xd;&#xa;                                 GROUP BY BRAND_ID&#xd;&#xa;                                         ,BRAND_NAME&#x29; BRAND&#xd;&#xa;                        ON PLN.BRAND_NAME &#x3d; BRAND.BRAND_NAME&#xd;&#xa;                      LEFT JOIN &#x28;SELECT TPMODE_ID&#xd;&#xa;                                      ,TPMODE_NAME&#xd;&#xa;                                  FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;                                 GROUP BY TPMODE_ID&#xd;&#xa;                                         ,TPMODE_NAME&#x29; TP&#xd;&#xa;                        ON PLN.TP_MODE &#x3d; TP.TPMODE_NAME&#xd;&#xa;                      LEFT JOIN &#x28;SELECT P.PROJ_ID&#xd;&#xa;                                      ,P.BASE_ID&#xd;&#xa;                                      ,P.BRAND_ID&#xd;&#xa;                                  FROM DIM.DIM_PROJ P&#xd;&#xa;                                 GROUP BY P.PROJ_ID&#xd;&#xa;                                         ,P.BASE_ID&#xd;&#xa;                                         ,P.BRAND_ID&#x29; P&#xd;&#xa;                        ON BASE.BASE_ID &#x3d; P.BASE_ID&#xd;&#xa;                       AND BRAND.BRAND_ID &#x3d; P.BRAND_ID&#xd;&#xa;                     WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;PLN.MONTH_ID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  4&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  4&#x29;&#xd;&#xa;                     GROUP BY NVL&#x28;NVL&#x28;RGN.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;NVL&#x28;BASE.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;NVL&#x28;BRAND.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;NVL&#x28;P.PROJ_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; WITH CUBE&#x29; AS X2&#xd;&#xa;            ON X1.RGN_ID &#x3d; X2.RGN_ID&#xd;&#xa;           AND X1.BASE_ID &#x3d; X2.BASE_ID&#xd;&#xa;           AND X1.BRAND_ID &#x3d; X2.BRAND_ID&#xd;&#xa;           AND X1.PROJ_ID &#x3d; X2.PROJ_ID&#x29; T&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; T1&#xd;&#xa;    ON T.RGN_ID &#x3d; T1.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; T2&#xd;&#xa;    ON T.BASE_ID &#x3d; T2.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; T3&#xd;&#xa;    ON T.BRAND_ID &#x3d; T3.BRAND_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT PROJ_ID&#xd;&#xa;                   ,PROJ_NAME&#xd;&#xa;               FROM DIM.DIM_PROJ&#xd;&#xa;              GROUP BY PROJ_ID&#xd;&#xa;                      ,PROJ_NAME&#x29; T4&#xd;&#xa;    ON T.PROJ_ID &#x3d; T4.PROJ_ID&#xd;&#xa; WHERE 1&#x3d;1&#xd;&#xa;   AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;   AND SUBSTR&#x28;T.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x3b;&#xd;&#xa;</sql>
