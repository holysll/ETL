      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;insert overwrite table dmr.dmr_tp_orderrelease partition&#x28;part_sc, part_dt&#x29;&#xd;&#xa;select &#xd;&#xa;substring&#x28;t.period_wid, 1, 4&#x29; as year                      ,&#xd;&#xa;substring&#x28;t.period_wid, 1, 6&#x29; as year_month                ,&#xd;&#xa;substring&#x28;t.period_wid, 1, 8&#x29; as date                      ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.arrive_day, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 4&#x29; as arrive_year   ,&#xd;&#xa;substr&#x28;REGEXP_REPLACE&#x28;T.ARRIVE_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS ARRIVE_MONTH  ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.ARRIVE_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS ARRIVE_DAY    ,&#xd;&#xa;t.base_id                         ,&#xd;&#xa;t.base_name                       ,&#xd;&#xa;t.brand_id                        ,&#xd;&#xa;c.brand_name                      ,&#xd;&#xa;t.change_address                  ,&#xd;&#xa;t.dest_cty_id                     ,&#xd;&#xa;t.dest_location_id                ,&#xd;&#xa;t.dest_location_type              ,&#xd;&#xa;t.dest_prvn_id                    ,&#xd;&#xa;t.dlr_id                          ,&#xd;&#xa;t.dlr_name                        ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DLVR_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 4&#x29; AS DLVR_YEAR    ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DLVR_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS DLVR_MONTH   ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DLVR_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS DLVR_DAY     ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DMS_PICKUP_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 4&#x29; AS DMS_PICK_UP_YEAR  ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DMS_PICKUP_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS DMS_PICK_UP_MONTH ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DMS_PICKUP_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS DMS_PICK_UP_DAY   ,&#xd;&#xa;t.factory_transportmode           ,&#xd;&#xa;t.first_supplier                  ,&#xd;&#xa;t.transportmode                   ,&#xd;&#xa;t.fixed_itinerary_gid             ,&#xd;&#xa;t.is_known_shipper                ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.LEAVE_CQ_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 4&#x29; AS LEAVE_CQ_YEAR   ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.LEAVE_CQ_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS LEAVE_CQ_MONTH  ,&#xd;&#xa;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.LEAVE_CQ_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS LEAVE_CQ_DAY    ,&#xd;&#xa;t.model_id                        ,&#xd;&#xa;t.oc_id                           ,&#xd;&#xa;a.oc_name                         ,&#xd;&#xa;t.on_passage_days                 ,&#xd;&#xa;t.or_dealername                   ,&#xd;&#xa;t.or_dest_address                 ,&#xd;&#xa;t.or_distributorname              ,&#xd;&#xa;t.or_plan_type                    ,&#xd;&#xa;t.or_shipdesc                     ,&#xd;&#xa;t.or_transfer_address             ,&#xd;&#xa;t.order_release_gid               ,&#xd;&#xa;t.order_staus                     ,&#xd;&#xa;t.out_notorder_num                ,&#xd;&#xa;t.proj_name                       ,&#xd;&#xa;t.rgn_id                          ,&#xd;&#xa;b.rgn_name                        ,&#xd;&#xa;t.second_supplier                 ,&#xd;&#xa;t.segment                         ,&#xd;&#xa;t.source_code                     ,&#xd;&#xa;t.source_location_id              ,&#xd;&#xa;t.source_location_type            ,&#xd;&#xa;t.src_cty_id                      ,&#xd;&#xa;t.src_prvn_id                     ,&#xd;&#xa;t.transfer_station                ,&#xd;&#xa;t.transportstatus                 ,&#xd;&#xa;t.vin                             ,&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x27;DW&#x27;      as part_sc      ,&#xd;&#xa;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMM&#x27;&#x29; AS PART_DT&#xd;&#xa;from dw.dw_tp_orderrelease t&#xd;&#xa;left join dim.dim_org a&#xd;&#xa;on a.oc_id &#x3d; t.oc_id and a.org_type &#x3d; &#x27;O&#x27; and a.enable_flag &#x3d; &#x27;T&#x27;&#xd;&#xa;left join dim.dim_org b&#xd;&#xa;on b.rgn_id &#x3d; t.rgn_id and b.org_type &#x3d; &#x27;R&#x27; and b.enable_flag &#x3d; &#x27;T&#x27;&#xd;&#xa;left join dim.dim_brand c&#xd;&#xa;on c.brand_id &#x3d; t.brand_id and c.enable_flag &#x3d; &#x27;T&#x27;&#xd;&#xa;&#xd;&#xa;</sql>
