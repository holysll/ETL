      </entry>
    <entry>
      <name>TO_TMP</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_MKT_SHARE_DTL_01&#xd;&#xa;SELECT A.month_id&#xd;&#xa;    ,NULL AS RGN_ID&#xd;&#xa;    ,NULL AS RGN_NAME&#xd;&#xa;    ,NULL AS BASE_ID&#xd;&#xa;    ,NULL AS BASE_NAME&#xd;&#xa;    ,COALESCE&#x28;C.BRAND_ID,&#x27;-1&#x27;&#x29; AS  BRAND_ID&#xd;&#xa;    ,A.brand_name&#xd;&#xa;    ,NULL AS PROD_ID&#xd;&#xa;    ,NULL AS PROD_NAME&#xd;&#xa;    ,NULL AS MKT_EXP_INCOME&#xd;&#xa;    ,SUM&#x28;NVL&#x28;A.yqyhtxl,0&#x29;&#x29;&#x2f;SUM&#x28;NVL&#x28;A.htqysy,0&#x29;&#x29;&#x2a;100 AS CUR_M_MKT_SHARE&#xd;&#xa;    ,SUM&#x28;NVL&#x28;B.yqyhtxl,0&#x29;&#x29;&#x2f;SUM&#x28;NVL&#x28;B.htqysy,0&#x29;&#x29;&#x2a;100 AS LST_M_MKT_SHARE&#xd;&#xa;FROM ODS_BLPT.T_HAND_MARKET_LV A  &#xd;&#xa;LEFT JOIN &#x28;&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;add_months&#x28;from_unixtime&#x28;unix_timestamp&#x28;month_id,&#x27;yyyyMM&#x27;&#x29;,&#x27;yyyy-MM-01&#x27;&#x29;,1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;        ,brand_name&#xd;&#xa;        ,yqyhtxl&#xd;&#xa;        ,htqysy&#xd;&#xa;    FROM ODS_BLPT.T_HAND_MARKET_LV&#xd;&#xa;&#x29; B&#xd;&#xa;ON A.month_id &#x3d; B.MONTH_ID AND A.brand_name &#x3d; B.brand_name&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT BRAND_ID,BRAND_NAME&#xd;&#xa;    FROM DIM.DIM_BRAND&#xd;&#xa;    GROUP BY BRAND_ID,BRAND_NAME&#xd;&#xa;    &#x29;C&#xd;&#xa;ON A.brand_name &#x3d; C.BRAND_NAME&#xd;&#xa;GROUP BY A.month_id,A.brand_name,C.BRAND_ID&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT A.month_id&#xd;&#xa;    ,NULL AS RGN_ID&#xd;&#xa;    ,NULL AS RGN_NAME&#xd;&#xa;    ,NULL AS BASE_ID&#xd;&#xa;    ,NULL AS BASE_NAME&#xd;&#xa;    ,&#x27;ALL&#x27; AS BRAND_ID&#xd;&#xa;    ,&#x27;&#x5168;&#x90e8;&#x27; AS BRAND_NAME&#xd;&#xa;    ,NULL AS PROD_ID&#xd;&#xa;    ,NULL AS PROD_NAME&#xd;&#xa;    ,C.MKT_EXP_INCOME&#xd;&#xa;    ,NVL&#x28;A.YQYHTXL,0&#x29;&#x2f;NVL&#x28;A.HTQYSY,0&#x29;&#x2a;100 AS CUR_M_MKT_SHARE&#xd;&#xa;    ,NVL&#x28;B.YQYHTXL,0&#x29;&#x2f;NVL&#x28;B.HTQYSY,0&#x29;&#x2a;100 AS LST_M_MKT_SHARE&#xd;&#xa;FROM &#x28;&#xd;&#xa;    SELECT month_id&#xd;&#xa;        ,sum&#x28;NVL&#x28;yqyhtxl,0&#x29;&#x29; AS YQYHTXL&#xd;&#xa;        ,sum&#x28;NVL&#x28;htqysy,0&#x29;&#x29; AS HTQYSY&#xd;&#xa;    FROM ODS_BLPT.T_HAND_MARKET_LV&#xd;&#xa;    GROUP BY month_id&#xd;&#xa;&#x29; A&#xd;&#xa;LEFT JOIN &#x28;&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;add_months&#x28;from_unixtime&#x28;unix_timestamp&#x28;month_id,&#x27;yyyyMM&#x27;&#x29;,&#x27;yyyy-MM-01&#x27;&#x29;,1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;        ,sum&#x28;NVL&#x28;yqyhtxl,0&#x29;&#x29; AS YQYHTXL&#xd;&#xa;        ,sum&#x28;NVL&#x28;htqysy,0&#x29;&#x29; AS HTQYSY&#xd;&#xa;    FROM ODS_BLPT.T_HAND_MARKET_LV&#xd;&#xa;    GROUP BY substr&#x28;regexp_replace&#x28;add_months&#x28;from_unixtime&#x28;unix_timestamp&#x28;month_id,&#x27;yyyyMM&#x27;&#x29;,&#x27;yyyy-MM-01&#x27;&#x29;,1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;&#x29; B&#xd;&#xa;ON A.month_id &#x3d; B.MONTH_ID &#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT period_wid&#xd;&#xa;        ,SUM&#x28;CASE WHEN cat &#x3d; &#x27;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x5b8c;&#x6210;&#x503c;&#x27; THEN exp_amt ELSE 0 END&#x29; AS MKT_EXP_INCOME&#xd;&#xa;    FROM DW.DW_FIN_MKT_EXP&#xd;&#xa;    GROUP BY period_wid&#xd;&#xa;&#x29; C&#xd;&#xa;ON A.month_id &#x3d; C.period_wid</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>176</xloc>
      <yloc>272</yloc>
      </entry>
    <entry>
      <name>CREATE_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_MKT_SHARE_DTL_01&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_MKT_SHARE_DTL_01&#x28;&#xd;&#xa;MONTH_ID    STRING    COMMENT    &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID    STRING    COMMENT    &#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,RGN_NAME    STRING    COMMENT    &#x27;&#x5927;&#x533a;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,BASE_ID    STRING    COMMENT    &#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BASE_NAME    STRING    COMMENT    &#x27;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,BRAND_ID    STRING    COMMENT    &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,BRAND_NAME    STRING    COMMENT    &#x27;&#x54c1;&#x724c;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,PROD_ID    STRING    COMMENT    &#x27;&#x4ea7;&#x54c1;ID&#x27;&#xd;&#xa;,PROD_NAME    STRING    COMMENT    &#x27;&#x4ea7;&#x54c1;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,MKT_EXP_INCOME    DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x27;&#xd;&#xa;,CUR_M_MKT_SHARE    DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x5f53;&#x6708;&#x5e02;&#x573a;&#x5360;&#x6709;&#x7387;&#x27;&#xd;&#xa;,LST_M_MKT_SHARE    DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x4e0a;&#x6708;&#x5e02;&#x573a;&#x5360;&#x6709;&#x7387;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x5e02;&#x573a;&#x5360;&#x6709;&#x7387;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
