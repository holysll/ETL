    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_MKT_SHARE_DTL PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT month_id&#xd;&#xa;    ,rgn_id&#xd;&#xa;    ,rgn_name&#xd;&#xa;    ,base_id&#xd;&#xa;    ,base_name&#xd;&#xa;    ,brand_id&#xd;&#xa;    ,brand_name&#xd;&#xa;    ,prod_id&#xd;&#xa;    ,prod_name&#xd;&#xa;    ,mkt_exp_income&#xd;&#xa;    ,cur_m_mkt_share&#xd;&#xa;    ,lst_m_mkt_share&#xd;&#xa;    ,cur_m_mkt_share-lst_m_mkt_share AS MKT_SHARE_M_MOM&#xd;&#xa;    ,CASE WHEN cur_m_mkt_share-lst_m_mkt_share &#x3c; 0 THEN -1&#xd;&#xa;        WHEN cur_m_mkt_share-lst_m_mkt_share &#x3d; 0 THEN 0&#xd;&#xa;        WHEN cur_m_mkt_share-lst_m_mkt_share &#x3e; 0 THEN 1&#xd;&#xa;        ELSE NULL END AS MOM_WRN_FLAG&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;YYYY-MM-DD HH&#x3a;MM&#x3a;SS&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;YYYY-MM-DD HH&#x3a;MM&#x3a;SS&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;BLPT&#x27; AS PART_SC&#xd;&#xa;    ,month_id AS PART_DT&#xd;&#xa;FROM TEMP.TEMP_DMR_YD_MKT_SHARE_DTL_01&#xd;&#xa;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
