    <entry>
      <name>TO_TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_FIN_KEY_INDEX_M_1&#xd;&#xa;SELECT SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID  --&#xd;&#xa;      ,T.RGN_ID AS RGN_ID&#xd;&#xa;      ,SUM&#x28;CASE WHEN T.period_wid &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.VCL_INCOME_AMT ELSE 0 END&#x29; AS CUR_INCOME_AMT  --&#x6536;&#x5165;&#xd;&#xa;	  ,SUM&#x28;CASE WHEN SUBSTR&#x28;T.period_wid,1,4&#x29; &#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; AND SUBSTR&#x28;T.PERIOD_WID,1,6&#x29; &#x3c;&#x3d; SUBSTR&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.VCL_INCOME_AMT ELSE 0 END&#x29; AS CUR_INCOME_AMT_Y  --&#x6536;&#x5165;&#x28;&#x5e74;&#x29;&#xd;&#xa;      ,SUM&#x28;CASE WHEN SUBSTR&#x28;T.PERIOD_WID,1,6&#x29; &#x3d; SUBSTR&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.VCL_INCOME_AMT ELSE 0 END&#x29; AS LST_INCOME_AMT  --&#x4e0a;&#x671f;&#x6536;&#x5165;&#xd;&#xa;      ,SUM&#x28;CASE WHEN SUBSTR&#x28;T.PERIOD_WID,1,6&#x29; &#x3d; SUBSTR&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.VCL_INCOME_AMT ELSE 0 END&#x29; AS LST_Y_INCOME_AMT  --&#x540c;&#x671f;&#x6536;&#x5165;&#xd;&#xa;	  ,SUM&#x28;CASE WHEN SUBSTR&#x28;T