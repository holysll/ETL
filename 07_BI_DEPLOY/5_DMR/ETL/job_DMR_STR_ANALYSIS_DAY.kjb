    <entry>
      <name>TO_TMP</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_STR_ANALYSIS_DAY_01&#xd;&#xa;SELECT X.PERIOD_WID AS PERIOD_WID&#xd;&#xa;      ,X.RGN_ID&#xd;&#xa;      ,R.RGN_NAME&#xd;&#xa;      ,X.BASE_ID&#xd;&#xa;      ,B.BASE_NAME&#xd;&#xa;      ,X.STR_ID&#xd;&#xa;      ,S.STR_NAME&#xd;&#xa;      ,NVL&#x28;X.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,NVL&#x28;BR.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME&#xd;&#xa;      ,NULL AS MODEL_ID&#xd;&#xa;      ,NULL AS MODEL_NAME&#xd;&#xa;      ,X.AGE_RANGE&#xd;&#xa;      ,X.USED_INV_QTY&#xd;&#xa;      ,NVL&#x28;X.ALL_INV_QTY, 0&#x29; - NVL&#x28;X.USED_INV_QTY, 0&#x29;&#xd;&#xa;      ,X.ALL_INV_QTY&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE&#xd;&#xa;  FROM &#x28;SELECT T.PERIOD_WID&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,T.BASE_ID&#xd;&#xa;              ,T.STR_ID&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,T.AGE_RANGE&#xd;&#xa;              ,SUM&#x28;USED_INV_QTY&#x29; AS USED_INV_QTY&#xd;&#xa;              ,SUM&#x28;ALL_INV_QTY&#x29; AS ALL_INV_QTY&#xd;&#xa;          FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                      ,A.RGN_ID&#xd;&#xa;                      ,A.BASE_ID&#xd;&#xa;                      ,A.STR_ID&#xd;&#xa;                      ,A.BRAND_ID&#xd;&#xa;                      ,&#x27;-1&#x27; AS AGE_RANGE&#xd;&#xa;                      ,0 AS USED_INV_QTY&#xd;&#xa;                      ,A.ALL_INV_QTY&#xd;&#xa;                  FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                  JOIN &#x28;SELECT T.RGN_ID&#xd;&#xa;                             ,T.BASE_ID&#xd;&#xa;                             ,T.STR_ID&#xd;&#xa;                             ,T.BRAND_ID&#xd;&#xa;                             ,SUM&#x28;MAX_STORE_QTY&#x29; AS ALL_INV_QTY&#xd;&#xa;                         FROM DMK.DMK_STR_MAXSTORE_M T&#xd;&#xa;                        WHERE PART_DT &#x3d;&#xd;&#xa;                              REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                             &#x27;-&#x27;,&#xd;&#xa;                                             &#x27;&#x27;&#x29;&#xd;&#xa;                          AND T.PART_SC &#x3d; &#x27;DIM&#x27;&#xd;&#xa;                        GROUP BY T.RGN_ID&#xd;&#xa;                                ,T.BASE_ID&#xd;&#xa;                                ,T.STR_ID&#xd;&#xa;                                ,T.BRAND_ID&#x29; A&#xd;&#xa;                    ON 1 &#x3d; 1&#xd;&#xa;                 WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                   AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                UNION ALL&#xd;&#xa;                SELECT A.PERIOD_WID&#xd;&#xa;                      ,A.RGN_ID&#xd;&#xa;                      ,A.BASE_ID&#xd;&#xa;                      ,A.STR_ID&#xd;&#xa;                      ,NVL&#x28;A.BRAND_ID, &#x27;FT&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                      ,CASE&#xd;&#xa;                         WHEN INVENTORY_DAY &#x3c; 30 THEN&#xd;&#xa;                          &#x27;30&#x5929;&#x5185;&#x27;&#xd;&#xa;                         WHEN INVENTORY_DAY &#x3e;&#x3d; 30 AND INVENTORY_DAY &#x3c; 90 THEN&#xd;&#xa;                          &#x27;30&#x7e;90&#x5929;&#x5185;&#x27;&#xd;&#xa;                         WHEN INVENTORY_DAY &#x3e;&#x3d; 90 AND INVENTORY_DAY &#x3c; 180 THEN&#xd;&#xa;                          &#x27;90&#x7e;180&#x5929;&#x5185;&#x27;&#xd;&#xa;                         WHEN INVENTORY_DAY &#x3e;&#x3d; 180 AND INVENTORY_DAY &#x3c; 365 THEN&#xd;&#xa;                          &#x27;180&#x7e;365&#x5929;&#x5185;&#x27;&#xd;&#xa;                         WHEN INVENTORY_DAY &#x3e;&#x3d; 365 THEN&#xd;&#xa;                          &#x27;365&#x5929;&#x4ee5;&#x4e0a;&#x27;&#xd;&#xa;                       END AS AGE_RANGE&#xd;&#xa;                      ,SUM&#x28;INVENTORY_QTY&#x29; AS USED_INV_QTY --&#x4f7f;&#x7528;&#x91cf;&#xd;&#xa;                      ,0 AS ALL_INV_QTY&#xd;&#xa;                  FROM DMK.DMK_STR_INVENTORY_D A&#xd;&#xa;                 WHERE SUBSTRING&#x28;A.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                       REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                      &#x27;-&#x27;,&#xd;&#xa;                                      &#x27;&#x27;&#x29;&#xd;&#xa;                 GROUP BY A.PERIOD_WID&#xd;&#xa;                         ,A.RGN_ID&#xd;&#xa;                         ,A.BASE_ID&#xd;&#xa;                         ,A.STR_ID&#xd;&#xa;                         ,A.BRAND_ID&#xd;&#xa;                         ,CASE&#xd;&#xa;                            WHEN INVENTORY_DAY &#x3c; 30 THEN&#xd;&#xa;                             &#x27;30&#x5929;&#x5185;&#x27;&#xd;&#xa;                            WHEN INVENTORY_DAY &#x3e;&#x3d; 30 AND INVENTORY_DAY &#x3c; 90 THEN&#xd;&#xa;                             &#x27;30&#x7e;90&#x5929;&#x5185;&#x27;&#xd;&#xa;                            WHEN INVENTORY_DAY &#x3e;&#x3d; 90 AND INVENTORY_DAY &#x3c; 180 THEN&#xd;&#xa;                             &#x27;90&#x7e;180&#x5929;&#x5185;&#x27;&#xd;&#xa;                            WHEN INVENTORY_DAY &#x3e;&#x3d; 180 AND INVENTORY_DAY &#x3c; 365 THEN&#xd;&#xa;                             &#x27;180&#x7e;365&#x5929;&#x5185;&#x27;&#xd;&#xa;                            WHEN INVENTORY_DAY &#x3e;&#x3d; 365 THEN&#xd;&#xa;                             &#x27;365&#x5929;&#x4ee5;&#x4e0a;&#x27;&#xd;&#xa;                          END&#x29; T&#xd;&#xa;         GROUP BY T.PERIOD_WID&#xd;&#xa;                 ,T.RGN_ID&#xd;&#xa;                 ,T.BASE_ID&#xd;&#xa;                 ,T.STR_ID&#xd;&#xa;                 ,T.BRAND_ID&#xd;&#xa;                 ,T.AGE_RANGE&#x29; AS X&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG R&#xd;&#xa;    ON X.RGN_ID &#x3d; R.RGN_ID&#xd;&#xa;   AND R.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG B&#xd;&#xa;    ON X.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;   AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG S&#xd;&#xa;    ON X.STR_ID &#x3d; S.STR_ID&#xd;&#xa;   AND S.ORG_TYPE &#x3d; &#x27;S&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_BRAND BR&#xd;&#xa;    ON X.BRAND_ID &#x3d; BR.BRAND_ID&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>352</xloc>
      <yloc>288</yloc>
      </entry>
    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_STR_ANALYSIS_DAY PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT T. PERIOD_WID&#xd;&#xa;      ,T. RGN_ID&#xd;&#xa;      ,T. RGN_NAME&#xd;&#xa;      ,T. BASE_ID&#xd;&#xa;      ,T. BASE_NAME&#xd;&#xa;      ,T. STR_ID&#xd;&#xa;      ,T. STR_NAME&#xd;&#xa;      ,T. BRAND_ID&#xd;&#xa;      ,T. BRAND_NAME&#xd;&#xa;      ,T. MODEL_ID&#xd;&#xa;      ,T. MODEL_NAME&#xd;&#xa;      ,T. AGE_RANGE&#xd;&#xa;      ,T. USED_INV_QTY&#xd;&#xa;      ,T. REMAIN_INV_QTY&#xd;&#xa;      ,T. ALL_INV_QTY&#xd;&#xa;      ,T. INSERT_TIME&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;  FROM TEMP.TEMP_DMR_STR_ANALYSIS_DAY_01 T&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT A. PERIOD_WID&#xd;&#xa;      ,A. RGN_ID&#xd;&#xa;      ,A. RGN_NAME&#xd;&#xa;      ,A. BASE_ID&#xd;&#xa;      ,A. BASE_NAME&#xd;&#xa;      ,A. STR_ID&#xd;&#xa;      ,A. STR_NAME&#xd;&#xa;      ,A. BRAND_ID&#xd;&#xa;      ,A. BRAND_NAME&#xd;&#xa;      ,A. MODEL_ID&#xd;&#xa;      ,A. MODEL_NAME&#xd;&#xa;      ,A. AGE_RANGE&#xd;&#xa;      ,A. USED_INV_QTY&#xd;&#xa;      ,A. REMAIN_INV_QTY&#xd;&#xa;      ,A. ALL_INV_QTY&#xd;&#xa;      ,A. INSERT_TIME&#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMM&#x27;&#x29; AS PART_DT&#xd;&#xa;  FROM DMR.DMR_STR_ANALYSIS_DAY A&#xd;&#xa;  LEFT JOIN TEMP.TEMP_DMR_STR_ANALYSIS_DAY_01 B&#xd;&#xa;    ON A.PERIOD_WID &#x3d; B.PERIOD_WID&#xd;&#xa;   AND A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;   AND A.RGN_NAME &#x3d; B.RGN_NAME&#xd;&#xa;   AND A.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;   AND A.BASE_NAME &#x3d; B.BASE_NAME&#xd;&#xa;   AND A.STR_ID &#x3d; B.STR_ID&#xd;&#xa;   AND A.STR_NAME &#x3d; B.STR_NAME&#xd;&#xa;   AND A.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;   AND A.BRAND_NAME &#x3d; B.BRAND_NAME&#xd;&#xa;   AND A.MODEL_ID &#x3d; B.MODEL_ID&#xd;&#xa;   AND A.MODEL_NAME &#x3d; B.MODEL_NAME&#xd;&#xa; WHERE B.PERIOD_WID IS NULL&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;&#x3b;&#xd;&#xa;</sql>
