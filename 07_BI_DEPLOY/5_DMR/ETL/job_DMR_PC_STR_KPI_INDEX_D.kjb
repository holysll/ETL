    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;--target table &#x3a; DMR.DMR_PC_STR_KPI_INDEX_M DMRPC&#x4ed3;&#x50a8;-&#x4ed3;&#x50a8;&#x6982;&#x89c8;-&#x4ed3;&#x50a8;&#x6982;&#x89c8;&#x28;&#x6708;&#x29;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_STR_KPI_INDEX_D PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;      ,A.RGN_ID&#xd;&#xa;      ,A.RGN_NAME&#xd;&#xa;      ,A.BASE_ID&#xd;&#xa;      ,A.BASE_NAME&#xd;&#xa;      ,A.BRAND_ID&#xd;&#xa;      ,A.BRAND_NAME&#xd;&#xa;      ,A.MODEL_ID&#xd;&#xa;      ,A.MODEL_NAME&#xd;&#xa;      ,A.INDEX_SEQ&#xd;&#xa;      ,A.INDEX_NAME&#xd;&#xa;      ,A.BIZ_QTY&#xd;&#xa;      ,A.BIZ_RATE&#xd;&#xa;      ,A.TB_RATE&#xd;&#xa;      ,A.HB_RATE&#xd;&#xa;      ,A.INSERT_DATE&#xd;&#xa;      ,A.UPDATE_DATE&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;A.PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT T1.&#x2a;&#xd;&#xa;          FROM &#x28;SELECT T.PERIOD_WID AS PERIOD_WID --&#x6708;&#x4efd;ID&#xd;&#xa;                      ,T.RGN_ID AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                      ,RGN.RGN_NAME AS RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.BASE_ID AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;                      ,BASE.BASE_NAME AS BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.BRAND_ID AS BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;                      ,BRAND.BRAND_NAME AS BRAND_NAME --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.MODEL_ID AS MODEL_ID --&#x8f66;&#x578b;ID&#xd;&#xa;                      ,MODEL.MODEL_NAME AS MODEL_NAME --&#x8f66;&#x578b;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.INDEX_SEQ AS INDEX_SEQ --&#x6307;&#x6807;&#x5e8f;&#x53f7;&#xd;&#xa;                      ,T.INDEX_NAME AS INDEX_NAME --&#x6307;&#x6807;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.BIZ_QTY AS BIZ_QTY --&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;                      ,T.BIZ_RATE AS BIZ_RATE --&#x4e1a;&#x52a1;&#x5360;&#x6bd4;&#xd;&#xa;                      ,T.TB_RATE AS TB_RATE --&#x540c;&#x6bd4;&#xd;&#xa;                      ,T.HB_RATE AS HB_RATE --&#x73af;&#x6bd4;&#xd;&#xa;                      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;                      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DATE --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;                  FROM &#x28;SELECT PERIOD_WID&#xd;&#xa;                              ,RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,BRAND_ID&#xd;&#xa;                              ,MODEL_ID&#xd;&#xa;                              ,INDEX_SEQ&#xd;&#xa;                              ,INDEX_NAME&#xd;&#xa;                              ,BIZ_QTY&#xd;&#xa;                              ,BIZ_RATE&#xd;&#xa;                              ,TB_RATE&#xd;&#xa;                              ,HB_RATE&#xd;&#xa;                          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_1&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT PERIOD_WID&#xd;&#xa;                              ,RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,BRAND_ID&#xd;&#xa;                              ,MODEL_ID&#xd;&#xa;                              ,INDEX_SEQ&#xd;&#xa;                              ,INDEX_NAME&#xd;&#xa;                              ,BIZ_QTY&#xd;&#xa;                              ,BIZ_RATE&#xd;&#xa;                              ,TB_RATE&#xd;&#xa;                              ,HB_RATE&#xd;&#xa;                          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_2&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT PERIOD_WID&#xd;&#xa;                              ,RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,BRAND_ID&#xd;&#xa;                              ,MODEL_ID&#xd;&#xa;                              ,INDEX_SEQ&#xd;&#xa;                              ,INDEX_NAME&#xd;&#xa;                              ,BIZ_QTY&#xd;&#xa;                              ,BIZ_RATE&#xd;&#xa;                              ,TB_RATE&#xd;&#xa;                              ,HB_RATE&#xd;&#xa;                          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_3&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT PERIOD_WID&#xd;&#xa;                              ,RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,BRAND_ID&#xd;&#xa;                              ,MODEL_ID&#xd;&#xa;                              ,INDEX_SEQ&#xd;&#xa;                              ,INDEX_NAME&#xd;&#xa;                              ,BIZ_QTY&#xd;&#xa;                              ,BIZ_RATE&#xd;&#xa;                              ,TB_RATE&#xd;&#xa;                              ,HB_RATE&#xd;&#xa;                          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_5&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT PERIOD_WID&#xd;&#xa;                              ,RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,BRAND_ID&#xd;&#xa;                              ,MODEL_ID&#xd;&#xa;                              ,INDEX_SEQ&#xd;&#xa;                              ,INDEX_NAME&#xd;&#xa;                              ,BIZ_QTY&#xd;&#xa;                              ,BIZ_RATE&#xd;&#xa;                              ,TB_RATE&#xd;&#xa;                              ,HB_RATE&#xd;&#xa;                          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_6&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT PERIOD_WID&#xd;&#xa;                              ,RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,BRAND_ID&#xd;&#xa;                              ,MODEL_ID&#xd;&#xa;                              ,INDEX_SEQ&#xd;&#xa;                              ,INDEX_NAME&#xd;&#xa;                              ,BIZ_QTY&#xd;&#xa;                              ,BIZ_RATE&#xd;&#xa;                              ,TB_RATE&#xd;&#xa;                              ,HB_RATE&#xd;&#xa;                          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_8&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT PERIOD_WID&#xd;&#xa;                              ,RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,BRAND_ID&#xd;&#xa;                              ,MODEL_ID&#xd;&#xa;                              ,INDEX_SEQ&#xd;&#xa;                              ,INDEX_NAME&#xd;&#xa;                              ,BIZ_QTY&#xd;&#xa;                              ,BIZ_RATE&#xd;&#xa;                              ,TB_RATE&#xd;&#xa;                              ,HB_RATE&#xd;&#xa;                          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_9&#xd;&#xa;                        UNION ALL&#xd;&#xa;                        SELECT PERIOD_WID&#xd;&#xa;                              ,RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,BRAND_ID&#xd;&#xa;                              ,MODEL_ID&#xd;&#xa;                              ,INDEX_SEQ&#xd;&#xa;                              ,INDEX_NAME&#xd;&#xa;                              ,BIZ_QTY&#xd;&#xa;                              ,BIZ_RATE&#xd;&#xa;                              ,TB_RATE&#xd;&#xa;                              ,HB_RATE&#xd;&#xa;                          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_4&#x29; T&#xd;&#xa;                --SHARED DIMENSION JOINS&#xd;&#xa;                  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                                  ,RGN_NAME&#xd;&#xa;                              FROM DIM.DIM_ORG&#xd;&#xa;                             WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;                             GROUP BY RGN_ID&#xd;&#xa;                                     ,RGN_NAME&#x29; RGN&#xd;&#xa;                    ON T.RGN_ID &#x3d; RGN.RGN_ID&#xd;&#xa;                  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                                  ,BASE_NAME&#xd;&#xa;                              FROM DIM.DIM_ORG&#xd;&#xa;                             WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;                             GROUP BY BASE_ID&#xd;&#xa;                                     ,BASE_NAME&#x29; BASE&#xd;&#xa;                    ON T.BASE_ID &#x3d; BASE.BASE_ID&#xd;&#xa;                  LEFT JOIN DIM.DIM_BRAND BRAND&#xd;&#xa;                    ON T.BRAND_ID &#x3d; BRAND.BRAND_ID&#xd;&#xa;                   AND BRAND.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;                  LEFT JOIN DIM.DIM_MODEL MODEL&#xd;&#xa;                    ON T.MODEL_ID &#x3d; MODEL.MODEL_ID&#xd;&#xa;                   AND MODEL.ENABLE_FLAG &#x3d; &#x27;Y&#x27;&#x29; AS T1&#xd;&#xa;&#x2f;&#x2a;        ----------------------------------&#x5e93;&#x5b58;&#x91cf;&#x5904;&#x7406;&#x955c;&#x50cf;&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT T2.&#x2a;&#xd;&#xa;          FROM &#x28;SELECT M.PERIOD_WID&#xd;&#xa;                      ,M.RGN_ID&#xd;&#xa;                      ,M.RGN_NAME&#xd;&#xa;                      ,M.BASE_ID&#xd;&#xa;                      ,M.BASE_NAME&#xd;&#xa;                      ,M.BRAND_ID&#xd;&#xa;                      ,M.BRAND_NAME&#xd;&#xa;                      ,M.MODEL_ID&#xd;&#xa;                      ,M.MODEL_NAME&#xd;&#xa;                      ,M.INDEX_SEQ&#xd;&#xa;                      ,M.INDEX_NAME&#xd;&#xa;                      ,M.BIZ_QTY&#xd;&#xa;                      ,M.BIZ_RATE&#xd;&#xa;                      ,M.TB_RATE&#xd;&#xa;                      ,M.HB_RATE&#xd;&#xa;                      ,M.INSERT_DATE&#xd;&#xa;                      ,M.UPDATE_DATE&#xd;&#xa;                  FROM DMR.DMR_PC_STR_KPI_INDEX_D M&#xd;&#xa;                  LEFT JOIN TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_4 P&#xd;&#xa;                    ON M.PERIOD_WID &#x3d; P.PERIOD_WID&#xd;&#xa;                   AND M.RGN_ID &#x3d; P.RGN_ID&#xd;&#xa;                   AND M.BASE_ID &#x3d; P.BASE_ID&#xd;&#xa;                   AND M.BRAND_ID &#x3d; P.BRAND_ID&#xd;&#xa;                   AND M.MODEL_ID &#x3d; P.MODEL_ID&#xd;&#xa;                 WHERE M.INDEX_SEQ &#x3d; &#x27;04&#x27;&#xd;&#xa;                   AND P.PERIOD_WID IS NULL&#x29; AS T2&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT T3.&#x2a;&#xd;&#xa;          FROM &#x28;SELECT T.PERIOD_WID AS PERIOD_WID --&#x6708;&#x4efd;ID&#xd;&#xa;                      ,T.RGN_ID AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                      ,RGN.RGN_NAME AS RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.BASE_ID AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;                      ,BASE.BASE_NAME AS BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.BRAND_ID AS BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;                      ,BRAND.BRAND_NAME AS BRAND_NAME --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.MODEL_ID AS MODEL_ID --&#x8f66;&#x578b;ID&#xd;&#xa;                      ,MODEL.MODEL_NAME AS MODEL_NAME --&#x8f66;&#x578b;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.INDEX_SEQ AS INDEX_SEQ --&#x6307;&#x6807;&#x5e8f;&#x53f7;&#xd;&#xa;                      ,T.INDEX_NAME AS INDEX_NAME --&#x6307;&#x6807;&#x540d;&#x79f0;&#xd;&#xa;                      ,T.BIZ_QTY AS BIZ_QTY --&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;                      ,T.BIZ_RATE AS BIZ_RATE --&#x4e1a;&#x52a1;&#x5360;&#x6bd4;&#xd;&#xa;                      ,T.TB_RATE AS TB_RATE --&#x540c;&#x6bd4;&#xd;&#xa;                      ,T.HB_RATE AS HB_RATE --&#x73af;&#x6bd4;&#xd;&#xa;                      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;                      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DATE --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;                  FROM &#x28;SELECT PERIOD_WID&#xd;&#xa;                              ,RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,BRAND_ID&#xd;&#xa;                              ,MODEL_ID&#xd;&#xa;                              ,INDEX_SEQ&#xd;&#xa;                              ,INDEX_NAME&#xd;&#xa;                              ,BIZ_QTY&#xd;&#xa;                              ,BIZ_RATE&#xd;&#xa;                              ,TB_RATE&#xd;&#xa;                              ,HB_RATE&#xd;&#xa;                          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_4&#x29; T&#xd;&#xa;                  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                                  ,RGN_NAME&#xd;&#xa;                              FROM DIM.DIM_ORG&#xd;&#xa;                             WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;                             GROUP BY RGN_ID&#xd;&#xa;                                     ,RGN_NAME&#x29; RGN&#xd;&#xa;                    ON T.RGN_ID &#x3d; RGN.RGN_ID&#xd;&#xa;                  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                                  ,BASE_NAME&#xd;&#xa;                              FROM DIM.DIM_ORG&#xd;&#xa;                             WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;                             GROUP BY BASE_ID&#xd;&#xa;                                     ,BASE_NAME&#x29; BASE&#xd;&#xa;                    ON T.BASE_ID &#x3d; BASE.BASE_ID&#xd;&#xa;                  LEFT JOIN DIM.DIM_BRAND BRAND&#xd;&#xa;                    ON T.BRAND_ID &#x3d; BRAND.BRAND_ID&#xd;&#xa;                   AND BRAND.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;                  LEFT JOIN DIM.DIM_MODEL MODEL&#xd;&#xa;                    ON T.MODEL_ID &#x3d; MODEL.MODEL_ID&#xd;&#xa;                   AND MODEL.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#x29; AS T3&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;&#x29; AS A&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
