      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;--09 &#x8fd4;&#x4fee;&#x91cf; &#x624b;&#x5de5;&#x62a5;&#x8868;&#xd;&#xa;--&#x624b;&#x5de5;&#x62a5;&#x8868;&#x4e0d;&#x80fd;&#x7ec6;&#x5206;&#x5230;&#x8f66;&#x578b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_9&#xd;&#xa;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;REPAIR_START_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS PERIOD_WID&#xd;&#xa;      ,NVL&#x28;R.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,NVL&#x28;B.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,NVL&#x28;BR.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,&#x27;-1&#x27; AS MODEL_ID&#xd;&#xa;      ,&#x27;09&#x27; AS INDEX_SEQ&#xd;&#xa;      ,&#x27;&#x8fd4;&#x4fee;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,SUM&#x28;NVL&#x28;T.REPAIR_CNT,0&#x29;&#x29; AS BIZ_QTY&#xd;&#xa;      ,NULL AS BIZ_RATE&#xd;&#xa;      ,NULL AS TB_RATE&#xd;&#xa;      ,NULL AS HB_RATE&#xd;&#xa;  FROM ODS_BLPT.T_HAND_REPAIR T&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG R&#xd;&#xa;    ON T.RGN_NAME &#x3d; R.RGN_NAME&#xd;&#xa;   AND R.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG B&#xd;&#xa;    ON T.BASE_NAME &#x3d; B.BASE_NAME&#xd;&#xa;   AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_BRAND BR&#xd;&#xa;    ON T.BRAND_NAME &#x3d; BR.BRAND_NAME&#xd;&#xa; WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;REPAIR_START_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;       REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa; GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;REPAIR_START_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;&#xd;&#xa;         ,NVL&#x28;R.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;B.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;BR.BRAND_ID, &#x27;-1&#x27;&#x29;&#x3b;</sql>
