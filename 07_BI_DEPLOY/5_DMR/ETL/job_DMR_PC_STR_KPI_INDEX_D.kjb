      <name>TO_TEMP4</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;--04 &#x5e93;&#x5b58;&#x91cf;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_4&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,NVL&#x28;O.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,NVL&#x28;O.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,T.BRAND_ID AS BRAND_ID&#xd;&#xa;      ,T.MODEL_ID AS MODEL_ID&#xd;&#xa;      ,&#x27;04&#x27; AS INDEX_SEQ&#xd;&#xa;      ,&#x27;&#x5e93;&#x5b58;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,SUM&#x28;USE_QTY&#x29; AS BIZ_QTY&#xd;&#xa;      ,SUM&#x28;USE_QTY&#x29; &#x2f; SUM&#x28;MAX_QTY&#x29; AS BIZ_RATE&#xd;&#xa;      ,NULL AS TB_RATE&#xd;&#xa;      ,NULL AS HB_RATE&#xd;&#xa;  FROM &#x28;SELECT NVL&#x28;EMP.PERIOD_WID, REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; AS PERIOD_WID&#xd;&#xa;              ,EMP.RGN_ID&#xd;&#xa;              ,EMP.BASE_ID&#xd;&#xa;              ,EMP.STR_ID&#xd;&#xa;              ,EMP.BRAND_ID&#xd;&#xa;              ,EMP.MODEL_ID&#xd;&#xa;              ,COALESCE&#x28;EMP.USE_QTY, 0&#x29; AS USE_QTY&#xd;&#xa;              ,TOT.MAX_QTY&#xd;&#xa;          FROM &#x28;SELECT A.PERIOD_WID&#xd;&#xa;                      ,A.RGN_ID&#xd;&#xa;                      ,A.BASE_ID&#xd;&#xa;                      ,A.STR_ID&#xd;&#xa;                      ,A.BRAND_ID&#xd;&#xa;                      ,A.MODEL_ID&#xd;&#xa;                      ,SUM&#x28;INVENTORY_QTY&#x29; AS USE_QTY --&#x4f7f;&#x7528;&#x91cf; &#xd;&#xa;                  FROM DMK.DMK_STR_INVENTORY_D A&#xd;&#xa;                 WHERE SUBSTRING&#x28;A.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                       REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                      &#x27;-&#x27;,&#xd;&#xa;                                      &#x27;&#x27;&#x29;&#xd;&#xa;                --AND RGN_ID &#x3c;&#x3e; &#x27;ZMAL&#x2f;ZC&#x2f;-1&#x27;&#xd;&#xa;                 GROUP BY A.PERIOD_WID&#xd;&#xa;                         ,A.RGN_ID&#xd;&#xa;                         ,A.BASE_ID&#xd;&#xa;                         ,A.STR_ID&#xd;&#xa;                         ,A.BRAND_ID&#xd;&#xa;                         ,A.MODEL_ID&#x29; EMP&#xd;&#xa;          LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                          ,BASE_ID&#xd;&#xa;                          ,STR_ID&#xd;&#xa;                          ,SUM&#x28;MAX_STORE_QTY&#x29; AS MAX_QTY --&#x603b;&#x91cf;&#xd;&#xa;                      FROM DMK.DMK_STR_MAXSTORE_M T&#xd;&#xa;                     WHERE PART_DT &#x3d;&#xd;&#xa;                           REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;&#xd;&#xa;                       AND T.PART_SC &#x3d; &#x27;DIM&#x27;&#xd;&#xa;                     GROUP BY RGN_ID&#xd;&#xa;                             ,BASE_ID&#xd;&#xa;                             ,STR_ID&#x29; TOT&#xd;&#xa;            ON TOT.STR_ID &#x3d; EMP.STR_ID&#x29; T&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.STR_ID &#x3d; O.STR_ID&#xd;&#xa;   AND O.ORG_TYPE &#x3d; &#x27;S&#x27;&#xd;&#xa; GROUP BY T.PERIOD_WID&#xd;&#xa;         ,NVL&#x28;O.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;O.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,T.BRAND_ID&#xd;&#xa;         ,T.MODEL_ID&#x3b;&#xd;&#xa;</sql>
