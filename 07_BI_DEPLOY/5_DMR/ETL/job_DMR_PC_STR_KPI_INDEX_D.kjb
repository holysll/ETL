      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;--04 &#x8054;&#x8fd0;&#x8f66;&#x91cf; &#x8054;&#x8fd0;&#x6570;&#x2f;&#x603b;&#x6536;&#x8f66;&#x6570;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_5&#xd;&#xa;SELECT M.PERIOD_WID AS PERIOD_WID&#xd;&#xa;      ,M.RGN_ID AS RGN_ID&#xd;&#xa;      ,M.BASE_ID AS BASE_ID&#xd;&#xa;      ,M.BRAND_ID AS BRAND_ID&#xd;&#xa;      ,M.MODEL_ID AS MODEL_ID&#xd;&#xa;      ,&#x27;05&#x27; AS INDEX_SEQ&#xd;&#xa;      ,&#x27;&#x8054;&#x8fd0;&#x8f66;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,SUM&#x28;M.BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;SUM&#x28;M.ALL_QTY&#x29;, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          SUM&#x28;M.BIZ_QTY&#x29; &#x2f; SUM&#x28;M.ALL_QTY&#x29;&#xd;&#xa;       END AS BIZ_RATE&#xd;&#xa;      ,NULL AS TB_RATE&#xd;&#xa;      ,NULL AS HB_RATE&#xd;&#xa;  FROM &#x28;SELECT T.PERIOD_WID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,SUM&#x28;TRANS_QTY&#x29; AS ALL_QTY&#xd;&#xa;              ,SUM&#x28;TRANS_QTY&#x29; AS BIZ_QTY&#xd;&#xa;          FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;         WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;         GROUP BY T.PERIOD_WID&#xd;&#xa;                 ,RGN_ID&#xd;&#xa;                 ,BASE_ID&#xd;&#xa;                 ,BRAND_ID&#xd;&#xa;                 ,MODEL_ID&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT B.PERIOD_WID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,SUM&#x28;INSTOCK_QTY&#x29; AS ALL_QTY&#xd;&#xa;              ,0 AS BIZ_QTY&#xd;&#xa;          FROM DMK.DMK_STR_OPERQTY_D B&#xd;&#xa;         WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;         GROUP BY B.PERIOD_WID&#xd;&#xa;                 ,RGN_ID&#xd;&#xa;                 ,BASE_ID&#xd;&#xa;                 ,BRAND_ID&#xd;&#xa;                 ,MODEL_ID&#x29; M&#xd;&#xa; GROUP BY M.PERIOD_WID&#xd;&#xa;         ,M.RGN_ID&#xd;&#xa;         ,M.BASE_ID&#xd;&#xa;         ,M.BRAND_ID&#xd;&#xa;         ,M.MODEL_ID&#x3b;&#xd;&#xa;</sql>
