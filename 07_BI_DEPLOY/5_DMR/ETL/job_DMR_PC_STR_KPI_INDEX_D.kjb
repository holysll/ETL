      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_STR_KPI_INDEX_D_2&#xd;&#xa;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;period_wid, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS PERIOD_WID&#xd;&#xa;        ,RGN_ID&#xd;&#xa;        ,BASE_ID&#xd;&#xa;        ,BRAND_ID&#xd;&#xa;        ,MODEL_ID&#xd;&#xa;        ,&#x27;02&#x27; AS INDEX_SEQ&#xd;&#xa;        ,&#x27;&#x51fa;&#x5e93;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;        ,sum&#x28;outstock_qty&#x29; AS BIZ_QTY&#xd;&#xa;        ,NULL AS BIZ_RATE&#xd;&#xa;        ,NULL AS TB_RATE&#xd;&#xa;        ,NULL AS HB_RATE&#xd;&#xa;    FROM DMK.DMK_STR_OPERQTY_D&#xd;&#xa;   WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;period_wid, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;         REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;   GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;period_wid, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;&#xd;&#xa;           ,RGN_ID&#xd;&#xa;           ,BASE_ID&#xd;&#xa;           ,BRAND_ID&#xd;&#xa;           ,MODEL_ID&#x3b;&#xd;&#xa;</sql>
