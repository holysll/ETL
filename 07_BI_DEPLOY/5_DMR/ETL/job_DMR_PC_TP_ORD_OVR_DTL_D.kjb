      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMR&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_ORD_OVR_DTL_D PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID AS DATE_ID --&#x65e5;&#x671f;ID&#xd;&#xa;      ,T.SUPPLIER_ID --&#x627f;&#x8fd0;&#x5546;ID&#xd;&#xa;      ,A.SERVPROV_NAME AS SUPPLIER_NAME --&#x627f;&#x8fd0;&#x5546;&#x540d;&#x79f0;&#xd;&#xa;      ,T.TPMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;      ,B.TPMODE_NAME --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x540d;&#x79f0;&#xd;&#xa;      ,T.ORDER_RELEASE_GID AS ORD_ID --&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;      ,T.VIN --VIN&#xd;&#xa;      ,T.SOURCE_PRVN_ID --&#x59cb;&#x53d1;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;      ,T.SOURCE_CTY_ID -- &#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;      ,T.DEST_PRVN_ID --&#x76ee;&#x7684;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;      ,T.DEST_CTY_ID --&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;      ,T.OUT_STORE_DAY --&#x51fa;&#x5e93;&#x65e5;&#x2f;&#x4e2d;&#x8f6c;&#x51fa;&#x5e93;&#x65e5;&#x28;&#x542f;&#x8fd0;&#x29;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;&#x28;UNIX_TIMESTAMP&#x28;T.OUT_STORE_DAY,&#xd;&#xa;                                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x2b;&#xd;&#xa;                     &#x28;3600 &#x2a; 24 &#x2a; CAST&#x28;T.ON_PASSAGE_DAYS AS INT&#x29;&#x29;&#x29;,&#xd;&#xa;                     &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS TGT_ARRIVAL_DAY --&#x8ba1;&#x5212;&#x5230;&#x8fbe;&#x65e5;&#xd;&#xa;      ,T.ARRIVAL_DAY --&#x5230;&#x6e2f;&#x65e5;&#x671f;-&#x4ea4;&#x4ed8