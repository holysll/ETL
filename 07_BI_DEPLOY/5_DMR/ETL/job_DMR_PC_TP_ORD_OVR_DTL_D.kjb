    <entry>
      <name>TO_TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_OVR_DTL_D_1&#xd;&#xa;SELECT D.PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;SUPPLIER_ID, &#x27;-1&#x27;&#x29; AS SUPPLIER_ID --&#x627f;&#x8fd0;&#x5546;ID&#xd;&#xa;      ,TRANSPORTMODE_ID AS TPMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;      ,ORDER_RELEASE_GID --OTM&#x5355;&#x53f7;&#xd;&#xa;      ,VIN --VIN&#xd;&#xa;      ,SOURCE_PRVN_ID AS SOURCE_PRVN_ID --&#xd;&#xa;      ,SOURCE_CTY_ID AS SOURCE_CTY_ID --&#xd;&#xa;      ,DEST_PRVN_ID AS DEST_PRVN_ID --&#xd;&#xa;      ,DEST_CTY_ID AS DEST_CTY_ID --&#xd;&#xa;      ,OUT_STORE_DAY AS OUT_STORE_DAY --&#xd;&#xa;      ,FACTORY_DAY --&#x53d1;&#x8fd0;&#x65f6;&#x95f4;&#xd;&#xa;      ,ON_PASSAGE_DAYS --&#x6807;&#x51c6;&#x5728;&#x9014;&#x5929;&#x6570;&#xd;&#xa;      ,ARRIVAL_DAY --&#x5230;&#x8fbe;&#x65f6;&#x95f4;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN unix_timestamp&#x28;concat&#x28;D.period_date,&#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;T.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;T.on_passage_days,0&#x29;&#xd;&#xa;         THEN&#xd;&#xa;          &#x27;Y&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;N&#x27;&#xd;&#xa;       END AS IS_OUTTINE --&#x662f;&#x5426;&#x8d85;&#x65f6;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN unix_timestamp&#x28;concat&#x28;D.period_date,&#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;T.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;T.on_passage_days,0&#x29;&#xd;&#xa;         THEN&#xd;&#xa;          UNIX_TIMESTAMP&#x28;NVL&#x28;D.PERIOD_DATE, &#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#xd;&#xa;                         &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; -&#xd;&#xa;          UNIX_TIMESTAMP&#x28;FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; -&#xd;&#xa;          24 &#x2a; 3600 &#x2a; NVL&#x28;T.ON_PASSAGE_DAYS,0&#x29;&#xd;&#xa;         ELSE&#xd;&#xa;          0&#xd;&#xa;       END AS OVERTM_TIME --&#xd;&#xa;  FROM DIM.DIM_PERIOD_D D&#xd;&#xa;  JOIN DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;    ON 1 &#x3d; 1&#xd;&#xa; WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;FACTORY_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d; D.PERIOD_WID&#xd;&#xa;   AND NVL&#x28;SUBSTR&#x28;REGEXP_REPLACE&#x28;NVL&#x28;DLVR_DAY,ARRIVAL_DAY&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;, &#x27;29991231&#x27;&#x29; &#x3e;&#xd;&#xa;       D.PERIOD_WID&#xd;&#xa;   AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;    AND substr&#x28;D.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT A.period_wid&#xd;&#xa;    ,COALESCE&#x28;B.supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;    ,B.segmetn_transportmode AS TPMODE_ID&#xd;&#xa;    ,B.order_release_gid AS ORDER_RELEASE_GID&#xd;&#xa;    ,B.VIN&#xd;&#xa;    ,B.source_prvn_id AS SOURCE_PRVN_ID&#xd;&#xa;    ,B.source_cty_id AS SOURCE_CTY_ID&#xd;&#xa;    ,B.dest_prvn_id AS DEST_PRVN_ID&#xd;&#xa;    ,B.dest_cty_id AS DEST_CTY_ID&#xd;&#xa;    ,B.out_store_day AS OUT_STORE_DAY&#xd;&#xa;    ,B.on_passage_days AS ON_PASSAGE_DAYS&#xd;&#xa;    ,B.arrival_day AS ARRIVAL_DAY&#xd;&#xa;    ,CASE WHEN &#x28;B.arrival_day IS NOT NULL AND UNIX_TIMESTAMP&#x28;B.arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x3e; 3600&#x2a;24&#x2a;B.on_passage_days&#x29; &#xd;&#xa;        OR &#x28;B.arrival_day IS NULL AND UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#x3c; unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29; THEN &#x27;Y&#x27; ELSE &#x27;N&#x27; END AS IS_OUTTIME&#xd;&#xa;    ,CASE WHEN &#x28;B.arrival_day IS NOT NULL AND UNIX_TIMESTAMP&#x28;B.arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x3e; 3600&#x2a;24&#x2a;B.on_passage_days&#x29;&#xd;&#xa;            THEN &#x28;UNIX_TIMESTAMP&#x28;B.arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-3600&#x2a;24&#x2a;B.on_passage_days&#x29;&#x2f;&#x28;3600&#x2a;24&#x29;&#xd;&#xa;          WHEN &#x28;B.arrival_day IS NULL AND UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#x3c; unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;            THEN &#x28;unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29; - UNIX_TIMESTAMP&#x28;B.out_store_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days&#x29;&#x2f;&#x28;3600&#x2a;24&#x29; &#xd;&#xa;          ELSE 0 END AS OVERTM_TIME&#xd;&#xa;FROM DIM.DIM_PERIOD_D A  &#xd;&#xa;LEFT JOIN DW.DW_TP_ORDERMOVEMENT B&#xd;&#xa;ON 1&#x3d;1&#xd;&#xa;LEFT JOIN DW.DW_TP_ORDERRELEASE C&#xd;&#xa;ON 1&#x3d;1&#xd;&#xa;WHERE 1&#x3d;1 &#xd;&#xa;    AND A.period_wid &#x3d; &#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#xd;&#xa;    AND A.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;    AND C.or_plan_type &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27; &#xd;&#xa;    AND B.brand_id &#x3d; &#x27;CA&#x27;&#xd;&#xa;    AND A.period_wid &#x3e;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;B.out_store_day, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    AND A.period_wid &#x3c; NVL&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;B.arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;,&#x27;29991231&#x27;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT A.period_wid&#xd;&#xa;    ,COALESCE&#x28;C.supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;    ,C.segmetn_transportmode AS TPMODE_ID&#xd;&#xa;    ,B.order_release_gid AS ORDER_RELEASE_GID&#xd;&#xa;    ,B.VIN&#xd;&#xa;    ,B.src_prvn_id AS SOURCE_PRVN_ID&#xd;&#xa;    ,B.src_cty_id AS SOURCE_CTY_ID&#xd;&#xa;    ,B.dest_prvn_id AS DEST_PRVN_ID&#xd;&#xa;    ,B.dest_cty_id AS DEST_CTY_ID&#xd;&#xa;    ,B.factory_day AS OUT_STORE_DAY&#xd;&#xa;    ,B.on_passage_days AS ON_PASSAGE_DAYS&#xd;&#xa;    ,B.dlvr_day AS ARRIVAL_DAY&#xd;&#xa;    ,CASE WHEN &#x28;B.dlvr_day IS NOT NULL AND UNIX_TIMESTAMP&#x28;B.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x3e; 3600&#x2a;24&#x2a;B.on_passage_days&#x29; &#xd;&#xa;        OR &#x28;B.dlvr_day IS NULL AND UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#x3c; unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29; THEN &#x27;Y&#x27; ELSE &#x27;N&#x27; END AS IS_OUTTIME&#xd;&#xa;        ,CASE WHEN &#x28;B.dlvr_day IS NOT NULL AND UNIX_TIMESTAMP&#x28;B.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; &#x3e; 3600&#x2a;24&#x2a;B.on_passage_days&#x29;&#xd;&#xa;            THEN UNIX_TIMESTAMP&#x28;B.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;-3600&#x2a;24&#x2a;B.on_passage_days&#xd;&#xa;          WHEN &#x28;B.dlvr_day IS NULL AND UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#x3c; unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x29;&#xd;&#xa;            THEN unix_timestamp&#x28;DATE_ADD&#x28;A.period_date,1&#x29;,&#x27;yyyy-MM-dd&#x27;&#x29; - UNIX_TIMESTAMP&#x28;B.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x2b;3600&#x2a;24&#x2a;B.on_passage_days &#xd;&#xa;          ELSE 0 END AS OVERTM_TIME&#xd;&#xa;FROM DIM.DIM_PERIOD_D A  &#xd;&#xa;JOIN DW.DW_TP_ORDERRELEASE B&#xd;&#xa;ON 1&#x3d;1&#xd;&#xa;JOIN DW.DW_TP_ORDERMOVEMENT C&#xd;&#xa;ON 1&#x3d;1&#xd;&#xa;WHERE 1&#x3d;1 &#xd;&#xa;    AND A.period_wid &#x3d; &#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#xd;&#xa;    AND A.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;    AND B.or_plan_type &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27; &#xd;&#xa;    AND B.brand_id &#x3c;&#x3e; &#x27;CA&#x27;&#xd;&#xa;    AND A.period_wid &#x3e;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;B.factory_day, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    AND A.period_wid &#x3c; NVL&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;B.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;,&#x27;29991231&#x27;&#x29;&#xd;&#xa;&#x2a;&#x2f;</sql>
