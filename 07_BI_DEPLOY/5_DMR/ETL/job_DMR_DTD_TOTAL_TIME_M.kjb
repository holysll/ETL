    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_DTD_TOTAL_TIME	DMR&#x5927;&#x5c4f;&#x7aef;DTD&#x603b;&#x65f6;&#x957f;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;06_DMR&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_DTD_TOTAL_TIME PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;--DTD&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#x91cf;&#x6307;&#x7684;&#x662f;&#xd;&#xa;SELECT coalesce&#x28;A.MONTH_ID,B.MONTH_ID,&#x27;-1&#x27;&#x29; AS MONTH_ID   --&#x6708;&#x4efd;ID&#xd;&#xa;    ,coalesce&#x28;A.TPMODE_CODE,B.TPMODE_CODE&#x29; AS TPMODE_CODE&#xd;&#xa;    ,coalesce&#x28;A.DTD_AVG_TIME,&#x27;UNKNOW&#x27;&#x29; AS DTD_AVG_TIME  --DTD&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;    ,coalesce&#x28;A.DTD_OVER_TIME_ORDER_CNT,&#x27;UNKNOW&#x27;&#x29; AS DTD_OVER_TIME_ORDER_CNT --DTD&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#x91cf;&#xd;&#xa;    ,coalesce&#x28;B.DTD_AVG_TOTAL_TIME,&#x27;UNKONW&#x27;&#x29; AS DTD_AVG_TOTAL_TIME  --&#x5e73;&#x5747;&#x603b;&#x8017;&#x65f6;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;                        AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;                        AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;    ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID   --&#x6708;&#x4efd;ID&#xd;&#xa;        ,T1.transportmode_id AS TPMODE_CODE --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x4ee3;&#x7801;&#xd;&#xa;        ,AVG&#x28;&#x28;UNIX_TIMESTAMP&#x28;T1.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600&#x29;&#x2f;24 AS DTD_AVG_TIME  --DTD&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;        ,SUM&#x28;CASE WHEN  &#x28;UNIX_TIMESTAMP&#x28;T1.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x2f;3600&#x29; &#x3e; ON_PASSAGE_DAYS&#x2a;24&#x2a;3600 THEN 1&#xd;&#xa;                      ELSE 0&#xd;&#xa;                 END&#x29; AS DTD_OVER_TIME_ORDER_CNT    --DTD&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#x91cf;&#xd;&#xa;    FROM TEMP.TEMP_DMR_DTD_TOTAL_TIME  T1&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;T1.arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    GROUP BY T1.transportmode_id&#xd;&#xa;    &#x29; A&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID   --&#x6708;&#x4efd;ID&#xd;&#xa;        ,T2.transportmode_id AS TPMODE_CODE --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x4ee3;&#x7801;&#xd;&#xa;        ,AVG&#x28;&#x28;UNIX_TIMESTAMP&#x28;T2.RECEIPT_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T2.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600&#x29;&#x2f;24 AS DTD_AVG_TOTAL_TIME  --&#x5e73;&#x5747;&#x603b;&#x8017;&#x65f6;&#xd;&#xa;    FROM TEMP.TEMP_DMR_DTD_TOTAL_TIME  T2&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;T2.receipt_date,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    GROUP BY T2.transportmode_id&#xd;&#xa;    &#x29; B&#xd;&#xa;ON A.MONTH_ID &#x3d; B.MONTH_ID AND A.TPMODE_CODE &#x3d; B.TPMODE_CODE&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_DTD_TOTAL_TIME PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT   SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;                            AS MONTH_ID                        --&#x6708;&#x4efd;ID&#xd;&#xa;        ,T1.TRANSPORTMODE_ID                                                      AS TPMODE_CODE                     --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x4ee3;&#x7801;&#xd;&#xa;        ,AVG&#x28;&#x28;UNIX_TIMESTAMP&#x28;T1.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600&#x29; AS DTD_AVG_TIME  --DTD&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;        ,SUM&#x28;CASE WHEN  &#x28;UNIX_TIMESTAMP&#x28;T1.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x2f;3600&#x29; &#x3e; ON_PASSAGE_DAYS&#x2a;24&#x2a;3600 THEN 1&#xd;&#xa;                  ELSE 0&#xd;&#xa;             END&#x29;                                                                      AS DTD_OVER_TIME_ORDER_CNT         --DTD&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x6570;&#x91cf;&#xd;&#xa;        ,AVG&#x28;&#x28;UNIX_TIMESTAMP&#x28;T1.RECEIPT_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600&#x29; AS DTD_AVG_TOTAL_TIME  --&#x5e73;&#x5747;&#x603b;&#x8017;&#x65f6;&#xd;&#xa;        ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;                        AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;        ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;                        AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;        ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;        ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM    TEMP.TEMP_DMR_DTD_TOTAL_TIME  T1&#xd;&#xa;GROUP BY   T1.TRANSPORTMODE_ID&#xd;&#xa;&#x2a;&#x2f;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>HIVE</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>448</xloc>
      <yloc>160</yloc>
      </entry>
    <entry>
      <name>TO_TEMP</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_DTD_TOTAL_TIME&#xd;&#xa;SELECT &#xd;&#xa;    T.PROJ_NAME AS PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;    ,T.VIN AS VIN  --VIN&#xd;&#xa;    ,T.BRAND_ID AS BRAND_ID  --&#x54c1;&#x724c;&#xd;&#xa;    ,T.ORDER_RELEASE_NAME AS ORDER_RELEASE_NAME  --&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;    ,T.SEQ_NO AS SEQ_NO  --&#x8fd0;&#x8f93;&#x6bb5;&#xd;&#xa;    ,T.ORDER_MOVEMENT_GID  --&#x5206;&#x6bb5;&#x8def;&#x7531;GID&#xd;&#xa;    ,T.TRANSPORTMODE_ID AS TRANSPORTMODE_ID  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;    ,T.EARLY_PICKUP_DATE AS EARLY_PICKUP_DATE  --&#x63a5;&#x5355;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.ALLOT_PLN_DAY AS ALLOT_PLN_DAY  --&#x5206;&#x914d;&#x65f6;&#x95f4; &#x82e5;&#x6ca1;&#x6709;&#x5219;&#x53d6;&#x627f;&#x8fd0;&#x5546;&#x6700;&#x65b0;&#x786e;&#x8ba4;&#x65f6;&#x95f4;  &#xd;&#xa;    ,T.OUT_STORE_DAY AS OUT_STORE_DAY  --&#x51fa;&#x5e93;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.FACTORY_DAY AS FACTORY_DAY  --&#x6253;&#x5355;&#x65f6;&#x95f4;  &#x51fa;&#x573a;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.ARRIVAL_DAY AS ARRIVAL_DAY  --&#x4ea4;&#x4ed8;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.RECEIPT_DATE AS RECEIPT_DATE  --&#x56de;&#x5355;&#x65f6;&#x95f4;&#xd;&#xa;    ,T.BILL_DATE AS BILL_DATE  --&#x7ed3;&#x7b97;&#x65f6;&#x95f4;&#x28;&#x4e0e;&#x627f;&#x8fd0;&#x5546;&#xff09;&#xd;&#xa;    ,T.DETAIL_FLAG AS DETAIL_FLAG  --&#x6563;&#x5355;&#x6807;&#x5fd7;&#x28;&#x6563;&#x5355;&#x7684;&#x5224;&#x65ad;&#x903b;&#x8f91;&#x29;&#xd;&#xa;	,T.ON_PASSAGE_DAYS AS ON_PASSAGE_DAYS&#xd;&#xa;FROM DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;WHERE &#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.RECEIPT_DATE, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&