    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW_TP_ORDERRELEASE &#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_TP_ARRIVE_D &#x8fd0;&#x8f93;&#x5230;&#x8fbe;&#x91cf;&#x7edf;&#x8ba1;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;02_&#x9700;&#x6c42;&#x5206;&#x6790;&#x5c;02_&#x4fe1;&#x606f;&#x7cfb;&#x7edf;&#x5206;&#x6790;&#x5c;01_&#x6e90;&#x7cfb;&#x7edf;&#x8d44;&#x6599;&#x5c;OTM&#x5c;&#x5176;&#x4ed6;OTM&#x524d;&#x53f0;&#x53c2;&#x8003;&#x5c;&#x957f;&#x5b89;&#x798f;&#x7279;&#x516c;&#x8def;&#x5728;&#x9014;&#x8ddf;&#x8e2a;&#x8868;.sql&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_AVG_LOAD_RATE PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,T1.RGN_NAME&#xd;&#xa;      ,T.CUR_M_ALL_QTY&#xd;&#xa;      ,T.CUR_M_USE_QTY&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.CUR_M_ALL_QTY &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         WHEN T.CUR_M_USE_QTY &#x3e; T.CUR_M_ALL_QTY THEN 1  &#xd;&#xa;         ELSE&#xd;&#xa;          T.CUR_M_USE_QTY &#x2f; T.CUR_M_ALL_QTY&#xd;&#xa;       END AS USE_RATE&#xd;&#xa;      ,T.LST_Y_M_ALL_QTY&#xd;&#xa;      ,T.LST_Y_M_USE_QTY&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;T.LST_Y_M_ALL_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;          WHEN T.LST_Y_M_USE_QTY &#x3e; T.LST_Y_M_ALL_QTY THEN 1&#xd;&#xa;         ELSE&#xd;&#xa;          T.LST_Y_M_USE_QTY &#x2f; T.LST_Y_M_ALL_QTY&#xd;&#xa;       END AS USE_RATE_LY&#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;      ,T.PERIOD_WID AS PART_DT&#xd;&#xa;FROM TEMP.TEMP_DMR_OV_AVG_LOAD_RATE_1 T&#xd;&#xa;LEFT JOIN &#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
