    <entry>
      <name>TO_TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_AVG_LOAD_RATE_1  &#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;T.factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN nvl&#x28;MAX_CAR_QTY,0&#x29; ELSE 0 END&#x29; AS CUR_M_ALL_QTY&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;T.factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN nvl&#x28;VIN_QTY,0&#x29; ELSE 0 END&#x29; AS CUR_M_USE_QTY&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN nvl&#x28;MAX_CAR_QTY,0&#x29; ELSE 0 END&#x29; AS LST_Y_M_ALL_QTY&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN nvl&#x28;VIN_QTY,0&#x29; ELSE 0 END&#x29; AS LST_Y_M_USE_QTY&#xd;&#xa;FROM&#x28;&#xd;&#xa;SELECT factory_day&#xd;&#xa;        ,A.RGN_ID&#xd;&#xa;        ,A.budget_shipment_gid&#xd;&#xa;        ,A.lic_no&#xd;&#xa;        ,A.VIN_QTY&#xd;&#xa;        ,B.MAX_CAR_QTY&#xd;&#xa;    FROM &#x28;&#xd;&#xa;        SELECT CASE WHEN T.brand_id &#x3d; &#x27;FT&#x27; THEN T1.factory_day WHEN T.brand_id &#x3d; &#x27;CA&#x27; THEN T.unload_day END AS FACTORY_DAY&#xd;&#xa;            ,coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,T.budget_shipment_gid&#xd;&#xa;            ,T.lic_no,count&#x28;T.vin&#x29; AS VIN_QTY &#xd;&#xa;        FROM DW.dw_tp_ordermovement  T   &#xd;&#xa;        INNER JOIN DW.dw_tp_orderrelease T1&#xd;&#xa;        ON T.order_release_gid &#x3d; T1.order_release_gid&#xd;&#xa;        WHERE T.perspective &#x3d; &#x27;B&#x27;&#xd;&#xa;        AND T.segmetn_transportmode &#x3d; &#x27;GL&#x27;&#xd;&#xa;        AND &#x28;substr&#x28;regexp_replace&#x28;CASE WHEN T.brand_id &#x3d; &#x27;FT&#x27; THEN T1.factory_day WHEN T.brand_id &#x3d; &#x27;CA&#x27; THEN T.unload_day END,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            OR substr&#x28;regexp_replace&#x28;CASE WHEN T.brand_id &#x3d; &#x27;FT&#x27; THEN T1.factory_day WHEN T.brand_id &#x3d; &#x27;CA&#x27; THEN T.unload_day END,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            &#x29;&#xd;&#xa;        GROUP BY CASE WHEN T.brand_id &#x3d; &#x27;FT&#x27; THEN T1.factory_day WHEN T.brand_id &#x3d; &#x27;CA&#x27; THEN T.unload_day END&#xd;&#xa;            ,coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,T.budget_shipment_gid&#xd;&#xa;            ,T.lic_no &#xd;&#xa;        &#x29; A&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT power_unit_id,power_unit_xid,if&#x28;max_car_qty &#x3e;&#x3d; max_minicar_qty, max_car_qty,max_minicar_qty&#x29; AS MAX_CAR_QTY&#xd;&#xa;        FROM DIM.dim_power_unit&#xd;&#xa;        &#x29; B &#xd;&#xa;    ON A.lic_no &#x3d; B.power_unit_id  &#xd;&#xa;    &#x29; T&#xd;&#xa;left join &#x28;&#xd;&#xa;    select rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    where org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    group by rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.RGN_ID &#x3d; T1.rgn_id&#xd;&#xa;GROUP BY T.RGN_ID&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT X.PERIOD_WID&#xd;&#xa;      ,X.RGN_ID&#xd;&#xa;      ,SUM&#x28;X.MAX_CAR_QTY&#x29; AS ALL_QTY&#xd;&#xa;      ,SUM&#x28;X.TOTAL_SHIP_UNIT_COUNT&#x29; AS USE_QTY&#xd;&#xa;  FROM &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;T.PERIOD_WID, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PERIOD_WID&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,T.BASE_ID&#xd;&#xa;              ,T.SHIPMENT_GID&#xd;&#xa;              ,T.POWER_UNIT_ID&#xd;&#xa;              ,MAX&#x28;T.TOTAL_SHIP_UNIT_COUNT&#x29; AS TOTAL_SHIP_UNIT_COUNT&#xd;&#xa;              ,MAX&#x28;CASE&#xd;&#xa;                     WHEN T.TOTAL_SHIP_UNIT_COUNT &#x3e; T.MAX_CAR_QTY THEN&#xd;&#xa;                      T.MAX_MINICAR_QTY&#xd;&#xa;                     ELSE&#xd;&#xa;                      T.MAX_CAR_QTY&#xd;&#xa;                   END&#x29; AS MAX_CAR_QTY&#xd;&#xa;          FROM &#x28;SELECT M.EARLY_PICKUP_DATE AS PERIOD_WID&#xd;&#xa;                      ,M.BASE_ID&#xd;&#xa;                      ,M.BASE_NAME&#xd;&#xa;                      ,M.RGN_ID&#xd;&#xa;                      ,M.SUPPLIER_ID&#xd;&#xa;                      ,M.BRAND_ID&#xd;&#xa;                      ,M.BUDGET_SHIPMENT_GID&#xd;&#xa;                      ,S.SHIPMENT_GID&#xd;&#xa;                      ,S.SHIPMENT_TYPE_GID&#xd;&#xa;                      ,M.SEGMETN_TRANSPORTMODE&#xd;&#xa;                      ,S.TRANSPORT_MODE_ID&#xd;&#xa;                      ,M.VIN&#xd;&#xa;                      ,P.POWER_UNIT_ID&#xd;&#xa;                      ,S.TOTAL_SHIP_UNIT_COUNT&#xd;&#xa;                      ,P.MAX_CAR_QTY&#xd;&#xa;                      ,P.MAX_MINICAR_QTY&#xd;&#xa;                  FROM DW.DW_TP_SHIPMENT S&#xd;&#xa;                  JOIN DW.DW_TP_ORDERMOVEMENT M&#xd;&#xa;                    ON S.SHIPMENT_GID &#x3d; M.BUDGET_SHIPMENT_GID&#xd;&#xa;                  LEFT JOIN DIM.DIM_POWER_UNIT P&#xd;&#xa;                    ON S.POWER_UNIT_GID &#x3d; P.POWER_UNIT_ID&#xd;&#xa;                  JOIN DW.DW_TP_ORDERRELEASE R&#xd;&#xa;                    ON M.ORDER_RELEASE_GID &#x3d; R.ORDER_RELEASE_GID&#xd;&#xa;                 WHERE M.SEGMETN_TRANSPORTMODE &#x3d; &#x27;GL&#x27;&#xd;&#xa;                   AND S.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;                   AND SUBSTR&#x28;REGEXP_REPLACE&#x28;M.EARLY_PICKUP_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                              1,&#xd;&#xa;                              6&#x29; &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#x29; AS T&#xd;&#xa;         GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;T.PERIOD_WID, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                 ,T.RGN_ID&#xd;&#xa;                 ,T.BASE_ID&#xd;&#xa;                 ,T.SHIPMENT_GID&#xd;&#xa;                 ,T.POWER_UNIT_ID&#x29; AS X&#xd;&#xa; GROUP BY X.PERIOD_WID&#xd;&#xa;         ,X.RGN_ID&#x3b;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>368</xloc>
      <yloc>288</yloc>
      </entry>
    <entry>
      <name>CREATE_TEMP_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Udf&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_OV_AVG_LOAD_RATE_1&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_OV_AVG_LOAD_RATE_1&#x28;&#xd;&#xa;PERIOD_WID                    STRING                             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;RGN_ID                        STRING                             COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;CUR_M_ALL_QTY                       BIGINT                             COMMENT  &#x27;&#x53ef;&#x7528;&#x88c5;&#x8f7d;&#x91cf;&#x27;,&#xd;&#xa;CUR_M_USE_QTY                       BIGINT                             COMMENT  &#x27;&#x5b9e;&#x9645;&#x88c5;&#x8f7d;&#x91cf;&#x27;,&#xd;&#xa;LST_Y_M_ALL_QTY                       BIGINT                             COMMENT  &#x27;&#x540c;&#x671f;&#x53ef;&#x7528;&#x88c5;&#x8f7d;&#x91cf;&#x27;,&#xd;&#xa;LST_Y_M_USE_QTY                       BIGINT                             COMMENT  &#x27;&#x540c;&#x671f;&#x5b9e;&#x9645;&#x88c5;&#x8f7d;&#x91cf;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x5f53;&#x6708;&#x6ee1;&#x8f7d;&#x60c5;&#x51b5;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
