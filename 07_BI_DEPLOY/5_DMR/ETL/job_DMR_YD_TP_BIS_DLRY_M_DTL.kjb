    <entry>
      <name>TO_TMP</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_TP_BIS_DLRY_M_DTL_01&#xd;&#xa;SELECT COALESCE&#x28;RGN_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;BASE_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;BRAND_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       SUM&#x28;CASE WHEN SUBSTR&#x28;ARRIVAL_DAY,1,7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29; &#xd;&#xa;            AND DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29; &#x3e;&#x3d; SUBSTR&#x28;ARRIVAL_DAY,1,10&#x29;&#xd;&#xa;              THEN 1 ELSE 0 END&#x29; AS DEL_OT_QTY_MON,&#xd;&#xa;       SUM&#x28;CASE WHEN SUBSTR&#x28;ARRIVAL_DAY,1,7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29; THEN 1 ELSE 0 END&#x29; AS DEL_QTY_MON&#xd;&#xa;  FROM DMK.DMK_TP_DTD_PROG_TIME_D&#xd;&#xa;    WHERE SUBSTR&#x28;ARRIVAL_DAY,1,7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;&#xd;&#xa;      GROUP BY RGN_ID,&#xd;&#xa;               BASE_ID,&#xd;&#xa;               BRAND_ID&#xd;&#xa;        WITH CUBE&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_TP_BIS_DLRY_M_DTL_02&#xd;&#xa;SELECT TMP.RGN_ID,&#xd;&#xa;       CASE WHEN TMP.RGN_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       TMP.BASE_ID,&#xd;&#xa;       CASE WHEN TMP.BASE_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       TMP.BRAND_ID,&#xd;&#xa;       CASE WHEN TMP.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;BRD.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       &#x27;&#x4ea4;&#x4ed8;&#x27; AS TP_PROCESS,&#xd;&#xa;       ROUND&#x28;TMP.DEL_OT_QTY_MON &#x2f; TMP.DEL_QTY_MON &#x2a; 100,2&#x29;&#xd;&#xa;  FROM TEMP.TEMP_DMR_YD_TP_BIS_DLRY_M_DTL_01 TMP&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;           ON TMP.RGN_ID &#x3d; RGN.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;           ON TMP.BASE_ID &#x3d; BAS.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;           ON TMP.BRAND_ID &#x3d; BRD.BRAND_ID&#xd;&#xa;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
