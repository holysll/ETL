      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_TP_BIS_DLRY_M_DTL PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;    ,T.rgn_id AS RNG_ID&#xd;&#xa;    ,CASE WHEN T.rgn_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.rgn_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.rgn_name END AS RNG_NAME&#xd;&#xa;    ,T.base_id&#xd;&#xa;    ,CASE WHEN T.base_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.base_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.base_name END AS BASE_NAME&#xd;&#xa;    ,T.brand_id&#xd;&#xa;    ,CASE WHEN T.brand_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.brand_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.brand_name END AS BRAND_NAME&#xd;&#xa;    ,&#x27;&#x4ea4;&#x4ed8;&#x27; AS TP_PROCESS&#xd;&#xa;    ,sum&#x28;CUR_M_DELY_OT_QTY&#x29;&#x2f;sum&#x28;CUR_M_TGT_DELY_QTY&#x29;&#x2a;100 AS CUR_M_ONTIME_ARV_RATE  --&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;A.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;A.BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;A.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,sum&#x28;CUR_M_TGT_DELY_QTY&#x29; AS CUR_M_TGT_DELY_QTY  --&#x5f53;&#x6708;&#x5e94;&#x5f53;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;        ,sum&#x28;CUR_M_DELY_OT_QTY&#x29; AS CUR_M_DELY_OT_QTY  --&#xd;&#xa;    FROM&#x28;&#xd;&#xa;        SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;            ,count&#x28;DISTINCT CASE WHEN substr&#x28;regexp_replace&#x28;date_add&#x28;to_date&#x28;factory_day&#x29;,cast&#x28;on_passage_days AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                                    AND substr&#x28;regexp_replace&#x28;to_date&#x28;arrival_day&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                                    AND unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3c;&#x3d; 24&#x2a;3600&#x2a;on_passage_days THEN VIN ELSE NULL END&#x29; AS CUR_M_DELY_OT_QTY &#xd;&#xa;            ,0 AS CUR_M_TGT_DELY_QTY&#xd;&#xa;&#xd;&#xa;        FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;        WHERE &#xd;&#xa;            substr&#x28;regexp_replace&#x28;date_add&#x28;to_date&#x28;factory_day&#x29;,cast&#x28;on_passage_days AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;            ,0 AS CUR_M_DELY_OT_QTY&#xd;&#xa;            ,count&#x28;DISTINCT CASE WHEN substr&#x28;regexp_replace&#x28;date_add&#x28;to_date&#x28;factory_day&#x29;,cast&#x28;on_passage_days AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VIN ELSE NULL END&#x29; AS CUR_M_TGT_DELY_QTY&#xd;&#xa;        FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;        GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        &#x29; A&#xd;&#xa;    GROUP BY A.RGN_ID,A.BASE_ID,A.BRAND_ID WITH CUBE    &#xd;&#xa;    &#x29; T&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT base_id,base_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY base_id,base_name&#xd;&#xa;    &#x29; T2&#xd;&#xa;ON T.base_id &#x3d; T2.base_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.dim_brand &#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29; T3&#xd;&#xa;ON T.brand_id &#x3d; T3.brand_id&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;GROUP BY T.RGN_ID, T1.rgn_name,T.BASE_ID, T2.base_name,T.BRAND_ID, T3.brand_name&#xd;&#xa;&#x3b;</sql>
