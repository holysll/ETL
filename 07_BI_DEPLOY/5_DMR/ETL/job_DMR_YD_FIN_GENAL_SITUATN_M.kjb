    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_FIN_GENAL_SITUATN_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT month_id&#xd;&#xa;    ,rgn_id&#xd;&#xa;    ,rgn_name&#xd;&#xa;    ,base_id&#xd;&#xa;    ,base_name&#xd;&#xa;    ,brand_id&#xd;&#xa;    ,brand_name&#xd;&#xa;    ,proj_id&#xd;&#xa;    ,proj_name&#xd;&#xa;    ,tm_type&#xd;&#xa;    ,fin_biz_qty&#xd;&#xa;    ,fin_income&#xd;&#xa;    ,fin_tgt_income&#xd;&#xa;    ,fin_income_cmp_rate&#xd;&#xa;    ,fin_income_mom&#xd;&#xa;    ,fin_income_yoy&#xd;&#xa;    ,fin_profit&#xd;&#xa;    ,fin_tgt_profit&#xd;&#xa;    ,fin_profit_cmp_rate&#xd;&#xa;    ,fin_profit_mom&#xd;&#xa;    ,fin_profit_yoy&#xd;&#xa;    ,fin_profit_rate&#xd;&#xa;    ,CASE WHEN tm_type &#x3d; &#x27;M&#x27; THEN IF&#x28;fin_income_cmp_rate &#x3e;&#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,9,2&#x29;&#x2f;DATEDIFF&#x28;CONCAT&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1&#x29;,1,8&#x29;,&#x27;01&#x27;&#x29;,CONCAT&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,8&#x29;,&#x27;01&#x27;&#x29;&#x29;&#x2a;100, 1, -1&#x29;&#xd;&#xa;		WHEN tm_type &#x3d; &#x27;Y&#x27;THEN IF&#x28;fin_income_cmp_rate &#x3e;&#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,6,2&#x29;&#x2f;12&#x2a;100, 1, -1&#x29; ELSE NULL END AS INCOME_CMP_WRN_FLAG&#xd;&#xa;    ,CASE WHEN tm_type &#x3d; &#x27;M&#x27; THEN IF&#x28;fin_profit_cmp_rate &#x3e;&#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,9,2&#x29;&#x2f;DATEDIFF&#x28;CONCAT&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1&#x29;,1,8&#x29;,&#x27;01&#x27;&#x29;,CONCAT&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,8&#x29;,&#x27;01&#x27;&#x29;&#x29;&#x2a;100, 1, -1&#x29;&#xd;&#xa;		WHEN tm_type &#x3d; &#x27;Y&#x27;THEN IF&#x28;fin_profit_cmp_rate &#x3e;&#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,6,2&#x29;&#x2f;12&#x2a;100, 1, -1&#x29; ELSE NULL END AS PROFIT_CMP_WRN_FLAG&#xd;&#xa;    ,IF&#x28;FIN_INCOME_MOM &#x3e;&#x3d; 0, 1, -1&#x29; AS INCOME_MOM_WRN_FLAG&#xd;&#xa;    ,IF&#x28;FIN_INCOME_YOY &#x3e;&#x3d; 0, 1, -1&#x29; AS INCOME_YOY_WRN_FLAG&#xd;&#xa;    ,IF&#x28;FIN_PROFIT_MOM &#x3e;&#x3d; 0, 1, -1&#x29; AS PROFIT_MOM_WRN_FLAG&#xd;&#xa;    ,IF&#x28;FIN_PROFIT_YOY &#x3e;&#x3d; 0, 1, -1&#x29; AS PROFIT_YOY_WRN_FLAG&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27;                                                    AS PART_SC&#xd;&#xa;    ,month_id AS PART_DT&#xd;&#xa;FROM TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_SUM&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT&#xd;&#xa;   REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS MONTH_ID                        --&#x6708;&#x4efd;ID&#xd;&#xa;  ,T.RGN_ID                                                 AS RNG_ID                          --&#x5927;&#x533a;ID&#xd;&#xa;  ,RGN.RGN_NAME                                             AS RNG_NAME                        --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;  ,T.BASE_ID                                                AS BASE_ID                         --&#x57fa;&#x5730;ID&#xd;&#xa;  ,BASE.BASE_NAME                                           AS BASE_NAME                       --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;  ,T.BRAND_ID                                               AS BRAND_ID                        --&#x54c1;&#x724c;ID&#xd;&#xa;  ,BRAND.BRAND_NAME                                         AS BRAND_NAME                      --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;  ,T.TM_TYPE                                                AS TM_TYPE                         --&#x65f6;&#x95f4;&#x7c7b;&#x578b; WE ONLY NEED TO COMPUTE MONTHLY AND YEARLY&#xd;&#xa;  ,T.FIN_BIZ_QTY                                            AS FIN_BIZ_QTY                     --&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;  ,T.FIN_INCOME                                             AS FIN_INCOME                      --&#x8d22;&#x52a1;&#x6536;&#x5165;&#xd;&#xa;  ,T.FIN_TGT_INCOME                                         AS FIN_TGT_INCOME                  --&#x8d22;&#x52a1;&#x76ee;&#x6807;&#x6536;&#x5165;&#xd;&#xa;  ,T.FIN_INCOME_CMP_RATE                                    AS FIN_INCOME_CMP_RATE             --&#x8d22;&#x52a1;&#x6536;&#x5165;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;  ,T.FIN_INCOME_MOM                                         AS FIN_INCOME_MOM                  --&#x8d22;&#x52a1;&#x6536;&#x5165;&#x73af;&#x6bd4;&#xd;&#xa;  ,T.FIN_INCOME_YOY                                         AS FIN_INCOME_YOY                  --&#x8d22;&#x52a1;&#x6536;&#x5165;&#x540c;&#x6bd4;&#xd;&#xa;  ,T.FIN_PROFIT                                             AS FIN_PROFIT                      --&#x8d22;&#x52a1;&#x5229;&#x6da6;&#xd;&#xa;  ,T.FIN_TGT_PROFIT                                         AS FIN_TGT_PROFIT                  --&#x8d22;&#x52a1;&#x76ee;&#x6807;&#x5229;&#x6da6;&#xd;&#xa;  ,T.FIN_PROFIT_CMP_RATE                                    AS FIN_PROFIT_CMP_RATE             --&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;  ,T.FIN_PROFIT_MOM                                         AS FIN_PROFIT_MOM                  --&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x73af;&#x6bd4;&#xd;&#xa;  ,T.FIN_PROFIT_YOY                                         AS FIN_PROFIT_YOY                  --&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x540c;&#x6bd4;&#xd;&#xa;  ,T.FIN_PROFIT_RATE                                        AS FIN_PROFIT_RATE                 --&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x7387;&#xd;&#xa;  ,NULL                                                     AS INCOME_CMP_WRN_FLAG             --&#x6536;&#x5165;&#x5b8c;&#x6210;&#x7387;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;  ,NULL                                                     AS PROFIT_CMP_WRN_FLAG             --&#x5229;&#x6da6;&#x5b8c;&#x6210;&#x7387;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;  ,IF&#x28;T.FIN_INCOME_MOM &#x3e; 0, 1, 0&#x29;                           AS INCOME_MOM_WRN_FLAG             --&#x6536;&#x5165;&#x73af;&#x6bd4;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;  ,IF&#x28;T.FIN_INCOME_YOY &#x3e; 0, 1, 0&#x29;                           AS INCOME_YOY_WRN_FLAG             --&#x6536;&#x5165;&#x540c;&#x6bd4;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;  ,IF&#x28;T.FIN_PROFIT_MOM &#x3e; 0, 1, 0&#x29;                           AS PROFIT_MOM_WRN_FLAG             --&#x5229;&#x6da6;&#x73af;&#x6bd4;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;  ,IF&#x28;T.FIN_PROFIT_YOY &#x3e; 0, 1, 0&#x29;                           AS PROFIT_YOY_WRN_FLAG             --&#x5229;&#x6da6;&#x540c;&#x6bd4;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;  ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;  ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;  ,&#x27;DMK&#x27;                                                    AS PART_SC&#xd;&#xa;  ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;FROM&#xd;&#xa;&#x28;&#xd;&#xa;SELECT RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,TM_TYPE&#xd;&#xa;      ,FIN_BIZ_QTY&#xd;&#xa;      ,FIN_INCOME&#xd;&#xa;      ,FIN_TGT_INCOME&#xd;&#xa;      ,FIN_INCOME_CMP_RATE&#xd;&#xa;      ,FIN_INCOME_MOM&#xd;&#xa;      ,FIN_INCOME_YOY&#xd;&#xa;      ,FIN_PROFIT&#xd;&#xa;      ,FIN_TGT_PROFIT&#xd;&#xa;      ,FIN_PROFIT_CMP_RATE&#xd;&#xa;      ,FIN_PROFIT_MOM&#xd;&#xa;      ,FIN_PROFIT_YOY&#xd;&#xa;      ,FIN_PROFIT_RATE&#xd;&#xa;  FROM TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_1&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,TM_TYPE&#xd;&#xa;      ,FIN_BIZ_QTY&#xd;&#xa;      ,FIN_INCOME&#xd;&#xa;      ,FIN_TGT_INCOME&#xd;&#xa;      ,FIN_INCOME_CMP_RATE&#xd;&#xa;      ,FIN_INCOME_MOM&#xd;&#xa;      ,FIN_INCOME_YOY&#xd;&#xa;      ,FIN_PROFIT&#xd;&#xa;      ,FIN_TGT_PROFIT&#xd;&#xa;      ,FIN_PROFIT_CMP_RATE&#xd;&#xa;      ,FIN_PROFIT_MOM&#xd;&#xa;      ,FIN_PROFIT_YOY&#xd;&#xa;      ,FIN_PROFIT_RATE&#xd;&#xa;  FROM TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_2&#xd;&#xa;&#x29;T&#xd;&#xa;LEFT JOIN &#x28;SELECT RGN_ID, RGN_NAME FROM DIM.DIM_ORG WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27; GROUP BY RGN_ID, RGN_NAME&#x29; RGN&#xd;&#xa;ON T.RGN_ID &#x3d; RGN.RGN_ID&#xd;&#xa;LEFT JOIN &#x28;SELECT BASE_ID, BASE_NAME FROM DIM.DIM_ORG WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27; GROUP BY BASE_ID, BASE_NAME&#x29; BASE&#xd;&#xa;ON T.BASE_ID &#x3d; BASE.BASE_ID&#xd;&#xa;LEFT JOIN DIM.DIM_BRAND BRAND&#xd;&#xa;ON T.BRAND_ID &#x3d; BRAND.BRAND_ID&#xd;&#xa;AND BRAND.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#x3b;&#xd;&#xa;&#x2a;&#x2f;</sql>
