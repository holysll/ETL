      <name>YR</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_2&#xd;&#xa;SELECT &#xd;&#xa;     T.OC_ID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.PROJ_ID&#xd;&#xa;    ,&#x27;Y&#x27;                                                AS TM_TYPE&#xd;&#xa;    ,BIZ.SHIPPING_NUM                                   AS BIZ_QTY&#xd;&#xa;    ,SUM&#x28;T.VCL_INCOME_AMT&#x29;                              AS FIN_INCOME&#xd;&#xa;    ,SUM&#x28;T.VCL_INCOME_TGT_AMT&#x29;                          AS FIN_TGT_INCOME&#xd;&#xa;    ,&#x28;1 &#x2b; &#x28;SUM&#x28;T.VCL_INCOME_AMT&#x29; - PLAN1.INCOME_PLAN_AMT&#x29; &#x2f; abs&#x28;PLAN1.INCOME_PLAN_AMT&#x29;&#x29;&#x2a;100      AS FIN_INCOME_CMP_RATE&#xd;&#xa;    ,NULL                                               AS FIN_INCOME_MOM--&#x8d22;&#x52a1;&#x6536;&#x5165;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;SUM&#x28;T.VCL_INCOME_AMT&#x29; &#x2f; TB.INCOME_AMT - 1&#x29;&#x2a;100              AS FIN_INCOEM_YOY--&#x8d22;&#x52a1;&#x6536;&#x5165;&#x540c;&#x6bd4;&#xd;&#xa;    ,SUM&#x28;T.PROFIT_AMT&#x29;                                  AS FIN_PROFIT&#xd;&#xa;    ,SUM&#x28;T.PROFIT_TGT_AMT&#x29;                              AS FIN_TGT_PROFIT&#xd;&#xa;    ,&#x28;1 &#x2b; &#x28;SUM&#x28;T.PROFIT_AMT&#x29; &#x2f; PLAN2.PROFIT_PLAN_AMT&#x29; &#x2f; abs&#x28;PLAN2.PROFIT_PLAN_AMT&#x29;&#x29;&#x2a;100          AS FIN_PROFIT_CMP_RATE&#xd;&#xa;    ,NULL                                               AS FIN_PROFIT_MOM--&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;SUM&#x28;T.PROFIT_AMT&#x29; &#x2f; TB.PROFIT_AMT - 1&#x29;&#x2a;100                  AS FIN_PROFIT_YOY--&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x540c;&#x6bd4;&#xd;&#xa;    ,&#x28;SUM&#x28;T.PROFIT_AMT&#x29; &#x2f; SUM&#x28;T.VCL_INCOME_AMT&#x29;&#x29;&#x2a;100          AS FIN_PROFIT_RATE--&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x7387; &#x5229;&#x6da6;&#x7387;&#x3d;&#x5229;&#x6da6;&#x603b;&#x989d;&#x2f; &#x603b;&#x8425;&#x4e1a;&#x6536;&#x5165;&#xd7;100&#x25;&#xd;&#xa;FROM&#xd;&#xa;DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;LEFT JOIN &#x28;select oc_id, rgn_id, base_id, brand_id, proj_id, sum&#x28;income_plan_amt&#x29; as income_plan_amt from DW.DW_FIN_INCOME_PLAN&#xd;&#xa;where substring&#x28;PART_DT, 1, 4&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;	AND PART_DT &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;group by oc_id, rgn_id, base_id, brand_id, proj_id&#x29; as PLAN1&#xd;&#xa;ON  T.OC_ID    &#x3d; PLAN1.OC_ID&#xd;&#xa;AND T.RGN_ID   &#x3d; PLAN1.RGN_ID&#xd;&#xa;AND T.BASE_ID  &#x3d; PLAN1.BASE_ID&#xd;&#xa;AND T.BRAND_ID &#x3d; PLAN1.BRAND_ID&#xd;&#xa;AND T.PROJ_ID  &#x3d; PLAN1.PROJ_ID&#xd;&#xa;LEFT JOIN &#x28;select oc_id, rgn_id, base_id, brand_id, proj_id, sum&#x28;profit_plan_amt&#x29; as profit_plan_amt from DW.DW_FIN_PROFIT_PLAN&#xd;&#xa;where substring&#x28;PART_DT, 1, 4&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;	AND PART_DT &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;group by oc_id, rgn_id, base_id, brand_id, proj_id&#x29; as PLAN2&#xd;&#xa;ON  T.OC_ID    &#x3d; PLAN2.OC_ID&#xd;&#xa;AND T.RGN_ID   &#x3d; PLAN2.RGN_ID&#xd;&#xa;AND T.BASE_ID  &#x3d; PLAN2.BASE_ID&#xd;&#xa;AND T.BRAND_ID &#x3d; PLAN2.BRAND_ID&#xd;&#xa;AND T.PROJ_ID  &#x3d; PLAN2.PROJ_ID&#xd;&#xa;LEFT JOIN &#x28;select oc_id, rgn_id, base_id, brand_id, proj_id, sum&#x28;shipping_num&#x29; as shipping_num from DMK.DMK_TP_OPERATE_D&#xd;&#xa;where SUBSTR&#x28;PART_DT, 1, 4&#x29;     &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;	AND PART_DT &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;group by oc_id, rgn_id, base_id, brand_id, proj_id&#x29; as BIZ&#xd;&#xa;ON  T.OC_ID    &#x3d; BIZ.OC_ID&#xd;&#xa;AND T.RGN_ID   &#x3d; BIZ.RGN_ID&#xd;&#xa;AND T.BASE_ID  &#x3d; BIZ.BASE_ID&#xd;&#xa;AND T.BRAND_ID &#x3d; BIZ.BRAND_ID&#xd;&#xa;AND T.PROJ_ID  &#x3d; BIZ.PROJ_ID&#xd;&#xa;--&#x8d22;&#x52a1;&#x6536;&#x5165;&#x540c;&#x6bd4; &#x53d6;&#x4e0a;&#x5e74;&#x6536;&#x5165;&#xd;&#xa;--&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x540c;&#x6bd4; &#x53d6;&#x4e0a;&#x5e74;&#x5229;&#x6da6;&#xd;&#xa;LEFT JOIN&#xd;&#xa;&#x28;&#xd;&#xa;SELECT &#xd;&#xa;     T.OC_ID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.PROJ_ID&#xd;&#xa;    ,SUM&#x28;T.VCL_INCOME_AMT&#x29;                              AS INCOME_AMT&#xd;&#xa;    ,SUM&#x28;T.PROFIT_AMT&#x29;                                  AS PROFIT_AMT&#xd;&#xa;FROM DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;WHERE T.PART_SC &#x3d; &#x27;DW&#x27;&#xd;&#xa;	AND SUBSTR&#x28;T.PART_DT,1,4&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 4&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;	AND T.PART_DT &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;GROUP BY&#xd;&#xa;     T.OC_ID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.PROJ_ID&#xd;&#xa;&#x29;TB&#xd;&#xa;ON  T.OC_ID    &#x3d; TB.OC_ID&#xd;&#xa;AND T.RGN_ID   &#x3d; TB.RGN_ID&#xd;&#xa;AND T.BASE_ID  &#x3d; TB.BASE_ID&#xd;&#xa;AND T.BRAND_ID &#x3d; TB.BRAND_ID&#xd;&#xa;AND T.PROJ_ID  &#x3d; TB.PROJ_ID&#xd;&#xa;WHERE T.PART_SC &#x3d; &#x27;DW&#x27;&#xd;&#xa;	AND SUBSTR&#x28;T.PART_DT, 1, 4&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;GROUP BY&#xd;&#xa;     T.OC_ID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.PROJ_ID&#xd;&#xa;    ,BIZ.shipping_num&#xd;&#xa;    ,plan1.income_plan_amt&#xd;&#xa;    ,plan2.profit_plan_amt&#xd;&#xa;    ,tb.income_amt&#xd;&#xa;    ,tb.profit_amt</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
