    <entry>
      <name>TO_DETAIL_M</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_3&#xd;&#xa;SELECT coalesce&#x28;T.rgn_id,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;T.base_id,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;T.proj_id,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,sum&#x28;T.BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;    ,sum&#x28;T.CUR_M_INCOME&#x29; AS CUR_M_INCOME&#xd;&#xa;    ,sum&#x28;T.LST_M_INCOME&#x29; AS LST_M_INCOME&#xd;&#xa;    ,sum&#x28;T.LST_Y_M_INCOME&#x29; AS LST_Y_M_INCOME&#xd;&#xa;    ,sum&#x28;T.CUR_M_TGT_INCOME&#x29; AS CUR_M_TGT_INCOME&#xd;&#xa;    ,sum&#x28;T.LST_M_TGT_INCOME&#x29; AS LST_M_TGT_INCOME&#xd;&#xa;    ,sum&#x28;T.LST_Y_M_TGT_INCOME&#x29; AS LST_Y_M_TGT_INCOME&#xd;&#xa;    ,sum&#x28;T.CUR_M_PROFIT&#x29; AS CUR_M_PROFIT&#xd;&#xa;    ,sum&#x28;T.LST_M_PROFIT&#x29; AS LST_M_PROFIT&#xd;&#xa;    ,sum&#x28;T.LST_Y_M_PROFIT&#x29; AS LST_Y_M_PROFIT&#xd;&#xa;    ,sum&#x28;T.CUR_M_TGT_PROFIT&#x29; AS CUR_M_TGT_PROFIT&#xd;&#xa;    ,sum&#x28;T.LST_M_TGT_PROFIT&#x29; AS LST_M_TGT_PROFIT&#xd;&#xa;    ,sum&#x28;T.LST_Y_M_TGT_PROFIT&#x29; AS LST_Y_M_TGT_PROFIT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,0 AS BIZ_QTY&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN vcl_income_amt ELSE 0 END&#x29; AS CUR_M_INCOME&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN vcl_income_amt ELSE 0 END&#x29; AS LST_M_INCOME&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN vcl_income_amt ELSE 0 END&#x29; AS LST_Y_M_INCOME&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN vcl_income_tgt_amt ELSE 0 END&#x29; AS CUR_M_TGT_INCOME&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN vcl_income_tgt_amt ELSE 0 END&#x29; AS LST_M_TGT_INCOME&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN vcl_income_tgt_amt ELSE 0 END&#x29; AS LST_Y_M_TGT_INCOME&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN profit_amt ELSE 0 END&#x29; AS CUR_M_PROFIT&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN profit_amt ELSE 0 END&#x29; AS LST_M_PROFIT&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN profit_amt ELSE 0 END&#x29; AS LST_Y_M_PROFIT&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN profit_tgt_amt ELSE 0 END&#x29; AS CUR_M_TGT_PROFIT&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN profit_tgt_amt ELSE 0 END&#x29; AS LST_M_TGT_PROFIT&#xd;&#xa;        ,SUM&#x28;CASE WHEN PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN profit_tgt_amt ELSE 0 END&#x29; AS LST_Y_M_TGT_PROFIT&#xd;&#xa;    FROM DMK.DMK_FIN_INCOMECOST_M &#xd;&#xa;    WHERE &#x28;PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;  --&#x5f53;&#x6708;&#xd;&#xa;        OR PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;  --&#x53bb;&#x5e74;&#x5f53;&#x6708; &#xd;&#xa;        OR PART_DT &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29;  --&#x4e0a;&#x6708; &#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,SUM&#x28;shipping_num&#x29; AS BIZ_QTY&#xd;&#xa;        ,0 AS CUR_M_INCOME&#xd;&#xa;        ,0 AS LST_M_INCOME&#xd;&#xa;        ,0 AS LST_Y_M_INCOME&#xd;&#xa;        ,0 AS CUR_M_TGT_INCOME&#xd;&#xa;        ,0 AS LST_M_TGT_INCOME&#xd;&#xa;        ,0 AS LST_Y_M_TGT_INCOME&#xd;&#xa;        ,0 AS CUR_M_PROFIT&#xd;&#xa;        ,0 AS LST_M_PROFIT&#xd;&#xa;        ,0 AS LST_Y_M_PROFIT&#xd;&#xa;        ,0 AS CUR_M_TGT_PROFIT&#xd;&#xa;        ,0 AS LST_M_TGT_PROFIT&#xd;&#xa;        ,0 AS LST_Y_M_TGT_PROFIT&#xd;&#xa;    FROM DMK.DMK_TP_OPERATE_D A&#xd;&#xa;    LEFT JOIN DIM.dim_period_d B&#xd;&#xa;    ON A.period_wid &#x3d; B.period_wid AND B.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;    WHERE B.fin_in_month &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.rgn_id,T.base_id,T.brand_id,T.proj_id WITH CUBE</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
