    <entry>
      <name>TO_SUM</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_SUM&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID &#xd;&#xa;    ,T.rgn_id&#xd;&#xa;    ,CASE WHEN T.rgn_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.rgn_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.rgn_name END AS RNG_NAME&#xd;&#xa;    ,T.base_id&#xd;&#xa;    ,CASE WHEN T.base_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.base_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.base_name END AS BASE_NAME&#xd;&#xa;    ,T.brand_id&#xd;&#xa;    ,CASE WHEN T.brand_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.brand_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.brand_name END AS BRAND_NAME&#xd;&#xa;    ,T.proj_id&#xd;&#xa;    ,CASE WHEN T.proj_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.proj_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T4.proj_name END AS PROJ_NAME&#xd;&#xa;    ,&#x27;Y&#x27; AS TM_TYPE &#xd;&#xa;    ,sum&#x28;T.biz_y_qty&#x29; AS FIN_BIZ_QTY&#xd;&#xa;    ,sum&#x28;T.cur_y_income&#x29; AS FIN_INCOME&#xd;&#xa;    ,sum&#x28;T.cur_y_tgt_income&#x29; AS FIN_TGT_INCOME&#xd;&#xa;    ,&#x28;1&#x2b;&#x28;SUM&#x28;T.cur_y_income&#x29;-SUM&#x28;T.cur_y_tgt_income&#x29;&#x29;&#x2f;abs&#x28;SUM&#x28;T.cur_y_tgt_income&#x29;&#x29;&#x29;&#x2a;100 AS FIN_INCOME_CMP_RATE&#xd;&#xa;    ,NULL AS FIN_INCOME_MOM&#xd;&#xa;    ,&#x28;SUM&#x28;T.cur_y_income&#x29;&#x2f;SUM&#x28;T.lst_y_y_income&#x29;-1&#x29;&#x2a;100 AS FIN_INCOME_YOY&#xd;&#xa;    ,sum&#x28;T.cur_y_profit&#x29; AS FIN_PROFIT&#xd;&#xa;    ,sum&#x28;T.cur_y_tgt_profit&#x29; AS FIN_TGT_PROFIT&#xd;&#xa;    ,&#x28;1&#x2b;&#x28;SUM&#x28;T.cur_y_profit&#x29;-SUM&#x28;T.cur_y_tgt_profit&#x29;&#x29;&#x2f;abs&#x28;SUM&#x28;T.cur_y_tgt_profit&#x29;&#x29;&#x29;&#x2a;100 AS FIN_PROFIT_CMP_RATE&#xd;&#xa;    ,NULL AS FIN_PROFIT_MOM&#xd;&#xa;    ,&#x28;SUM&#x28;T.cur_y_profit&#x29;&#x2f;SUM&#x28;T.lst_y_y_profit&#x29;-1&#x29;&#x2a;100 AS FIN_PROFIT_YOY&#xd;&#xa;    ,SUM&#x28;T.cur_y_profit&#x29;&#x2f;SUM&#x28;T.cur_y_income&#x29;&#x2a;100 AS FIN_PROFIT_RATE	&#xd;&#xa;FROM TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_4 T&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT base_id,base_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY base_id,base_name&#xd;&#xa;    &#x29; T2&#xd;&#xa;ON T.base_id &#x3d; T2.base_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.dim_brand &#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29; T3&#xd;&#xa;ON T.brand_id &#x3d; T3.brand_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT proj_id,proj_name&#xd;&#xa;    FROM DIM.dim_proj &#xd;&#xa;    GROUP BY proj_id,proj_name&#xd;&#xa;    &#x29; T4&#xd;&#xa;ON T.proj_id &#x3d; T4.proj_id&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;GROUP BY T.rgn_id,T.base_id,T.brand_id,T.proj_id,T1.rgn_name,T2.base_name,T3.brand_name,T4.proj_name&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID &#xd;&#xa;    ,T.rgn_id&#xd;&#xa;    ,CASE WHEN T.rgn_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.rgn_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.rgn_name END AS RNG_NAME&#xd;&#xa;    ,T.base_id&#xd;&#xa;    ,CASE WHEN T.base_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.base_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.base_name END AS BASE_NAME&#xd;&#xa;    ,T.brand_id&#xd;&#xa;    ,CASE WHEN T.brand_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.brand_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.brand_name END AS BRAND_NAME&#xd;&#xa;    ,T.proj_id&#xd;&#xa;    ,CASE WHEN T.proj_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.proj_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T4.proj_name END AS PROJ_NAME&#xd;&#xa;    ,&#x27;M&#x27; AS TM_TYPE &#xd;&#xa;    ,sum&#x28;T.biz_qty&#x29; AS FIN_BIZ_QTY&#xd;&#xa;    ,sum&#x28;T.cur_m_income&#x29; AS FIN_INCOME&#xd;&#xa;    ,sum&#x28;T.cur_m_tgt_income&#x29; AS FIN_TGT_INCOME&#xd;&#xa;    ,&#x28;1&#x2b;&#x28;SUM&#x28;T.cur_m_income&#x29;-SUM&#x28;T.cur_m_tgt_income&#x29;&#x29;&#x2f;abs&#x28;SUM&#x28;T.cur_m_tgt_income&#x29;&#x29;&#x29;&#x2a;100 AS FIN_INCOME_CMP_RATE&#xd;&#xa;    ,&#x28;SUM&#x28;T.cur_m_income&#x29;&#x2f;SUM&#x28;T.lst_m_income&#x29;-1&#x29;&#x2a;100 AS FIN_INCOME_MOM&#xd;&#xa;    ,&#x28;SUM&#x28;T.cur_m_income&#x29;&#x2f;SUM&#x28;T.lst_y_m_income&#x29;-1&#x29;&#x2a;100 AS FIN_INCOME_YOY&#xd;&#xa;    ,sum&#x28;T.cur_m_profit&#x29; AS FIN_PROFIT&#xd;&#xa;    ,sum&#x28;T.cur_m_tgt_profit&#x29; AS FIN_TGT_PROFIT&#xd;&#xa;    ,&#x28;1&#x2b;&#x28;SUM&#x28;T.cur_m_profit&#x29;-SUM&