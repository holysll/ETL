      <name>CREATE_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;--Dont need to re-create temporary table unless an update of table structure is asked&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_SUM&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_SUM&#x28;&#xd;&#xa;	MONTH_ID				STRING			 COMMENT	&#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;    ,RGN_ID                 STRING           COMMENT    &#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;	,RGN_NAME				STRING			 COMMENT	&#x27;&#x5927;&#x533a;&#x540d;&#x79f0;&#x27;&#xd;&#xa;    ,BASE_ID                STRING           COMMENT    &#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;	,BASE_NAME				STRING			 COMMENT	&#x27;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;&#xd;&#xa;    ,BRAND_ID               STRING           COMMENT    &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;	,BRAND_NAME				STRING			 COMMENT	&#x27;&#x54c1;&#x724c;&#x540d;&#x79f0;&#x27;&#xd;&#xa;    ,PROJ_ID                STRING           COMMENT    &#x27;&#x9879;&#x76ee;ID&#x27;&#xd;&#xa;	,PROJ_NAME				STRING			 COMMENT	&#x27;&#x9879;&#x76ee;&#x540d;&#x79f0;&#x27;&#xd;&#xa;    ,TM_TYPE                STRING           COMMENT    &#x27;&#x65f6;&#x95f4;&#x7c7b;&#x578b;&#x27;&#xd;&#xa;    ,FIN_BIZ_QTY            DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;    ,FIN_INCOME             DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x6536;&#x5165;&#x27;&#xd;&#xa;    ,FIN_TGT_INCOME         DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x76ee;&#x6807;&#x6536;&#x5165;&#x27;&#xd;&#xa;    ,FIN_INCOME_CMP_RATE    DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x6536;&#x5165;&#x5b8c;&#x6210;&#x7387;&#x27;&#xd;&#xa;    ,FIN_INCOME_MOM         DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x6536;&#x5165;&#x73af;&#x6bd4;&#x27;&#xd;&#xa;    ,FIN_INCOME_YOY         DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x6536;&#x5165;&#x540c;&#x6bd4;&#x27;&#xd;&#xa;    ,FIN_PROFIT             DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x27;&#xd;&#xa;    ,FIN_TGT_PROFIT         DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x76ee;&#x6807;&#x5229;&#x6da6;&#x27;&#xd;&#xa;    ,FIN_PROFIT_CMP_RATE    DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x5b8c;&#x6210;&#x7387;&#x27;&#xd;&#xa;    ,FIN_PROFIT_MOM         DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x73af;&#x6bd4;&#x27;&#xd;&#xa;    ,FIN_PROFIT_YOY         DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x540c;&#x6bd4;&#x27;&#xd;&#xa;    ,FIN_PROFIT_RATE        DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x8d22;&#x52a1;&#x5229;&#x6da6;&#x7387;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x6574;&#x8f66;&#x7269;&#x6d41;&#x8d22;&#x52a1;&#x6982;&#x51b5;&#x4e34;&#x65f6;&#x8868;1&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_3&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_3&#x28;&#xd;&#xa;    RGN_ID                STRING           COMMENT    &#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;    ,BASE_ID              STRING           COMMENT    &#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;    ,BRAND_ID             STRING           COMMENT    &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;    ,PROJ_ID              STRING           COMMENT    &#x27;&#x9879;&#x76ee;ID&#x27;&#xd;&#xa;    ,BIZ_QTY           	  DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x5f53;&#x6708;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;	,CUR_M_INCOME         DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x6536;&#x5165;&#x27;&#xd;&#xa;	,LST_M_INCOME         DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x865e;&#x6536;&#x5165;&#x27;&#xd;&#xa;	,LST_Y_M_INCOME       DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x6536;&#x5165;&#x27;&#xd;&#xa;	,CUR_M_TGT_INCOME     DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x76ee;&#x6807;&#x6536;&#x5165;&#x27;&#xd;&#xa;	,LST_M_TGT_INCOME     DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x6708;&#x76ee;&#x6807;&#x6536;&#x5165;&#x27;&#xd;&#xa;	,LST_Y_M_TGT_INCOME   DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x76ee;&#x6807;&#x6536;&#x5165;&#x27;&#xd;&#xa;	,CUR_M_PROFIT         DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x5229;&#x6da6;&#x27;&#xd;&#xa;	,LST_M_PROFIT         DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x6708;&#x5229;&#x6da6;&#x27;&#xd;&#xa;	,LST_Y_M_PROFIT       DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x5229;&#x6da6;&#x27;&#xd;&#xa;	,CUR_M_TGT_PROFIT     DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x6708;&#x76ee;&#x6807;&#x5229;&#x6da6;&#x27;&#xd;&#xa;	,LST_M_TGT_PROFIT     DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x6708;&#x5229;&#x6da6;&#x27;&#xd;&#xa;	,LST_Y_M_TGT_PROFIT   DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x5229;&#x6da6;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x6574;&#x8f66;&#x7269;&#x6d41;&#x8d22;&#x52a1;&#x6982;&#x51b5;&#x4e34;&#x65f6;&#x8868;3&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_4&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_4&#x28;&#xd;&#xa;    RGN_ID                STRING           COMMENT    &#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;    ,BASE_ID              STRING           COMMENT    &#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;    ,BRAND_ID             STRING           COMMENT    &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;    ,PROJ_ID              STRING           COMMENT    &#x27;&#x9879;&#x76ee;ID&#x27;&#xd;&#xa;    ,BIZ_Y_QTY            DECIMAL&#x28;32,8&#x29;    COMMENT    &#x27;&#x5f53;&#x5e74;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;	,CUR_Y_INCOME         DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x5e74;&#x6536;&#x5165;&#x27;&#xd;&#xa;	,LST_Y_Y_INCOME       DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x5e74;&#x6536;&#x5165;&#x27;&#xd;&#xa;	,CUR_Y_TGT_INCOME     DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x6536;&#x5165;&#x27;&#xd;&#xa;	,LST_Y_Y_TGT_INCOME   DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x5e74;&#x76ee;&#x6807;&#x6536;&#x5165;&#x27;&#xd;&#xa;	,CUR_Y_PROFIT         DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x5e74;&#x5229;&#x6da6;&#x27;&#xd;&#xa;	,LST_Y_Y_PROFIT       DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x5e74;&#x5229;&#x6da6;&#x27;&#xd;&#xa;	,CUR_Y_TGT_PROFIT     DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5f53;&#x5e74;&#x76ee;&#x6807;&#x5229;&#x6da6;&#x27;&#xd;&#xa;	,LST_Y_Y_TGT_PROFIT   DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x4e0a;&#x5e74;&#x76ee;&#x6807;&#x5229;&#x6da6;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x6574;&#x8f66;&#x7269;&#x6d41;&#x8d22;&#x52a1;&#x6982;&#x51b5;&#x4e34;&#x65f6;&#x8868;3&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;</sql>
