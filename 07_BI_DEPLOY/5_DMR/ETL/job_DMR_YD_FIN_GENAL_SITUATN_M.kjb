    <entry>
      <name>TO_DETAIL_Y</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_FIN_GENAL_SITUATN_M_4&#xd;&#xa;SELECT coalesce&#x28;T.rgn_id,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;T.base_id,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;T.proj_id,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,sum&#x28;T.BIZ_Y_QTY&#x29; AS BIZ_Y_QTY&#xd;&#xa;    ,sum&#x28;T.CUR_Y_INCOME&#x29; AS CUR_Y_INCOME&#xd;&#xa;    ,sum&#x28;T.LST_Y_Y_INCOME&#x29; AS LST_Y_Y_INCOME&#xd;&#xa;    ,sum&#x28;T.CUR_Y_TGT_INCOME&#x29; AS CUR_Y_TGT_INCOME&#xd;&#xa;    ,sum&#x28;T.LST_Y_Y_TGT_INCOME&#x29; AS LST_Y_Y_TGT_INCOME&#xd;&#xa;    ,sum&#x28;T.CUR_Y_PROFIT&#x29; AS CUR_Y_PROFIT&#xd;&#xa;    ,sum&#x28;T.LST_Y_Y_PROFIT&#x29; AS LST_Y_Y_PROFIT&#xd;&#xa;    ,sum&#x28;T.CUR_Y_TGT_PROFIT&#x29; AS CUR_Y_TGT_PROFIT&#xd;&#xa;    ,sum&#x28;T.LST_Y_Y_TGT_PROFIT&#x29; AS LST_Y_Y_TGT_PROFIT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,0 AS BIZ_Y_QTY&#xd;&#xa;        ,SUM&#x28;CASE WHEN substr&#x28;part_dt,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND PART_DT &#x3c;&#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN vcl_income_amt ELSE 0 END&#x29; AS CUR_Y_INCOME&#xd;&#xa;        ,SUM&#x28;CASE WHEN substr&#x28;part_dt,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29; AND PART_DT &#x3c;&#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN vcl_income_amt ELSE 0 END&#x29; AS LST_Y_Y_INCOME&#xd;&#xa;        ,0 AS CUR_Y_TGT_INCOME&#xd;&#xa;        ,0 AS LST_Y_Y_TGT_INCOME&#xd;&#xa;        ,SUM&#x28;CASE WHEN substr&#x28;part_dt,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND PART_DT &#x3c;&#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN profit_amt ELSE 0 END&#x29; AS CUR_Y_PROFIT&#xd;&#xa;        ,SUM&#x28;CASE WHEN substr&#x28;part_dt,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29; AND PART_DT &#x3c;&#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; THEN profit_amt ELSE 0 END&#x29; AS LST_Y_Y_PROFIT&#xd;&#xa;        ,0 AS CUR_Y_TGT_PROFIT&#xd;&#xa;        ,0 AS LST_Y_Y_TGT_PROFIT&#xd;&#xa;    FROM DMK.DMK_FIN_INCOMECOST_M &#xd;&#xa;    WHERE &#x28;substr&#x28;part_dt,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#x29;  --&#x5f53;&#x6708;&#xd;&#xa;        OR &#x28;substr&#x28;part_dt,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29;&#x29;  --&#x53bb;&#x5e74;&#x5f53;&#x6708;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;	UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,0 AS BIZ_Y_QTY&#xd;&#xa;        ,0 AS CUR_Y_INCOME&#xd;&#xa;        ,0 AS LST_Y_Y_INCOME&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; THEN income_plan_amt ELSE 0 END&#x29; AS CUR_Y_TGT_INCOME&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29; THEN income_plan_amt ELSE 0 END&#x29; AS LST_Y_Y_TGT_INCOME&#xd;&#xa;        ,0 AS CUR_Y_PROFIT&#xd;&#xa;        ,0 AS LST_Y_Y_PROFIT&#xd;&#xa;        ,0 AS CUR_Y_TGT_PROFIT&#xd;&#xa;        ,0 AS LST_Y_Y_TGT_PROFIT&#xd;&#xa;    FROM DW.DW_FIN_INCOME_PLAN&#xd;&#xa;    WHERE &#x28;substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;        OR substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29;&#xd;&#xa;		&#x29;&#xd;&#xa;		AND TRIM&#x28;PROJ_NAME&#x29; &#x3c;&#x3e; &#x27;&#x4e00;&#x4f53;&#x5316;&#x8d2d;&#x8f66;&#x9879;&#x76ee;&#x7ec4;&#x27;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;     &#xd;&#xa;	UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,0 AS BIZ_Y_QTY&#xd;&#xa;        ,0 AS CUR_Y_INCOME&#xd;&#xa;        ,0 AS LST_Y_Y_INCOME&#xd;&#xa;        ,0 AS CUR_Y_TGT_INCOME&#xd;&#xa;        ,0 AS LST_Y_Y_TGT_INCOME&#xd;&#xa;        ,0 AS CUR_Y_PROFIT&#xd;&#xa;        ,0 AS LST_Y_Y_PROFIT&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; THEN profit_plan_amt ELSE 0 END&#x29; AS CUR_Y_TGT_PROFIT&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29; THEN profit_plan_amt ELSE 0 END&#x29; AS LST_Y_Y_TGT_PROFIT&#xd;&#xa;    FROM DW.DW_FIN_PROFIT_PLAN&#xd;&#xa;    WHERE substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;        OR substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;     &#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,SUM&#x28;shipping_num&#x29; AS BIZ_Y_QTY&#xd;&#xa;        ,0 AS CUR_Y_INCOME&#xd;&#xa;        ,0 AS LST_Y_Y_INCOME&#xd;&#xa;        ,0 AS CUR_Y_TGT_INCOME&#xd;&#xa;        ,0 AS LST_Y_Y_TGT_INCOME&#xd;&#xa;        ,0 AS CUR_Y_PROFIT&#xd;&#xa;        ,0 AS LST_Y_Y_PROFIT&#xd;&#xa;        ,0 AS CUR_Y_TGT_PROFIT&#xd;&#xa;        ,0 AS LST_Y_Y_TGT_PROFIT&#xd;&#xa;    FROM DMK.DMK_TP_OPERATE_D A&#xd;&#xa;    LEFT JOIN DIM.dim_period_d B&#xd;&#xa;    ON A.period_wid &#x3d; B.period_wid AND B.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;    WHERE substr&#x28;B.fin_in_month,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; AND B.fin_in_month &#x3c;&#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.rgn_id,T.base_id,T.brand_id,T.proj_id WITH CUBE</sql>
