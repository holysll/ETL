      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_ALL_COL_M_30&#xd;&#xa;SELECT PERIOD_WID&#xd;&#xa;      ,PERIOD_TYPE&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,RGN_NAME&#xd;&#xa;      ,PROJ_ID&#xd;&#xa;      ,PROJ_NAME&#xd;&#xa;      ,MASSLOSE_QTY&#xd;&#xa;      ,INTRAINT_QTY&#xd;&#xa;      ,MASSLOSE_RATE&#xd;&#xa;  FROM &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PERIOD_WID&#xd;&#xa;              ,&#x27;M&#x27; AS PERIOD_TYPE&#xd;&#xa;              ,NVL&#x28;A.RGN_ID, P.RGN_ID&#x29; AS RGN_ID&#xd;&#xa;              ,NVL&#x28;A.RGN_NAME, P.RGN_NAME&#x29; AS RGN_NAME&#xd;&#xa;              ,NVL&#x28;A.PROJ_ID, P.PROJ_ID&#x29; AS PROJ_ID&#xd;&#xa;              ,NVL&#x28;A.PROJ_NAME, P.PROJ_NAME&#x29; AS PROJ_NAME&#xd;&#xa;              ,A.MASSLOSE_QTY AS MASSLOSE_QTY&#xd;&#xa;              ,P.PLAN_QTY_MON AS INTRAINT_QTY&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;P.PLAN_QTY_MON, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  A.MASSLOSE_QTY &#x2f; P.PLAN_QTY_MON&#xd;&#xa;               END AS MASSLOSE_RATE&#xd;&#xa;          FROM &#x28;SELECT NVL&#x28;NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;T.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;T.PRO_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;                      ,COUNT&#x28;1&#x29; AS MASSLOSE_QTY --&#x5728;&#x9014;&#x8d28;&#x635f;&#x6570;&#xd;&#xa;                  FROM DW.DW_TP_INROAD_LOSS T&#xd;&#xa;                 WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;T.RECEIVING_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                 GROUP BY NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;T.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;T.PRO_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; WITH CUBE&#x29; AS A&#xd;&#xa;          FULL JOIN &#x28;SELECT NVL&#x28;NVL&#x28;R.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;R.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;P.PROJ_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;P.PROJ_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;                          ,SUM&#x28;PLN.REV_PLAN&#x29; AS PLAN_QTY_MON&#xd;&#xa;                      FROM ODS_BLPT.T_HAND_REV_TICKET_PLAN PLN&#xd;&#xa;                      LEFT JOIN DIM.DIM_ORG R&#xd;&#xa;                        ON R.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                       AND PLN.RGN_NAME &#x3d; R.RGN_NAME&#xd;&#xa;                      LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;                        ON PLN.PRO_NAME &#x3d; P.PROJ_NAME&#xd;&#xa;                     WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;PLN.MONTH_ID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                     GROUP BY NVL&#x28;R.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;R.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;P.PROJ_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;P.PROJ_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; WITH CUBE&#x29; AS P&#xd;&#xa;            ON A.RGN_ID &#x3d; P.RGN_ID&#xd;&#xa;           AND A.RGN_NAME &#x3d; P.RGN_NAME&#xd;&#xa;           AND A.PROJ_ID &#x3d; P.PROJ_ID&#xd;&#xa;           AND A.PROJ_NAME &#x3d; P.PROJ_NAME&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PERIOD_WID&#xd;&#xa;              ,&#x27;Y&#x27; AS PERIOD_TYPE&#xd;&#xa;              ,NVL&#x28;A.RGN_ID, P.RGN_ID&#x29; AS RGN_ID&#xd;&#xa;              ,NVL&#x28;A.RGN_NAME, P.RGN_NAME&#x29; AS RGN_NAME&#xd;&#xa;              ,NVL&#x28;A.PROJ_ID, P.PROJ_ID&#x29; AS PROJ_ID&#xd;&#xa;              ,NVL&#x28;A.PROJ_NAME, P.PROJ_NAME&#x29; AS PROJ_NAME&#xd;&#xa;              ,A.MASSLOSE_QTY AS MASSLOSE_QTY&#xd;&#xa;              ,P.PLAN_QTY_MON AS INTRAINT_QTY&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;P.PLAN_QTY_MON, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  A.MASSLOSE_QTY &#x2f; P.PLAN_QTY_MON&#xd;&#xa;               END AS MASSLOSE_RATE&#xd;&#xa;          FROM &#x28;SELECT NVL&#x28;NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;T.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;T.PRO_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;                      ,COUNT&#x28;1&#x29; AS MASSLOSE_QTY --&#x5728;&#x9014;&#x8d28;&#x635f;&#x6570;&#xd;&#xa;                  FROM DW.DW_TP_INROAD_LOSS T&#xd;&#xa;                 WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;T.RECEIVING_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 4&#x29;&#xd;&#xa;                   AND SUBSTR&#x28;REGEXP_REPLACE&#x28;T.RECEIVING_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3c;&#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                 GROUP BY NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;T.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;T.PRO_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; WITH CUBE&#x29; AS A&#xd;&#xa;          FULL JOIN &#x28;SELECT NVL&#x28;NVL&#x28;R.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;R.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;P.PROJ_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;P.PROJ_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;                          ,SUM&#x28;PLN.REV_PLAN&#x29; AS PLAN_QTY_MON&#xd;&#xa;                      FROM ODS_BLPT.T_HAND_REV_TICKET_PLAN PLN&#xd;&#xa;                      LEFT JOIN DIM.DIM_ORG R&#xd;&#xa;                        ON R.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                       AND PLN.RGN_NAME &#x3d; R.RGN_NAME&#xd;&#xa;                      LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;                        ON PLN.PRO_NAME &#x3d; P.PROJ_NAME&#xd;&#xa;                     WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;PLN.MONTH_ID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  4&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 4&#x29;&#xd;&#xa;                       AND SUBSTR&#x28;REGEXP_REPLACE&#x28;PLN.MONTH_ID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29; &#x3c;&#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                     GROUP BY NVL&#x28;R.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;R.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;P.PROJ_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;P.PROJ_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; WITH CUBE&#x29; AS P&#xd;&#xa;            ON A.RGN_ID &#x3d; P.RGN_ID&#xd;&#xa;           AND A.RGN_NAME &#x3d; P.RGN_NAME&#xd;&#xa;           AND A.PROJ_ID &#x3d; P.PROJ_ID&#xd;&#xa;           AND A.PROJ_NAME &#x3d; P.PROJ_NAME&#x29; AS T&#xd;&#xa; WHERE CASE&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;ALL&#x27; AND T.RGN_NAME NOT IN &#x28;&#x27;&#x5168;&#x90e8;&#x27;&#x29; THEN&#xd;&#xa;          1&#xd;&#xa;          WHEN T.RGN_NAME &#x3d; &#x27;&#x5168;&#x90e8;&#x27; AND T.RGN_ID NOT IN &#x28;&#x27;ALL&#x27;&#x29; THEN &#xd;&#xa;          1&#xd;&#xa;         WHEN T.PROJ_ID &#x3d; &#x27;ALL&#x27; AND T.PROJ_NAME NOT IN &#x28;&#x27;&#x5168;&#x90e8;&#x27;&#x29; THEN&#xd;&#xa;          1&#xd;&#xa;          WHEN T.PROJ_NAME &#x3d; &#x27;&#x5168;&#x90e8;&#x27; AND T.PROJ_ID NOT IN &#x28;&#x27;ALL&#x27;&#x29; THEN&#xd;&#xa;          1&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;ALL&#x27; AND T.PROJ_ID NOT IN &#x28;&#x27;ALL&#x27;&#x29; THEN&#xd;&#xa;          1&#xd;&#xa;         ELSE&#xd;&#xa;          0&#xd;&#xa;       END &#x3d; 0 &#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
