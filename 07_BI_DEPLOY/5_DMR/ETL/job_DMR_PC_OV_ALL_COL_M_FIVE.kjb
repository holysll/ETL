    <entry>
      <name>TMP27</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_ALL_COL_M_27&#xd;&#xa;SELECT PERIOD_WID&#xd;&#xa;      ,PERIOD_TYPE&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,RGN_NAME&#xd;&#xa;      ,PROJ_ID&#xd;&#xa;      ,PROJ_NAME&#xd;&#xa;      ,MASS_LOSS_QTY&#xd;&#xa;      ,T_D_ALL_INSTORE_QTY&#xd;&#xa;      ,RATE --&#x8d28;&#x635f;&#x7387;&#xd;&#xa;      ,RATE_YOY --&#x540c;&#x671f;&#x503c; &#xd;&#xa;      ,RATE_MOM --&#x4e0a;&#x6708;&#x503c;&#xd;&#xa;  FROM &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PERIOD_WID&#xd;&#xa;              ,&#x27;M&#x27; AS PERIOD_TYPE&#xd;&#xa;              ,C.RGN_ID AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;              ,C.RGN_NAME&#xd;&#xa;              ,C.PROJ_ID&#xd;&#xa;              ,C.PROJ_NAME&#xd;&#xa;              ,D.T_D_MASS_LOSS_QTY AS MASS_LOSS_QTY --&#x8d28;&#x635f;&#x6570;&#x91cf;&#xd;&#xa;              ,C.T_D_ALL_INSTORE_QTY AS T_D_ALL_INSTORE_QTY --&#x603b;&#x5e93;&#x5b58;&#x91cf;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;C.T_D_ALL_INSTORE_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  NULL&#xd;&#xa;                 ELSE&#xd;&#xa;                  D.T_D_MASS_LOSS_QTY &#x2f; C.T_D_ALL_INSTORE_QTY&#xd;&#xa;               END AS RATE --&#x8d28;&#x635f;&#x7387;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;C.L_Y_ALL_INSTORE_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  NULL&#xd;&#xa;                 ELSE&#xd;&#xa;                  E.L_Y_MASS_LOSS_QTY &#x2f; C.L_Y_ALL_INSTORE_QTY&#xd;&#xa;               END AS RATE_YOY --&#x540c;&#x671f;&#x503c; &#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;C.L_M_ALL_INSTORE_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  NULL&#xd;&#xa;                 ELSE&#xd;&#xa;                  F.L_M_MASS_LOSS_QTY &#x2f; C.L_M_ALL_INSTORE_QTY&#xd;&#xa;               END AS RATE_MOM --&#x4e0a;&#x6708;&#x503c;&#xd;&#xa;          FROM &#x28;SELECT NVL&#x28;NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;B.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;A.PROJ_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;P.PROJ_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;                      ,SUM&#x28;CASE&#xd;&#xa;                             WHEN A.PART_DT &#x3d;&#xd;&#xa;                                  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                         1,&#xd;&#xa;                                         6&#x29; THEN&#xd;&#xa;                              NVL&#x28;A.IN_STORE_QTY, 0&#x29;&#xd;&#xa;                             ELSE&#xd;&#xa;                              0&#xd;&#xa;                           END&#x29; AS T_D_ALL_INSTORE_QTY --&#x5f53;&#x6708;&#x5165;&#x5e93;&#x6570;&#xd;&#xa;                      ,SUM&#x28;CASE&#xd;&#xa;                             WHEN A.PART_DT &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                               -12&#x29;,&#xd;&#xa;                                                                    &#x27;-&#x27;,&#xd;&#xa;                                                                    &#x27;&#x27;&#x29;,&#xd;&#xa;                                                     1,&#xd;&#xa;                                                     6&#x29; THEN&#xd;&#xa;                              NVL&#x28;A.IN_STORE_QTY, 0&#x29;&#xd;&#xa;                             ELSE&#xd;&#xa;                              0&#xd;&#xa;                           END&#x29; AS L_Y_ALL_INSTORE_QTY --&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x5165;&#x5e93;&#x6570;&#xd;&#xa;                      ,SUM&#x28;CASE&#xd;&#xa;                             WHEN A.PART_DT &#x3d;&#xd;&#xa;                                  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                         1,&#xd;&#xa;                                         6&#x29; THEN&#xd;&#xa;                              NVL&#x28;A.IN_STORE_QTY, 0&#x29;&#xd;&#xa;                             ELSE&#xd;&#xa;                              0&#xd;&#xa;                           END&#x29; AS L_M_ALL_INSTORE_QTY --&#x4e0a;&#x6708;&#x5165;&#x5e93;&#x6570;&#xd;&#xa;                  FROM DMK.DMK_STR_OPERATE_D A&#xd;&#xa;                  LEFT JOIN &#x28;SELECT RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                                  ,RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                              FROM DIM.DIM_ORG&#xd;&#xa;                             WHERE ORG_TYPE &#x3d; &#x27;R&#x27; --&#x533a;&#x57df;&#x7c7b;&#x578b;&#xd;&#xa;                            &#x29; B&#xd;&#xa;                    ON A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;                  LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;                    ON A.PROJ_ID &#x3d; P.PROJ_ID&#xd;&#xa;                 WHERE A.PART_DT &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                    OR A.PART_DT &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#xd;&#xa;                                             &#x27;-&#x27;,&#xd;&#xa;                                             &#x27;&#x27;&#x29;,&#xd;&#xa;                              1,&#xd;&#xa;                              6&#x29;&#xd;&#xa;                    OR A.PART_DT &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                              1,&#xd;&#xa;                              6&#x29;&#xd;&#xa;                 GROUP BY NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;B.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;A.PROJ_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                         ,NVL&#x28;P.PROJ_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; WITH CUBE&#x29; C&#xd;&#xa;          LEFT JOIN &#x28;SELECT NVL&#x28;NVL&#x28;RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;PRO_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;                          ,SUM&#x28;NVL&#x28;LOSS_CNT, 0&#x29;&#x29; AS T_D_MASS_LOSS_QTY --&#x5f53;&#x6708;&#x8d28;&#x635f;&#x6570;&#x91cf;&#xd;&#xa;                      FROM ODS_BLPT.T_HAND_IN_STOCK_LOSS_LV&#xd;&#xa;                     WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29;&#xd;&#xa;                     GROUP BY NVL&#x28;RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;PRO_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; WITH CUBE&#x29; D&#xd;&#xa;            ON C.RGN_NAME &#x3d; D.RGN_NAME&#xd;&#xa;           AND C.PROJ_NAME &#x3d; D.PROJ_NAME&#xd;&#xa;          LEFT JOIN &#x28;SELECT NVL&#x28;NVL&#x28;RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;PRO_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;                          ,SUM&#x28;NVL&#x28;LOSS_CNT, 0&#x29;&#x29; AS L_Y_MASS_LOSS_QTY --&#x53bb;&#x5e74;&#x5f53;&#x6708;&#x8d28;&#x635f;&#x6570;&#x91cf;&#xd;&#xa;                      FROM ODS_BLPT.T_HAND_IN_STOCK_LOSS_LV&#xd;&#xa;                     WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                            -12&#x29;,&#xd;&#xa;                                                 &#x27;-&#x27;,&#xd;&#xa;                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29;&#xd;&#xa;                     GROUP BY NVL&#x28;RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;PRO_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; WITH CUBE&#x29; E&#xd;&#xa;            ON C.RGN_NAME &#x3d; E.RGN_NAME&#xd;&#xa;           AND C.PROJ_NAME &#x3d; E.PROJ_NAME&#xd;&#xa;          LEFT JOIN &#x28;SELECT NVL&#x28;NVL&#x28;RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;PRO_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;                          ,SUM&#x28;NVL&#x28;LOSS_CNT, 0&#x29;&#x29; AS L_M_MASS_LOSS_QTY --&#x4e0a;&#x6e38;&#x5f53;&#x6708;&#x8d28;&#x635f;&#x6570;&#x91cf;&#xd;&#xa;                      FROM ODS_BLPT.T_HAND_IN_STOCK_LOSS_LV&#xd;&#xa;                     WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;CHECK_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29;&#xd;&#xa;                     GROUP BY NVL&#x28;RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;PRO_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; WITH CUBE&#x29; F&#xd;&#xa;            ON C.RGN_NAME &#x3d; F.RGN_NAME&#xd;&#xa;           AND C.PROJ_NAME &#x3d; F.PROJ_NAME&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS PERIOD_WID&#xd;&#xa;              ,&#x27;Y&#x27; AS PERIOD_TYPE&#xd;&#xa;              ,C.RGN_ID AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;              ,C.RGN_NAME&#xd;&#xa;              ,C.PROJ_ID&#xd;&#xa;              ,C.PROJ_NAME&#xd;&#xa;              ,D.T_D_MASS_LOSS_QTY AS MASS_LOSS_QTY --&#x8d28;&#x635f;&#x6570;&#x91cf;&#xd;&#xa;              ,C.T_D_ALL_INSTORE_QTY AS T_D_ALL_INSTORE_QTY --&#x603b;&#x5e93;&#x5b58;&#x91cf;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;C.T_D_ALL_INSTORE_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  NULL&#xd;&#xa;                 ELSE&#xd;&#xa;                  D.T_D_MASS_LOSS_QTY &#x2f; C.T_D_ALL_INSTORE_QTY&#xd;&#xa;               END AS RATE --&#x8d28;&#x635f;&#x7387;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;C.L_Y_ALL_INSTORE_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  NULL&#xd;&#xa;                 ELSE&#xd;&#xa;                  E.L_Y_MASS_LOSS_QTY &#x2f; C.L_Y_ALL_INSTORE_QTY&#xd;&#xa;               END AS RATE_YOY --&#x540c;&#x671f;&#x503c; &#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;C.L_M_ALL_INSTORE_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  NULL&#xd;&#xa;                 ELSE&#xd;&#xa;                  F.L_M_MASS_LOSS_QTY &#x2f; C.L_M_ALL_INSTORE_QTY&#xd;&#xa;               END AS RATE_MOM --&#x4e0a;&#x6708;&#x503c;&#xd;&#xa;          FROM &#x28;SELECT NVL&#x28;NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;B.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;A.PROJ_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                      ,NVL&#x28;NVL&#x28;P.PROJ_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;, &#x27;&#x5168;&#x90e8;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;                      ,SUM&#x28;CASE&#xd;&#xa;                             WHEN SUBSTR&#x28;A.PART_DT, 1, 4&#x29; &#x3d;&#xd;&#xa;                                  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                         1,&#xd;&#xa;                                         4&#x29; AND&#xd;&#xa;                                  SUBSTR&#x28;A.PART_DT, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                                  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                     