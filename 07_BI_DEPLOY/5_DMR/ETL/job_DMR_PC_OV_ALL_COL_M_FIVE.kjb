      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_ALL_COL_M_26&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,T.PERIOD_TYPE&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          R.RGN_NAME&#xd;&#xa;       END AS RGN_NAME&#xd;&#xa;      ,T.PROJ_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.PROJ_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.PROJ_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          P.PROJ_NAME&#xd;&#xa;       END AS PROJ_NAME&#xd;&#xa;      ,T.ONTIME_ARRIVE_NUM&#xd;&#xa;      ,T.TOTAL_ARRIVE_NUM&#xd;&#xa;  FROM &#x28;		SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID&#xd;&#xa;            ,&#x27;M&#x27; AS PERIOD_TYPE&#xd;&#xa;            ,nvl&#x28;nvl&#x28;rgn_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,nvl&#x28;nvl&#x28;proj_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;            ,sum&#x28;arrive_ontime_qty&#x29; AS ONTIME_ARRIVE_NUM&#xd;&#xa;            ,sum&#x28;arrive_plan_qty&#x29; AS TOTAL_ARRIVE_NUM&#xd;&#xa;        FROM DMK.dmk_tp_shipqty_d&#xd;&#xa;        WHERE substr&#x28;period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        GROUP BY  nvl&#x28;rgn_id,&#x27;-1&#x27;&#x29;,nvl&#x28;proj_id,&#x27;-1&#x27;&#x29; WITH CUBE&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID&#xd;&#xa;            ,&#x27;Y&#x27; AS PERIOD_TYPE&#xd;&#xa;            ,nvl&#x28;nvl&#x28;rgn_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,nvl&#x28;nvl&#x28;proj_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;            ,sum&#x28;arrive_ontime_qty&#x29; AS ONTIME_ARRIVE_NUM&#xd;&#xa;            ,sum&#x28;arrive_plan_qty&#x29; AS TOTAL_ARRIVE_NUM&#xd;&#xa;        FROM DMK.dmk_tp_shipqty_d&#xd;&#xa;        WHERE SUBSTR&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29;&#xd;&#xa;            AND SUBSTR&#x28;period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        GROUP BY  nvl&#x28;rgn_id,&#x27;-1&#x27;&#x29;,nvl&#x28;proj_id,&#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS T&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG R&#xd;&#xa;    ON T.RGN_ID &#x3d; R.RGN_ID&#xd;&#xa;   AND R.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;    ON T.PROJ_ID &#x3d; P.PROJ_ID&#xd;&#xa; WHERE CASE&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;ALL&#x27; AND T.PROJ_ID NOT IN &#x28;&#x27;ALL&#x27;&#x29; THEN&#xd;&#xa;          1&#xd;&#xa;         ELSE&#xd;&#xa;          0&#xd;&#xa;       END &#x3d; 0&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
