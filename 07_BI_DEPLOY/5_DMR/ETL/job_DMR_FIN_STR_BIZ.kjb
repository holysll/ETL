d;&#x52a1;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN OTHER_VAT_INCOME_AMT ELSE 0 END&#x29; AS OTHER_VAT_INCOME_AMT_LM,&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN OTHER_VAT_INCOME_AMT ELSE 0 END&#x29; AS OTHER_VAT_INCOME_AMT_LYM,&#xd;&#xa;       SUM&#x28;CASE WHEN SUBSTR&#x28;PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                  THEN OTHER_VAT_INCOME_AMT ELSE 0 END&#x29; AS OTHER_VAT_INCOME_AMT_Y,&#xd;&#xa;       SUM&#x28;CASE WHEN SUBSTR&#x28;PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;&#xd;&#xa;                  THEN OTHER_VAT_INCOME_AMT ELSE 0 END&#x29; AS OTHER_VAT_INCOME_AMT_LY,&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN HUMAN_COST_AMT ELSE 0 END&#x29; AS HUMAN_COST_AMT_M,          --&#x4eba;&#x529b;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN HUMAN_COST_AMT ELSE 0 END&#x29; AS HUMAN_COST_AMT_LM,&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN HUMAN_COST_AMT ELSE 0 END&#x29; AS HUMAN_COST_AMT_LYM,&#xd;&#xa;       SUM&#x28;CASE WHEN SUBSTR&#x28;PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;&#x