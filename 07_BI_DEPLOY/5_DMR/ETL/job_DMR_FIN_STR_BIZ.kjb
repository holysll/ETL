    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_FIN_STR_BIZ PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT TMP.PERIOD_WID,&#xd;&#xa;       TMP.RGN_ID,&#xd;&#xa;       coalesce&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29;,&#xd;&#xa;       TMP.BASE_ID,&#xd;&#xa;       coalesce&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29;,&#xd;&#xa;       TMP.BRAND_ID,&#xd;&#xa;       coalesce&#x28;BRD.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29;,&#xd;&#xa;       PROFIT_AMT_M,&#xd;&#xa;       PROFIT_AMT_LM,&#xd;&#xa;       PROFIT_AMT_LYM,&#xd;&#xa;       PROFIT_AMT_Y,&#xd;&#xa;       PROFIT_AMT_LY,&#xd;&#xa;       INCOME_AMT_M,&#xd;&#xa;       INCOME_AMT_LM,&#xd;&#xa;       INCOME_AMT_LYM,&#xd;&#xa;       INCOME_AMT_Y,&#xd;&#xa;       INCOME_AMT_LY,&#xd;&#xa;       COST_AMT_M,&#xd;&#xa;       COST_AMT_LM,&#xd;&#xa;       COST_AMT_LYM,&#xd;&#xa;       COST_AMT_Y,&#xd;&#xa;       COST_AMT_LY,&#xd;&#xa;       MGR_INCOME_AMT_M,&#xd;&#xa;       MGR_INCOME_AMT_LM,&#xd;&#xa;       MGR_INCOME_AMT_LYM,&#xd;&#xa;       MGR_INCOME_AMT_Y,&#xd;&#xa;       MGR_INCOME_AMT_LY,&#xd;&#xa;       RENT_INCOME_AMT_M,&#xd;&#xa;       RENT_INCOME_AMT_LM,&#xd;&#xa;       RENT_INCOME_AMT_LYM,&#xd;&#xa;       RENT_INCOME_AMT_Y,&#xd;&#xa;       RENT_INCOME_AMT_LY,&#xd;&#xa;       SEC_CLN_INCOME_AMT_M,&#xd;&#xa;       SEC_CLN_INCOME_AMT_LM,&#xd;&#xa;       SEC_CLN_INCOME_AMT_LYM,&#xd;&#xa;       SEC_CLN_INCOME_AMT_Y,&#xd;&#xa;       SEC_CLN_INCOME_AMT_LY,&#xd;&#xa;       OTHER_VAT_INCOME_AMT_M,&#xd;&#xa;       OTHER_VAT_INCOME_AMT_LM,&#xd;&#xa;       OTHER_VAT_INCOME_AMT_LYM,&#xd;&#xa;       OTHER_VAT_INCOME_AMT_Y,&#xd;&#xa;       OTHER_VAT_INCOME_AMT_LY,&#xd;&#xa;       HUMAN_COST_AMT_M,&#xd;&#xa;       HUMAN_COST_AMT_LM,&#xd;&#xa;       HUMAN_COST_AMT_LYM,&#xd;&#xa;       HUMAN_COST_AMT_Y,&#xd;&#xa;       HUMAN_COST_AMT_LY,&#xd;&#xa;       RENT_COST_AMT_M,&#xd;&#xa;       RENT_COST_AMT_LM,&#xd;&#xa;       RENT_COST_AMT_LYM,&#xd;&#xa;       RENT_COST_AMT_Y,&#xd;&#xa;       RENT_COST_AMT_LY,&#xd;&#xa;       SEC_CLN_COST_AMT_M,&#xd;&#xa;       SEC_CLN_COST_AMT_LM,&#xd;&#xa;       SEC_CLN_COST_AMT_LYM,&#xd;&#xa;       SEC_CLN_COST_AMT_Y,&#xd;&#xa;       SEC_CLN_COST_AMT_LY,&#xd;&#xa;       OTHER_VAT_COST_AMT_M,&#xd;&#xa;       OTHER_VAT_COST_AMT_LM,&#xd;&#xa;       OTHER_VAT_COST_AMT_LYM,&#xd;&#xa;       OTHER_VAT_COST_AMT_Y,&#xd;&#xa;       OTHER_VAT_COST_AMT_LY,&#xd;&#xa;       FOUR_ITM_FEE_AMT_M,&#xd;&#xa;       FOUR_ITM_FEE_AMT_LM,&#xd;&#xa;       FOUR_ITM_FEE_AMT_LYM,&#xd;&#xa;       FOUR_ITM_FEE_AMT_Y,&#xd;&#xa;       FOUR_ITM_FEE_AMT_LY,&#xd;&#xa;       DEPRECIATION_AMT_M,&#xd;&#xa;       DEPRECIATION_AMT_LM,&#xd;&#xa;       DEPRECIATION_AMT_LYM,&#xd;&#xa;       DEPRECIATION_AMT_Y,&#xd;&#xa;       DEPRECIATION_AMT_LY,&#xd;&#xa;       EMPLOYEE_NUM_M,&#xd;&#xa;       EMPLOYEE_NUM_LM,&#xd;&#xa;       EMPLOYEE_NUM_LYM,&#xd;&#xa;       EMPLOYEE_NUM_Y,&#xd;&#xa;       EMPLOYEE_NUM_LY,&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;&#x29; AS W_INSERT_DT, --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;       &#x27;DW&#x27;                            AS PART_SC,     --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;       TMP.PERIOD_WID                  AS PART_DT      --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM TEMP.TEMP_DMR_FIN_STR_BIZ_01 TMP&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT rgn_id,rgn_name&#xd;&#xa;        FROM DIM.DIM_ORG&#xd;&#xa;        WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;        GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; RGN&#xd;&#xa;           ON TMP.RGN_ID &#x3d; RGN.rgn_id&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT base_id,base_name&#xd;&#xa;        FROM DIM.DIM_ORG&#xd;&#xa;        WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;        GROUP BY base_id,base_name&#xd;&#xa;    &#x29; BAS&#xd;&#xa;           ON TMP.BASE_ID &#x3d; BAS.base_id&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT brand_id,brand_name&#xd;&#xa;        FROM DIM.DIM_BRAND&#xd;&#xa;        GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29; BRD&#xd;&#xa;           ON TMP.BRAND_ID &#x3d; BRD.BRAND_ID&#xd;&#xa;&#x3b;</sql>
