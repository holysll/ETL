    <entry>
      <name>TO_TMP1</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_FIN_STR_BIZ_01&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; AS PERIOD_WID,&#xd;&#xa;       COALESCE&#x28;A.RGN_ID,B.RGN_ID,C.RGN_ID&#x29;,&#xd;&#xa;       COALESCE&#x28;A.BASE_ID,B.BASE_ID,C.BASE_ID&#x29;,&#xd;&#xa;       COALESCE&#x28;A.BRAND_ID,B.BRAND_ID,C.BRAND_ID&#x29;,&#xd;&#xa;       NVL&#x28;A.INCOME_AMT_M,0&#x29; - NVL&#x28;A.COST_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.INCOME_AMT_LM,0&#x29; - NVL&#x28;A.COST_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.INCOME_AMT_LYM,0&#x29; - NVL&#x28;A.COST_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.INCOME_AMT_Y,0&#x29; - NVL&#x28;A.COST_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.INCOME_AMT_LY,0&#x29; - NVL&#x28;A.COST_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;A.INCOME_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.INCOME_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.INCOME_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.INCOME_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.INCOME_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;A.COST_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.COST_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.COST_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.COST_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.COST_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;A.MGR_INCOME_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.MGR_INCOME_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.MGR_INCOME_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.MGR_INCOME_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.MGR_INCOME_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_INCOME_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_INCOME_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_INCOME_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_INCOME_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_INCOME_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SEC_CLN_INCOME_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SEC_CLN_INCOME_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SEC_CLN_INCOME_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SEC_CLN_INCOME_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SEC_CLN_INCOME_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_INCOME_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_INCOME_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_INCOME_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_INCOME_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_INCOME_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;A.HUMAN_COST_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.HUMAN_COST_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.HUMAN_COST_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.HUMAN_COST_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.HUMAN_COST_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_COST_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_COST_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_COST_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_COST_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.RENT_COST_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SEC_CLN_COST_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SEC_CLN_COST_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SEC_CLN_COST_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SEC_CLN_COST_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.SEC_CLN_COST_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_COST_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_COST_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_COST_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_COST_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;A.OTHER_VAT_COST_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;B.FOUR_ITM_FEE_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;B.FOUR_ITM_FEE_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;B.FOUR_ITM_FEE_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;B.FOUR_ITM_FEE_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;B.FOUR_ITM_FEE_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;B.DEPRECIATION_AMT_M,0&#x29;,&#xd;&#xa;       NVL&#x28;B.DEPRECIATION_AMT_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;B.DEPRECIATION_AMT_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;B.DEPRECIATION_AMT_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;B.DEPRECIATION_AMT_LY,0&#x29;,&#xd;&#xa;       NVL&#x28;C.EMPLOYEE_NUM_M,0&#x29;,&#xd;&#xa;       NVL&#x28;C.EMPLOYEE_NUM_LM,0&#x29;,&#xd;&#xa;       NVL&#x28;C.EMPLOYEE_NUM_LYM,0&#x29;,&#xd;&#xa;       NVL&#x28;C.EMPLOYEE_NUM_Y,0&#x29;,&#xd;&#xa;       NVL&#x28;C.EMPLOYEE_NUM_LY,0&#x29;&#xd;&#xa;  FROM &#x28;&#xd;&#xa;SELECT RGN_ID,&#xd;&#xa;       BASE_ID,&#xd;&#xa;       BRAND_ID,&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN STORE_BIZ_INCOME_AMT ELSE 0 END&#x29; AS INCOME_AMT_M,  --&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN STORE_BIZ_INCOME_AMT ELSE 0 END&#x29; AS INCOME_AMT_LM,&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN STORE_BIZ_INCOME_AMT ELSE 0 END&#x29; AS INCOME_AMT_LYM,&#xd;&#xa;       SUM&#x28;CASE WHEN SUBSTR&#x28;PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                  THEN STORE_BIZ_INCOME_AMT ELSE 0 END&#x29; AS INCOME_AMT_Y,&#xd;&#xa;       SUM&#x28;CASE WHEN SUBSTR&#x28;PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;&#xd;&#xa;                  THEN STORE_BIZ_INCOME_AMT ELSE 0 END&#x29; AS INCOME_AMT_LY,&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN STORE_BIZ_COST_AMT ELSE 0 END&#x29; AS COST_AMT_M,  --&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x6210;&#x672c;&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN STORE_BIZ_COST_AMT ELSE 0 END&#x29; AS COST_AMT_LM,&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN STORE_BIZ_COST_AMT ELSE 0 END&#x29; AS COST_AMT_LYM,&#xd;&#xa;       SUM&#x28;CASE WHEN SUBSTR&#x28;PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                  THEN STORE_BIZ_COST_AMT ELSE 0 END&#x29; AS COST_AMT_Y,&#xd;&#xa;       SUM&#x28;CASE WHEN SUBSTR&#x28;PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;&#xd;&#xa;                  THEN STORE_BIZ_COST_AMT ELSE 0 END&#x29; AS COST_AMT_LY,&#xd;&#xa;       SUM&#x28;CASE WHEN PERIOD_WID &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                  THEN VCL_MANAGER_INCOME_AMT ELSE 0 END&#x29; AS MGR_INCOME_AMT_M,  --&#x7ba1;&#x7406;&#x8d39;&#x6536;&#x5165;&#xd;&#xa;       SUM&#x28;CASE WH