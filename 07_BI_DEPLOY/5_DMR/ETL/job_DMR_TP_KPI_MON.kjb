      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_KPI_MON_03&#xd;&#xa;SELECT coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID &#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID &#xd;&#xa;    ,&#x27;K03&#x27; AS INDEX_ID&#xd;&#xa;    ,&#x27;&#x79fb;&#x5e93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.shipping_qty ELSE 0 END&#x29; AS MOV_ACT_QTY_M&#xd;&#xa;    ,NULL AS MOV_TAR_QTY_M&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.shipping_qty ELSE 0 END&#x29; AS MOV_ACT_QTY_LM&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.shipping_qty ELSE 0 END&#x29; AS MOV_ACT_QTY_LYM&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; THEN T.shipping_qty ELSE 0 END&#x29; AS MOV_ACT_QTY_Y&#xd;&#xa;    ,NULL AS MOV_TAR_QTY_Y&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; THEN T.shipping_qty ELSE 0 END&#x29; AS MOV_ACT_QTY_LY&#xd;&#xa;FROM DMK.dmk_tp_orderqty_d T &#xd;&#xa;WHERE &#x28;&#x28;substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;        AND substr&#x28;T.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND substr&#x28;T.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    &#x29;&#xd;&#xa;    OR &#x28;substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29;&#xd;&#xa;        AND substr&#x28;T.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    &#x29;&#x29;&#xd;&#xa;    AND T.plan_type &#x3d; &#x27;2&#x27;&#xd;&#xa;GROUP BY coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT coalesce&#x28;R.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;O.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,&#x27;K03&#x27; AS INDEX_ID&#xd;&#xa;    ,&#x27;&#x79fb;&#x5e93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;    ,count&#x28;CASE WHEN substr&#x28;P.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN O.vin ELSE NULL END&#x29; AS MOV_ACT_QTY_M&#xd;&#xa;    ,NULL AS MOV_TAR_QTY_M&#xd;&#xa;    ,count&#x28;CASE WHEN substr&#x28;P.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN O.vin ELSE NULL END&#x29; AS MOV_ACT_QTY_LM&#xd;&#xa;    ,count&#x28;CASE WHEN substr&#x28;P.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN O.vin ELSE NULL END&#x29; AS MOV_ACT_QTY_LYM&#xd;&#xa;    ,count&#x28;CASE WHEN substr&#x28;P.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; THEN O.vin ELSE NULL END&#x29; AS MOV_ACT_QTY_Y&#xd;&#xa;    ,NULL AS MOV_TAR_QTY_Y&#xd;&#xa;    ,count&#x28;CASE WHEN substr&#x28;P.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; THEN O.vin ELSE NULL END&#x29; AS MOV_ACT_QTY_LY&#xd;&#xa;FROM DW.dw_tp_ordermovement T&#xd;&#xa;INNER JOIN DW.dw_tp_orderrelease O&#xd;&#xa;ON T.order_release_gid &#x3d; O.order_release_gid&#xd;&#xa;INNER JOIN &#x28;&#xd;&#xa;    SELECT rgn_id&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id&#xd;&#xa;    &#x29; R&#xd;&#xa;ON O.RGN_ID &#x3d; R.RGN_ID&#xd;&#xa;INNER JOIN DIM.dim_period_d P  &#xd;&#xa;ON FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;O.FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - 28800,&#x27;yyyyMMdd&#x27;&#x29; &#x3d; P.period_wid AND P.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;WHERE &#x28;&#x28;substr&#x28;P.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29;&#xd;&#xa;            AND substr&#x28;P.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;        OR &#x28;substr&#x28;P.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29;&#xd;&#xa;            AND substr&#x28;P.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;    &#x29;&#xd;&#xa;    AND O.or_plan_type &#x3d; &#x27;&#x79fb;&#x5e93;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;    AND O.brand_id &#x3d; &#x27;FT&#x27;&#xd;&#xa;    AND O.is_known_shipper &#x3d; &#x27;N&#x27;&#xd;&#xa;    AND O.vin IS NOT NULL&#xd;&#xa;    AND O.arrive_day IS NOT NULL&#xd;&#xa;GROUP BY coalesce&#x28;R.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;O.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT coalesce&#x28;R.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,&#x27;K03&#x27; AS INDEX_ID&#xd;&#xa;    ,&#x27;&#x79fb;&#x5e93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;    ,count&#x28;CASE WHEN substr&#x28;P.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN O.vin ELSE NULL END&#x29; AS MOV_ACT_QTY_M&#xd;&#xa;    ,NULL AS MOV_TAR_QTY_M&#xd;&#xa;    ,count&#x28;CASE WHEN substr&#x28;P.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN O.vin ELSE NULL END&#x29; AS MOV_ACT_QTY_LM&#xd;&#xa;    ,count&#x28;CASE WHEN substr&#x28;P.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN O.vin ELSE NULL END&#x29; AS MOV_ACT_QTY_LYM&#xd;&#xa;    ,count&#x28;CASE WHEN substr&#x28;P.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; THEN O.vin ELSE NULL END&#x29; AS MOV_ACT_QTY_Y&#xd;&#xa;    ,NULL AS MOV_TAR_QTY_Y&#xd;&#xa;    ,count&#x28;CASE WHEN substr&#x28;P.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; THEN O.vin ELSE NULL END&#x29; AS MOV_ACT_QTY_LY&#xd;&#xa;FROM DW.dw_tp_ordermovement T&#xd;&#xa;INNER JOIN DW.dw_tp_orderrelease O&#xd;&#xa;ON T.order_release_gid &#x3d; O.order_release_gid&#xd;&#xa;INNER JOIN &#x28;&#xd;&#xa;    SELECT rgn_id&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id&#xd;&#xa;    &#x29; R&#xd;&#xa;ON O.RGN_ID &#x3d; R.RGN_ID&#xd;&#xa;INNER JOIN DIM.dim_period_d P  &#xd;&#xa;ON FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.unload_day, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - 28800,&#x27;yyyyMMdd&#x27;&#x29; &#x3d; P.period_wid AND P.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;WHERE &#x28;&#x28;substr&#x28;P.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29;&#xd;&#xa;            AND substr&#x28;P.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;        OR &#x28;substr&#x28;P.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29;&#xd;&#xa;            AND substr&#x28;P.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;    &#x29;&#xd;&#xa;    AND O.or_plan_type &#x3d; &#x27;&#x79fb;&#x5e93;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;    AND T.brand_id &#x3d; &#x27;CA&#x27;&#xd;&#xa;    AND O.is_known_shipper &#x3d; &#x27;N&#x27;&#xd;&#xa;    AND O.vin IS NOT NULL&#xd;&#xa;    AND O.arrive_day IS NOT NULL&#xd;&#xa;GROUP BY coalesce&#x28;R.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;SELECT O.RGN_ID&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,&#x27;K03&#x27; AS INDEX_ID&#xd;&#xa;      ,&#x27;&#x79fb;&#x5e93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                           THEN T.VIN ELSE NULL END&#x29; AS MOV_ACT_QTY_M&#xd;&#xa;      ,NULL AS MOV_TAR_QTY_M&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                           THEN T.VIN ELSE NULL END&#x29; AS MOV_ACT_QTY_LM&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                           THEN T.VIN ELSE NULL END&#x29; AS MOV_ACT_QTY_LYM&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN SUBSTR&#x28;D.PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                           THEN T.VIN ELSE NULL END&#x29; AS MOV_ACT_QTY_Y&#xd;&#xa;      ,NULL AS MOV_TAR_QTY_Y&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN SUBSTR&#x28;D.PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;&#xd;&#xa;                           THEN T.VIN ELSE NULL END&#x29; AS MOV_ACT_QTY_LY&#xd;&#xa;  FROM DW.DW_TP_ORDERRELEASE T&#xd;&#xa;    INNER JOIN DIM.DIM_ORG O&#xd;&#xa;            ON T.DEST_LOCATION_ID &#x3d; O.ORG_ID AND O.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;     LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;            ON O.BASE_ID &#x3d; P.BASE_ID&#xd;&#xa;           AND T.BRAND_ID &#x3d; P.BRAND_ID&#xd;&#xa;    INNER JOIN DIM.DIM_PERIOD_D D&#xd;&#xa;            ON FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - 28800,&#x27;yyyyMMdd&#x27;&#x29; &#x3d; D.PERIOD_WID&#xd;&#xa;      WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;        AND SUBSTR&#x28;D.PERIOD_WID,1,4&#x29; &#x3c;&#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;        AND SUBSTR&#x28;D.PERIOD_WID,1,4&#x29; &#x3e;&#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;&#xd;&#xa;        AND SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3c;&#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;        AND O.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;        AND T.OR_PLAN_TYPE &#x3d; &#x27;&#x79fb;&#x5e93;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;        AND T.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;        AND T.IS_KNOWN_SHIPPER &#x3d; &#x27;N&#x27; --&#x53bb;&#x9664;&#x5728;&#x9014;&#x542f;&#x7968;&#xd;&#xa;        AND T.VIN IS NOT NULL&#xd;&#xa;        AND T.FACTORY_DAY IS NOT NULL&#xd;&#xa;          GROUP BY O.RGN_ID&#xd;&#xa;                  ,T.BRAND_ID&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT O.RGN_ID&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,&#x27;K03&#x27; AS INDEX_ID&#xd;&#xa;      ,&#x27;&#x79fb;&#x5e93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                           THEN T.VIN ELSE NULL END&#x29; AS MOV_ACT_QTY_M&#xd;&#xa;      ,NULL AS MOV_TAR_QTY_M&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                           THEN T.VIN ELSE NULL END&#x29; AS MOV_ACT_QTY_LM&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                           THEN T.VIN ELSE NULL END&#x29; AS MOV_ACT_QTY_LYM&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN SUBSTR&#x28;D.PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                           THEN T.VIN ELSE NULL END&#x29; AS MOV_ACT_QTY_Y&#xd;&#xa;      ,NULL AS MOV_TAR_QTY_Y&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN SUBSTR&#x28;D.PERIOD_WID,1,4&#x29; &#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;&#xd;&#xa;                           THEN T.VIN ELSE NULL END&#x29; AS MOV_ACT_QTY_LY&#xd;&#xa;  FROM DW.DW_TP_ORDERRELEASE T&#xd;&#xa; INNER JOIN DW.DW_TP_ORDERMOVEMENT OM&#xd;&#xa;    ON T.ORDER_RELEASE_GID &#x3d; OM.ORDER_RELEASE_GID&#xd;&#xa;    INNER JOIN DIM.DIM_ORG O&#xd;&#xa;            ON T.DEST_LOCATION_ID &#x3d; O.ORG_ID AND O.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;     LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;            ON O.BASE_ID &#x3d; P.BASE_ID&#xd;&#xa;           AND T.BRAND_ID &#x3d; P.BRAND_ID&#xd;&#xa;    INNER JOIN DIM.DIM_PERIOD_D D&#xd;&#xa;            ON FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;OM.UNLOAD_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - 28800,&#x27;yyyyMMdd&#x27;&#x29; &#x3d; D.PERIOD_WID&#xd;&#xa;      WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;        AND SUBSTR&#x28;D.PERIOD_WID,1,4&#x29; &#x3c;&#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;        AND SUBSTR&#x28;D.PERIOD_WID,1,4&#x29; &#x3e;&#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;&#xd;&#xa;        AND SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3c;&#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;        AND O.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;        AND T.ORDER_RELEASE_NAME LIKE &#x27;5&#x25;&#x27;&#xd;&#xa;        AND T.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;        AND T.IS_KNOWN_SHIPPER &#x3d; &#x27;N&#x27; --&#x53bb;&#x9664;&#x5728;&#x9014;&#x542f;&#x7968;&#xd;&#xa;        AND T.VIN IS NOT NULL&#xd;&#xa;        AND OM.UNLOAD_DAY IS NOT NULL&#xd;&#xa;          GROUP BY O.RGN_ID&#xd;&#xa;                  ,T.BRAND_ID&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2a;&#x2f;</sql>
