x28;FACTORY_DAY, 1, 10&#x29;,&#xd;&#xa;                                      CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                             &#x27;-&#x27;,&#xd;&#xa;                             &#x27;&#x27;&#x29;,&#xd;&#xa;              1,&#xd;&#xa;              8&#x29;&#xd;&#xa;             THEN VIN ELSE NULL END&#x29; AS ARRIVAL_QTY_LM&#xd;&#xa;      ,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; &#xd;&#xa;             THEN VIN ELSE NULL END&#x29; AS ARRIVAL_QTY_LYM&#xd;&#xa;      ,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; &#xd;&#xa;                AND  SUBSTR&#x28;REGEXP_REPLACE&#x28;dlvr_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY, 1, 10&#x29;,&#xd;&#xa;                                      CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                             &#x27;-&#x27;,&#xd;&#xa;                             &#x27;&#x27;&#x29;,&#xd;&#xa;              1,&#xd;&#xa;              8&#x29;&#xd;&#xa;             THEN VIN ELSE NULL END&#x29; AS ARRIVAL_QTY_Y&#xd;&#xa;      ,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;SUBSTR&#x28;FACTORY_DAY,1,10&#x29;,CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;  &#xd;&#xa;              THEN VIN ELSE NULL END&#x29; AS ARRIVAL_TAR_QTY_Y&#xd;&#xa;      ,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_R