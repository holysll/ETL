      </entry>
    <entry>
      <name>TMP1</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_KPI_MON_01&#xd;&#xa;SELECT A.RGN_ID&#xd;&#xa;    ,A.BRAND_ID&#xd;&#xa;    ,A.INDEX_ID&#xd;&#xa;    ,A.INDEX_NAME&#xd;&#xa;    ,sum&#x28;BIZ_ACT_QTY_M&#x29; AS BIZ_ACT_QTY_M&#xd;&#xa;    ,sum&#x28;BIZ_TAR_QTY_M&#x29; AS BIZ_TAR_QTY_M&#xd;&#xa;    ,sum&#x28;BIZ_ACT_QTY_LM&#x29; AS BIZ_ACT_QTY_LM&#xd;&#xa;    ,sum&#x28;BIZ_ACT_QTY_LYM&#x29; AS BIZ_ACT_QTY_LYM&#xd;&#xa;    ,sum&#x28;BIZ_ACT_QTY_Y&#x29; AS BIZ_ACT_QTY_Y &#xd;&#xa;    ,sum&#x28;BIZ_TAR_QTY_Y&#x29; AS BIZ_TAR_QTY_Y&#xd;&#xa;    ,sum&#x28;BIZ_ACT_QTY_LY&#x29; AS BIZ_ACT_QTY_LY&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,&#x27;K01&#x27; AS INDEX_ID&#xd;&#xa;        ,&#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;        ,0 AS BIZ_ACT_QTY_M&#xd;&#xa;        ,sum&#x28;CASE WHEN T.period_wid &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.biz_qty ELSE 0 END&#x29; AS BIZ_TAR_QTY_M&#xd;&#xa;        ,0 AS BIZ_ACT_QTY_LM&#xd;&#xa;        ,0 AS BIZ_ACT_QTY_LYM&#xd;&#xa;        ,0 AS BIZ_ACT_QTY_Y &#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;T.period_wid  ,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29; THEN T.biz_qty ELSE 0 END&#x29; AS BIZ_TAR_QTY_Y&#xd;&#xa;        ,0 AS BIZ_ACT_QTY_LY&#xd;&#xa;    FROM DMK.dmk_tp_bizqtytgt_m T&#xd;&#xa;    LEFT JOIN DIM.dim_org T2&#xd;&#xa;    ON T.base_name &#x3d; T2.base_name AND T2.org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    WHERE &#x28;substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29;&#xd;&#xa;        AND T.period_wid &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;        AND T2.base_type &#x3d; &#x27;VDC&#x27;&#xd;&#xa;        AND T.rgn_id NOT IN &#x28;&#x27;BOYU&#x27;,&#x27;CENTER&#x27;&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,&#x27;K01&#x27; AS INDEX_ID&#xd;&#xa;        ,&#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.shipping_qty ELSE 0 END&#x29; AS BIZ_ACT_QTY_M&#xd;&#xa;        ,0 AS BIZ_TAR_QTY_M&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.shipping_qty ELSE 0 END&#x29; AS BIZ_ACT_QTY_LM&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.shipping_qty ELSE 0 END&#x29; AS BIZ_ACT_QTY_LYM&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; THEN T.shipping_qty ELSE 0 END&#x29; AS BIZ_ACT_QTY_Y&#xd;&#xa;        ,0 AS BIZ_TAR_QTY_Y&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29; THEN T.shipping_qty ELSE 0 END&#x29; AS BIZ_ACT_QTY_LY&#xd;&#xa;    FROM DMK.dmk_tp_shipqty_d T&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG T1&#xd;&#xa;    ON T.BASE_ID &#x3d; T1.BASE_ID AND T1.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;    WHERE &#x28;&#x28;substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;        AND substr&#x28;T.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND substr&#x28;T.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#x29;&#xd;&#xa;        OR &#x28;substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29;&#xd;&#xa;        AND substr&#x28;T.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;		AND T1.BASE_TYPE &#x3d; &#x27;VDC&#x27;	&#xd;&#xa;		AND T.tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;		AND T.brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; A&#xd;&#xa;GROUP BY A.RGN_ID&#xd;&#xa;    ,A.BRAND_ID&#xd;&#xa;    ,A.INDEX_ID&#xd;&#xa;    ,A.INDEX_NAME&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT ACT.RGN_ID&#xd;&#xa;      ,ACT.BRAND_ID&#xd;&#xa;      ,&#x27;K01&#x27; AS INDEX_ID&#xd;&#xa;      ,&#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,ACT.BIZ_ACT_QTY_M&#xd;&#xa;      ,TAR.BIZ_TAR_QTY_M&#xd;&#xa;      ,ACT.BIZ_ACT_QTY_LM&#xd;&#xa;      ,ACT.BIZ_ACT_QTY_LYM&#xd;&#xa;      ,ACT.BIZ_ACT_QTY_Y&#xd;&#xa;      ,TAR.BIZ_TAR_QTY_Y&#xd;&#xa;      ,ACT.BIZ_ACT_QTY_LY&#xd;&#xa;  FROM &#x28;SELECT T.RGN_ID&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN T.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,1,6&#x29; THEN&#xd;&#xa;                      T.BIZ_QTY&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BIZ_ACT_QTY_M&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN T.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,1,6&#x29; THEN&#xd;&#xa;                      T.BIZ_QTY&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BIZ_ACT_QTY_LM&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN T.PERIOD_WID &#x3d;&#xd;&#xa;                          SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#xd;&#xa;                                         &#x27;-&#x27;,&#xd;&#xa;                                         &#x27;&#x27;&#x29;,1,6&#x29; THEN&#xd;&#xa;                      T.BIZ_QTY&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BIZ_ACT_QTY_LYM&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN YEAR &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; THEN&#xd;&#xa;                      T.BIZ_QTY&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BIZ_ACT_QTY_Y&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN YEAR &#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;&#x29; THEN&#xd;&#xa;                      T.BIZ_QTY&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BIZ_ACT_QTY_LY&#xd;&#xa;          FROM DMK.DMK_TP_PROJBIZQTY_M T&#xd;&#xa;          JOIN DIM.DIM_ORG B&#xd;&#xa;            ON T.BASE_ID &#x3d; B.BASE_ID &#xd;&#xa;           AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;         WHERE YEAR &#x3e;&#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;&#x29;&#xd;&#xa;           AND YEAR &#x3c;&#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;           AND B.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;           AND T.PERIOD_WID &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;         GROUP BY T.RGN_ID&#xd;&#xa;                 ,T.BRAND_ID&#x29; ACT&#xd;&#xa;  LEFT JOIN &#x28;SELECT A.RGN_ID&#xd;&#xa;                   ,A.BRAND_ID&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN A.MONTH &#x3d; MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; THEN&#xd;&#xa;                           A.BIZ_QTY&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_TAR_QTY_M&#xd;&#xa;                   ,SUM&#x28;CASE&#xd;&#xa;                          WHEN A.YEAR &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; THEN&#xd;&#xa;                           A.BIZ_QTY&#xd;&#xa;                          ELSE&#xd;&#xa;                           0&#xd;&#xa;                        END&#x29; AS BIZ_TAR_QTY_Y&#xd;&#xa;               FROM DMK.DMK_TP_BIZQTYTGT_M A&#xd;&#xa;               LEFT JOIN DIM.DIM_ORG B&#xd;&#xa;                 ON A.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;                AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              WHERE A.YEAR &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                AND A.PERIOD_WID &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                AND B.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;              GROUP BY A.RGN_ID&#xd;&#xa;                      ,A.BRAND_ID&#x29; TAR&#xd;&#xa;    ON ACT.RGN_ID &#x3d; TAR.RGN_ID&#xd;&#xa;   AND ACT.BRAND_ID &#x3d; TAR.BRAND_ID&#x3b;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;</sql>
