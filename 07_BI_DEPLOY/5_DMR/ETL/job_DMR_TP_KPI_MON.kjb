      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_KPI_MON_01&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_KPI_MON_01&#x28;&#xd;&#xa;RGN_ID             STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BRAND_ID           STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;INDEX_ID           STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME         STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LM       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x4e0a;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_LYM      DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LY       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8fd0;&#x8f93;&#x6982;&#x89c8;&#xff08;&#x6708;&#xff09;-&#x8fd0;&#x8f93;KPI&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_KPI_MON_02&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_KPI_MON_02&#x28;&#xd;&#xa;RGN_ID             STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BRAND_ID           STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;INDEX_ID           STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME         STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LM       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x4e0a;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_LYM      DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LY       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8fd0;&#x8f93;&#x6982;&#x89c8;&#xff08;&#x6708;&#xff09;-&#x8fd0;&#x8f93;KPI&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_KPI_MON_03&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_KPI_MON_03&#x28;&#xd;&#xa;RGN_ID             STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BRAND_ID           STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;INDEX_ID           STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME         STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LM       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x4e0a;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_LYM      DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LY       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8fd0;&#x8f93;&#x6982;&#x89c8;&#xff08;&#x6708;&#xff09;-&#x8fd0;&#x8f93;KPI&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_KPI_MON_04&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_KPI_MON_04&#x28;&#xd;&#xa;RGN_ID             STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BRAND_ID           STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;INDEX_ID           STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME         STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LM       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x4e0a;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_LYM      DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LY       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8fd0;&#x8f93;&#x6982;&#x89c8;&#xff08;&#x6708;&#xff09;-&#x8fd0;&#x8f93;KPI&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_KPI_MON_05&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_KPI_MON_05&#x28;&#xd;&#xa;RGN_ID             STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BRAND_ID           STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;INDEX_ID           STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME         STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LM       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x4e0a;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_LYM      DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LY       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8fd0;&#x8f93;&#x6982;&#x89c8;&#xff08;&#x6708;&#xff09;-&#x8fd0;&#x8f93;KPI&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_KPI_MON_06&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_KPI_MON_06&#x28;&#xd;&#xa;RGN_ID             STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BRAND_ID           STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;INDEX_ID           STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME         STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LM       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x4e0a;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_LYM      DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LY       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8fd0;&#x8f93;&#x6982;&#x89c8;&#xff08;&#x6708;&#xff09;-&#x8fd0;&#x8f93;KPI&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_KPI_MON_07&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_KPI_MON_07&#x28;&#xd;&#xa;RGN_ID             STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BRAND_ID           STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;INDEX_ID           STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME         STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_M        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LM       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x4e0a;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_LYM      DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;ACT_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;,&#xd;&#xa;TAR_VALUE_Y        DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8ba1;&#x5212;&#x27;,&#xd;&#xa;ACT_VALUE_LY       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x540c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x8fbe;&#x6210;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8fd0;&#x8f93;&#x6982;&#x89c8;&#xff08;&#x6708;&#xff09;-&#x8fd0;&#x8f93;KPI&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;</sql>
