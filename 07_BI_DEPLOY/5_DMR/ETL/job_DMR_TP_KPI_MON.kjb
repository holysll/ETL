      <name>TMP5</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_KPI_MON_05&#xd;&#xa;SELECT A.RGN_ID&#xd;&#xa;      ,A.BRAND_ID&#xd;&#xa;      ,&#x27;K05&#x27; AS INDEX_ID&#xd;&#xa;      ,&#x27;&#x542f;&#x7968;&#x5206;&#x914d;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,SUM&#x28;A.ASSIGN_ACT_QTY_M&#x29; AS ASSIGN_ACT_QTY_M&#xd;&#xa;      ,SUM&#x28;A.ASSIGN_PLAN_QTY_M&#x29; AS ASSIGN_PLAN_QTY_M&#xd;&#xa;      ,SUM&#x28;A.ASSIGN_ACT_QTY_LM&#x29; AS ASSIGN_ACT_QTY_LM&#xd;&#xa;      ,SUM&#x28;A.ASSIGN_ACT_QTY_LYM&#x29; AS ASSIGN_ACT_QTY_LYM&#xd;&#xa;      ,SUM&#x28;A.ASSIGN_ACT_QTY_Y&#x29; AS ASSIGN_ACT_QTY_Y&#xd;&#xa;      ,SUM&#x28;A.ASSIGN_PLAN_QTY_Y&#x29; AS ASSIGN_PLAN_QTY_Y&#xd;&#xa;      ,SUM&#x28;A.ASSIGN_ACT_QTY_LY&#x29; AS ASSIGN_ACT_QTY_LY&#xd;&#xa;  FROM &#x28;&#xd;&#xa;    SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN allot_qty ELSE 0 END&#x29; AS ASSIGN_ACT_QTY_M&#xd;&#xa;        ,0 AS ASSIGN_PLAN_QTY_M&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN allot_qty ELSE 0 END&#x29; AS ASSIGN_ACT_QTY_LM&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN allot_qty ELSE 0 END&#x29; AS ASSIGN_ACT_QTY_LYM&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; THEN ticket_qty ELSE 0 END&#x29; AS ASSIGN_ACT_QTY_Y&#xd;&#xa;        ,0 AS ASSIGN_PLAN_QTY_Y&#xd;&#xa;        ,sum&#x28;CASE WHEN substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29; THEN allot_qty ELSE 0 END&#x29; AS ASSIGN_ACT_QTY_LY&#xd;&#xa;    FROM DMK.dmk_tp_orderqty_d &#xd;&#xa;    WHERE &#x28;&#x28;substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;        AND substr&#x28;period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;        OR &#x28;substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29;&#xd;&#xa;        AND substr&#x28;period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#x29;&#xd;&#xa;	GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT coalesce&#x28;RGN.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,coalesce&#x28;BRD.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,0 AS ASSIGN_ACT_QTY_M&#xd;&#xa;      ,SUM&#x28;CASE WHEN SUBSTR&#x28;MONTH_ID,5,2&#x29; &#x3d; MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                THEN TKT.REV_PLAN ELSE NULL END&#x29; AS ASSIGN_PLAN_QTY_M&#xd;&#xa;      ,0 AS ASSIGN_ACT_QTY_LM&#xd;&#xa;      ,0 AS ASSIGN_ACT_QTY_LYM&#xd;&#xa;      ,0 AS ASSIGN_ACT_QTY_Y&#xd;&#xa;      ,SUM&#x28;TKT.REV_PLAN&#x29; AS ASSIGN_PLAN_QTY_Y&#xd;&#xa;      ,0 AS ASSIGN_ACT_QTY_LY&#xd;&#xa;    FROM ODS_BLPT.T_HAND_REV_TICKET_PLAN TKT&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;           ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;          AND TKT.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;    LEFT JOIN DIM.DIm_BRAND BRD&#xd;&#xa;           ON TKT.BRAND_NAME &#x3d; BRD.BRAND_NAME&#xd;&#xa;    WHERE SUBSTR&#x28;MONTH_ID,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;        AND SUBSTR&#x28;MONTH_ID,5,2&#x29; &#x3c;&#x3d; MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;RGN.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;      ,coalesce&#x28;BRD.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; A&#xd;&#xa;GROUP BY A.RGN_ID&#xd;&#xa;     ,A.BRAND_ID&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT ACT.RGN_ID&#xd;&#xa;      ,ACT.BRAND_ID&#xd;&#xa;      ,&#x27;K05&#x27; AS INDEX_ID&#xd;&#xa;      ,&#x27;&#x542f;&#x7968;&#x5206;&#x914d;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,ACT.ASSIGN_ACT_QTY_M&#xd;&#xa;      ,TAR.ASSIGN_PLAN_QTY_M&#xd;&#xa;      ,ACT.ASSIGN_ACT_QTY_LM&#xd;&#xa;      ,ACT.ASSIGN_ACT_QTY_LYM&#xd;&#xa;      ,ACT.ASSIGN_ACT_QTY_Y&#xd;&#xa;      ,TAR.ASSIGN_PLAN_QTY_Y&#xd;&#xa;      ,ACT.ASSIGN_ACT_QTY_LY&#xd;&#xa;  FROM &#x28;SELECT RGN_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                          THEN ORDER_RELEASE_GID ELSE NULL END&#x29; AS ASSIGN_ACT_QTY_M&#xd;&#xa;              ,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                          THEN ORDER_RELEASE_GID ELSE NULL END&#x29; AS ASSIGN_ACT_QTY_LM&#xd;&#xa;              ,COUNT&#x28;CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                          THEN ORDER_RELEASE_GID ELSE NULL END&#x29; AS ASSIGN_ACT_QTY_LYM&#xd;&#xa;              ,COUNT&#x28;CASE WHEN SUBSTR&#x28;EARLY_PICKUP_DATE,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                          THEN ORDER_RELEASE_GID ELSE NULL END&#x29; AS ASSIGN_ACT_QTY_Y&#xd;&#xa;              ,COUNT&#x28;CASE WHEN SUBSTR&#x28;EARLY_PICKUP_DATE,1,4&#x29; &#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;&#xd;&#xa;                          THEN ORDER_RELEASE_GID ELSE NULL END&#x29; AS ASSIGN_ACT_QTY_LY&#xd;&#xa;          FROM DW.DW_TP_ORDERRELEASE &#xd;&#xa;            WHERE 1&#x3d;1&#xd;&#xa;              --AND OR_PLAN_TYPE&#x3d;&#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;              AND ORDER_STAUS &#x3c;&#x3e; &#x27;&#x672a;&#x5206;&#x914d;&#x27;&#xd;&#xa;              AND SUBSTR&#x28;EARLY_PICKUP_DATE,1,4&#x29; &#x3c;&#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;              AND SUBSTR&#x28;EARLY_PICKUP_DATE,1,4&#x29; &#x3e;&#x3d; YEAR&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;&#xd;&#xa;              AND SUBSTR&#x28;REGEXP_REPLACE&#x28;EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3c;&#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;                GROUP BY RGN_ID&#xd;&#xa;                        ,BRAND_ID&#xd;&#xa;       &#x29; ACT&#xd;&#xa;    LEFT JOIN &#x28;SELECT RGN.RGN_ID&#xd;&#xa;                     ,BRD.BRAND_ID&#xd;&#xa;                     ,SUM&#x28;CASE WHEN SUBSTR&#x28;MONTH_ID,5,2&#x29; &#x3d; MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                               THEN TKT.REV_PLAN ELSE NULL END&#x29; AS ASSIGN_PLAN_QTY_M&#xd;&#xa;                     ,SUM&#x28;TKT.REV_PLAN&#x29; AS ASSIGN_PLAN_QTY_Y&#xd;&#xa;                 FROM ODS_BLPT.T_HAND_REV_TICKET_PLAN TKT&#xd;&#xa;                   LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;                          ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                         AND TKT.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;                   LEFT JOIN DIM.DIm_BRAND BRD&#xd;&#xa;                          ON TKT.BRAND_NAME &#x3d; BRD.BRAND_NAME&#xd;&#xa;                     WHERE SUBSTR&#x28;MONTH_ID,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                       AND SUBSTR&#x28;MONTH_ID,5,2&#x29; &#x3c;&#x3d; MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                         GROUP BY RGN.RGN_ID&#xd;&#xa;                                 ,BRD.BRAND_ID&#xd;&#xa;              &#x29; TAR ON ACT.RGN_ID &#x3d; TAR.RGN_ID&#xd;&#xa;                   AND ACT.BRAND_ID &#x3d; TAR.BRAND_ID&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;&#x3b;</sql>
