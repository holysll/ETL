      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_KPI_MON_07&#xd;&#xa;SELECT coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,&#x27;K07&#x27; AS INDEX_ID&#xd;&#xa;    ,&#x27;&#x51c6;&#x65f6;&#x4ea4;&#x4ed8;&#x6570;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.dlvr_ontime_qty ELSE 0 END&#x29; AS ARRIVAL_QTY_M&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.dlvr_plan_qty ELSE 0 END&#x29; AS ARRIVAL_TAR_QTY_M&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.dlvr_ontime_qty ELSE 0 END&#x29; AS ARRIVAL_QTY_LM&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.dlvr_ontime_qty ELSE 0 END&#x29; AS ARRIVAL_QTY_LYM&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; THEN T.dlvr_ontime_qty ELSE 0 END&#x29; AS ARRIVAL_QTY_Y&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; THEN T.dlvr_plan_qty ELSE 0 END&#x29; AS ARRIVAL_TAR_QTY_Y&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29; THEN T.dlvr_ontime_qty ELSE 0 END&#x29; AS ARRIVAL_QTY_LY&#xd;&#xa;FROM DMK.DMK_TP_SHIPQTY_D T   &#xd;&#xa;LEFT JOIN DIM.dim_org B  &#xd;&#xa;ON T.base_id &#x3d; B.base_id AND B.org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;WHERE &#x28;&#x28;substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;        AND substr&#x28;T.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND substr&#x28;T.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;    OR &#x28;substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,1,4&#x29;&#xd;&#xa;        AND substr&#x28;T.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;    &#x29;&#xd;&#xa;    AND T.tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;    AND T.brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;GROUP BY coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT coalesce&#x28;RGN_ID,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,