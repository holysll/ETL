      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_3&#xd;&#xa;SELECT  COALESCE&#x28;T1.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID  --&#x5927;&#x533a;ID&#xd;&#xa;        ,COALESCE&#x28;T1.BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID  --&#x57fa;&#x5730;ID&#xd;&#xa;        ,COALESCE&#x28;T1.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID  --&#x54c1;&#x724c;ID&#xd;&#xa;        ,COALESCE&#x28;T1.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID  --&#x9879;&#x76ee;ID&#xd;&#xa;        ,COALESCE&#x28;T1.TRANSPORTMODE_ID,&#x27;ALL&#x27;&#x29; AS TP_MODE_ID  --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;        ,&#x27;CATD_04&#x27; AS CATD_ID  --&#xd;&#xa;        ,&#x27;&#x53d1;&#x8fd0;-&#x4ea4;&#x4ed8;&#x27; AS CATD_NAME  --&#xd;&#xa;        ,AVG&#x28;CASE WHEN SUBSTR&#x28;regexp_replace&#x28;T1.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                  THEN &#x28;UNIX_TIMESTAMP&#x28;T1.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600&#xd;&#xa;                  ELSE 0 END&#x29; AS AVG_DTD_TIME  --&#xd;&#xa;FROM    TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_1  T1&#xd;&#xa;GROUP BY T1.RGN_ID,T1.base_id,T1.brand_id,T1.transportmode_id,T1.PROJ_ID WITH CUBE&#xd;&#xa;UNION ALL&#xd;&#xa;&#xd;&#xa;SELECT A.RGN_ID AS RGN_ID  --&#x5927;&#x533a;ID&#xd;&#xa;      ,A.BASE_ID AS BASE_ID  --&#x57fa;&#x5730;ID&#xd;&#xa;      ,A.BRAND_ID AS BRAND_ID  --&#x54c1;&#x724c;ID&#xd;&#xa;      ,A.PROJ_ID AS PROJ_ID  --&#x9879;&#x76ee;ID&#xd;&#xa;      ,A.TP_MODE_ID AS TP_MODE_ID  --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;      ,CASE WHEN B.CATD_NAME &#x3d; &#x27;&#x63a5;&#x5355;-&#x5206;&#x914d;&#x27; THEN &#x27;CATD_01&#x27;&#xd;&#xa;            WHEN B.CATD_NAME &#x3d; &#x27;&#x5206;&#x914d;-&#x51fa;&#x5e93;&#x27; THEN &#x27;CATD_02&#x27;&#xd;&#xa;            WHEN B.CATD_NAME &#x3d; &#x27;&#x63a5;&#x8f66;-&#x6253;&#x5355;&#x27; THEN &#x27;CATD_03&#x27;&#xd;&#xa;            ELSE &#x27;-1&#x27; END AS CATD_ID  --&#xd;&#xa;      ,B.CATD_NAME AS CATD_NAME  --&#xd;&#xa;      ,B.STAND_VALUE AS AVG_DTD_TIME  --&#xd;&#xa;FROM&#x28; &#xd;&#xa;SELECT COALESCE&#x28;T1.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID  --&#x5927;&#x533a;ID&#xd;&#xa;      ,COALESCE&#x28;T1.BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID  --&#x57fa;&#x5730;ID&#xd;&#xa;      ,COALESCE&#x28;T1.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID  --&#x54c1;&#x724c;ID&#xd;&#xa;      ,COALESCE&#x28;T1.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID  --&#x9879;&#x76ee;ID&#xd;&#xa;      ,COALESCE&#x28;T1.TRANSPORTMODE_ID,&#x27;ALL&#x27;&#x29; AS TP_MODE_ID  --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;FROM TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_1 T1&#xd;&#xa;GROUP BY T1.RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;        ,T1.BASE_ID  --&#x57fa;&#x5730;ID&#xd;&#xa;        ,T1.BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;        ,T1.PROJ_ID  --&#x9879;&#x76ee;ID&#xd;&#xa;        ,T1.TRANSPORTMODE_ID WITH CUBE  --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;&#x29; A&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT  CASE WHEN TPMODE_NAME &#x3d; &#x27;&#x516c;&#x8def;&#x27; THEN &#x27;GL&#x27;&#xd;&#xa;                 WHEN TPMODE_NAME &#x3d; &#x27;&#x6c34;&#x8def;&#x27; THEN &#x27;SL&#x27;&#xd;&#xa;                 WHEN TPMODE_NAME &#x3d; &#x27;&#x94c1;&#x8def;&#x27; THEN &#x27;TL&#x27;&#xd;&#xa;                 WHEN TPMODE_NAME &#x3d; &#x27;&#x4eba;&#x5de5;&#x27; THEN &#x27;LP&#x27;&#xd;&#xa;                 WHEN TPMODE_NAME &#x3d; &#x27;&#x6d77;&#x8fd0;&#x27; THEN &#x27;HY&#x27; &#xd;&#xa;                 WHEN TPMODE_NAME &#x3d; &#x27;&#x81ea;&#x63d0;&#x27; THEN &#x27;ZT&#x27;&#xd;&#xa;                 ELSE &#x27;-1&#x27; END AS TPMODE_CODE  --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x4ee3;&#x7801;&#xd;&#xa;           ,CATD_NAME AS CATD_NAME  --&#x8fd0;&#x8f93;&#x73af;&#x8282;&#x540d;&#x79f0;&#xd;&#xa;           ,CASE WHEN DAY_VALUE &#x3d; &#x27;&#x5c0f;&#x65f6;&#x27; THEN REGEXP_REPLACE&#x28;STAND_VALUE,&#x27;h&#x27;,&#x27;&#x27;&#x29; &#xd;&#xa;                 WHEN DAY_VALUE &#x3d; &#x27;&#x5929;&#x27; THEN STAND_VALUE&#x2a;24 &#xd;&#xa;                 ELSE STAND_VALUE END AS STAND_VALUE  --&#x8fd0;&#x8f93;&#x73af;&#x8282;&#x6807;&#x51c6;&#x65f6;&#x95f4;&#xd;&#xa;           ,&#x27;&#x5c0f;&#x65f6;&#x27; AS DAY_VALUE  --&#x5355;&#x4f4d;&#xd;&#xa;    FROM ODS_BLPT.T_HAND_DTD_SEGM_TIME_PLAN&#xd;&#xa;    WHERE YEAR_ID &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;        AND catd_name NOT IN&#x28;&#x27;&#x53d1;&#x8fd0;-&#x4ea4;&#x4ed8;&#x27;,&#x27;&#x56de;&#x5355;-&#x7ed3;&#x7b97;&#x27;&#x29;&#xd;&#xa;    &#x29; B&#xd;&#xa;ON A.TP_MODE_ID &#x3d; B.TPMODE_CODE</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
