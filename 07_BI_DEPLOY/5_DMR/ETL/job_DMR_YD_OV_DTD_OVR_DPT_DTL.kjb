B.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,coalesce&#x28;B.TPMODE_ID,&#x27;ALL&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,0 AS CATD_ONE_OVER_QTY&#xd;&#xa;        ,0 AS CATD_TWO_OVER_QTY&#xd;&#xa;        ,0 AS CATD_THREE_OVER_QTY&#xd;&#xa;        ,sum&#x28;CUR_M_OT_QTY&#x29; AS CATD_FOUR_OVER_QTY&#xd;&#xa;    FROM&#x28;&#xd;&#xa;        SELECT coalesce&#x28;A.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,coalesce&#x28;A.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;            ,coalesce&#x28;A.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;            ,coalesce&#x28;A.proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;            ,coalesce&#x28;A.TPMODE_ID,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;            ,sum&#x28;CUR_M_OT_QTY&#x29; AS CUR_M_OT_QTY&#xd;&#xa;        FROM&#x28; &#xd;&#xa;            SELECT SD.rgn_id&#xd;&#xa;                ,SD.base_id&#xd;&#xa;                ,SD.brand_id&#xd;&#xa;                ,SD.proj_id&#xd;&#xa;                ,SD.transportmode_id AS TPMODE_ID&#xd;&#xa;                ,SD.vin&#xd;&#xa;                ,CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;SD.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; 24&#x2a;SD.on_passage_days&#xd;&#xa;                        THEN 1 ELSE 0 END AS CUR_M_OT_QTY  --&#xd;&#xa;            FROM TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_1 SD&#xd;&#xa;            WHERE &#x28;&#xd;&#xa;                   SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                &#x29;&#xd;&#xa;            &#x29; A&#xd;&#xa;        GROUP BY coalesce&#x28;A.rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;            ,coalesce&#x28;A.base_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;            ,coalesce&#x28;A.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;A.proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;A.TPMODE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;        &#x29; B&#xd;&#xa;    GROUP BY B.RGN_ID,B.BASE_ID,B.BRAND_ID,B.PROJ_ID,B.PROJ_ID,B.TPMODE_ID WITH CUBE&#xd;&#xa;    &#x29; T&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT base_id,base_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY base_id,base_name&#xd;&#xa;    &#x29; T2&#xd;&#xa;ON T.base_id &#x3d; T2.base_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.dim_brand &#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29; T3&#xd;&#xa;ON T.brand_id &#x3d; T3.brand_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT proj_id,proj_name&#xd;&#xa;    FROM DIM.dim_proj &#xd;&#xa;    GROUP BY proj_id,proj_name&#xd;&#xa;    &#x29; T4&#xd;&#xa;ON T.proj_id &#x3d; T4.proj_id&#xd;&#xa;LEFT JOIN &#x28;&#xd;&#xa;    SELECT tpmode_id,tpmode_name&#xd;&#xa;    FROM DIM.dim_transport_mode&#xd;&#xa;    GROUP BY tpmode_id,tpmode_name&#xd;&#xa;    &#x29; T5&#xd;&#xa;ON T.TPMODE_ID &#x3d; T5.TPMODE_ID&#xd;&#xa;WHERE  1&#x3d;1 &#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
