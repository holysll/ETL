    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_OV_DTD_OVR_DPT_DTL PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT T.MONTH_ID&#xd;&#xa;    ,T.RNG_ID&#xd;&#xa;    ,T.RNG_NAME&#xd;&#xa;    ,T.base_id&#xd;&#xa;    ,T.BASE_NAME&#xd;&#xa;    ,T.brand_id&#xd;&#xa;    ,T.BRAND_NAME&#xd;&#xa;    ,T.TP_MODE_ID&#xd;&#xa;    ,T.TP_MODE_NAME&#xd;&#xa;    ,T.RALATION_DPT_ID&#xd;&#xa;    ,T.RELATION_DPT_NAME&#xd;&#xa;    ,SUM&#x28;T.CATD_ONE_OVER_QTY&#x29; AS CATD_ONE_OVER_QTY&#xd;&#xa;    ,SUM&#x28;T.CATD_TWO_OVER_QTY&#x29; AS CATD_TWO_OVER_QTY&#xd;&#xa;    ,SUM&#x28;T.CATD_THREE_OVER_QTY&#x29; AS CATD_THREE_OVER_QTY&#xd;&#xa;    ,SUM&#x28;T.CATD_FOUR_OVER_QTY&#x29; AS CATD_FOUR_OVER_QTY&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM &#x28;&#xd;&#xa;    SELECT &#x2a;&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_JD_FP&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT &#x2a;&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_FP_CK&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT &#x2a;&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_JC_DD&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT &#x2a;&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_FY_JF&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.MONTH_ID&#xd;&#xa;    ,T.RNG_ID&#xd;&#xa;    ,T.RNG_NAME&#xd;&#xa;    ,T.base_id&#xd;&#xa;    ,T.BASE_NAME&#xd;&#xa;    ,T.brand_id&#xd;&#xa;    ,T.BRAND_NAME&#xd;&#xa;    ,T.TP_MODE_ID&#xd;&#xa;    ,T.TP_MODE_NAME&#xd;&#xa;    ,T.RALATION_DPT_ID&#xd;&#xa;    ,T.RELATION_DPT_NAME&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID  --&#x6708;&#x4efd;ID &#xd;&#xa;      ,T1.rgn_id AS RGN_ID  --&#xd;&#xa;      ,T4.RGN_NAME AS RGN_NAME  --&#xd;&#xa;      ,T1.base_id AS BASE_ID  --&#xd;&#xa;      ,T5.BASE_NAME AS BASE_NAME  --&#xd;&#xa;      ,T1.brand_id AS BRAND_ID  --&#xd;&#xa;      ,T6.BRAND_NAME AS BRAND_NAME  --&#xd;&#xa;      ,T1.PROJ_ID AS PROJ_ID  --&#xd;&#xa;      ,T8.PROJ_NAME AS PROJ_NAME  --&#xd;&#xa;      ,T1.transportmode_id AS TP_MODE_ID  --&#xd;&#xa;      ,T7.TPMODE_NAME AS TPMODE_NAME  --&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;T1.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; T2.AVG_DTD_TIME_ONE&#xd;&#xa;                         THEN T1.order_release_name ELSE NULL END&#x29; AS CATD_ONE_OVER_QTY &#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;T1.OUT_STORE_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; T2.AVG_DTD_TIME_ONE&#xd;&#xa;                         THEN T1.order_release_name ELSE NULL END&#x29; AS CATD_TWO_OVER_QTY &#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;T1.FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.OUT_STORE_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; T2.AVG_DTD_TIME_ONE&#xd;&#xa;                         THEN T1.order_release_name ELSE NULL END&#x29; AS CATD_THREE_OVER_QTY &#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;T1.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; T2.AVG_DTD_TIME_ONE&#xd;&#xa;                         THEN T1.order_release_name ELSE NULL END&#x29; AS CATD_FOUR_OVER_QTY &#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;OTM&#x27; AS PART_SC  --&#xd;&#xa;      ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT  -- &#xd;&#xa;FROM TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_1 T1&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id&#xd;&#xa;          ,base_id&#xd;&#xa;          ,brand_id&#xd;&#xa;          ,proj_id&#xd;&#xa;          ,tp_mode_id&#xd;&#xa;          ,AVG&#x28;CASE WHEN catd_id &#x3d; &#x27;CATD_01&#x27; THEN avg_dtd_time END&#x29; AS AVG_DTD_TIME_ONE&#xd;&#xa;          ,AVG&#x28;CASE WHEN catd_id &#x3d; &#x27;CATD_02&#x27; THEN avg_dtd_time END&#x29; AS AVG_DTD_TIME_TWO&#xd;&#xa;          ,AVG&#x28;CASE WHEN catd_id &#x3d; &#x27;CATD_03&#x27; THEN avg_dtd_time END&#x29; AS AVG_DTD_TIME_THREE&#xd;&#xa;          ,AVG&#x28;CASE WHEN catd_id &#x3d; &#x27;CATD_04&#x27; THEN avg_dtd_time END&#x29; AS AVG_DTD_TIME_FOUR&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_3&#xd;&#xa;    GROUP BY rgn_id&#xd;&#xa;          ,base_id&#xd;&#xa;          ,brand_id&#xd;&#xa;          ,proj_id&#xd;&#xa;          ,tp_mode_id&#xd;&#xa;    &#x29; T2&#xd;&#xa;ON T1.rgn_id &#x3d; T2.rgn_id AND T1.base_id &#x3d; T2.base_id AND T1.brand_id &#x3d; T2.brand_id AND T1.transportmode_id &#x3d; T2.tp_mode_id AND T1.PROJ_ID &#x3d; T2.PROJ_ID&#xd;&#xa;INNER JOIN&#x28;&#xd;&#xa;    SELECT RGN_ID,RGN_NAME&#xd;&#xa;    FROM DIM.DIM_ORG&#xd;&#xa;    WHERE ORG_TYPE IN &#x28;&#x27;R&#x27;&#x29;&#xd;&#xa;    GROUP BY RGN_ID,RGN_NAME&#xd;&#xa;    &#x29; T4&#xd;&#xa;ON T1.RGN_ID &#x3d; T4.RGN_ID&#xd;&#xa;INNER JOIN&#x28;&#xd;&#xa;    SELECT BASE_ID,BASE_NAME&#xd;&#xa;    FROM DIM.DIM_ORG&#xd;&#xa;    WHERE ORG_TYPE IN &#x28;&#x27;B&#x27;&#x29;&#xd;&#xa;    GROUP BY BASE_ID,BASE_NAME&#xd;&#xa;    &#x29; T5&#xd;&#xa;ON T1.BASE_ID &#x3d; T5.BASE_ID&#xd;&#xa;INNER JOIN&#x28;&#xd;&#xa;    SELECT BRAND_ID,BRAND_NAME&#xd;&#xa;    FROM DIM.DIM_BRAND&#xd;&#xa;    GROUP BY BRAND_ID,BRAND_NAME&#xd;&#xa;    &#x29; T6&#xd;&#xa;ON T1.BRAND_ID &#x3d; T6.BRAND_ID&#xd;&#xa;INNER JOIN&#x28;&#xd;&#xa;    SELECT TPMODE_ID,TPMODE_NAME&#xd;&#xa;    FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;    GROUP BY TPMODE_ID,TPMODE_NAME&#xd;&#xa;    &#x29; T7&#xd;&#xa;ON T1.transportmode_id &#x3d; T7.TPMODE_ID&#xd;&#xa;INNER JOIN&#x28;&#xd;&#xa;    SELECT PROJ_ID,PROJ_NAME&#xd;&#xa;    FROM DIM.DIM_PROJ&#xd;&#xa;    GROUP BY PROJ_ID,PROJ_NAME&#xd;&#xa;    &#x29; T8 ON t1.proj_id &#x3d; t8.proj_id&#xd;&#xa;WHERE &#x28;UNIX_TIMESTAMP&#x28;T1.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; T2.AVG_DTD_TIME_ONE&#xd;&#xa;    OR &#x28;UNIX_TIMESTAMP&#x28;T1.OUT_STORE_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; T2.AVG_DTD_TIME_TWO&#xd;&#xa;    OR &#x28;UNIX_TIMESTAMP&#x28;T1.FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.OUT_STORE_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; T2.AVG_DTD_TIME_THREE&#xd;&#xa;    OR &#x28;UNIX_TIMESTAMP&#x28;T1.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;T1.FACTORY_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 &#x3e; T2.AVG_DTD_TIME_FOUR&#xd;&#xa;GROUP BY T1.rgn_id&#xd;&#xa;        ,T4.RGN_NAME&#xd;&#xa;        ,T1.base_id&#xd;&#xa;        ,T5.BASE_NAME&#xd;&#xa;        ,T1.brand_id&#xd;&#xa;        ,T6.BRAND_NAME&#xd;&#xa;        ,T1.PROJ_ID&#xd;&#xa;        ,T8.PROJ_NAME&#xd;&#xa;        ,T1.transportmode_id&#xd;&#xa;        ,T7.TPMODE_NAME&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
