    <entry>
      <name>JD_FP</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_JD_FP&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;    ,T.rgn_id AS RNG_ID&#xd;&#xa;    ,CASE WHEN T.rgn_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.rgn_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.rgn_name END AS RNG_NAME&#xd;&#xa;    ,T.base_id&#xd;&#xa;    ,CASE WHEN T.base_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.base_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.base_name END AS BASE_NAME&#xd;&#xa;    ,T.brand_id&#xd;&#xa;    ,CASE WHEN T.brand_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.brand_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.brand_name END AS BRAND_NAME&#xd;&#xa;    ,T.TPMODE_ID AS TP_MODE_ID&#xd;&#xa;    ,CASE WHEN T.TPMODE_ID &#x3d; &#x27;LP&#x27; THEN &#x27;&#x4eba;&#x5de5;&#x27; &#xd;&#xa;        ELSE T5.TPMODE_NAME END AS TP_MODE_NAME&#xd;&#xa;    ,T.PROJ_ID AS RALATION_DPT_ID&#xd;&#xa;    ,T4.proj_name AS RELATION_DPT_NAME&#xd;&#xa;    ,T.CATD_ONE_OVER_QTY&#xd;&#xa;    ,T.CATD_TWO_OVER_QTY&#xd;&#xa;    ,T.CATD_THREE_OVER_QTY&#xd;&#xa;    ,T.CATD_FOUR_OVER_QTY&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT coalesce&#x28;B.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;B.BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;B.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;B.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,coalesce&#x28;B.TPMODE_ID,&#x27;ALL&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,sum&#x28;CUR_M_OT_QTY&#x29; AS CATD_ONE_OVER_QTY&#xd;&#xa;        ,0 AS CATD_TWO_OVER_QTY&#xd;&#xa;        ,0 AS CATD_THREE_OVER_QTY&#xd;&#xa;        ,0 AS CATD_FOUR_OVER_QTY&#xd;&#xa;    FROM&#x28;&#xd;&#xa;        SELECT coalesce&#x28;A.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,coalesce&#x28;A.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;            ,coalesce&#x28;A.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;            ,coalesce&#x28;A.proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;            ,coalesce&#x28;A.TPMODE_ID,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;            ,sum&#x28;CUR_M_OT_QTY&#x29; AS CUR_M_OT_QTY&#xd;&#xa;        FROM&#x28; &#xd;&#xa;            SELECT SD.rgn_id&#xd;&#xa;                ,SD.base_id&#xd;&#xa;                ,SD.brand_id&#xd;&#xa;                ,SD.proj_id&#xd;&#xa;                ,SD.transportmode_id AS TPMODE_ID&#xd;&#xa;                ,SD.vin&#xd;&#xa;                ,CASE WHEN &#x28;UNIX_TIMESTAMP&#x28;SD.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;&#x29;&#x2f;3600 &#x3c; T.cur_m_stand_value &#xd;&#xa;                        THEN 1 ELSE 0 END AS CUR_M_OT_QTY  --&#xd;&#xa;            FROM TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_1 SD&#xd;&#xa;            LEFT JOIN TEMP.TEMP_DMR_YD_OV_DTD_OVR_DPT_DTL_STAND T&#xd;&#xa;            ON SD.rgn_id &#x3d; T.rgn_id AND SD.base_id &#x3d; T.base_id AND SD.brand_id &#x3d; T.brand_id AND SD.PROJ_ID &#x3d; T.PROJ_ID AND SD.transportmode_id &#x3d; T.tpmode_code AND T.catd_name &#x3d; &#x27;&#x63a5;&#x5355;-&#x5206;&#x914d;&#x27;&#xd;&#xa;            WHERE &#x28;&#xd;&#xa;                   SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x2