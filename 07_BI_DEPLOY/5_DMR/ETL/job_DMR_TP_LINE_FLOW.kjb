         &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;                                ELSE&#xd;&#xa;                                 F.SOURCE_CTY_ID&#xd;&#xa;                              END,&#xd;&#xa;                              &#x27;&#x5e02;&#x27;,&#xd;&#xa;                              &#x27;&#x27;&#x29; AS SOURCE_CTY_ID&#xd;&#xa;              ,REGEXP_REPLACE&#x28;CASE&#xd;&#xa;                                WHEN F.DEST_CTY_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;                                 &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;                                ELSE&#xd;&#xa;                                 F.DEST_CTY_ID&#xd;&#xa;                              END,&#xd;&#xa;                              &#x27;&#x5e02;&#x27;,&#xd;&#xa;                              &#x27;&#x27;&#x29; AS DEST_CTY_ID&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN F.FLAG &#x3d; &#x27;1&#x27; THEN&#xd;&#xa;                      F.SHIPPING_NUM&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS FORWARD_QTY&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN F.FLAG &#x3d; &#x27;0&#x27; THEN&#xd;&#xa;                      F.SHIPPING_NUM&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BACKWARD_QTY&#xd;&#xa;          FROM &#x28;SELECT GREATEST&#x28;src_city_id, dest_city_id&#x29; AS SOURCE_CTY_ID&#xd;&#xa;                      ,LEAST&#x28;src_city_id, dest_city_id&#x29; AS DEST_CTY_ID&#xd;&#xa;                      ,CASE&#xd;&#xa;                         WHEN GREATEST&#x28;src_city_id, dest_city_id&#x29; &#x3d;&#xd;&#xa;                              src_city_id THEN&#xd;&#xa;                          &#x27;1&#x27;&#xd;&#xa;                         ELSE&#xd;&#xa;                          &#x27;0&#x27;&#xd;&#xa;                       END AS FLAG&#xd;&#xa;                      ,shipping_qty AS SHIPPING_NUM&#xd;&#xa;                  FROM DMK.dmk_tp_shipqty_d&#xd;&#xa;                 WHERE PART_DT &#x3d;&#xd;&#xa;                       REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                      &#x27;-&#x27;,&#xd;&#xa;                                      &#x27;&#x27;&#x29; and tpmode_id &#x3d; &#x27;GL&#x27;&#xd;&#xa;										AND brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;	&#x29; F&#xd;&#xa;         GROUP BY REGEXP_REPLACE&#x28;CASE&#xd;&#xa;                                   WHEN F.SOURCE_CTY_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;                                    &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;                                   ELSE&#xd;&#xa;                                    F.SOURCE_CTY_ID&#xd;&#xa;                                 END,&#xd;&#xa;                                 &#x27;&#x5e02;&#x27;,&#xd;&#xa;                                 &#x27;&#x27;&#x29;&#xd;&#xa;                 ,REGEXP_REPLACE&#x28;CASE&#xd;&#xa;                                   WHEN F.DEST_CTY_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;                                    &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;                                   ELSE&#xd;&#xa;                                    F.DEST_CTY_ID&#xd;&#xa;                                 END,&#xd;&#xa;                                 &#x27;&#x5e02;&#x27;,&#xd;&#xa;                                 &#x27;&#x27;&#x29;&#x29; C&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
