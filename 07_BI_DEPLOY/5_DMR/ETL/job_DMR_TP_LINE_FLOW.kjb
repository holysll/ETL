    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_TP_LINE_FLOW PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT TMP.PERIOD_TYPE&#xd;&#xa;      ,TMP.PERIOD_WID&#xd;&#xa;      ,TMP.SOURCE_CTY_ID&#xd;&#xa;      ,TMP.DEST_CTY_ID&#xd;&#xa;      ,TMP.CITY&#xd;&#xa;      ,TMP.ASC_RANK_ID&#xd;&#xa;      ,TMP.DESC_RANK_ID&#xd;&#xa;      ,TMP.CONSEQUENT_QTY&#xd;&#xa;      ,TMP.REVERSE_QTY&#xd;&#xa;      ,TMP.VERSUS_QTY&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;&#x29;&#xd;&#xa;      ,&#x27;DW&#x27;&#xd;&#xa;      ,SUBSTR&#x28;TMP.PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_LINE_FLOW_01&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_LINE_FLOW_02&#x29; TMP&#x3b;&#xd;&#xa;</sql>
