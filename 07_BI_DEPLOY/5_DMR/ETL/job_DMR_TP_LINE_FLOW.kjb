    <entry>
      <name>CREATE_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_LINE_FLOW_01&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_LINE_FLOW_01&#x28;&#xd;&#xa;PERIOD_TYPE       STRING         COMMENT  &#x27;&#x671f;&#x95f4;&#x7c7b;&#x578b;&#x27;,&#xd;&#xa;PERIOD_WID        STRING         COMMENT  &#x27;&#x671f;&#x95f4;&#x27;,   &#xd;&#xa;SOURCE_CTY_ID     STRING         COMMENT  &#x27;&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#x27;,&#xd;&#xa;DEST_CTY_ID       STRING         COMMENT  &#x27;&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#x27;, &#xd;&#xa;CITY              STRING         COMMENT  &#x27;&#x57ce;&#x5e02;&#x5bf9;&#x27;,&#xd;&#xa;ASC_RANK_ID       BIGINT         COMMENT  &#x27;&#x5347;&#x5e8f;&#x6392;&#x540d;&#x53f7;&#x27;,&#xd;&#xa;DESC_RANK_ID      BIGINT         COMMENT  &#x27;&#x964d;&#x5e8f;&#x6392;&#x540d;&#x53f7;&#x27;,&#xd;&#xa;CONSEQUENT_QTY    DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x987a;&#x5411;&#x6570;&#x91cf;&#x27;,&#xd;&#xa;REVERSE_QTY       DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x9006;&#x5411;&#x6570;&#x91cf;&#x27;,&#xd;&#xa;VERSUS_QTY        DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x5bf9;&#x6d41;&#x91cf;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x5206;&#x6790;&#xff08;&#x65e5;&#x2f;&#x6708;&#xff09;-&#x5bf9;&#x6d41;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_LINE_FLOW_02&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_LINE_FLOW_02&#x28;&#xd;&#xa;PERIOD_TYPE       STRING         COMMENT  &#x27;&#x671f;&#x95f4;&#x7c7b;&#x578b;&#x27;,&#xd;&#xa;PERIOD_WID        STRING         COMMENT  &#x27;&#x671f;&#x95f4;&#x27;,   &#xd;&#xa;SOURCE_CTY_ID     STRING         COMMENT  &#x27;&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#x27;,&#xd;&#xa;DEST_CTY_ID       STRING         COMMENT  &#x27;&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#x27;, &#xd;&#xa;CITY              STRING         COMMENT  &#x27;&#x57ce;&#x5e02;&#x5bf9;&#x27;,&#xd;&#xa;ASC_RANK_ID       BIGINT         COMMENT  &#x27;&#x5347;&#x5e8f;&#x6392;&#x540d;&#x53f7;&#x27;,&#xd;&#xa;DESC_RANK_ID      BIGINT         COMMENT  &#x27;&#x964d;&#x5e8f;&#x6392;&#x540d;&#x53f7;&#x27;,&#xd;&#xa;CONSEQUENT_QTY    DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x987a;&#x5411;&#x6570;&#x91cf;&#x27;,&#xd;&#xa;REVERSE_QTY       DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x9006;&#x5411;&#x6570;&#x91cf;&#x27;,&#xd;&#xa;VERSUS_QTY        DECIMAL&#x28;32,8&#x29;  COMMENT  &#x27;&#x5bf9;&#x6d41;&#x91cf;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x5206;&#x6790;&#xff08;&#x65e5;&#x2f;&#x6708;&#xff09;-&#x5bf9;&#x6d41;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
