    <entry>
      <name>TO_TMP2</name>
      <description/>
      <type>SQL</type>
      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_LINE_FLOW_02&#xd;&#xa;SELECT &#x27;D&#x27; AS PERIOD_TYPE&#xd;&#xa;      ,C.PERIOD_WID AS PERIOD_WID --&#x6708;&#x4efd;ID&#xd;&#xa;      ,C.SOURCE_CTY_ID AS SOURCE_CTY_ID --&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID    &#xd;&#xa;      ,C.DEST_CTY_ID AS DEST_CTY_ID --&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID  &#xd;&#xa;      ,REGEXP_REPLACE&#x28;CONCAT_WS&#x28;&#x27;--&#x27;, C.SOURCE_CTY_ID, C.DEST_CTY_ID&#x29;,&#xd;&#xa;                      &#x27;&#x5e02;&#x27;,&#xd;&#xa;                      &#x27;&#x27;&#x29; AS CITY --&#x57ce;&#x5e02;&#x540d;&#x79f0;&#xd;&#xa;      ,ROW_NUMBER&#x28;&#x29; OVER&#x28;PARTITION BY C.PERIOD_WID ORDER BY LEAST&#x28;NVL&#x28;C.FORWARD_QTY, 0&#x29;, NVL&#x28;C.BACKWARD_QTY, 0&#x29;&#x29; ASC, C.FORWARD_QTY ASC, C.BACKWARD_QTY ASC&#x29; AS ASC_RANK_ID --&#x5347;&#x5e8f;&#x6392;&#x540d;&#x53f7;&#xd;&#xa;      ,ROW_NUMBER&#x28;&#x29; OVER&#x28;PARTITION BY C.PERIOD_WID ORDER BY LEAST&#x28;NVL&#x28;C.FORWARD_QTY, 0&#x29;, NVL&#x28;C.BACKWARD_QTY, 0&#x29;&#x29; DESC, C.FORWARD_QTY DESC, C.BACKWARD_QTY DESC&#x29; AS DESC_RANK_ID --&#x964d;&#x5e8f;&#x6392;&#x540d;&#x53f7;&#xd;&#xa;      ,C.FORWARD_QTY --&#x987a;&#x5411;&#x6570;&#x91cf;&#xd;&#xa;      ,C.BACKWARD_QTY --&#x9006;&#x5411;&#x6570;&#x91cf;&#xd;&#xa;      ,LEAST&#x28;NVL&#x28;C.FORWARD_QTY, 0&#x29;, NVL&#x28;C.BACKWARD_QTY, 0&#x29;&#x29; AS CONVECTION_QTY --&#x5bf9;&#x6d41;&#x91cf;, MIN VALUE BETWEEN FORWARD AND BACKWARD QTY  &#xd;&#xa;  FROM &#x28;SELECT F.PERIOD_WID&#xd;&#xa;              ,REGEXP_REPLACE&#x28;CASE&#xd;&#xa;                                WHEN F.SOURCE_CTY_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;                                 &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;                                ELSE&#xd;&#xa;                                 F.SOURCE_CTY_ID&#xd;&#xa;                              END,&#xd;&#xa;                              &#x27;&#x5e02;&#x27;,&#xd;&#xa;                              &#x27;&#x27;&#x29; AS SOURCE_CTY_ID&#xd;&#xa;              ,REGEXP_REPLACE&#x28;CASE&#xd;&#xa;                                WHEN F.DEST_CTY_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;                                 &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;                                ELSE&#xd;&#xa;                                 F.DEST_CTY_ID&#xd;&#xa;                              END,&#xd;&#xa;                              &#x27;&#x5e02;&#x27;,&#xd;&#xa;                              &#x27;&#x27;&#x29; AS DEST_CTY_ID&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN F.FLAG &#x3d; &#x27;1&#x27; THEN&#xd;&#xa;                      F.SHIPPING_NUM&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS FORWARD_QTY&#xd;&#xa;              ,SUM&#x28;CASE&#xd;&#xa;                     WHEN F.FLAG &#x3d; &#x27;0&#x27; THEN&#xd;&#xa;                      F.SHIPPING_NUM&#xd;&#xa;                     ELSE&#xd;&#xa;                      0&#xd;&#xa;                   END&#x29; AS BACKWARD_QTY&#xd;&#xa;          FROM &#x28;SELECT PERIOD_WID&#xd;&#xa;                      ,GREATEST&#x28;src_city_id, dest_city_id&#x29; AS SOURCE_CTY_ID&#xd;&#xa;                      ,LEAST&#x28;src_city_id, dest_city_id&#x29; AS DEST_CTY_ID&#xd;&#xa;                      ,CASE&#xd;&#xa;                         WHEN GREATEST&#x28;src_city_id, dest_city_id&#x29; &#x3d;&#xd;&#xa;                              src_city_id THEN&#xd;&#xa;                          &#x27;1&#x27;&#xd;&#xa;                         ELSE&#xd;&#xa;                          &#x27;0&#x27;&#xd;&#xa;                       END AS FLAG&#xd;&#xa;                      ,shipping_qty AS SHIPPING_NUM&#xd;&#xa;                  FROM DMK.dmk_tp_shipqty_d&#xd;&#xa;                 WHERE PART_DT &#x3d;&#xd;&#xa;                       REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                      &#x27;-&#x27;,&#xd;&#xa;                                      &#x27;&#x27;&#x29; &#xd;&#xa;                    and tpmode_id &#x3d; &#x27;GL&#x27;&#xd;&#xa;                    AND brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#x29; F&#xd;&#xa;         GROUP BY F.PERIOD_WID&#xd;&#xa;                 ,REGEXP_REPLACE&#x28;CASE&#xd;&#xa;                                   WHEN F.SOURCE_CTY_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;                                    &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;                                   ELSE&#xd;&#xa;                                    F.SOURCE_CTY_ID&#xd;&#xa;                                 END,&#xd;&#xa;                                 &#x27;&#x5e02;&#x27;,&#xd;&#xa;                                 &#x27;&#x27;&#x29;&#xd;&#xa;                 ,REGEXP_REPLACE&#x28;CASE&#xd;&#xa;                                   WHEN F.DEST_CTY_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;                                    &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;                                   ELSE&#xd;&#xa;                                    F.DEST_CTY_ID&#xd;&#xa;                                 END,&#xd;&#xa;                                 &#x27;&#x5e02;&#x27;,&#xd;&#xa;                                 &#x27;&#x27;&#x29;&#x29; C&#xd;&#xa;&#x3b;</sql>
