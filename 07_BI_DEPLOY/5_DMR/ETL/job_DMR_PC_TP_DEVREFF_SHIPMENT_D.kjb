    <entry>
      <name>TO_TEMP</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_DEVREFF_SHIPMENT_D&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;      ,A.BRAND_ID&#xd;&#xa;      ,A.RGN_ID&#xd;&#xa;      ,SUM&#x28;PLAN_OUTSTORE_QTY&#x29; AS PLAN_OUTSTORE_QTY&#xd;&#xa;      ,SUM&#x28;FACT_OUTSTORE_QTY&#x29; AS FACT_OUTSTORE_QTY&#xd;&#xa;      ,SUM&#x28;PLAN_SHIPMENT_QTY&#x29; AS PLAN_SHIPMENT_QTY&#xd;&#xa;      ,SUM&#x28;FACT_SHIPMENT_QTY&#x29; AS FACT_SHIPMENT_QTY&#xd;&#xa;  FROM &#x28;SELECT RGN_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,EARLY_PICKUP_DATE AS PERIOD_WID&#xd;&#xa;              ,1                 AS PLAN_OUTSTORE_QTY --&#x8ba1;&#x5212;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;              ,0                 AS FACT_OUTSTORE_QTY&#xd;&#xa;              ,0                 AS PLAN_SHIPMENT_QTY&#xd;&#xa;              ,0                 AS FACT_SHIPMENT_QTY&#xd;&#xa;          FROM DW.DW_TP_ORDERRELEASE&#xd;&#xa;         WHERE SUBSTRING&#x28;EARLY_PICKUP_DATE, 1, 6&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;            OR SUBSTRING&#x28;EARLY_PICKUP_DATE, 1, 6&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT RGN_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,REGEXP_REPLACE&#x28;SUBSTR&#x28;OUTSTOCKDATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,0 AS PLAN_OUTSTORE_QTY --&#x8ba1;&#x5212;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;              ,1 AS FACT_OUTSTORE_QTY&#xd;&#xa;              ,0 AS PLAN_SHIPMENT_QTY&#xd;&#xa;              ,0 AS FACT_SHIPMENT_QTY&#xd;&#xa;          FROM DW.DW_STR_STOCKDEPART&#xd;&#xa;         WHERE SUBSTRING&#x28;OUTSTOCKDATE, 1, 7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT RGN_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,REGEXP_REPLACE&#x28;SUBSTR&#x28;SCHSHIPPINGDATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,0 AS PLAN_OUTSTORE_QTY --&#x8ba1;&#x5212;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;              ,0 AS FACT_OUTSTORE_QTY&#xd;&#xa;              ,1 AS PLAN_SHIPMENT_QTY&#xd;&#xa;              ,0 AS FACT_SHIPMENT_QTY&#xd;&#xa;          FROM DW.DW_STR_STOCKDEPART&#xd;&#xa;         WHERE SUBSTRING&#x28;SCHSHIPPINGDATE, 1, 7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT RGN_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,REGEXP_REPLACE&#x28;SUBSTR&#x28;SHIPPINGDATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,0 AS PLAN_OUTSTORE_QTY --&#x8ba1;&#x5212;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;              ,0 AS FACT_OUTSTORE_QTY&#xd;&#xa;              ,0 AS PLAN_SHIPMENT_QTY&#xd;&#xa;              ,1 AS FACT_SHIPMENT_QTY&#xd;&#xa;          FROM DW.DW_STR_STOCKDEPART&#xd;&#xa;         WHERE SUBSTRING&#x28;SHIPPINGDATE, 1, 7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;&#x29; A&#xd;&#xa; GROUP BY A.RGN_ID, A.BRAND_ID, A.PERIOD_WID WITH CUBE&#x3b;&#xd;&#xa;</sql>
