    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--target table &#x3a; DMP_PC_TP_DEVREFF_SHIPMENT_D &#x4ea4;&#x4ed8;&#x4e0e;&#x6548;&#x7387;&#x7ba1;&#x7406;&#xff08;&#x65e5;&#xff09;-&#x53d1;&#x8fd0;&#x6548;&#x7387;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_DEVREFF_SHIPMENT_D PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID AS PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,COALESCE&#x28;B.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;      ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,COALESCE&#x28;D.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME&#xd;&#xa;      ,COALESCE&#x28;T.TPMODE_ID, &#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;      ,COALESCE&#x28;E.TPMODE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS TPMODE_NAME&#xd;&#xa;      ,NULL AS PLAN_OUTSOTRE_QTY --&#x8ba1;&#x5212;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;      ,NULL AS FACT_OUTSOTRE_QTY --&#x5b9e;&#x9645;&#x51fa;&#x5e93;&#x6570;&#xd;&#xa;      ,SUM&#x28;T.SHIPPING_PLAN_QTY&#x29; AS PLAN_SHIPMENT_QTY --&#x8ba1;&#x5212;&#x53d1;&#x8fd0;&#x6570;&#xd;&#xa;      ,SUM&#x28;T.SHIPPING_ONTIME_QTY&#x29; AS FACT_SHIPMENT_QTY --&#x5b9e;&#x9645;&#x53d1;&#x8fd0;&#x6570;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DATE --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;  FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; B&#xd;&#xa;    ON T.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; D&#xd;&#xa;    ON T.BRAND_ID &#x3d; D.BRAND_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT TPMODE_ID&#xd;&#xa;                   ,TPMODE_NAME&#xd;&#xa;               FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;              GROUP BY TPMODE_ID&#xd;&#xa;                      ,TPMODE_NAME&#x29; E&#xd;&#xa;    ON T.TPMODE_ID &#x3d; E.TPMODE_ID&#xd;&#xa; WHERE T.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;   AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa; GROUP BY T.PERIOD_WID&#xd;&#xa;         ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;B.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;D.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;T.TPMODE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;E.TPMODE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
