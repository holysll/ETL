    <entry>
      <name>CREATE_TEMP</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_DEVREFF_SHIPMENT_D&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_DEVREFF_SHIPMENT_D&#xd;&#xa;&#x28;&#xd;&#xa;    PERIOD_WID             STRING                   COMMENT &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;    RGN_ID                 STRING                   COMMENT &#x27;&#x5927;&#x533a;ID,&#x27;&#xd;&#xa;    BRAND_ID               STRING                   COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;    PLAN_OUTSTORE_QTY      BIGINT                   COMMENT &#x27;&#x8ba1;&#x5212;&#x51fa;&#x5e93;&#x6570;&#x27;,&#xd;&#xa;    FACT_OUTSTORE_QTY      BIGINT                   COMMENT &#x27;&#x5b9e;&#x9645;&#x51fa;&#x5e93;&#x6570;&#x27;,&#xd;&#xa;    PLAN_SHIPMENT_QTY      BIGINT                   COMMENT &#x27;&#x8ba1;&#x5212;&#x53d1;&#x8fd0;&#x6570;&#x27;,&#xd;&#xa;    FACT_SHIPMENT_QTY      BIGINT                   COMMENT &#x27;&#x5b9e;&#x9645;&#x53d1;&#x8fd0;&#x6570;&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;COMMENT &#x27;Temporary table for DMR_PC_TP_DEVREFF_SHIPMENT_D&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27; STORED AS PARQUET&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
