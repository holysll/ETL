    <entry>
      <name>TO_TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_FIN_PROFIT_DTL_M_1&#xd;&#xa;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;    ,T1.rgn_id&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VCL_INCOME_AMT ELSE 0 END&#x29; AS CUR_VCL_INCOME_AMT  --&#x672c;&#x671f;&#x6574;&#x8f66;&#x6536;&#x5165;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VCL_INCOME_AMT ELSE 0 END&#x29; AS LST_VCL_INCOME_AMT  --&#x4e0a;&#x671f;&#x6574;&#x8f66;&#x6536;&#x5165;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VCL_COST_AMT ELSE 0 END&#x29; AS CUR_VCL_COST_AMT  --&#x672c;&#x671f;&#x6574;&#x8f66;&#x6210;&#x672c;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN VCL_COST_AMT ELSE 0 END&#x29; AS LST_VCL_COST_AMT  --&#x4e0a;&#x671f;&#x6574;&#x8f66;&#x6210;&#x672c;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN TP_BIZ_INCOME_AMT ELSE 0 END&#x29; AS CUR_TP_INCOME_AMT  --&#x672c;&#x671f;&#x8fd0;&#x8f93;&#x6536;&#x5165;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN TP_BIZ_INCOME_AMT ELSE 0 END&#x29; AS LST_TP_INCOME_AMT  --&#x4e0a;&#x671f;&#x8fd0;&#x8f93;&#x6536;&#x5165;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN TP_BIZ_COST_AMT ELSE 0 END&#x29; AS CUR_TP_COST_AMT  --&#x672c;&#x671f;&#x8fd0;&#x8f93;&#x6210;&#x672c;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN TP_BIZ_COST_AMT ELSE 0 END&#x29; AS LST_TP_COST_AMT  --&#x672c;&#x671f;&#x8fd0;&#x8f93;&#x6210;&#x672c;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN STORE_BIZ_INCOME_AMT ELSE 0 END&#x29; AS CUR_STORE_INCOME_AMT  --&#x672c;&#x671f;&#x4ed3;&#x50a8;&#x6536;&#x5165;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN STORE_BIZ_INCOME_AMT ELSE 0 END&#x29; AS LST_STORE_INCOME_AMT  --&#x4e0a;&#x671f;&#x4ed3;&#x50a8;&#x6536;&#x5165;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN STORE_BIZ_COST_AMT ELSE 0 END&#x29; AS CUR_STORE_COST_AMT  --&#x672c;&#x671f;&#x4ed3;&#x50a8;&#x6210;&#x672c;&#xd;&#xa;    ,SUM&#x28;CASE WHEN T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN STORE_BIZ_COST_AMT ELSE 0 END&#x29; AS LST_STORE_COST_AMT  --&#x4e0a;&#x671f;&#x4ed3;&#x50a8;&#x6210;&#x672c;&#xd;&#xa;FROM DMK.DMK_FIN_INCOMECOST_M T1&#xd;&#xa;WHERE T1.PERIOD_WID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR T1.period_wid &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY T1.rgn_id</sql>
