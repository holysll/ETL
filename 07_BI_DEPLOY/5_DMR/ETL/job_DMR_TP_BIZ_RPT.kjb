      <name>TMP3</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_RPT_03&#xd;&#xa;--&#x5728;&#x9014;&#xd;&#xa;SELECT D.PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,COALESCE&#x28;PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,COUNT&#x28;vin&#x29; AS IN_TRANSIT_QTY&#xd;&#xa;      ,NULL&#xd;&#xa;      ,COUNT&#x28;CASE&#xd;&#xa;               WHEN unix_timestamp&#x28;concat&#x28;D.period_date,&#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-&#xd;&#xa;                    unix_timestamp&#x28;T.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;nvl&#x28;T.on_passage_days,0&#x29;&#xd;&#xa;               THEN&#xd;&#xa;                vin&#xd;&#xa;               ELSE&#xd;&#xa;                NULL&#xd;&#xa;             END&#x29; AS IN_TRANSIT_OT_QTY&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;  FROM &#x28;SELECT period_date,period_wid&#xd;&#xa;        FROM DIM.DIM_PERIOD_D &#xd;&#xa;        WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;            AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;            AND SUBSTR&#x28;PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#x29; D&#xd;&#xa;  JOIN &#x28;SELECT rgn_id&#xd;&#xa;            ,base_id&#xd;&#xa;            ,brand_id&#xd;&#xa;            ,proj_id&#xd;&#xa;            ,vin&#xd;&#xa;            ,order_release_gid&#xd;&#xa;            ,factory_day&#xd;&#xa;            ,arrival_day&#xd;&#xa;            ,dlvr_day&#xd;&#xa;            ,on_passage_days&#xd;&#xa;        FROM DMK.DMK_TP_DTD_PROG_TIME_D&#x29; T&#xd;&#xa;    ON 1 &#x3d; 1&#xd;&#xa; WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;T.FACTORY_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d; D.PERIOD_WID&#xd;&#xa;   AND NVL&#x28;SUBSTR&#x28;REGEXP_REPLACE&#x28;NVL&#x28;T.DLVR_DAY,T.ARRIVAL_DAY&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;, &#x27;29991231&#x27;&#x29; &#x3e;&#xd;&#xa;       D.PERIOD_WID&#xd;&#xa; GROUP BY D.PERIOD_WID&#xd;&#xa;         ,COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;PROJ_ID, &#x27;-1&#x27;&#x29;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT D.PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,COALESCE&#x28;PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,COUNT&#x28;DISTINCT VIN&#x29; AS IN_TRANSIT_QTY&#xd;&#xa;      ,NULL&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE&#xd;&#xa;               WHEN DATEDIFF&#x28;NVL&#x28;ARRIVAL_DAY, D.PERIOD_DATE&#x29;, FACTORY_DAY&#x29; -&#xd;&#xa;                    ON_PASSAGE_DAYS &#x3e; 0 THEN&#xd;&#xa;                VIN&#xd;&#xa;               ELSE&#xd;&#xa;                NULL&#xd;&#xa;             END&#x29; AS IN_TRANSIT_OT_QTY&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;  FROM DIM.DIM_PERIOD_D D&#xd;&#xa;  JOIN DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;    ON 1 &#x3d; 1&#xd;&#xa; WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;FACTORY_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d; D.PERIOD_WID&#xd;&#xa;   AND NVL&#x28;SUBSTR&#x28;REGEXP_REPLACE&#x28;ARRIVAL_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;, &#x27;29991231&#x27;&#x29; &#x3e;&#xd;&#xa;       D.PERIOD_WID&#xd;&#xa;   AND ON_PASSAGE_DAYS IS NOT NULL&#xd;&#xa;   AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa; GROUP BY D.PERIOD_WID&#xd;&#xa;         ,COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;PROJ_ID, &#x27;-1&#x27;&#x29;&#x3b;&#xd;&#xa;&#x2a;&#x2f;</sql>
