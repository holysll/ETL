      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_RPT_07&#xd;&#xa;--&#x5f85;&#x53d1;&#xd;&#xa;SELECT D.PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,COALESCE&#x28;PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,COUNT&#x28;DISTINCT VIN&#x29; AS DUE_SEND_NUM_D&#xd;&#xa;  FROM DIM.DIM_PERIOD_D D&#xd;&#xa;  JOIN DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;    ON 1 &#x3d; 1&#xd;&#xa; WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;EARLY_PICKUP_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d; D.PERIOD_WID&#xd;&#xa;   AND NVL&#x28;SUBSTR&#x28;REGEXP_REPLACE&#x28;FACTORY_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;, &#x27;29991231&#x27;&#x29; &#x3e;&#xd;&#xa;       D.PERIOD_WID&#xd;&#xa;   AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;   AND SUBSTR&#x28;D.PERIOD_WID,1,8&#x29; &#x3c;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa; GROUP BY D.PERIOD_WID&#xd;&#xa;         ,COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;PROJ_ID, &#x27;-1&#x27;&#x29;&#x3b;</sql>
