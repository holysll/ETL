      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_RPT_01&#xd;&#xa;--&#x542f;&#x7968;&#xd;&#xa;SELECT COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;      ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID  --&#xd;&#xa;      ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;      ,SUM&#x28;CASE WHEN SUBSTR&#x28;regexp_replace&#x28;T.EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3d; substr&#x28;regexp_replace&#x28;DATE_SUB&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; THEN 1 ELSE 0 END&#x29; AS TICKET_OPEN_QTY_D --&#x65e5;&#x542f;&#x7968;&#x6570;&#xd;&#xa;      ,sum&#x28;CASE WHEN SUBSTR&#x28;regexp_replace&#x28;T.EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END&#x29; AS TICKET_OPEN_QTY_M --&#x6708;&#x7d2f;&#x542f;&#x7968;&#x6570; &#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;  FROM DW.DW_TP_ORDERRELEASE T --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;  LEFT JOIN DW.DW_TP_ORDERMOVEMENT M&#xd;&#xa;    ON T.ORDER_RELEASE_GID &#x3d; M.ORDER_RELEASE_GID&#xd;&#xa;   AND NVL&#x28;M.SEQ_NO, 1&#x29; &#x3d; &#x27;1&#x27;&#xd;&#xa;   AND M.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;   LEFT JOIN &#x28;&#xd;&#xa;    SELECT servprov_name AS SUPPLIER_ID FROM dim.dim_servprov WHERE servprov_id LIKE &#x27;&#x25;&#x7279;&#x9510;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x957f;&#x4e45;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x5b89;&#x76db;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x7f8e;&#x8054;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x4f17;&#x5347;&#x25;&#x27;&#xd;&#xa;    &#x29; P&#xd;&#xa;  ON M.LSP &#x3d; P.SUPPLIER_ID&#xd;&#xa; WHERE &#x28;SUBSTR&#x28;regexp_replace&#x28;T.EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3d; substr&#x28;regexp_replace&#x28;DATE_SUB&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;    OR &#x28;SUBSTR&#x28;regexp_replace&#x28;T.EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND SUBSTR&#x28;regexp_replace&#x28;T.EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;    &#x29;&#xd;&#xa;	AND P.SUPPLIER_ID IS NULL&#xd;&#xa;	AND T.or_plan_type &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27;&#xd;&#xa; GROUP BY COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29;</sql>
