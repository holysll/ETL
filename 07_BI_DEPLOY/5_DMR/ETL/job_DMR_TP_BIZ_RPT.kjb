      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_RPT_05&#xd;&#xa;SELECT T.period_wid&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.PROJ_ID&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;    ,sum&#x28;ARRIVE_OT_QTY&#x29; OVER&#x28;PARTITION BY substr&#x28;T.period_wid,1,6&#x29;,T.RGN_ID,T.BASE_ID,T.BRAND_ID,T.PROJ_ID ORDER BY T.period_wid&#x29; AS CUR_M_OT_QTY&#xd;&#xa;    ,sum&#x28;ARRIVE_PLAN_QTY&#x29; OVER&#x28;PARTITION BY substr&#x28;T.period_wid,1,6&#x29;,T.RGN_ID,T.BASE_ID,T.BRAND_ID,T.PROJ_ID ORDER BY T.period_wid&#x29; AS CUR_M_PLAN_QTY&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT T.period_wid&#xd;&#xa;        ,nvl&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,nvl&#x28;T.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,nvl&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,nvl&#x28;T.proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,sum&#x28;T.arrive_ontime_qty&#x29; AS ARRIVE_OT_QTY&#xd;&#xa;        ,sum&#x28;T.arrive_plan_qty&#x29; AS ARRIVE_PLAN_QTY&#xd;&#xa;    FROM DMK.DMK_TP_SHIPQTY_D T   &#xd;&#xa;    LEFT JOIN DIM.DIM_ORG B  &#xd;&#xa;    ON T.base_id &#x3d; B.base_id AND B.org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    WHERE substr&#x28;T.period_wid,1,6&#x29; &#x3e;&#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND substr&#x28;T.period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    GROUP BY T.period_wid&#xd;&#xa;        ,T.rgn_id&#xd;&#xa;        ,T.base_id&#xd;&#xa;        ,T.brand_id&#xd;&#xa;        ,T.proj_id&#xd;&#xa;    &#x29; T&#xd;&#xa;WHERE substr&#x28;T.period_wid ,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    AND substr&#x28;T.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT X.PERIOD_WID&#xd;&#xa;      ,X.RGN_ID&#xd;&#xa;      ,X.BASE_ID&#xd;&#xa;      ,X.BRAND_ID&#xd;&#xa;      ,X.PROJ_ID&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,SUM&#x28;ONTINE_ARRIVE_NUM&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X.PERIOD_WID, 1, 6&#x29;, X.RGN_ID, X.BASE_ID, X.BRAND_ID, X.PROJ_ID ORDER BY X.PERIOD_WID&#x29; AS ONTINE_ARRIVE_NUM&#xd;&#xa;      ,SUM&#x28;ALL_ARRIVE_NUM&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X.PERIOD_WID, 1, 6&#x29;, X.RGN_ID, X.BASE_ID, X.BRAND_ID, X.PROJ_ID ORDER BY X.PERIOD_WID&#x29; AS ONTINE_ARRIVE_NUM&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;  FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,T.BASE_ID&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,T.PROJ_ID&#xd;&#xa;          FROM DIM.DIM_PERIOD_D D&#xd;&#xa;          JOIN &#x28;SELECT COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;                     ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;                     ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                     ,COALESCE&#x28;PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                 FROM DMK.DMK_TP_DTD_PROG_TIME_D&#xd;&#xa;                WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;TO_DATE&#x28;FACTORY_DAY&#x29;,&#xd;&#xa;                                                     CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                                            &#x27;-&#x27;,&#xd;&#xa;                                            &#x27;&#x27;&#x29;,&#xd;&#xa;                             1,&#xd;&#xa;                             6&#x29; &#x3d;&#xd;&#xa;                      SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                GROUP BY COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                        ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                        ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                        ,COALESCE&#x28;PROJ_ID, &#x27;-1&#x27;&#x29;&#x29; AS T&#xd;&#xa;            ON 1 &#x3d; 1&#xd;&#xa;         WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;           AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;           AND D.PERIOD_WID &#x3c;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; X --&#x4f7f;&#x7528;&#x7b1b;&#x5361;&#x5c14;&#x79ef;&#x9020;&#x51fa;&#x7ef4;&#x5ea6;&#x6846;&#x67b6;&#xd;&#xa;  LEFT JOIN &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;TO_DATE&#x28;FACTORY_DAY&#x29;,&#xd;&#xa;                                                   CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                           1,&#xd;&#xa;                           8&#x29; AS PERIOD_WID&#xd;&#xa;                   ,COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;                   ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;                   ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                   ,COALESCE&#x28;PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                   ,COUNT&#x28;CASE&#xd;&#xa;                            WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;ARRIVAL_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;                                 SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;TO_DATE&#x28;FACTORY_DAY&#x29;,&#xd;&#xa;                                                                CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                                                       &#x27;-&#x27;,&#xd;&#xa;                                                       &#x27;&#x27;&#x29;,&#xd;&#xa;                                        1,&#xd;&#xa;                                        8&#x29; THEN&#xd;&#xa;                             VIN&#xd;&#xa;                            ELSE&#xd;&#xa;                             NULL&#xd;&#xa;                          END&#x29; AS ONTINE_ARRIVE_NUM&#xd;&#xa;                   ,COUNT&#x28;1&#x29; AS ALL_ARRIVE_NUM&#xd;&#xa;               FROM DMK.DMK_TP_DTD_PROG_TIME_D&#xd;&#xa;              WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;TO_DATE&#x28;FACTORY_DAY&#x29;,&#xd;&#xa;                                                   CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                           1,&#xd;&#xa;                           6&#x29; &#x3d;&#xd;&#xa;                    SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;              GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;TO_DATE&#x28;FACTORY_DAY&#x29;,&#xd;&#xa;                                                      CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                                             &#x27;-&#x27;,&#xd;&#xa;                                             &#x27;&#x27;&#x29;,&#xd;&#xa;                              1,&#xd;&#xa;                              8&#x29;&#xd;&#xa;                      ,COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                      ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                      ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                      ,COALESCE&#x28;PROJ_ID, &#x27;-1&#x27;&#x29;&#x29; O --&#x5ea6;&#x91cf;&#x6570;&#x503c;&#x5728;&#x6846;&#x67b6;&#x5185;&#x586b;&#x5145;&#x5373;&#x53ef;&#xd;&#xa;    ON X.PERIOD_WID &#x3d; O.PERIOD_WID&#xd;&#xa;   AND X.RGN_ID &#x3d; O.RGN_ID&#xd;&#xa;   AND X.BASE_ID &#x3d; O.BASE_ID&#xd;&#xa;   AND X.BRAND_ID &#x3d; O.BRAND_ID&#xd;&#xa;   AND X.PROJ_ID &#x3d; O.PROJ_ID&#x3b;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;</sql>
