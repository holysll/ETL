    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_TP_BIZ_RPT PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT TMP.PERIOD_WID AS PERIOD_WID      &#xd;&#xa;      ,RGN.RGN_ID&#xd;&#xa;      ,NVL&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;      ,BAS.BASE_ID&#xd;&#xa;      ,NVL&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME&#xd;&#xa;      ,BRD.BRAND_ID&#xd;&#xa;      ,NVL&#x28;BRD.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME&#xd;&#xa;      ,PRJ.PROJ_ID&#xd;&#xa;      ,NVL&#x28;PRJ.PROJ_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;      ,SUM&#x28;TMP.TICKET_OPEN_QTY_D&#x29; AS ZRQP&#xd;&#xa;      ,SUM&#x28;TMP.TICKET_OPEN_QTY_M&#x29; AS YLQP&#xd;&#xa;      ,SUM&#x28;TMP.DESP_QTY_D&#x29; AS ZRSJFY&#xd;&#xa;      ,SUM&#x28;TMP.DESP_QTY_M&#x29; AS YLFY&#xd;&#xa;      ,SUM&#x28;TMP.IN_TRANSIT_QTY&#x29; AS ZZT&#xd;&#xa;      ,SUM&#x28;TMP.PICKUP_DELAY_QTY&#x29; AS DDZL&#xd;&#xa;      ,SUM&#x28;TMP.IN_TRANSIT_OT_QTY&#x29; AS ZTCS&#xd;&#xa;      ,SUM&#x28;TMP.ON_DELIVERY_NUM_D&#x29; AS ZRJF&#xd;&#xa;      ,SUM&#x28;TMP.ON_DELIVERY_NUM_M&#x29; AS YLJF&#xd;&#xa;      ,SUM&#x28;TMP.ONTINE_ARRIVE_NUM&#x29; AS YLZSDD&#xd;&#xa;      ,SUM&#x28;TMP.ALL_ARRIVE_NUM&#x29; AS YLZDD&#xd;&#xa;      ,SUM&#x28;TMP.SEND_NUM_D&#x29; AS DRYF&#xd;&#xa;      ,SUM&#x28;TMP.DUE_SEND_NUM_D&#x29; AS DRDF&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;&#x29; AS INSERT_TIME&#xd;&#xa;      , --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;       &#x27;DW&#x27; AS PART_SC&#xd;&#xa;      , --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;yyyy-MM-dd&#x27;&#x29;,&#xd;&#xa;                     &#x27;yyyyMM&#x27;&#x29; AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM &#x28;SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_RPT_01&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_RPT_02&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_RPT_03&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_RPT_04&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_RPT_05&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_RPT_07&#x29; TMP&#xd;&#xa;  LEFT JOIN &#x28;&#xd;&#xa;	SELECT RGN_ID,RGN_NAME&#xd;&#xa;	FROM DIM.DIM_ORG&#xd;&#xa;	WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;	GROUP BY RGN_ID,RGN_NAME&#xd;&#xa;	&#x29; RGN&#xd;&#xa;    ON TMP.RGN_ID &#x3d; RGN.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;&#xd;&#xa;	SELECT BASE_ID,BASE_NAME&#xd;&#xa;	FROM DIM.DIM_ORG &#xd;&#xa;	WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;	GROUP BY BASE_ID,BASE_NAME&#xd;&#xa;	&#x29;BAS&#xd;&#xa;    ON TMP.BASE_ID &#x3d; BAS.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;&#xd;&#xa;	SELECT BRAND_ID,BRAND_NAME&#xd;&#xa;	FROM DIM.DIM_BRAND &#xd;&#xa;	GROUP BY BRAND_ID,BRAND_NAME&#xd;&#xa;	&#x29;BRD&#xd;&#xa;    ON TMP.BRAND_ID &#x3d; BRD.BRAND_ID&#xd;&#xa;  LEFT JOIN &#x28;&#xd;&#xa;	SELECT PROJ_ID,PROJ_NAME&#xd;&#xa;	FROM DIM.DIM_PROJ &#xd;&#xa;	GROUP BY PROJ_ID,PROJ_NAME&#xd;&#xa;	&#x29;PRJ&#xd;&#xa;    ON TMP.PROJ_ID &#x3d; PRJ.PROJ_ID&#xd;&#xa; GROUP BY TMP.PERIOD_WID   &#xd;&#xa;      ,RGN.RGN_ID&#xd;&#xa;      ,NVL&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; &#xd;&#xa;      ,BAS.BASE_ID&#xd;&#xa;      ,NVL&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;      ,BRD.BRAND_ID&#xd;&#xa;      ,NVL&#x28;BRD.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;      ,PRJ.PROJ_ID&#xd;&#xa;      ,NVL&#x28;PRJ.PROJ_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
