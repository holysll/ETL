      </entry>
    <entry>
      <name>TMP1</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_RPT_01&#xd;&#xa;--&#x542f;&#x7968;&#xd;&#xa;SELECT X.PERIOD_WID&#xd;&#xa;      ,X.RGN_ID&#xd;&#xa;      ,X.BASE_ID&#xd;&#xa;      ,X.BRAND_ID&#xd;&#xa;      ,X.PROJ_ID&#xd;&#xa;      ,LAG&#x28;O.TICKET_QTY&#x29; OVER&#x28;PARTITION BY X.RGN_ID, X.BASE_ID, X.BRAND_ID, X.PROJ_ID ORDER BY X.PERIOD_WID&#x29; AS TICKET_OPEN_QTY_D&#xd;&#xa;      ,SUM&#x28;NVL&#x28;O.TICKET_QTY, 0&#x29;&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X.PERIOD_WID, 1, 6&#x29;, X.RGN_ID, X.BASE_ID, X.BRAND_ID, X.PROJ_ID ORDER BY X.PERIOD_WID&#x29; AS TICKET_OPEN_QTY_M&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;  FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,T.BASE_ID&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,T.PROJ_ID&#xd;&#xa;          FROM DIM.DIM_PERIOD_D D&#xd;&#xa;          JOIN &#x28;SELECT COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                     ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;                     ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID --&#xd;&#xa;                     ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID --&#x9879;&#x76ee;ID &#xd;&#xa;                 FROM DMK.DMK_TP_ORDERQTY_D T&#xd;&#xa;                WHERE 1 &#x3d; 1&#xd;&#xa;                  AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3e;&#x3d;&#xd;&#xa;                      SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                  AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                      SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                  AND SUBSTR&#x28;PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d; &#xd;&#xa;                      FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;                GROUP BY COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                        ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                        ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                        ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29;&#x29; AS T&#xd;&#xa;            ON 1 &#x3d; 1&#xd;&#xa;         WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;           AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3e;&#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;           AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;           AND D.PERIOD_WID &#x3c;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; X --&#x4f7f;&#x7528;&#x7b1b;&#x5361;&#x5c14;&#x79ef;&#x9020;&#x51fa;&#x7ef4;&#x5ea6;&#x6846;&#x67b6;&#xd;&#xa;  LEFT JOIN &#x28;SELECT PERIOD_WID&#xd;&#xa;                   ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                   ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;                   ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID --&#xd;&#xa;                   ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;                   ,SUM&#x28;TICKET_QTY&#x29; AS TICKET_QTY&#xd;&#xa;               FROM DMK.DMK_TP_ORDERQTY_D T&#xd;&#xa;              WHERE 1 &#x3d; 1&#xd;&#xa;                  AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3e;&#x3d;&#xd;&#xa;                      SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                  AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                      SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                  AND SUBSTR&#x28;PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d; &#xd;&#xa;                      FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;              GROUP BY PERIOD_WID&#xd;&#xa;                      ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                      ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                      ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                      ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29;&#x29; O --&#x5ea6;&#x91cf;&#x6570;&#x503c;&#x5728;&#x6846;&#x67b6;&#x5185;&#x586b;&#x5145;&#x5373;&#x53ef;&#xd;&#xa;    ON X.PERIOD_WID &#x3d; O.PERIOD_WID&#xd;&#xa;   AND X.RGN_ID &#x3d; O.RGN_ID&#xd;&#xa;   AND X.BASE_ID &#x3d; O.BASE_ID&#xd;&#xa;   AND X.BRAND_ID &#x3d; O.BRAND_ID&#xd;&#xa;   AND X.PROJ_ID &#x3d; O.PROJ_ID&#xd;&#xa;WHERE SUBSTR&#x28;X.PERIOD_WID,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;    AND SUBSTR&#x28;X.PERIOD_WID,1,8&#x29; &#x3c;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;</sql>
