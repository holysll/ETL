      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_RPT_03&#xd;&#xa;--&#x5728;&#x9014;&#xd;&#xa;SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,NULL&#xd;&#xa;    ,NULL&#xd;&#xa;    ,NULL&#xd;&#xa;    ,NULL&#xd;&#xa;    ,count&#x28;VIN&#x29; AS IN_TRANSIT_QTY --&#x5728;&#x9014;&#x6570;&#xd;&#xa;    ,NULL&#xd;&#xa;    ,count&#x28;CASE WHEN &#x28;arrival_day IS NULL OR regexp_replace&#x28;to_date&#x28;arrival_day&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; &#x3e; regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#x29; &#xd;&#xa;					AND unix_timestamp&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;on_passage_days THEN VIN&#xd;&#xa;                WHEN regexp_replace&#x28;to_date&#x28;arrival_day&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; &#x3c;&#x3d; regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29; &#xd;&#xa;					AND unix_timestamp&#x28;arrival_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;on_passage_days THEN vin&#xd;&#xa;                    ELSE NULL END&#x29; AS IN_TRANSIT_OT_QTY --&#x5728;&#x9014;&#x8d85;&#x65f6;&#x6570;&#xd;&#xa;    ,NULL&#xd;&#xa;    ,NULL&#xd;&#xa;    ,NULL&#xd;&#xa;    ,NULL&#xd;&#xa;    ,NULL&#xd;&#xa;    ,NULL&#xd;&#xa;FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;WHERE substr&#x28;regexp_replace&#x28;factory_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;    AND &#x28;substr&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3e; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; OR arrival_day IS NULL&#x29;&#xd;&#xa;    AND on_passage_days IS NOT NULL&#xd;&#xa;GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;&#x3b;</sql>
