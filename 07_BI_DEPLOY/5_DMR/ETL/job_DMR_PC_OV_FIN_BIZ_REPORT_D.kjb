      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--target table &#x3a; DMR_PC_OV_FIN_BIZ_REPORT_D &#x62a5;&#x8868;-&#x5546;&#x52a1;&#x7ba1;&#x7406;-&#x62a5;&#x8868;&#x660e;&#x7ec6;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_OV_FIN_BIZ_REPORT_D PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,T.APPROVE_DATE AS APV_END_DATE&#xd;&#xa;      ,S.CONTRACT_TYPE_NAME AS CONT_TYPE_ID&#xd;&#xa;      ,T.IS_TEMP_CONTRANCT AS IS_TEMP_CONT&#xd;&#xa;      ,T.CONTRACT_NUMBER AS CONT_NO&#xd;&#xa;      ,T.CONTRACT_NAME&#xd;&#xa;      ,T.YOUR_UNIT AS UNIT_NAME&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN D.PERIOD_DATE IS NOT NULL THEN&#xd;&#xa;          T.CONTRACT_END_DATE&#xd;&#xa;         ELSE&#xd;&#xa;          NULL&#xd;&#xa;       END AS CONT_VALID_DATE --&#x5408;&#x540c;&#x6709;&#x6548;&#x671f; &#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN DATEDIFF&#x28;D.PERIOD_DATE,&#xd;&#xa;                       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd&#x27;&#x29;&#x29; BETWEEN 0 AND 90 THEN&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;0&#x27;&#xd;&#xa;       END AS VALID_PERIOD_WARNING --&#x6709;&#x6548;&#x671f;&#x9884;&#x8b66;&#xd;&#xa;      ,T.IS_BOND --&#x6709;&#x65e0;&#x4fdd;&#x8bc1;&#x91d1;&#xd;&#xa;      ,T.BOND_AMT --&#x5408;&#x540c;&#x4fdd;&#x8bc1;&#x91d1;&#xd;&#xa;      ,OM. NAME AS OPERATOR --&#x7ecf;&#x529e;&#x4eba;&#xd;&#xa;      ,NULL AS EST_PUR_AMT --&#x9884;&#x4f30;&#x91c7;&#x8d2d;&#x91d1;&#x989d;&#xd;&#xa;      ,NULL AS BRGN_AMT --&#x8bae;&#x4ef7;&#x7ed3;&#x679c;&#xd;&#xa;      ,A.CONT_APV_DUR AS APV_DUR --&#x5ba1;&#x6279;&#x65f6;&#x957f;&#x28;&#x5408;&#x540c;&#x5ba1;&#x6279;&#x5404;&#x4e2a;&#x73af;&#x8282;&#x7684;&#x7528;&#x65f6;&#x957f;&#x5ea6;&#x29;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTRING&#x28;T.PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM DW.DW_BIZ_CONTRACT T&#xd;&#xa;  LEFT JOIN &#x28;SELECT CONTRACT_NUMBER&#xd;&#xa;                   ,AVG&#x28;DATEDIFF&#x28;APPROVE_DATE, START_DATE&#x29;&#x29; AS CONT_APV_DUR&#xd;&#xa;               FROM DW.DW_BIZ_CONTRACT T&#xd;&#xa;              GROUP BY CONTRACT_NUMBER&#x29; A&#xd;&#xa;    ON T.CONTRACT_NUMBER &#x3d; A.CONTRACT_NUMBER&#xd;&#xa;  LEFT JOIN ODS_OA.ORG_MEMBER OM&#xd;&#xa;    ON T.PARTY_NUMBER &#x3d; OM.ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT NVL&#x28;M1.ENUMVALUE, T.FIELD0003&#x29; AS CONTRACT_TYPE_ID --  &#x5408;&#x540c;&#x7c7b;&#x578b;ID&#xd;&#xa;                   ,M1.SHOWVALUE AS CONTRACT_TYPE_NAME --  &#x5408;&#x540c;&#x7c7b;&#x578b;&#x540d;&#x79f0;&#xd;&#xa;               FROM ODS_OA.FORMMAIN_1125 T&#xd;&#xa;               LEFT JOIN ODS_OA.CTP_ENUM_ITEM M1&#xd;&#xa;                 ON T.FIELD0003 &#x3d; M1.ID&#xd;&#xa;              GROUP BY NVL&#x28;M1.ENUMVALUE, T.FIELD0003&#x29;&#xd;&#xa;                      ,M1.SHOWVALUE&#xd;&#xa;                      ,M1.ID&#x29; S&#xd;&#xa;    ON T.CONTRACT_TYPE_ID &#x3d; S.CONTRACT_TYPE_ID&#xd;&#xa;  LEFT JOIN DIM.DIM_PERIOD_D D&#xd;&#xa;    ON SUBSTR&#x28;T.CONTRACT_END_DATE, 10, 8&#x29; &#x3d; D.PERIOD_WID&#xd;&#xa;   AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa; WHERE 1 &#x3d; 1&#xd;&#xa;   AND T. STATE &#x3d; 1&#xd;&#xa;   AND S.CONTRACT_TYPE_ID IS NOT NULL&#xd;&#xa;   AND T.BIZ_TYPE &#x3d; &#x27;&#x6574;&#x8f66;&#x4e1a;&#x52a1;&#x27;&#x3b;&#xd;&#xa;</sql>
