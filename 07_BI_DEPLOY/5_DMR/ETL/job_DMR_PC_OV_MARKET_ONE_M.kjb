      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_OV_MARKET_ONE_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.RGN_NAME&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.BRAND_NAME&#xd;&#xa;    ,T.BIZATTR_ID&#xd;&#xa;    ,T.BIZATTR_NAME&#xd;&#xa;    ,SUM&#x28;T.cust_num_m&#x29; AS CUST_NUM&#xd;&#xa;    ,SUM&#x28;T.cust_num_lm&#x29; AS CUST_MOM_NUM&#xd;&#xa;    ,SUM&#x28;T.cust_num_m&#x29;-SUM&#x28;T.cust_num_lm&#x29; AS CUST_MOM_RATE&#xd;&#xa;    ,SUM&#x28;T.cust_num_lym&#x29; AS CUST_YOY_NUM&#xd;&#xa;    ,SUM&#x28;T.cust_num_m&#x29;-SUM&#x28;T.cust_num_lym&#x29; AS CUST_YOY_RATE&#xd;&#xa;    ,1&#x2b;&#x28;SUM&#x28;T.market_income_fact_amt&#x29;-SUM&#x28;T.market_income_tgt_amt&#x29;&#x29;&#x2f;abs&#x28;SUM&#x28;T.market_income_tgt_amt&#x29;&#x29; AS INCOME_COMP_RATE&#xd;&#xa;    ,SUM&#x28;T.market_income_tgt_amt&#x29; AS MARKET_INCOME_TGT_AMT&#xd;&#xa;    ,SUM&#x28;T.market_income_fact_amt&#x29; AS MARKET_INCOME_FACT_AMT&#xd;&#xa;    ,SUM&#x28;T.sale_qty&#x29;&#x2f;SUM&#x28;T.mkt_volume&#x29; AS MARKET_SHARE_RATE&#xd;&#xa;    ,1&#x2b;&#x28;SUM&#x28;T.profit_market_fact_amt&#x29;-SUM&#x28;T.profit_market_tgt_amt&#x29;&#x29;&#x2f;abs&#x28;SUM&#x28;T.profit_market_tgt_amt&#x29;&#x29; AS PROFIT_COMP_RATE&#xd;&#xa;    ,SUM&#x28;T.profit_market_tgt_amt&#x29; AS PROFIT_MARKET_TGT_AMT&#xd;&#xa;    ,SUM&#x28;T.profit_market_fact_amt&#x29; AS PROFIT_MARKET_FACT_AMT&#xd;&#xa;    ,SUM&#x28;T.edu_act_num&#x29;&#x2f;SUM&#x28;T.edu_tar_num&#x29; AS TRAIN_COMP_RATE&#xd;&#xa;    ,SUM&#x28;T.edu_tar_num&#x29; AS TRAIN_PLAN_NUM&#xd;&#xa;    ,SUM&#x28;T.edu_act_num&#x29; AS TRAIN_FACT_NUM&#xd;&#xa;    ,SUM&#x28;T.dev_act_num&#x29;&#x2f;SUM&#x28;T.dev_tar_num&#x29; AS DEV_COMP_RATE&#xd;&#xa;    ,SUM&#x28;T.dev_tar_num&#x29; AS DEV_CUST_PLAN_NUM&#xd;&#xa;    ,SUM&#x28;T.dev_act_num&#x29; AS DEV_CUST_FACT_NUM&#xd;&#xa;    ,SUM&#x28;T.vis_act_num&#x29;&#x2f;SUM&#x28;T.vis_tar_num&#x29; AS VISIT_COMP_RATE&#xd;&#xa;    ,SUM&#x28;T.vis_tar_num&#x29; AS VISIT_CUST_PLAN_NUM&#xd;&#xa;    ,SUM&#x28;T.vis_act_num&#x29; AS VISIT_CUST_FACT_NUM&#xd;&#xa;    ,SUM&#x28;T.mkt_inf_m&#x29; AS INFO_RETURN_QTY&#xd;&#xa;    ,SUM&#x28;T.mkt_inf_lm&#x29; AS INFO_RETURN_MOM_QTY&#xd;&#xa;    ,&#x28;SUM&#x28;T.mkt_inf_m&#x29;-SUM&#x28;T.mkt_inf_lm&#x29;&#x29;&#x2f;SUM&#x28;T.mkt_inf_lm&#x29; AS INFO_RETURN_MOM_RATE&#xd;&#xa;    ,SUM&#x28;T.mkt_inf_lym&#x29; AS INFO_RETURN_YOY_QTY&#xd;&#xa;    ,&#x28;SUM&#x28;T.mkt_inf_m&#x29;-SUM&#x28;T.mkt_inf_lym&#x29;&#x29;&#x2f;SUM&#x28;T.mkt_inf_lym&#x29; AS INFO_RETURN_YOY_RATE&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;&#x29;  AS W_INSERT_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DW&#x26;ODS&#x27;                         AS PART_SC    -- &#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;    ,T.PERIOD_WID                       AS PART_DT      -- &#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;FROM &#x28; &#xd;&#xa;    SELECT &#x2a; FROM TEMP.TEMP_DMR_PC_OV_MARKET_ONE_M_01 &#xd;&#xa;    UNION ALL &#xd;&#xa;    SELECT &#x2a; FROM TEMP.TEMP_DMR_PC_OV_MARKET_ONE_M_02&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT &#x2a; FROM TEMP.TEMP_DMR_PC_OV_MARKET_ONE_M_03&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.PERIOD_WID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.RGN_NAME&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.BRAND_NAME&#xd;&#xa;    ,T.BIZATTR_ID&#xd;&#xa;    ,T.BIZATTR_NAME</sql>
