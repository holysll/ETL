    <entry>
      <name>TO_TMP2</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_OV_MARKET_ONE_M_02&#xd;&#xa;SELECT A.month_id AS PERIOD_WID&#xd;&#xa;    ,CASE WHEN A.RGN_NAME &#x3d; &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27; THEN &#x27;ALL&#x27; ELSE B.rgn_id END AS RGN_ID&#xd;&#xa;    ,A.rgn_name&#xd;&#xa;    ,&#x27;ALL&#x27; AS BRAND_ID&#xd;&#xa;    ,&#x27;&#x5168;&#x90e8;&#x27; AS BRAND_NAME&#xd;&#xa;    ,CASE WHEN bus_type &#x3d; &#x27;&#x5168;&#x90e8;&#x27; THEN &#x27;ALL&#x27; ELSE NULL END AS BIZATTR_ID&#xd;&#xa;    ,A.bus_type AS BIZATTR_NAME&#xd;&#xa;    ,0&#xd;&#xa;    ,0&#xd;&#xa;    ,0&#xd;&#xa;    ,0&#xd;&#xa;    ,SUM&#x28;A.byl&#x29; AS CUST_NUM_M &#xd;&#xa;    ,SUM&#x28;NVL&#x28;C.byl,0&#x29;&#x29; AS CUST_NUM_LM &#xd;&#xa;    ,SUM&#x28;NVL&#x28;D.byl,0&#x29;&#x29; AS CUST_NUM_LYM&#xd;&#xa;    ,SUM&#x28;A.sj_kf&#x29; AS DEV_ACT_NUM &#xd;&#xa;    ,SUM&#x28;A.jh_kf&#x29; AS DEV_TAR_NUM &#xd;&#xa;    ,SUM&#x28;A.sj_bf&#x29; AS VIS_ACT_NUM&#xd;&#xa;    ,SUM&#x28;A.jh_bf&#x29; AS VIS_TAR_NUM &#xd;&#xa;    ,SUM&#x28;A.sjpxcs&#x29; AS EDU_ACT_NUM &#xd;&#xa;    ,SUM&#x28;A.jhpxcs&#x29; AS EDU_TAR_NUM &#xd;&#xa;    ,SUM&#x28;A.scxxfk&#x29; AS MKT_INF_M &#xd;&#xa;    ,SUM&#x28;NVL&#x28;C.scxxfk,0&#x29;&#x29; AS MKT_INF_LM &#xd;&#xa;    ,SUM&#x28;NVL&#x28;D.scxxfk,0&#x29;&#x29; AS MKT_INF_LYM&#xd;&#xa;    ,0&#xd;&#xa;    ,0&#xd;&#xa;FROM ODS_BLPT.T_HAND_MARKET A&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;add_months&#x28;from_unixtime&#x28;unix_timestamp&#x28;month_id,&#x27;yyyyMM&#x27;&#x29;,&#x27;yyyy-MM-01&#x27;&#x29;,1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;        ,rgn_name&#xd;&#xa;        ,bus_type&#xd;&#xa;        ,byl&#xd;&#xa;        ,kfbf_lv&#xd;&#xa;        ,sj_bf&#xd;&#xa;        ,jh_bf&#xd;&#xa;        ,tzxgpg&#xd;&#xa;        ,tzcb&#xd;&#xa;        ,tzcc&#xd;&#xa;        ,kf_lv&#xd;&#xa;        ,sj_kf&#xd;&#xa;        ,jh_kf&#xd;&#xa;        ,scxxfk&#xd;&#xa;        ,jhpxcs&#xd;&#xa;    FROM ODS_BLPT.T_HAND_MARKET&#xd;&#xa;    &#x29; C&#xd;&#xa;ON A.month_id &#x3d; C.MONTH_ID AND A.rgn_name &#x3d; C.rgn_name AND A.bus_type &#x3d; C.bus_type&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;add_months&#x28;from_unixtime&#x28;unix_timestamp&#x28;month_id,&#x27;yyyyMM&#x27;&#x29;,&#x27;yyyy-MM-01&#x27;&#x29;,12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;        ,rgn_name&#xd;&#xa;        ,bus_type&#xd;&#xa;        ,byl&#xd;&#xa;        ,kfbf_lv&#xd;&#xa;        ,sj_bf&#xd;&#xa;        ,jh_bf&#xd;&#xa;        ,tzxgpg&#xd;&#xa;        ,tzcb&#xd;&#xa;        ,tzcc&#xd;&#xa;        ,kf_lv&#xd;&#xa;        ,sj_kf&#xd;&#xa;        ,jh_kf&#xd;&#xa;        ,scxxfk&#xd;&#xa;        ,jhpxcs&#xd;&#xa;    FROM ODS_BLPT.T_HAND_MARKET&#xd;&#xa;    &#x29; D&#xd;&#xa;ON A.month_id &#x3d; D.MONTH_ID AND A.rgn_name &#x3d; D.rgn_name AND A.bus_type &#x3d; D.bus_type&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; B&#xd;&#xa;ON A.RGN_NAME &#x3d; B.RGN_NAME&#xd;&#xa;GROUP BY A.rgn_name,B.rgn_id,A.bus_type,A.month_id&#xd;&#xa;</sql>
