7f;&#xd;&#xa;    SELECT T1.SEGMETN_TRANSPORTMODE AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;           ,&#x27;CARD005&#x27; AS CARD_ID  --&#x5361;&#x7247;ID&#xd;&#xa;           ,&#x27;&#x4ea4;&#x4ed8;-&#x56de;&#x5355;&#x28;&#x5929;&#x29;&#x27; AS CORD_NAME  --&#x5361;&#x7247;&#x540d;&#x79f0;&#xd;&#xa;           ,SUM&#x28;T_D_TP_TIME&#x29;&#x2f;SUM&#x28;T_D_TP_QTY&#x29; AS T_AVG_TIME  --&#x672c;&#x671f;&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;           ,SUM&#x28;L_M_TP_TIME&#x29;&#x2f;SUM&#x28;L_M_TP_QTY&#x29; AS L_M_AVG_TIME --&#x4e0a;&#x671f;&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;           ,SUM&#x28;L_Y_TP_TIME&#x29;&#x2f;SUM&#x28;L_Y_TP_QTY&#x29; AS L_Y_AVG_TIME  --&#x540c;&#x671f;&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;    FROM &#x28;&#xd;&#xa;        SELECT SD.ORDER_RELEASE_NAME AS ORDER_RELEASE_NAME  --&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;               ,SD.TRANSPORTMODE_ID AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;			   ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN &#x28;UNIX_TIMESTAMP&#x28;SD.RECEIPT_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END  AS T_D_TP_TIME  --&#x672c;&#x671f;&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;			   ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END AS T_D_TP_QTY --&#x672c;&#x671f;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;               ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN &#x28;UNIX_TIMESTAMP&#x28;SD.RECEIPT_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END AS L_M_TP_TIME  --&#x4e0a;&#x671f;&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;			   ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END AS L_M_TP_QTY --&#x672c;&#x671f;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;               ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN &#x28;UNIX_TIMESTAMP&#x28;SD.RECEIPT_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END AS L_Y_TP_TIME  --&#x540c;&#x671f;&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;			   ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END AS L_Y_TP_QTY --&#x672c;&#x671f;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;        FROM TEMP.TEMP_DMR_DTD_SEGM_TIME SD&#xd;&#xa;        WHERE &#x28;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            OR SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            OR SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;    &#x29; T1&#xd;&#xa;    GROUP BY T1.SEGMETN_TRANSPORTMODE&#xd;&#xa;&#x29; A&#xd;&#xa;&#xd;&#xa;--&#x6807;&#x51c6;&#x65f6;&#x957f;&#xd;&#xa;LEFT JOIN TEMP.TEMP_DMR_DTD_SEGM_TIME_STAND B&#xd;&#xa;ON A.SEGMETN_TRANSPORTMODE &#x3d; B.TPMODE_CODE AND A.CORD_NAME &#x3d; B.CATD_NAME&#xd;&#xa;&#xd;&#xa;--&#x6700;&#x5927;&#x65f6;&#x957f;&#x53ca;&#x5176;&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT  SD1.ORDER_RELEASE_NAME AS ORDER_RELEASE_NAME  --&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;           ,SD1.SEGMETN_TRANSPORTMODE AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;           ,SD2.MAX_TP_VALUE AS MAX_TP_VALUE  --&#x6700;&#x5927;&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;    FROM&#x28;&#xd;&#xa;        SELECT SD.ORDER_RELEASE_NAME AS ORDER_RELEASE_NAME  --&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;               ,SD.TRANSPORTMODE_ID AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;               ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN &#x28;UNIX_TIMESTAMP&#x28;SD.RECEIPT_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END  AS T_D_TP_TIME  --&#x672c;&#x671f;&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;        FROM TEMP.TEMP_DMR_DTD_SEGM_TIME SD&#xd;&#xa;        WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29; SD1&#xd;&#xa;    LEFT JOIN&#x28;&#xd;&#xa;        SELECT &#xd;&#xa;               SD.TRANSPORTMODE_ID AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;               ,MAX&#x28;&#x28;UNIX_TIMESTAMP&#x28;SD.RECEIPT_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.ARRIVAL_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600&#x29; AS MAX_TP_VALUE  --&#x6700;&#x5927;&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;        FROM TEMP.TEMP_DMR_DTD_SEGM_TIME SD&#xd;&#xa;        WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.RECEIPT_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        GROUP BY SD.TRANSPORTMODE_ID&#xd;&#xa;    &#x29; SD2&#xd;&#xa;   ON SD1.SEGMETN_TRANSPORTMODE &#x3d; SD2.SEGMETN_TRANSPORTMODE AND SD1.T_D_TP_TIME &#x3d; SD2.MAX_TP_VALUE&#xd;&#xa;   WHERE SD2.MAX_TP_VALUE IS NOT NULL&#xd;&#xa;&#x29; C&#xd;&#xa;ON A.SEGMETN_TRANSPORTMODE &#x3d; C.SEGMETN_TRANSPORTMODE&#xd;&#xa;&#x3b;&#xd;&#xa;</sql>
