      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_DTD_SEGM_TIME_STAND&#xd;&#xa;SELECT  CASE WHEN TPMODE_NAME &#x3d; &#x27;&#x516c;&#x8def;&#x27; THEN &#x27;GL&#x27;&#xd;&#xa;             WHEN TPMODE_NAME &#x3d; &#x27;&#x6c34;&#x8def;&#x27; THEN &#x27;SL&#x27;&#xd;&#xa;             WHEN TPMODE_NAME &#x3d; &#x27;&#x94c1;&#x8def;&#x27; THEN &#x27;TL&#x27;&#xd;&#xa;             WHEN TPMODE_NAME &#x3d; &#x27;&#x4eba;&#x5de5;&#x27; THEN &#x27;LP&#x27;&#xd;&#xa;             WHEN TPMODE_NAME &#x3d; &#x27;&#x6d77;&#x8fd0;&#x27; THEN &#x27;HY&#x27; &#xd;&#xa;             WHEN TPMODE_NAME &#x3d; &#x27;&#x81ea;&#x63d0;&#x27; THEN &#x27;ZT&#x27;&#xd;&#xa;             ELSE &#x27;-1&#x27; END AS TPMODE_CODE  --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x4ee3;&#x7801;&#xd;&#xa;       ,CONCAT&#x28;CATD_NAME,&#x27;&#x28;&#x5c0f;&#x65f6;&#x29;&#x27;&#x29; AS CATD_NAME  --&#x8fd0;&#x8f93;&#x73af;&#x8282;&#x540d;&#x79f0;&#xd;&#xa;       ,CASE WHEN DAY_VALUE &#x3d; &#x27;&#x5c0f;&#x65f6;&#x27; THEN REGEXP_REPLACE&#x28;STAND_VALUE,&#x27;h&#x27;,&#x27;&#x27;&#x29; &#xd;&#xa;             WHEN DAY_VALUE &#x3d; &#x27;&#x5929;&#x27; THEN STAND_VALUE&#x2a;24 &#xd;&#xa;             ELSE STAND_VALUE END AS STAND_VALUE  --&#x8fd0;&#x8f93;&#x73af;&#x8282;&#x6807;&#x51c6;&#x65f6;&#x95f4;&#xd;&#xa;       ,&#x27;&#x5c0f;&#x65f6;&#x27; AS DAY_VALUE  --&#x5355;&#x4f4d;&#xd;&#xa;FROM ODS_BLPT.T_HAND_DTD_SEGM_TIME_PLAN&#xd;&#xa;WHERE YEAR_ID &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;    AND catd_name NOT IN&#x28;&#x27;&#x53d1;&#x8fd0;-&#x4ea4;&#x4ed8;&#x27;,&#x27;&#x56de;&#x5355;-&#x7ed3;&#x7b97;&#x27;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT transportmode_id AS TPMODE_CODE&#xd;&#xa;    ,&#x27;&#x53d1;&#x8fd0;-&#x4ea4;&#x4ed8;&#x28;&#x5929;&#x29;&#x27; AS CATD_NAME&#xd;&#xa;    ,AVG&#x28;COALESCE&#x28;unix_timestamp&#x28;arrival_day,&#x27;yyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;factory_day,&#x27;yyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;,0&#x29;&#x2f;3600&#x29; AS STAND_VALUE&#xd;&#xa;    ,&#x27;&#x5c0f;&#x65f6;&#x27; AS DAY_VALUE&#xd;&#xa;FROM TEMP.TEMP_DMR_DTD_SEGM_TIME&#xd;&#xa;	WHERE SUBSTR&#x28;regexp_replace&#x28;arrival_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY transportmode_id</sql>
