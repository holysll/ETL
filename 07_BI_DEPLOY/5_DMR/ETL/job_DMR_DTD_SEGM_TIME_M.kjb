    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_DTD_SEGM_TIME	DMR&#x5927;&#x5c4f;&#x7aef;DTD&#x603b;&#x65f6;&#x957f;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;06_DMR&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;64&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set hive.execution.engine&#x3d;mr&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_DTD_SEGM_TIME PARTITION &#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;--&#x63a5;&#x5355;-&#x5206;&#x914d;&#xd;&#xa;SELECT  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID  --&#x6708;&#x4efd;ID&#xd;&#xa;       ,A.SEGMETN_TRANSPORTMODE AS TPMODE_CODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;       ,A.CARD_ID AS CARD_ID  --&#x5361;&#x7247;ID&#xd;&#xa;       ,A.CORD_NAME AS CORD_NAME  --&#x5361;&#x7247;&#x540d;&#x79f0;&#xd;&#xa;       ,B.STAND_VALUE AS STAND_VALUE  --&#x6807;&#x51c6;&#x503c;&#xd;&#xa;       ,A.T_AVG_TIME AS AVG_TIME  --&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;       ,CASE WHEN A.T_AVG_TIME &#x3e; B.STAND_VALUE THEN 1 ELSE 0 END AS WARM_FLAG  --&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;       ,&#x28;A.T_AVG_TIME-A.L_Y_AVG_TIME&#x29;&#x2f;A.L_Y_AVG_TIME AS TB_RATE --&#x540c;&#x6bd4;&#xd;&#xa;       ,CASE WHEN &#x28;A.T_AVG_TIME-A.L_Y_AVG_TIME&#x29;&#x2f;A.L_Y_AVG_TIME &#x3e; 0 THEN 1&#xd;&#xa;             WHEN &#x28;A.T_AVG_TIME-A.L_Y_AVG_TIME&#x29;&#x2f;A.L_Y_AVG_TIME &#x3c; 0 THEN 0&#xd;&#xa;             WHEN &#x28;A.T_AVG_TIME-A.L_Y_AVG_TIME&#x29;&#x2f;A.L_Y_AVG_TIME &#x3d; 0 THEN 2 &#xd;&#xa;             ELSE &#x27;&#x672a;&#x77e5;&#x27; END AS TB_RATE_FLAG  --&#x540c;&#x6bd4;&#x6807;&#x5fd7;&#x28;1&#x4e0a;&#x5347;&#xff0c;0&#x4e0b;&#x964d;&#xff0c;2&#x4e0d;&#x53d8;&#xd;&#xa;       ,&#x28;A.T_AVG_TIME-A.L_M_AVG_TIME&#x29;&#x2f;A.L_M_AVG_TIME AS HB_RATE  --&#x73af;&#x6bd4;&#xd;&#xa;       ,CASE WHEN &#x28;A.T_AVG_TIME-A.L_M_AVG_TIME&#x29;&#x2f;A.L_M_AVG_TIME &#x3e; 0 THEN 1&#xd;&#xa;             WHEN &#x28;A.T_AVG_TIME-A.L_M_AVG_TIME&#x29;&#x2f;A.L_M_AVG_TIME &#x3c; 0 THEN 0&#xd;&#xa;             WHEN &#x28;A.T_AVG_TIME-A.L_M_AVG_TIME&#x29;&#x2f;A.L_M_AVG_TIME &#x3d; 0 THEN 2 &#xd;&#xa;             ELSE &#x27;&#x672a;&#x77e5;&#x27; END AS HB_RATE_FLAG  --&#x73af;&#x6bd4;&#x6807;&#x5fd7;&#xd;&#xa;       ,C.MAX_TP_VALUE AS LONGEST_TIME  --&#x6700;&#x957f;&#x8017;&#x65f6;&#xd;&#xa;       ,C.ORDER_RELEASE_NAME AS BILL_NO  --&#x5355;&#x53f7;&#xd;&#xa;       ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;       ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;       ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;       ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT  --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;FROM &#x28;&#xd;&#xa;--&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;    SELECT T1.SEGMETN_TRANSPORTMODE AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;           ,&#x27;CARD001&#x27; AS CARD_ID  --&#x5361;&#x7247;ID&#xd;&#xa;           ,&#x27;&#x63a5;&#x5355;-&#x5206;&#x914d;&#x28;&#x5c0f;&#x65f6;&#x29;&#x27; AS CORD_NAME  --&#x5361;&#x7247;&#x540d;&#x79f0;&#xd;&#xa;           ,SUM&#x28;T_D_TP_TIME&#x29;&#x2f;SUM&#x28;T_D_TP_QTY &#x29; AS T_AVG_TIME  --&#x672c;&#x671f;&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;           ,SUM&#x28;L_M_TP_TIME&#x29;&#x2f;SUM&#x28;L_M_TP_QTY&#x29; AS L_M_AVG_TIME --&#x4e0a;&#x671f;&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;           ,SUM&#x28;L_Y_TP_TIME&#x29;&#x2f;SUM&#x28;L_Y_TP_QTY&#x29; AS L_Y_AVG_TIME  --&#x540c;&#x671f;&#x5e73;&#x5747;&#x65f6;&#x957f;&#xd;&#xa;    FROM &#x28;&#xd;&#xa;        SELECT SD.ORDER_RELEASE_NAME AS ORDER_RELEASE_NAME  --&#x8ba2;&#x5355;&#x53f7;&#xd;&#xa;               ,SD.TRANSPORTMODE_ID AS SEGMETN_TRANSPORTMODE  --&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#xd;&#xa;               ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN &#x28;UNIX_TIMESTAMP&#x28;SD.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END  AS T_D_TP_TIME  --&#x672c;&#x671f;&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;			   ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END AS T_D_TP_QTY --&#x672c;&#x671f;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;               ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN &#x28;UNIX_TIMESTAMP&#x28;SD.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END AS L_M_TP_TIME  --&#x4e0a;&#x671f;&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;			   ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END AS L_M_TP_QTY --&#x672c;&#x671f;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;               ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN &#x28;UNIX_TIMESTAMP&#x28;SD.ALLOT_PLN_DAY,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - UNIX_TIMESTAMP&#x28;SD.EARLY_PICKUP_DATE,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;&#x29;&#x2f;3600 ELSE 0 END AS L_Y_TP_TIME  --&#x540c;&#x671f;&#x8fd0;&#x8f93;&#x65f6;&#x957f;&#xd;&#xa;			   ,CASE WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN 1 ELSE 0 END AS L_Y_TP_QTY --&#x672c;&#x671f;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;        FROM TEMP.TEMP_DMR_DTD_SEGM_TIME SD&#xd;&#xa;        WHERE &#x28;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            OR SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            OR SUBSTR&#x28;REGEXP_REPLACE&#x28;SD.ALLOT_PLN_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; SUBSTR&#x28;REGE