    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_FIN_TPBUDGET_D	&#x8fd0;&#x8f93;&#x8d22;&#x52a1;&#x9884;&#x7b97;&#x65e5;&#x660e;&#x7ec6;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;05_DMK&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_TP_OPERATE_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT A1.PERIOD_WID AS DATE_ID --&#x65e5;&#x671f;&#xd;&#xa;      ,A1.RGN_ID AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;      ,MAX&#x28;CASE&#xd;&#xa;             WHEN A1.RGN_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;              &#x27;&#x5168;&#x90e8;&#x5927;&#x533a;&#x27;&#xd;&#xa;             WHEN A1.RGN_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;              &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;             ELSE&#xd;&#xa;              COALESCE&#x28;A2.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;           END&#x29; AS RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;      ,A1.BASE_ID AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,MAX&#x28;CASE&#xd;&#xa;             WHEN A1.BASE_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;              &#x27;&#x5168;&#x90e8;&#x57fa;&#x5730;&#x27;&#xd;&#xa;             WHEN A1.BASE_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;              &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;             ELSE&#xd;&#xa;              COALESCE&#x28;A3.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;           END&#x29; AS BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;      ,A1.BRAND_ID AS BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;      ,MAX&#x28;CASE&#xd;&#xa;             WHEN A1.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;              &#x27;&#x5168;&#x90e8;&#x54c1;&#x724c;&#x27;&#xd;&#xa;             WHEN A1.BRAND_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;              &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;             ELSE&#xd;&#xa;              COALESCE&#x28;A4.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;           END&#x29; AS BRAND_NAME --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;      ,SUM&#x28;A1.CUR_D_TRANS_BIZ_QTY&#x29; AS CUR_D_TRANS_BIZ_QTY --&#x5f53;&#x65e5;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_TRANS_BIZ_QTY&#x29; AS CUR_M_TRANS_BIZ_QTY --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_FNSH_RATE&#x29; AS CUR_M_FNSH_RATE --&#x5f53;&#x6708;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,SUM&#x28;A1.MONTH_HB_RATE&#x29; AS MONTH_HB_RATE --&#x6708;&#x73af;&#x6bd4;&#xd;&#xa;      ,SUM&#x28;A1.CUR_Y_TRANS_BIZ_QTY&#x29; AS CUR_Y_TRANS_BIZ_QTY --&#x5f53;&#x5e74;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,SUM&#x28;A1.CUR_Y_FNSH_RATE&#x29; AS CUR_Y_FNSH_RATE --&#x5f53;&#x5e74;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,SUM&#x28;A1.CUR_D_TP_BIZ_NUM&#x29; AS CUR_D_TP_BIZ_NUM --&#x5f53;&#x65e5;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_TP_BIZ_NUM&#x29; AS CUR_M_TP_BIZ_NUM --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_RATE&#x29; AS CUR_M_RATE --&#x5f53;&#x6708;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_YOY_RATE&#x29; AS CUR_M_YOY_RATE --&#x6708;&#x73af;&#x6bd4;&#xd;&#xa;      ,SUM&#x28;A1.CUR_Y_TP_BIZ_QTY&#x29; AS CUR_Y_TP_BIZ_QTY --&#x5f53;&#x5e74;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,SUM&#x28;A1.CUR_Y_RATE&#x29; AS CUR_Y_RATE --&#x5f53;&#x5e74;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,SUM&#x28;A1.LST_D_QP_QTY&