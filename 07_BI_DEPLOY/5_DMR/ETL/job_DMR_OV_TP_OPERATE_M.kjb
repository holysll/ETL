            ,X.BRAND_ID&#xd;&#xa;              ,X.CUR_D_TRANS_BIZ_QTY --&#x5f53;&#x65e5;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,X.CUR_M_TRANS_BIZ_QTY --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;X.CUR_M_TARGET_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  ROUND&#x28;&#x28;X.CUR_M_TRANS_BIZ_QTY &#x2f; X.CUR_M_TARGET_QTY&#x29; &#x2a; 100, 2&#x29;&#xd;&#xa;               END AS CUR_M_FNSH_RATE --&#x5f53;&#x6708;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;X.LST_M_TRANS_BIZ_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  ROUND&#x28;&#x28;&#x28;X.CUR_M_TRANS_BIZ_QTY - X.LST_M_TRANS_BIZ_QTY&#x29; &#x2f;&#xd;&#xa;                        X.LST_M_TRANS_BIZ_QTY&#x29; &#x2a; 100,&#xd;&#xa;                        2&#x29;&#xd;&#xa;               END AS MONTH_HB_RATE --&#x6708;&#x73af;&#x6bd4; &#xd;&#xa;              ,X.CUR_Y_TRANS_BIZ_QTY --&#x5f53;&#x5e74;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf; &#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;X.CUR_Y_TARGET_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  ROUND&#x28;&#x28;X.CUR_Y_TRANS_BIZ_QTY &#x2f; X.CUR_Y_TARGET_QTY&#x29; &#x2a; 100, 2&#x29;&#xd;&#xa;               END AS CUR_Y_FNSH_RATE --&#x5f53;&#x5e74;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,0 AS CUR_D_TP_BIZ_NUM --&#x5f53;&#x65e5;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,0 AS CUR_M_TP_BIZ_NUM --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,0.00 AS CUR_M_RATE --&#x5f53;&#x6708;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,0.00 AS CUR_M_YOY_RATE --&#x6708;&#x73af;&#x6bd4;&#xd;&#xa;              ,0 AS CUR_Y_TP_BIZ_QTY --&#x5f53;&#x5e7