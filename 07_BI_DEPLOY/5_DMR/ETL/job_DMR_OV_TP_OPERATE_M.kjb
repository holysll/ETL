                               ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                              ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                    --&#x535a;&#x5b87;&#x4e1a;&#x52a1;   --&#x6309;&#x6708;&#x8865;&#x5f55;&#xff0c;&#x5c06;&#x6570;&#x636e;&#x8bbe;&#x7f6e;&#x4e3a;&#x5f53;&#x6708;&#x6700;&#x540e;&#x4e00;&#x5929;&#xd;&#xa;                                    UNION ALL&#xd;&#xa;                                    SELECT &#x27;BOYU&#x27; AS RGN_ID&#xd;&#xa;                                           ,&#x27;-1&#x27; AS BASE_ID&#xd;&#xa;                                           ,&#x27;-1&#x27; AS BRAND_ID&#xd;&#xa;                                           ,SUM&#x28;BIZ_QTY_PER_CAR&#x29; AS SHIPPING_NUM&#xd;&#xa;                                      FROM DW.DW_TP_BOYUBIZ B --&#x535a;&#x5b87;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf; &#xd;&#xa;                                     WHERE SUBSTR&#x28;B.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                                           SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                            -1&#x29;,&#xd;&#xa;                                                                 &#x27;-&#x27;,&#xd;&#xa;                                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                                  1,&#xd;&#xa;                                                  6&#x29;&#xd;&#xa;                                       AND TRIM&#x28;B.REMARK&#x29; &#x3d; &#x27;&#x65b0;&#x4e1a;&#x52a1;&#x27;&#xd;&#xa;                                    --&#x4f53;&#x7cfb;&#x5916;&#x4e1a;&#x52a1;  --&#x6309;&#x6708;&#x8865;&#x5f55;&#xff0c;&#x5c06;&#x6570;&#x636e;&#x8bbe;&#x7f6e;&#x4e3a;&#x5f53;&#x6708;&#x6700;&#x540e;&#x4e00;&#x5929;&#xd;&#xa;                                    UNION ALL&#xd;&#xa;                          