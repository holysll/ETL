E_ID &#x3d; X2.BASE_ID&#xd;&#xa;                   AND X1.BRAND_ID &#x3d; X2.BRAND_ID&#xd;&#xa;                  LEFT JOIN &#x28;SELECT NVL&#x28;NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                  ,SUM&#x28;CASE&#xd;&#xa;                                         WHEN SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                                              SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                    &#x27;-&#x27;,&#xd;&#xa;                                                                    &#x27;&#x27;&#x29;,&#xd;&#xa;                                                     1,&#xd;&#xa;                                                     6&#x29; THEN&#xd;&#xa;                                          COALESCE&#x28;BIZ_QTY, 0&#x29;&#xd;&#xa;                                         ELSE&#xd;&#xa;                                          0&#xd;&#xa;                                       END&#x29; AS CUR_M_TARGET_QTY --&#x5f53;&#x6708;&#x4e1a;&#x52a1;&#x91cf;&#x8ba1;&#x5212;&#x28;&#x5305;&#x542b;&#x5404;&#x9879;&#x76ee;&#x3001;&#x535a;&#x5b87;&#x81ea;&#x6709;&#x90e8;&#x5206;&#x548c;&#x672c;&#x90e8; &#x29;&#xd;&#xa;                                  ,SUM&#x28;CASE&#xd;&#xa;                                         WHEN SUBSTR&#x28;PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                                              SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                    &#x27;-&#x27;,&#xd;&#xa;                                                                    &#x27;&#x27;&#x29;,&#xd;&#xa;                                                     1,&#xd;&#xa;                                                     4&#x29; THEN&#xd;&#xa;                                          COALESCE&#x28;BIZ_QTY, 0&#x29;&#xd;&#xa;                                         ELSE&#xd;&#xa;                                          0&#xd;&#xa;                                       END&#x29; AS CUR_Y_TARGET_QTY --&#x5f53;&#x5e74;&#x4e1a;&#x52a1;&#x91cf;&#x8ba1;&#x5212;&#xd;&#xa;                              FROM DW.DW_FIN_TGT_BIZ_QTY&#xd;&#xa;                             WHERE SUBSTR&#x28;PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                                   SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                    -1&#x29;,&#xd;&#xa;                                                         &#x27;-&#x27;,&#xd;&#xa;                                                         &#x27;&#x27;&#x29;,&#xd;&#xa;                                          1,&#xd;&#xa;                                          4&#x29;&#xd;&#xa;                             GROUP BY NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                     ,NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                     ,NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS X3 --&#x5f97;&#x5230;&#x76ee;&#x6807;&#x4e1a;&#x52a1;&#x91cf; &#xd;&#xa;                    ON X1.RGN_ID &#x3d; X3.RGN_ID&#xd;&#xa;                   AND X1.BASE_ID &#x3d; X3.BASE_ID&#xd;&#xa;                   AND X1.BRAND_ID &#x3d; X3.BRAND_ID&#x29; AS X --&#x5f97;&#x5230;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;         WHERE SUBSTR&#x28;X.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -0&#x29;,&#xd;&#xa;                                     &#x27;-&#x27;,&#xd;&#xa;                                     &#x27;&#x27;&#x29;,&#xd;&#xa;                      1,&#xd;&#xa;                      6&#x29;&#xd;&#xa;        UNION ALL&#xd;&#xa;        --&#x5f97;&#x5230;&#x5728;&#x9014;&#x3001;&#x4ea4;&#x4ed8;&#x3001;&#x51c6;&#x5230;&#xd;&#xa;        SELECT B.PERIOD_WID&#xd;&#xa;              ,B.RGN_ID&#xd;&#xa;              ,B.BASE_ID&#xd;&#xa;              ,B.BRAND_ID&#xd;&#xa;              ,CUR_D_TRANS_BIZ_QTY --&#x5f53;&#x65e5;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,CUR_M_TRANS_BIZ_QTY --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,CUR_M_FNSH_RATE --&#x5f53;&#x6708;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,MONTH_HB_RATE --&#x6708;&#x73af;&#x6bd4;&#xd;&#xa;              ,CUR_Y_TRANS_BIZ_QTY --&#x5f53;&#x5e74;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,CUR_Y_FNSH_RATE --&#x5f53;&#x5e74;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,0                        AS CUR_D_TP_BIZ_NUM --&#x5f53;&#x65e5;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,0                        AS CUR_M_TP_BIZ_NUM --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,0.00                     AS CUR_M_RATE --&#x5f53;&#x6708;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,0.00                     AS CUR_M_YOY_RATE --&#x6708;&#x73af;&#x6bd4;&#xd;&#xa;              ,0                        AS CUR_Y_TP_BIZ_QTY --&#x5f53;&#x5e74;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,0.00                     AS CUR_Y_RATE --&#x5f53;&#x5e74;&#x5b8c;&#x6210;&#x7387;              &#xd;&#xa;              ,LST_D_QP_QTY --&#x4e0a;&#x65e5;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;              ,CUR_D_DUE_OUT_QTY --&#x5f53;&#x65e5;&#x5f85;&#x53d1;&#x91cf;  &#xd;&#xa;              ,LST_D_DLV_QTY --&#x4e0a;&#x65e5;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;              ,CUR_M_QP_QTY --&#x5f53;&#x6708;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;              ,CUR_D_ON_ROAD_QTY --&#x5f53;&#x65e5;&#x5728;&#x9014;&#x91cf;&#xd;&#xa;              ,CUR_M_DLV_QTY --&#x5f53;&#x6708;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;              ,CUR_Y_QP_QTY --&#x5f53;&#x5e74;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;              ,CUR_D_ON_ROAD_OVT_QTY --&#x5f53;&#x65e5;&#x5728;&#x9014;&#x8d85;&#x65f6;&#x91cf;&#xd;&#xa;              ,CUR_Y_DLV_QTY --&#x5f53;&#x5e74;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;              ,CUR_D_QP_FNSH_RATE --&#x5f53;&#x65e5;&#x542f;&#x7968;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,CUR_D_ON_ROAD_OVT_RATE --&#x5f53;&#x65e5;&#x5728;&#x9014;&#x8d85;&#x65f6;&#x7387;&#xd;&#xa;              ,CUR_D_ONTIME_ARRIVE_RATE --&#x5f53;&#x65e5;&#x51c6;&#x65f6;&#x5230;&#x8fbe;&#x7387; &#xd;&#xa;          FROM &#x28;SELECT X1.PERIOD_WID AS PERIOD_WID&#xd;&#xa;                      ,X1.RGN_ID AS RGN_ID --&#x5927;&#x533a;&#xd;&#xa;                      ,X1.BASE_ID AS BASE_ID --&#x57fa;&#x5730;&#xd;&#xa;                      ,X1.BRAND_ID AS BRAND_ID --&#x54c1;&#x724c;&#xd;&#xa;                      ,0 AS CUR_D_TRANS_BIZ_QTY --&#x5f53;&#x65e5;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;                      ,0 AS CUR_M_TRANS_BIZ_QTY --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;                      ,0.00 AS CUR_M_FNSH_RATE --&#x5f53;&#x6708;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;                      ,0.00 AS MONTH_HB_RATE --&#x6708;&#x73af;&#x6bd4;&#xd;&#xa;                      ,0 AS CUR_Y_TRANS_BIZ_QTY --&#x5f53;&#x5e74;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;                      ,0.00 AS CUR_Y_FNSH_RATE --&#x5f53;&#x5e74;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;                      ,0 AS LST_D_QP_QTY --&#x4e0a;&#x65e5;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;                      ,0 AS CUR_D_DUE_OUT_QTY --&#x5f53;&#x65e5;&#x5f85;&#x53d1;&#x91cf;&#xd;&#xa;                      ,LAG&#x28;X2.TOTAL_ARRIVE_NUM, 1, 0&#x29; OVER&#x28;PARTITION BY X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS LST_D_DLV_QTY --&#x4e0a;&#x65e5;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;                      ,0 AS CUR_M_QP_QTY --&#x5f53;&#x6708;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;                      ,X2.ON_PASSAGE_NUM AS CUR_D_ON_ROAD_QTY --&#x5f53;&#x65e5;&#x5728;&#x9014;&#x91cf;&#xd;&#xa;                      ,SUM&#x28;X2.TOTAL_ARRIVE_NUM&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 6&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_M_DLV_QTY --&#x5f53;&#x6708;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;                      ,0 AS CUR_Y_QP_QTY --&#x5f53;&#x5e74;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;                      ,X2.ON_PASSAGE_OUTTIME_NUM AS CUR_D_ON_ROAD_OVT_QTY --&#x5f53;&#x65e5;&#x5728;&#x9014;&#x8d85;&#x65f6;&#x91cf;&#xd;&#xa;                      ,SUM&#x28;X2.TOTAL_ARRIVE_NUM&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 4&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_Y_DLV_QTY --&#x5f53;&#x5e74;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;                      ,0.00 AS CUR_D_QP_FNSH_RATE --&#x5f53;&#x65e5;&#x542f;&#x7968;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;                      ,CASE&#xd;&#xa;                         WHEN NVL&#x28;X2.ON_PASSAGE_NUM, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                          0.00&#xd;&#xa;                         ELSE&#xd;&#xa;                          X2.ON_PASSAGE_OUTTIME_NUM &#x2f; X2.ON_PASSAGE_NUM&#xd;&#xa;                       END AS CUR_D_ON_ROAD_OVT_RATE --&#x5f53;&#x65e5;&#x5728;&#x9014;&#x8d85;&#x65f6;&#x7387;&#xd;&#xa;                      ,CASE&#xd;&#xa;                         WHEN NVL&#x28;X2.PLAN_ARRIVE_NUM, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                          0.00&#xd;&#xa;                         ELSE&#xd;&#xa;                          X2.ONTIME_ARRIVE_NUM &#x2f; X2.PLAN_ARRIVE_NUM&#xd;&#xa;                       END AS CUR_D_ONTIME_ARRIVE_RATE --&#x5f53;&#x65e5;&#x51c6;&#x65f6;&#x5230;&#x8fbe;&#x7387; &#xd;&#xa;                  FROM &#x28;SELECT D.PERIOD_WID AS PERIOD_WID&#xd;&#xa;                              ,T.RGN_ID     AS RGN_ID&#xd;&#xa;                              ,T.BASE_ID    AS BASE_ID&#xd;&#xa;                              ,T.BRAND_ID   AS BRAND_ID&#xd;&#xa;                          FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                          LEFT JOIN &#x28;SELECT NVL&#x28;COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;&#xd;&#xa;                                          ,NVL&#x28;COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;&#xd;&#xa;                                          ,NVL&#x28;COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;,&#xd;&#xa;                                               &#x27;ALL&#x27;&#x29; AS BRAND_ID --&#x54c1;&#x724c;&#xd;&#xa;                                      FROM DMK.DMK_TP_ARRIVE_D T&#xd;&#xa;                                     WHERE SUBSTR&#x28;PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                 &#x27;-&#x27;,&#xd;&#xa;                                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                                  1,&#xd;&#xa;                                                  4&#x29;&#xd;&#xa;                                       AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                 &#x27;-&#x27;,&#xd;&#xa;                                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                                  1,&#xd;&#xa;                                                  6&#x29;&#xd;&#xa;                                       AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                                           FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#xd;&#xa;                                                         &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;                                     GROUP BY COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                             ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                             ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS T&#xd;&#xa;                            ON 1 &#x3d; 1&#xd;&#xa;                         WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                           AND SUBSTR&#x28;D.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      4&#x29;&#xd;&#xa;                           AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      6&#x29;&#xd;&#xa;                           AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                               FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; AS X1&#xd;&#xa;                  LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                                  ,NVL&#x28;T.RGN_ID, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                  ,NVL&#x28;T.BASE_ID, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                  ,NVL&#x28;T.BRAND_ID, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                  ,SUM&#x28;T.TOTAL_ARRIVE_NUM&#x29; AS TOTAL_ARRIVE_NUM&#xd;&#xa;                                  ,SUM&#x28;T.ON_PASSAGE_NUM&#x29; AS ON_PASSAGE_NUM&#xd;&#xa;                                  ,SUM&#x28;T.ONTIME_ARRIVE_NUM&#x29; AS ONTIME_ARRIVE_NUM&#xd;&#xa;                                  ,SUM&#x28;T.PLAN_ARRIVE_NUM&#x29; AS PLAN_ARRIVE_NUM&#xd;&#xa;                                  ,SUM&#x28;T.ON_PASSAGE_OUTTIME_NUM&#x29; AS ON_PASSAGE_OUTTIME_NUM&#xd;&#xa;                              FROM &#x28;SELECT T.PERIOD_WID AS PERIOD_WID&#xd;&#xa;                                          ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;&#xd;&#xa;                                          ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;&#xd;&#xa;                                          ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID --&#x54c1;&#x724c;&#xd;&#xa;                                          ,SUM&#x28;T.DLVR_QTY&#x29; AS TOTAL_ARRIVE_NUM&#xd;&#xa;                                          ,0 AS ON_PASSAGE_NUM&#xd;&#xa;                                          ,SUM&#x28;T.ARRIVE_ONTIME_QTY&#x29; AS ONTIME_ARRIVE_NUM&#xd;&#xa;                                          ,SUM&#x28;T.ARRIVE_PLAN_QTY&#x29; AS PLAN_ARRIVE_NUM&#xd;&#xa;                                          ,0 AS ON_PASSAGE_OUTTIME_NUM&#xd;&#xa;                                      FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                                      LEFT JOIN DIM.DIM_ORG B&#xd;&#xa;                                        ON T.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;                                       AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                                     WHERE SUBSTR&#x28;PERIOD_WID, 1, 4&#x29; &#x3e;&#x3d;&#xd;&#xa;                                           SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                            -12&#x29;,&#xd;&#xa;                                                                 &#x27;-&#x27;,&#xd;&#xa;                                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                                  1,&#xd;&#xa;                                                  4&#x29;&#xd;&#xa;                                       AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                 &#x27;-&#x27;,&#xd;&#xa;                                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                                  1,&#xd;&#xa;                                                  6&#x29;&#xd;&#xa;                                       AND SUBSTR&#x28;PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;                                           FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#xd;&#xa;                                                         &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;                                       AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                                       AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;                                     GROUP BY T.PERIOD_WID&#xd;&#xa;                                             ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                             ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                             ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                    UNION ALL&#xd;&#xa;                                    SELECT P.PERIOD_WID&#xd;&#xa;                                          ,NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                          ,NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                          ,NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                          ,0 AS TOTAL_ARRIVE_NUM&#xd;&#xa;                                          ,COUNT&#x28;DISTINCT VIN&#x29; AS ON_PASSAGE_NUM&#xd;&#xa;                                          ,0 AS ONTIME_ARRIVE_NUM&#xd;&#xa;                                          ,0 AS PLAN_ARRIVE_NUM&#xd;&#xa;                                          ,COUNT&#x28;DISTINCT CASE&#xd;&#xa;                                                   WHEN UNIX_TIMESTAMP&#x28;NVL&#x28;ARRIVAL_DAY,&#xd;&#xa;                                                                           CONCAT&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                                  &#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;&#x29;,&#xd;&#xa;                                                                       &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; -&#xd;&#xa;                                                        UNIX_TIMESTAMP&#x28;FACTORY_DAY,&#xd;&#xa;                                                                       &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e;&#xd;&#xa;                                                        24 &#x2a; 3600 &#x2a; ON_PASSAGE_DAYS THEN&#xd;&#xa;                                                    VIN&#xd;&#xa;                                                   ELSE&#xd;&#xa;                                                    NULL&#xd;&#xa;                                                 END&#x29; AS ON_PASSAGE_OUTTIME_NUM&#xd;&#xa;                                      FROM &#x28;SELECT PERIOD_WID&#xd;&#xa;                                              FROM DIM.DIM_PERIOD_D&#xd;&#xa;                                             WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                                               AND SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    