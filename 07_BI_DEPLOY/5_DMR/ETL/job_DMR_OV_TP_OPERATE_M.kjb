                                    &#x27;2999-12-31&#x27;&#x29;,&#xd;&#xa;                                                                 &#x27;-&#x27;,&#xd;&#xa;                                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                                  1,&#xd;&#xa;                                                  8&#x29; &#x3e; P.PERIOD_WID&#xd;&#xa;                                     GROUP BY P.PERIOD_WID&#xd;&#xa;                                             ,NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                             ,NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                             ,NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;&#x29; T&#xd;&#xa;                             GROUP BY T.PERIOD_WID&#xd;&#xa;                                     ,T.RGN_ID&#xd;&#xa;                                     ,T.BASE_ID&#xd;&#xa;                                     ,T.BRAND_ID WITH CUBE&#x29; AS X2&#xd;&#xa;                    ON X1.PERIOD_WID &#x3d; X2.PERIOD_WID&#xd;&#xa;                   AND X1.RGN_ID &#x3d; X2.RGN_ID&#xd;&#xa;                   AND X1.BASE_ID &#x3d; X2.BASE_ID&#xd;&#xa;                   AND X1.BRAND_ID &#x3d; X2.BRAND_ID&#x29; AS B&#xd;&#xa;         WHERE SUBSTR&#x28;B.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT B.PERIOD_WID&#xd;&#xa;              ,B.RGN_ID&#xd;&#xa;              ,B.BASE_ID&#xd;&#xa;              ,B.BRAND_ID&#xd;&#xa;              ,0 AS CUR_D_TRANS_BIZ_QTY --&#x5f53;&#x65e5;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,0 AS CUR_M_TRANS_BIZ_QTY --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,0.00 AS CUR_M_FNSH_RATE --&#x5f53;&#x6708;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,0.00 AS MONTH_HB_RATE --&#x6708;&#x73af;&#x6bd4;&#xd;&#xa;              ,0 AS CUR_Y_TRANS_BIZ_QTY --&#x5f53;&#x5e74;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,0.00 AS CUR_Y_FNSH_RATE --&#x5f53;&#x5e74;&#x5b8c;&#x6210;&#x7387;    &#xd;&#xa;              ,B.BIZ_QTY AS CUR_D_TP_BIZ_NUM --&#x5f53;&#x65e5;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,B.BIZ_QTY_M AS CUR_M_TP_BIZ_NUM --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;B.TKT_QTY_M, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0.00&#xd;&#xa;                 ELSE&#xd;&#xa;                  B.BIZ_QTY_M &#x2f; B.TKT_QTY_M&#xd;&#xa;               END AS CUR_M_RATE --&#x5f53;&#x6708;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;B.BIZ_QTY_SQ, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0.00&#xd;&#xa;                 ELSE&#xd;&#xa;                  &#x28;B.BIZ_QTY_M - B.BIZ_QTY_SQ&#x29; &#x2f; B.BIZ_QTY_SQ&#xd;&#xa;               END AS CUR_M_YOY_RATE --&#x6708;&#x73af;&#x6bd4;&#xd;&#xa;              ,B.BIZ_QTY_Y AS CUR_Y_TP_BIZ_QTY --&#x5f53;&#x5e74;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;B.TKT_QTY_Y, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0.00&#xd;&#xa;                 ELSE&#xd;&#xa;                  B.BIZ_QTY_Y &#x2f; B.TKT_QTY_Y&#xd;&#xa;               END AS CUR_Y_RATE --&#x5f53;&#x5e74;&#x5b8c;&#x6210;&#x7387;              &#xd;&#xa;              ,0 AS LST_D_QP_QTY --&#x4e0a;&#x65e5;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;              ,0 AS CUR_D_DUE_OUT_QTY --&#x5f53;&#x65e5;&#x5f85;&#x53d1;&#x91cf;  &#xd;&#xa;              ,0 AS LST_D_DLV_QTY --&#x4e0a;&#x65e5;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;              ,0 AS CUR_M_QP_QTY --&#x5f53;&#x6708;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;              ,0 AS CUR_D_ON_ROAD_QTY --&#x5f53;&#x65e5;&#x5728;&#x9014;&#x91cf;&#xd;&#xa;              ,0 AS CUR_M_DLV_QTY --&#x5f53;&#x6708;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;              ,0 AS CUR_Y_QP_QTY --&#x5f53;&#x5e74;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;              ,0 AS CUR_D_ON_ROAD_OVT_QTY --&#x5f53;&#x65e5;&#x5728;&#x9014;&#x8d85;&#x65f6;&#x91cf;&#xd;&#xa;              ,0 AS CUR_Y_DLV_QTY --&#x5f53;&#x5e74;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;              ,0 AS CUR_D_QP_FNSH_RATE --&#x5f53;&#x65e5;&#x542f;&#x7968;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;              ,0 AS CUR_D_ON_ROAD_OVT_RATE --&#x5f53;&#x65e5;&#x5728;&#x9014;&#x8d85;&#x65f6;&#x7387;&#xd;&#xa;              ,0 AS CUR_D_ONTIME_ARRIVE_RATE --&#x5f53;&#x65e5;&#x51c6;&#x65f6;&#x5230;&#x8fbe;&#x7387; &#xd;&#xa;          FROM &#x28;SELECT B1.PERIOD_WID&#xd;&#xa;                      ,B1.RGN_ID&#xd;&#xa;                      ,B1.BASE_ID&#xd;&#xa;                      ,B1.BRAND_ID&#xd;&#xa;                      ,B1.BIZ_QTY AS BIZ_QTY&#xd;&#xa;                      ,B1.TKT_QTY AS TKT_QTY&#xd;&#xa;                      ,SUM&#x28;B1.BIZ_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;B1.PERIOD_WID, 1, 6&#x29;, B1.RGN_ID, B1.BASE_ID, B1.BRAND_ID ORDER BY B1.PERIOD_WID&#x29; AS BIZ_QTY_M&#xd;&#xa;                      ,SUM&#x28;B1.TKT_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;B1.PERIOD_WID, 1, 6&#x29;, B1.RGN_ID, B1.BASE_ID, B1.BRAND_ID&#x29; AS TKT_QTY_M&#xd;&#xa;                      ,SUM&#x28;B1.BIZ_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;B1.PERIOD_WID, 1, 4&#x29;, B1.RGN_ID, B1.BASE_ID, B1.BRAND_ID ORDER BY B1.PERIOD_WID&#x29; AS BIZ_QTY_Y&#xd;&#xa;                      ,SUM&#x28;B1.TKT_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;B1.PERIOD_WID, 1, 4&#x29;, B1.RGN_ID, B1.BASE_ID, B1.BRAND_ID&#x29; AS TKT_QTY_Y&#xd;&#xa;                      ,C.BIZ_QTY AS BIZ_QTY_SQ&#xd;&#xa;                  FROM &#x28;SELECT A.PERIOD_WID&#xd;&#xa;                              ,NVL&#x28;NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                              ,NVL&#x28;NVL&#x28;A.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                              ,NVL&#x28;NVL&#x28;A.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                              ,SUM&#x28;A.BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;                              ,SUM&#x28;A.TKT_QTY&#x29; AS TKT_QTY&#xd;&#xa;                          FROM &#x28;SELECT P.PERIOD_WID AS PERIOD_WID&#xd;&#xa;                                      ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                      ,COALESCE&#x28;T2.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                      ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                      ,0 AS BIZ_QTY&#xd;&#xa;                                      ,SUM&#x28;T.BIZ_QTY&#x29; &#x2f; MAX&#x28;P.CNT&#x29; AS TKT_QTY&#xd;&#xa;                                  FROM &#x28;SELECT PERIOD_WID&#xd;&#xa;                                              ,COUNT&#x28;1&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;PERIOD_WID, 1, 6&#x29;&#x29; CNT&#xd;&#xa;                                          FROM DIM.DIM_PERIOD_D&#xd;&#xa;                                         WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#x29; P&#xd;&#xa;                                  LEFT JOIN DMK.DMK_TP_BIZQTYTGT_M T&#xd;&#xa;                                    ON SUBSTR&#x28;P.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                                       T.PERIOD_WID&#xd;&#xa;                                  LEFT JOIN DIM.DIM_ORG T2&#xd;&#xa;                                    ON T.BASE_NAME &#x3d; T2.BASE_NAME&#xd;&#xa;                                   AND T2.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                                 WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                                       SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;&#xd;&#xa;                                   AND T.PERIOD_WID &#x3c;&#x3d;&#xd;&#xa;                                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                             &#x27;-&#x27;,&#xd;&#xa;                                                             &#x27;&#x27;&#x29;,&#xd;&#xa;                                              1,&#xd;&#xa;                                              6&#x29;&#xd;&#xa;                                   AND T2.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;                                   AND T.RGN_ID NOT IN &#x28;&#x27;BOYU&#x27;, &#x27;CENTER&#x27;&#x29;&#xd;&#xa;                                 GROUP BY P.PERIOD_WID&#xd;&#xa;                                         ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                         ,COALESCE&#x28;T2.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                         ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                UNION ALL&#xd;&#xa;                                SELECT T.PERIOD_WID AS PERIOD_WID&#xd;&#xa;                                      ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                      ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                      ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                      ,SUM&#x28;SHIPPING_QTY&#x29; AS BIZ_QTY&#xd;&#xa;                                      ,0 AS TKT_QTY&#xd;&#xa;                                  FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                                  LEFT JOIN DIM.DIM_ORG T1&#xd;&#xa;                                    ON T.BASE_ID &#x3d; T1.BASE_ID&#xd;&#xa;                                   AND T1.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                                 WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3d;&#xd;&#xa;                                       SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 4&#x29;&#xd;&#xa;                                   AND SUBSTR&#x28;T.PERIOD_WID,1,6&#x29; &#x3c;&#x3d;&#xd;&#xa;                                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                             &#x27;-&#x27;,&#xd;&#xa;                                                             &#x27;&#x27;&#x29;,&#xd;&#xa;                                              1,&#xd;&#xa;                                              6&#x29;&#xd;&#xa;                                   AND SUBSTR&#x28;T.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;                                       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#xd;&#xa;                                                     &#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;                                   AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                                   AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;                                   AND T1.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;                                 GROUP BY T.PERIOD_WID&#xd;&#xa;                                         ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                         ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                         ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;&#x29; A&#xd;&#xa;                         WHERE 1 &#x3d; 1&#xd;&#xa;                         GROUP BY A.PERIOD_WID&#xd;&#xa;                                 ,NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                 ,NVL&#x28;A.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                 ,NVL&#x28;A.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; B1&#xd;&#xa;                  LEFT JOIN &#x28;SELECT T.PERIOD_WID AS PERIOD_WID&#xd;&#xa;                                  ,NVL&#x28;COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                  ,NVL&#x28;COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                  ,NVL&#x28;COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                  ,SUM&#x28;SHIPPING_QTY&#x29; AS BIZ_QTY&#xd;&#xa;                              FROM DMK.DMK_TP_SHIPQTY_D T&#xd;&#xa;                              LEFT JOIN DIM.DIM_ORG T1&#xd;&#xa;                                ON T.BASE_ID &#x3d; T1.BASE_ID&#xd;&#xa;                               AND T1.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                             WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                                   SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                    -1&#x29;,&#xd;&#xa;                                                         &#x27;-&#x27;,&#xd;&#xa;                                                         &#x27;&#x27;&#x29;,&#xd;&#xa;                                          1,&#xd;&#xa;                                          6&#x29;&#xd;&#xa;                               AND T.TPMODE_ID NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                               AND T.BRAND_ID IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;                               AND T1.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;                             GROUP BY T.PERIOD_WID&#xd;&#xa;                                     ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                     ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                     ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; C&#xd;&#xa;                    ON B1.PERIOD_WID &#x3d; C.PERIOD_WID&#xd;&#xa;                   AND B1.RGN_ID &#x3d; C.RGN_ID&#xd;&#xa;                   AND B1.BASE_ID &#x3d; C.BASE_ID&#xd;&#xa;                   AND B1.BRAND_ID &#x3d; C.BRAND_ID&#xd;&#xa;                 WHERE B1.PERIOD_WID IS NOT NULL&#x29; AS B&#xd;&#xa;         WHERE SUBSTR&#x28;B.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;           AND NVL&#x28;B.PERIOD_WID, &#x27;-1&#x27;&#x29; NOT IN &#x28;&#x27;-1&#x27;&#x29;&#xd;&#xa;           AND B.PERIOD_WID &#x3c;&#x3e; &#x27;&#x27;&#x29; AS A1&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; A2&#xd;&#xa;    ON A1.RGN_ID &#x3d; A2.RGN_ID&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG A3&#xd;&#xa;    ON A1.BASE_ID &#x3d; A3.BASE_ID&#xd;&#xa;   AND A3.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;   AND A3.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_BRAND A4&#xd;&#xa;    ON A4.BRAND_ID &#x3d; A1.BRAND_ID&#xd;&#xa;   AND A4.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa; WHERE NVL&#x28;A1.PERIOD_WID, &#x27;ALL&#x27;&#x29; NOT IN &#x28;&#x27;ALL&#x27;&#x29;&#xd;&#xa;   AND SUBSTR&#x28;A1.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa; GROUP BY A1.PERIOD_WID&#xd;&#xa;         ,A1.RGN_ID&#xd;&#xa;         ,A1.BASE_ID&#xd;&#xa;         ,A1.BRAND_ID&#x3b;&#xd;&#xa;</sql>
