    <entry>
      <name>TO_DETAIL</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_DETAIL&#xd;&#xa;SELECT coalesce&#x28;A.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;A.BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;A.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;A.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,coalesce&#x28;A.TPMODE_ID,&#x27;ALL&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;    ,sum&#x28;A.CUR_TP_INCOME&#x29; AS CUR_TP_INCOME&#xd;&#xa;    ,sum&#x28;A.LST_TP_INCOME&#x29; AS LST_TP_INCOME&#xd;&#xa;    ,sum&#x28;A.CUR_TP_COST&#x29; AS CUR_TP_COST&#xd;&#xa;    ,sum&#x28;A.LST_TP_COST&#x29; AS LST_TP_COST&#xd;&#xa;    ,sum&#x28;A.CUR_TP_BIZ&#x29; AS CUR_TP_BIZ&#xd;&#xa;    ,sum&#x28;A.LST_TP_BIZ&#x29; AS LST_TP_BIZ&#xd;&#xa;    ,sum&#x28;A.CUR_TP_MILEGE&#x29; AS CUR_TP_MILEGE&#xd;&#xa;    ,sum&#x28;A.LST_TP_MILEGE&#x29; AS LST_TP_MILEGE&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT T.RGN_ID&#xd;&#xa;        ,T.BASE_ID&#xd;&#xa;        ,T.BRAND_ID&#xd;&#xa;        ,T.PROJ_ID&#xd;&#xa;        ,T.TPMODE_ID&#xd;&#xa;        ,sum&#x28;T.CUR_TP_INCOME&#x29; AS CUR_TP_INCOME&#xd;&#xa;        ,sum&#x28;T.LST_TP_INCOME&#x29; AS LST_TP_INCOME&#xd;&#xa;        ,sum&#x28;T.CUR_TP_COST&#x29; AS CUR_TP_COST&#xd;&#xa;        ,sum&#x28;T.LST_TP_COST&#x29; AS LST_TP_COST&#xd;&#xa;        ,sum&#x28;T.CUR_TP_BIZ&#x29; AS CUR_TP_BIZ&#xd;&#xa;        ,sum&#x28;T.LST_TP_BIZ&#x29; AS LST_TP_BIZ&#xd;&#xa;        ,sum&#x28;T.CUR_TP_MILEGE&#x29; AS CUR_TP_MILEGE&#xd;&#xa;        ,sum&#x28;T.LST_TP_MILEGE&#x29; AS LST_TP_MILEGE&#xd;&#xa;    FROM&#x28;&#xd;&#xa;--&#x8ba1;&#x7b97;&#x5404;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x8fd0;&#x8f93;&#x8d39;&#x7528;&#xff0c;&#x6210;&#x672c;&#xd;&#xa;        SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;            ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;            ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;            ,sum&#x28;CASE WHEN B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN income_amt ELSE 0 END&#x29; AS CUR_TP_INCOME&#xd;&#xa;            ,sum&#x28;CASE WHEN B.fin_in_month&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN income_amt ELSE 0 END&#x29; AS LST_TP_INCOME&#xd;&#xa;            ,sum&#x28;CASE WHEN B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN cost_amt ELSE 0 END&#x29; AS CUR_TP_COST&#xd;&#xa;            ,sum&#x28;CASE WHEN B.fin_in_month&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN cost_amt ELSE 0 END&#x29; AS LST_TP_COST&#xd;&#xa;            ,0 AS CUR_TP_BIZ&#xd;&#xa;            ,0 AS LST_TP_BIZ&#xd;&#xa;            ,0 AS CUR_TP_MILEGE&#xd;&#xa;            ,0 AS LST_TP_MILEGE&#xd;&#xa;        FROM DMK.DMK_FIN_TPBUDGET_D A&#xd;&#xa;        JOIN DIM.DIM_PERIOD_D B&#xd;&#xa;        ON A.PERIOD_WID &#x3d; B.PERIOD_WID AND B.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;        WHERE &#x28;B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;		    OR B.fin_in_month&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;			&#x29;&#xd;&#xa;            AND transportmode_id &#x3c;&#x3e; &#x27;ZT&#x27;&#xd;&#xa;        GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        UNION ALL&#xd;&#xa;--&#x8ba1;&#x7b97;&#x5404;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x4e1a;&#x52a1;&#x91cf;&#x548c;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#xd;&#xa;        SELECT coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;            ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;            ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;            ,0 AS CUR_TP_INCOME&#xd;&#xa;            ,0 AS LST_TP_INCOME&#xd;&#xa;            ,0 AS CUR_TP_COST&#xd;&#xa;            ,0 AS LST_TP_COST&#xd;&#xa;            ,sum&#x28;CASE WHEN B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN shipping_num ELSE 0 END&#x29; AS CUR_TP_BIZ&#xd;&#xa;            ,sum&#x28;CASE WHEN B.fin_in_month&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN shipping_num ELSE 0 END&#x29; AS LST_TP_BIZ&#xd;&#xa;            ,sum&#x28;CASE WHEN B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN milege&#x2a;A.shipping_num ELSE 0 END&#x29; AS CUR_TP_MILEGE&#xd;&#xa;            ,sum&#x28;CASE WHEN B.fin_in_month&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN milege&#x2a;A.shipping_num ELSE 0 END&#x29; AS LST_TP_MILEGE&#xd;&#xa;        FROM DMK.DMK_TP_OPERATE_D A&#xd;&#xa;        JOIN DIM.DIM_PERIOD_D B&#xd;&#xa;        ON A.PERIOD_WID &#x3d; B.PERIOD_WID AND B.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;        WHERE &#x28;B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;		    OR B.fin_in_month&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;			&#x29;&#xd;&#xa;            AND transportmode_id &#x3c;&#x3e; &#x27;ZT&#x27;&#xd;&#xa;        GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        &#x29; T&#xd;&#xa;    GROUP BY T.RGN_ID,T.BASE_ID,T.BRAND_ID,T.PROJ_ID,T.TPMODE_ID&#xd;&#xa;&#x29; A&#xd;&#xa;GROUP BY A.RGN_ID,A.BASE_ID,A.BRAND_ID,A.PROJ_ID,A.TPMODE_ID WITH CUBE</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
