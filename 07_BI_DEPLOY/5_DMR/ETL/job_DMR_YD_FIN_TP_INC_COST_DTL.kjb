      <name>TO_FIN_ALL</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_FIN&#xd;&#xa;SELECT coalesce&#x28;T.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;T.BASE_ID ,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;T.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;T.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;    ,SUM&#x28;CUR_ALL_TP_INCOME&#x29; AS CUR_ALL_TP_INCOME&#xd;&#xa;    ,SUM&#x28;LST_ALL_TP_INCOME&#x29; AS LST_ALL_TP_INCOME&#xd;&#xa;    ,SUM&#x28;CUR_ALL_TP_COST&#x29; AS CUR_ALL_TP_COST&#xd;&#xa;    ,SUM&#x28;LST_ALL_TP_COST&#x29; AS LST_ALL_TP_COST&#xd;&#xa;FROM&#x28;&#xd;&#xa;--&#x8ba1;&#x7b97;&#x6240;&#x6709;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x7684;&#x603b;&#x8d39;&#x7528;&#xd;&#xa;    SELECT coalesce&#x28;A.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;A.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;A.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;A.proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,sum&#x28;CASE WHEN B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN income_amt ELSE 0 END&#x29; AS CUR_ALL_TP_INCOME	&#xd;&#xa;        ,sum&#x28;CASE WHEN B.fin_in_month&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN income_amt ELSE 0 END&#x29; AS LST_ALL_TP_INCOME	&#xd;&#xa;        ,sum&#x28;CASE WHEN B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN cost_amt ELSE 0 END&#x29; AS CUR_ALL_TP_COST	&#xd;&#xa;        ,sum&#x28;CASE WHEN B.fin_in_month&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN cost_amt ELSE 0 END&#x29; AS LST_ALL_TP_COST	&#xd;&#xa;    FROM DMK.DMK_FIN_TPBUDGET_D A&#xd;&#xa;	JOIN DIM.DIM_PERIOD_D B&#xd;&#xa;	ON A.PERIOD_WID &#x3d; B.PERIOD_WID AND B.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;    WHERE &#x28;B.fin_in_month &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;		OR B.fin_in_month&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;			&#x29;&#xd;&#xa;        AND transportmode_id &#x3c;&#x3e; &#x27;ZT&#x27;&#xd;&#xa;    GROUP BY rgn_id,base_id,brand_id,proj_id &#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.RGN_ID,T.BASE_ID,T.BRAND_ID,T.PROJ_ID WITH CUBE&#xd;&#xa;&#x3b;</sql>
