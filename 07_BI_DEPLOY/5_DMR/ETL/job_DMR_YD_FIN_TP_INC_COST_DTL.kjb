      <type>SQL</type>
      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;--Dont need to re-create temporary table unless an update of table structure is asked&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_DETAIL&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_DETAIL&#xd;&#xa;&#x28;&#xd;&#xa;    RGN_ID                       STRING                  COMMENT &#x27;&#x533a;&#x57df;ID&#x27;,&#xd;&#xa;    BASE_ID                      STRING                  COMMENT &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;    BRAND_ID                     STRING                  COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;    PROJ_ID                      STRING                  COMMENT &#x27;&#x9879;&#x76ee;ID&#x27;,&#xd;&#xa;    TPMODE_ID		             STRING                  COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;,&#xd;&#xa;    CUR_TP_INCOME	             DECIMAL&#x28;32,8&#x29;           COMMENT &#x27;&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x6536;&#x5165;&#x27;,&#xd;&#xa;    LST_TP_INCOME	             DECIMAL&#x28;32,8&#x29;           COMMENT &#x27;&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x6536;&#x5165;&#x27;,&#xd;&#xa;    CUR_TP_COST		             DECIMAL&#x28;32,8&#x29;           COMMENT &#x27;&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x6210;&#x672c;&#x27;,&#xd;&#xa;    LST_TP_COST		             DECIMAL&#x28;32,8&#x29;           COMMENT &#x27;&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x6210;&#x672c;&#x27;,&#xd;&#xa;    CUR_TP_BIZ		             BIGINT		             COMMENT &#x27;&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;    LST_TP_BIZ		             BIGINT		             COMMENT &#x27;&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;    CUR_TP_MILEGE		         DECIMAL&#x28;32,8&#x29;		     COMMENT &#x27;&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#x27;,&#xd;&#xa;    LST_TP_MILEGE		         DECIMAL&#x28;32,8&#x29;		     COMMENT &#x27;&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;COMMENT &#x27;&#x6536;&#x5165;&#x3001;&#x6210;&#x672c;&#x3001;&#x91cc;&#x7a0b;&#xff0c;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27; STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_FIN&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_FIN&#xd;&#xa;&#x28;&#xd;&#xa;    RGN_ID                       STRING                  COMMENT &#x27;&#x533a;&#x57df;ID&#x27;,&#xd;&#xa;    BASE_ID                      STRING                  COMMENT &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;    BRAND_ID                     STRING                  COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;    PROJ_ID                      STRING                  COMMENT &#x27;&#x9879;&#x76ee;ID&#x27;,&#xd;&#xa;    CUR_ALL_TP_INCOME	         DECIMAL&#x28;32,8&#x29;           COMMENT &#x27;&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x603b;&#x6536;&#x5165;&#x27;,&#xd;&#xa;    LST_ALL_TP_INCOME	         DECIMAL&#x28;32,8&#x29;           COMMENT &#x27;&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x603b;&#x6536;&#x5165;&#x27;,&#xd;&#xa;    CUR_ALL_TP_COST	         	 DECIMAL&#x28;32,8&#x29;           COMMENT &#x27;&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x603b;&#x6210;&#x672c;&#x27;,&#xd;&#xa;    LST_ALL_TP_COST	         	 DECIMAL&#x28;32,8&#x29;           COMMENT &#x27;&#x4e0a;&#x6708;&#x8fd0;&#x8f93;&#x603b;&#x6210;&#x672c;&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;COMMENT &#x27;&#x6536;&#x5165;&#x3001;&#x6210;&#x672c;&#x3001;&#x91cc;&#x7a0b;&#xff0c;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27; STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
