    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_FIN_TP_INC_COST_DTL PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID  --&#x6708;&#x4efd;ID&#xd;&#xa;    ,T.rgn_id  --&#x5927;&#x533a;ID&#xd;&#xa;    ,CASE WHEN T.rgn_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.rgn_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.rgn_name END AS RNG_NAME  --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;    ,T.base_id  --&#x57fa;&#x5730;ID&#xd;&#xa;    ,CASE WHEN T.base_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.base_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.base_name END AS BASE_NAME  --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;    ,T.brand_id  --&#x54c1;&#x724c;ID&#xd;&#xa;    ,CASE WHEN T.brand_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.brand_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.brand_name END AS BRAND_NAME  --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;    ,&#x27;TP&#x27; AS BIZ_TYPE_ID  --&#x4e1a;&#x52a1;&#x5c5e;&#x6027;ID&#xd;&#xa;    ,&#x27;&#x8fd0;&#x8f93;&#x27; AS BIZ_TYPE_NAME  --&#x4e1a;&#x52a1;&#x5c5e;&#x6027;&#x540d;&#x79f0;&#xd;&#xa;    ,&#x27;-1&#x27; AS SROUCE_ID  --&#x6765;&#x6e90;ID&#xd;&#xa;    ,&#x27;-1&#x27; AS SROUCE_NAME    --&#x6765;&#x6e90;&#x540d;&#x79f0;&#xd;&#xa;    ,T.tpmode_id AS TP_TYPE_ID  --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;    ,CASE WHEN T.tpmode_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x4f53;&#x27;&#xd;&#xa;        WHEN T.tpmode_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T5.tpmode_name END AS TP_TYPE_NAME   --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x540d;&#x79f0;&#xd;&#xa;    ,T.proj_id  --&#x9879;&#x76ee;ID&#xd;&#xa;    ,CASE WHEN T.proj_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x4f53;&#x27;&#xd;&#xa;        WHEN T.proj_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T4.proj_name END AS PROJ_NAME  --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;    ,sum&#x28;T.cur_tp_income&#x29;&#x2f;sum&#x28;T.cur_tp_biz&#x29; AS CUR_M_AVG_INCOME --&#x5f53;&#x6708;&#x5355;&#x8f66;&#x6536;&#x5165;&#xd;&#xa;    ,sum&#x28;T.lst_tp_income&#x29;&#x2f;sum&#x28;T.lst_tp_biz&#x29; AS LST_M_AVG_INCOME --&#x4e0a;&#x6708;&#x5355;&#x8f66;&#x6536;&#x5165;&#xd;&#xa;    ,sum&#x28;T.cur_tp_cost&#x29;&#x2f;sum&#x28;T.cur_tp_biz&#x29; AS CUR_M_AVG_COST    --&#x5f53;&#x6708;&#x5355;&#x8f66;&#x6210;&#x672c;&#xd;&#xa;    ,sum&#x28;T.lst_tp_cost&#x29;&#x2f;sum&#x28;T.lst_tp_biz&#x29; AS LST_M_AVG_COST    --&#x4e0a;&#x6708;&#x5355;&#x8f66;&#x6210;&#x672c;&#xd;&#xa;    ,sum&#x28;T.cur_tp_income&#x29;&#x2f;sum&#x28;T.cur_all_tp_income&#x29;&#x2a;100 AS CUR_M_INCOME_TYPE_RATE    --&#x5f53;&#x6708;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x7c7b;&#x578b;&#x5360;&#x6bd4;&#xd;&#xa;    ,sum&#x28;T.lst_tp_income&#x29;&#x2f;sum&#x28;T.lst_all_tp_income&#x29;&#x2a;100 AS LST_M_INCOME_TYPE_RATE    --&#x4e0a;&#x6708;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x7c7b;&#x578b;&#x5360;&#x6bd4;&#xd;&#xa;    ,sum&#x28;T.cur_tp_cost&#x29;&#x2f;sum&#x28;T.cur_all_tp_cost&#x29;&#x2a;100 AS CUR_M_COST_TYPE_RATE    --&#x5f53;&#x6708;&#x6210;&#x672c;&#x8fd0;&#x8f93;&#x7c7b;&#x578b;&#x5360;&#x6bd4;&#xd;&#xa;    ,sum&#x28;T.lst_tp_cost&#x29;&#x2f;sum&#x28;T.lst_all_tp_cost&#x29;&#x2a;100 AS LST_M_COST_TYPE_RATE    --&#x4e0a;&#x6708;&#x6210;&#x672c;&#x8fd0;&#x8f93;&#x7c7b;&#x578b;&#x5360;&#x6bd4;&#xd;&#xa;    ,sum&#x28;T.cur_tp_milege&#x29;&#x2f;sum&#x28;T.cur_tp_biz&#x29; AS CUR_M_INCOME_MILEAGE   --&#x5f53;&#x6708;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#xd;&#xa;    ,sum&#x28;T.lst_tp_milege&#x29;&#x2f;sum&#x28;T.lst_tp_biz&#x29; AS LST_M_INCOME_MILEAGE   --&#x4e0a;&#x6708;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#xd;&#xa;    ,sum&#x28;T.cur_tp_milege&#x29;&#x2f;sum&#x28;T.cur_tp_biz&#x29; AS CUR_M_COST_MILEAGE   --&#x5f53;&#x6708;&#x6210;&#x672c;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#xd;&#xa;    ,sum&#x28;T.lst_tp_milege&#x29;&#x2f;sum&#x28;T.lst_tp_biz&#x29; AS LST_M_COST_MILEAGE   --&#x4e0a;&#x6708;&#x6210;&#x672c;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#xd;&#xa;    ,&#x28;&#x28;sum&#x28;T.cur_tp_income&#x29;&#x2f;sum&#x28;T.cur_tp_biz&#x29;-sum&#x28;T.lst_tp_income&#x29;&#x2f;sum&#x28;T.lst_tp_biz&#x29;&#x29;&#x2f;&#x28;sum&#x28;T.lst_tp_income&#x29;&#x2f;sum&#x28;T.lst_tp_biz&#x29;&#x29;&#x29;&#x2a;100 AS INCOME_M_MOM   --&#x5355;&#x8f66;&#x6536;&#x5165;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;&#x28;sum&#x28;T.cur_tp_cost&#x29;&#x2f;sum&#x28;T.cur_tp_biz&#x29;-sum&#x28;T.lst_tp_cost&#x29;&#x2f;sum&#x28;T.lst_tp_biz&#x29;&#x29;&#x2f;&#x28;sum&#x28;T.lst_tp_cost&#x29;&#x2f;sum&#x28;T.lst_tp_biz&#x29;&#x29;&#x29;&#x2a;100 AS COST_M_MOM   --&#x6210;&#x672c;&#x6536;&#x5165;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;sum&#x28;T.cur_tp_income&#x29;&#x2f;sum&#x28;T.cur_all_tp_income&#x29;-sum&#x28;T.lst_tp_income&#x29;&#x2f;sum&#x28;T.lst_all_tp_income&#x29;&#x29;&#x2f;&#x28;sum&#x28;T.lst_tp_income&#x29;&#x2f;sum&#x28;T.lst_all_tp_income&#x29;&#x29;&#x2a;100 AS INCOME_TYPE_M_MOM   --&#x5355;&#x8f66;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x5360;&#x6bd4;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;sum&#x28;T.cur_tp_cost&#x29;&#x2f;sum&#x28;T.cur_all_tp_cost&#x29;-sum&#x28;T.lst_tp_cost&#x29;&#x2f;sum&#x28;T.lst_all_tp_cost&#x29;&#x29;&#x2f;&#x28;sum&#x28;T.lst_tp_cost&#x29;&#x2f;sum&#x28;T.lst_all_tp_cost&#x29;&#x29;&#x2a;100 AS COST_TYPE_M_MOM   --&#x6210;&#x672c;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x5360;&#x6bd4;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;sum&#x28;cur_tp_milege&#x29;-sum&#x28;lst_tp_milege&#x29;&#x29;&#x2f;sum&#x28;lst_tp_milege&#x29;&#x2a;100 AS INCOME_MILEAGE_M_MOM --&#x5355;&#x8f66;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;sum&#x28;cur_tp_milege&#x29;-sum&#x28;lst_tp_milege&#x29;&#x29;&#x2f;sum&#x28;lst_tp_milege&#x29;&#x2a;100 AS COST_MILEAGE_M_MOM --&#x6210;&#x672c;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#x73af;&#x6bd4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT A.rgn_id&#xd;&#xa;        ,A.base_id&#xd;&#xa;        ,A.brand_id&#xd;&#xa;        ,A.proj_id&#xd;&#xa;        ,A.tpmode_id&#xd;&#xa;        ,B.cur_all_tp_income&#xd;&#xa;        ,B.lst_all_tp_income&#xd;&#xa;        ,B.cur_all_tp_cost&#xd;&#xa;        ,B.lst_all_tp_cost&#xd;&#xa;        ,A.cur_tp_income&#xd;&#xa;        ,A.lst_tp_income&#xd;&#xa;        ,A.cur_tp_cost&#xd;&#xa;        ,A.lst_tp_cost&#xd;&#xa;        ,A.cur_tp_biz&#xd;&#xa;        ,A.lst_tp_biz&#xd;&#xa;        ,A.cur_tp_milege&#xd;&#xa;        ,A.lst_tp_milege&#xd;&#xa;    FROM TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_DETAIL A  &#xd;&#xa;    LEFT JOIN TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_FIN B  &#xd;&#xa;    ON A.rgn_id &#x3d; B.rgn_id AND A.base_id &#x3d; B.base_id AND A.brand_id &#x3d; B.brand_id AND A.proj_id &#x3d; B.proj_id&#xd;&#xa;    &#x29; T&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT base_id,base_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY base_id,base_name&#xd;&#xa;    &#x29; T2&#xd;&#xa;ON T.base_id &#x3d; T2.base_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.dim_brand &#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29; T3&#xd;&#xa;ON T.brand_id &#x3d; T3.brand_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT proj_id,proj_name&#xd;&#xa;    FROM DIM.dim_proj &#xd;&#xa;    GROUP BY proj_id,proj_name&#xd;&#xa;    &#x29; T4&#xd;&#xa;ON T.proj_id &#x3d; T4.proj_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT tpmode_id,tpmode_name&#xd;&#xa;    FROM DIM.dim_transport_mode &#xd;&#xa;    GROUP BY tpmode_id,tpmode_name&#xd;&#xa;    &#x29; T5&#xd;&#xa;ON T.tpmode_id &#x3d; T5.tpmode_id&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;GROUP BY T.rgn_id,T1.rgn_name,T.base_id,T2.base_name,T.brand_id,T3.brand_name,T.proj_id,T4.proj_name,T.tpmode_id,T5.tpmode_name&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT&#xd;&#xa;     REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS MONTH_ID                 --&#x6708;&#x4efd;ID&#xd;&#xa;    ,T.RGN_ID                                                 AS RNG_ID                   --&#x5927;&#x533a;ID&#xd;&#xa;    ,RGN.RGN_NAME                                             AS RNG_NAME                 --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;    ,T.BASE_ID                                                AS BASE_ID                  --&#x57fa;&#x5730;ID&#xd;&#xa;    ,BASE.BASE_NAME                                           AS BASE_NAME                --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;    ,T.BRAND_ID                                               AS BRAND_ID                 --&#x54c1;&#x724c;ID&#xd;&#xa;    ,BRAND.BRAND_NAME                                         AS BRAND_NAME               --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;    ,BIZTYPE.BIZTYPE_ID                                       AS BIZ_TYPE_ID              --&#x4e1a;&#x52a1;&#x5c5e;&#x6027;ID&#xd;&#xa;    ,BIZTYPE.BIZTYPE_NAME                                     AS BIZ_TYPE_NAME            --&#x4e1a;&#x52a1;&#x5c5e;&#x6027;&#x540d;&#x79f0;&#xd;&#xa;    ,&#x27;-1&#x27;                                                     AS SROUCE_ID                --&#x6765;&#x6e90;ID&#xd;&#xa;    ,&#x27;-1&#x27;                                                     AS SROUCE_NAME              --&#x6765;&#x6e90;&#x540d;&#x79f0;&#xd;&#xa;    ,T.TRANSPORTMODE_ID                                       AS TP_TYPE_ID               --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;    ,TP.TPMODE_NAME                                           AS TP_TYPE_NAME             --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x540d;&#x79f0;&#xd;&#xa;    ,T.PROJ_ID                                                AS PROJ_ID                  --&#x9879;&#x76ee;ID&#xd;&#xa;    ,PROJ.PROJ_NAME                                           AS PROJ_NAME                --&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;    --&#x5355;&#x8f66;&#x8fd0;&#x8f93;&#x6536;&#x5165;	&#x8d22;&#x52a1;&#x7ba1;&#x7406;&#x79d1;	&#x901a;&#x8fc7;&#x5173;&#x8054;&#x6307;&#x6807;&#x8ba1;&#x7b97;	&#x5355;&#x4e2a;&#x8f66;&#x8f86;&#x8fd0;&#x8f93;&#x8fc7;&#x7a0b;&#x4e2d;&#x4ea7;&#x751f;&#x7684;&#x603b;&#x6d41;&#x5165; &#x3d;&#x3e; &#x8fd0;&#x8f93;&#x6536;&#x5165;&#x2f; &#x603b;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;    --&#x5355;&#x8f66;&#x8fd0;&#x8f93;&#x6210;&#x672c;	&#x8d22;&#x52a1;&#x7ba1;&#x7406;&#x79d1;	OTM&#x7cfb;&#x7edf;	&#x6574;&#x8f66;&#x8fd0;&#x8f93;&#x8fc7;&#x7a0b;&#x4e2d;&#xff0c;&#x5355;&#x4e2a;&#x8f66;&#x8f86;&#x6240;&#x53d1;&#x751f;&#x7684;&#x6240;&#x6709;&#x8fd0;&#x8f93;&#x6210;&#x672c;&#x5408;&#x8ba1; &#x3d;&#x3e; &#x603b;&#x8fd0;&#x8f93;&#x6210;&#x672c;&#x2f;&#x603b;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;    ,T.CUR_M_INCOME_AMT &#x2f; T.CUR_M_BIZ_QTY                     AS CUR_M_AVG_INCOME         --&#x5f53;&#x6708;&#x5355;&#x8f66;&#x6536;&#x5165;&#xd;&#xa;    ,T.LST_M_INCOME_AMT &#x2f; T.LST_M_BIZ_QTY                     AS LST_M_AVG_INCOME         --&#x4e0a;&#x6708;&#x5355;&#x8f66;&#x6536;&#x5165;&#xd;&#xa;    ,T.CUR_M_COST_AMT &#x2f; T.CUR_M_BIZ_QTY                       AS CUR_M_AVG_COST           --&#x5f53;&#x6708;&#x5355;&#x8f66;&#x6210;&#x672c;&#xd;&#xa;    ,T.LST_M_COST_AMT &#x2f; T.LST_M_BIZ_QTY                       AS LST_M_AVG_COST           --&#x4e0a;&#x6708;&#x5355;&#x8f66;&#x6210;&#x672c;&#xd;&#xa;    ,&#x28;T.CUR_M_INCOME_AMT &#x2f; T1.CUR_M_INCOME_AMT&#x29;&#x2a;100                 AS CUR_M_INCOME_TYPE_RATE   --&#x5f53;&#x6708;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x7c7b;&#x578b;&#x5360;&#x6bd4;&#xd;&#xa;    ,&#x28;T.LST_M_INCOME_AMT &#x2f; T1.LST_M_INCOME_AMT&#x29;&#x2a;100                 AS LST_M_INCOME_TYPE_RATE   --&#x4e0a;&#x6708;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x7c7b;&#x578b;&#x5360;&#x6bd4;&#xd;&#xa;    ,&#x28;T.CUR_M_COST_AMT &#x2f; T1.CUR_M_COST_AMT&#x29;&#x2a;100                     AS CUR_M_COST_TYPE_RATE     --&#x5f53;&#x6708;&#x6210;&#x672c;&#x8fd0;&#x8f93;&#x7c7b;&#x578b;&#x5360;&#x6bd4;&#xd;&#xa;    ,&#x28;T.LST_M_COST_AMT &#x2f; T1.LST_M_COST_AMT&#x29;&#x2a;100                     AS LST_M_COST_TYPE_RATE     --&#x4e0a;&#x6708;&#x6210;&#x672c;&#x8fd0;&#x8f93;&#x7c7b;&#x578b;&#x5360;&#x6bd4;&#xd;&#xa;    ,T.CUR_M_MILEGE                                           AS CUR_M_INCOME_MILEAGE     --&#x5f53;&#x6708;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#xd;&#xa;    ,T.LST_M_MILEGE                                           AS LST_M_INCOME_MILEAGE     --&#x4e0a;&#x6708;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#xd;&#xa;    ,T.CUR_M_MILEGE                                           AS CUR_M_COST_MILEAGE       --&#x5f53;&#x6708;&#x6210;&#x672c;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#xd;&#xa;    ,T.LST_M_MILEGE                                           AS LST_M_COST_MILEAGE       --&#x4e0a;&#x6708;&#x6210;&#x672c;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#xd;&#xa;    ,&#x28;&#x28;T.CUR_M_INCOME_AMT &#x2f; T.CUR_M_BIZ_QTY&#x29; &#x2f; &#x28;T.LST_M_INCOME_AMT &#x2f; T.LST_M_BIZ_QTY&#x29;&#x29;&#x2a;100 AS INCOME_M_MOM             --&#x5355;&#x8f66;&#x6536;&#x5165;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;&#x28;T.CUR_M_COST_AMT &#x2f; T.CUR_M_BIZ_QTY&#x29; &#x2f; &#x28;T.LST_M_COST_AMT &#x2f; T.LST_M_BIZ_QTY&#x29;&#x29;&#x2a;100 AS COST_M_MOM               --&#x6210;&#x672c;&#x6536;&#x5165;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;&#x28;T.CUR_M_INCOME_AMT &#x2f; T1.CUR_M_INCOME_AMT&#x29; &#x2f; &#x28;T.LST_M_INCOME_AMT &#x2f; T1.LST_M_INCOME_AMT&#x29;&#x29;&#x2a;100 AS INCOME_TYPE_M_MOM        --&#x5355;&#x8f66;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x5360;&#x6bd4;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;&#x28;T.CUR_M_COST_AMT &#x2f; T1.CUR_M_COST_AMT&#x29; &#x2f; &#x28;T.LST_M_COST_AMT &#x2f; T1.LST_M_COST_AMT&#x29;&#x29;&#x2a;100 AS COST_TYPE_M_MOM          --&#x6210;&#x672c;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x5360;&#x6bd4;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;T.CUR_M_MILEGE &#x2f; T.LST_M_MILEGE&#x29;&#x2a;100                          AS INCOME_MILEAGE_M_MOM     --&#x5355;&#x8f66;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#x73af;&#x6bd4;&#xd;&#xa;    ,&#x28;T.CUR_M_MILEGE &#x2f; T.LST_M_MILEGE&#x29;&#x2a;100                          AS COST_MILEAGE_M_MOM       --&#x6210;&#x672c;&#x6536;&#x5165;&#x8fd0;&#x8f93;&#x91cc;&#x7a0b;&#x73af;&#x6bd4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27;                                                    AS PART_SC&#xd;&#xa;    ,REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;&#xd;&#xa;FROM&#xd;&#xa;&#x28;&#xd;&#xa;SELECT S.RGN_ID&#xd;&#xa;      ,S.BASE_ID&#xd;&#xa;      ,S.BRAND_ID&#xd;&#xa;      ,S.PROJ_ID&#xd;&#xa;      ,MAX&#x28;S.TRANSPORTMODE_ID&#x29; AS TRANSPORTMODE_ID&#xd;&#xa;      ,MAX&#x28;S.CUR_M_INCOME_AMT&#x29; AS CUR_M_INCOME_AMT&#xd;&#xa;      ,MAX&#x28;S.LST_M_INCOME_AMT&#x29; AS LST_M_INCOME_AMT&#xd;&#xa;      ,MAX&#x28;S.CUR_M_COST_AMT&#x29;   AS CUR_M_COST_AMT&#xd;&#xa;      ,MAX&#x28;S.LST_M_COST_AMT&#x29;   AS LST_M_COST_AMT&#xd;&#xa;      ,MAX&#x28;S.CUR_M_MILEGE&#x29;     AS CUR_M_MILEGE&#xd;&#xa;      ,MAX&#x28;S.LST_M_MILEGE&#x29;     AS LST_M_MILEGE&#xd;&#xa;      ,MAX&#x28;S.CUR_M_BIZ_QTY&#x29;    AS CUR_M_BIZ_QTY&#xd;&#xa;      ,MAX&#x28;S.LST_M_BIZ_QTY&#x29;    AS LST_M_BIZ_QTY&#xd;&#xa;      FROM &#xd;&#xa;      &#x28;&#xd;&#xa;      SELECT RGN_ID&#xd;&#xa;            ,BASE_ID&#xd;&#xa;            ,BRAND_ID&#xd;&#xa;            ,PROJ_ID&#xd;&#xa;            ,TRANSPORTMODE_ID&#xd;&#xa;            ,CUR_M_INCOME_AMT&#xd;&#xa;            ,LST_M_INCOME_AMT&#xd;&#xa;            ,CUR_M_COST_AMT&#xd;&#xa;            ,LST_M_COST_AMT&#xd;&#xa;            ,CUR_M_MILEGE&#xd;&#xa;            ,LST_M_MILEGE&#xd;&#xa;            ,NULL AS CUR_M_BIZ_QTY&#xd;&#xa;            ,NULL AS LST_M_BIZ_QTY&#xd;&#xa;        FROM TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_1&#xd;&#xa;        &#xd;&#xa;      UNION ALL&#xd;&#xa;      &#xd;&#xa;      SELECT RGN_ID&#xd;&#xa;            ,BASE_ID&#xd;&#xa;            ,BRAND_ID&#xd;&#xa;            ,PROJ_ID&#xd;&#xa;            ,NULL AS TRANSPORTMODE_ID&#xd;&#xa;            ,NULL AS CUR_M_INCOME_AMT&#xd;&#xa;            ,NULL AS LST_M_INCOME_AMT&#xd;&#xa;            ,NULL AS CUR_M_COST_AMT&#xd;&#xa;            ,NULL AS LST_M_COST_AMT&#xd;&#xa;            ,NULL AS CUR_M_MILEGE&#xd;&#xa;            ,NULL AS LST_M_MILEGE&#xd;&#xa;            ,CUR_M_BIZ_QTY&#xd;&#xa;            ,LST_M_BIZ_QTY&#xd;&#xa;       FROM TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_2&#xd;&#xa;       &#x29;S&#xd;&#xa;       GROUP BY S.RGN_ID&#xd;&#xa;               ,S.BASE_ID&#xd;&#xa;               ,S.BRAND_ID&#xd;&#xa;               ,S.PROJ_ID&#xd;&#xa;&#x29;T&#xd;&#xa;LEFT JOIN &#xd;&#xa;&#x28;&#xd;&#xa;SELECT RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,PROJ_ID&#xd;&#xa;      ,SUM&#x28;CUR_M_INCOME_AMT&#x29; AS CUR_M_INCOME_AMT&#xd;&#xa;      ,SUM&#x28;LST_M_INCOME_AMT&#x29; AS LST_M_INCOME_AMT&#xd;&#xa;      ,SUM&#x28;CUR_M_COST_AMT&#x29; AS CUR_M_COST_AMT&#xd;&#xa;      ,SUM&#x28;LST_M_COST_AMT&#x29; AS LST_M_COST_AMT&#xd;&#xa;      ,SUM&#x28;CUR_M_MILEGE&#x29; AS CUR_M_MILEGE&#xd;&#xa;      ,SUM&#x28;LST_M_MILEGE&#x29; AS LST_M_MILEGE&#xd;&#xa;  FROM TEMP.TEMP_DMR_YD_FIN_TP_INC_COST_DTL_1&#xd;&#xa; GROUP BY RGN_ID, BASE_ID, BRAND_ID, PROJ_ID&#xd;&#xa;&#x29;T1&#xd;&#xa;ON  T.RGN_ID   &#x3d; T1.RGN_ID&#xd;&#xa;AND T.BASE_ID  &#x3d; T1.BASE_ID&#xd;&#xa;AND T.BRAND_ID &#x3d; T1.BRAND_ID&#xd;&#xa;AND T.PROJ_ID  &#x3d; T1.PROJ_ID&#xd;&#xa;LEFT JOIN DIM.DIM_BIZTYPE BIZTYPE&#xd;&#xa;ON BIZTYPE.BIZTYPE_ID &#x3d; &#x27;TP&#x27;&#xd;&#xa;LEFT JOIN &#x28;SELECT RGN_ID, RGN_NAME FROM DIM.DIM_ORG WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27; GROUP BY RGN_ID, RGN_NAME&#x29; RGN&#xd;&#xa;ON T.RGN_ID   &#x3d; RGN.RGN_ID&#xd;&#xa;LEFT JOIN &#x28;SELECT BASE_ID, BASE_NAME FROM DIM.DIM_ORG WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27; GROUP BY BASE_ID, BASE_NAME&#x29; BASE&#xd;&#xa;ON T.BASE_ID  &#x3d; BASE.BASE_ID&#xd;&#xa;LEFT JOIN DIM.DIM_BRAND BRAND&#xd;&#xa;ON T.BRAND_ID &#x3d; BRAND.BRAND_ID&#xd;&#xa;AND BRAND.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;LEFT JOIN DIM.DIM_PROJ PROJ&#xd;&#xa;ON T.PROJ_ID &#x3d; PROJ.PROJ_ID&#xd;&#xa;AND PROJ.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;LEFT JOIN DIM.DIM_TRANSPORT_MODE TP&#xd;&#xa;ON T.TRANSPORTMODE_ID &#x3d; TP.TPMODE_ID&#xd;&#xa;AND TP.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#x3b;&#xd;&#xa;&#x2a;&#x2f;</sql>
