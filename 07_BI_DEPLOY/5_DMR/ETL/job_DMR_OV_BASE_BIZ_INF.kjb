    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_BASE_BIZ_INF PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;&#xd;&#xa;SELECT       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;     AS DATE_ID,          --&#x671f;&#x95f4;ID  8&#x4f4d;&#x65e5;&#x671f;  ETL&#x53c2;&#x6570;&#xd;&#xa;             T2.BASE_TYPE_IND                                   AS BASE_TYPE_IND,    --&#x57fa;&#x5730;&#x7c7b;&#x578b;&#x6807;&#x5fd7;&#xff08;1-&#x751f;&#x4ea7;&#x57fa;&#x5730;&#xff0c;2-&#x4e2d;&#x8f6c;&#x7ad9;&#x3001;3-&#x57ce;&#x5e02;&#x914d;&#x9001;&#x4e2d;&#x5fc3;&#xff09;&#xd;&#xa;             T2.BASE_NAME                                       AS ST_BASE_NAME,     --&#x59cb;&#x53d1;&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;             T2.CITY_NAME                                       AS ST_CITY_NAME,     --&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;&#xd;&#xa;             SUM&#x28;T1.CUR_M_SHIP_QTY&#x29;                             AS CUR_M_SHIP_QTY,         --&#x5f53;&#x6708;&#x53d1;&#x8fd0;&#x91cf;&#xd;&#xa;             SUM&#x28;T1.CUR_M_STOCK_IN_QTY&#x29;                         AS CUR_M_STOCK_IN_QTY,     --&#x5f53;&#x6708;&#x5165;&#x5e93;&#x91cf;&#xd;&#xa;             SUM&#x28;T1.IN_STOCK&#x29;                                   AS IN_STOCK,               --&#x5728;&#x5e93;&#x5b58;&#x5e93;&#xd;&#xa;             MAX&#x28;T1.W_UPDATE_DT&#x29;                                AS UPDATE_DT           -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;             ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;             ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;     AS PART_DT&#xd;&#xa;&#xd;&#xa;FROM       &#x28;SELECT      A2.CITY_ID                                      AS CITY_ID,             --&#x57fa;&#x5730;ID&#xd;&#xa;                        SUM&#x28;A1.shipping_qty&#x29;                            AS CUR_M_SHIP_QTY,      --&#x53d1;&#x8fd0;&#x91cf;&#x6b21;&#xd;&#xa;                        0                                               AS CUR_M_STOCK_IN_QTY,  --&#x5165;&#x5e93;&#x91cf;,&#xd;&#xa;                        0                                               AS IN_STOCK,            -- &#x5728;&#x5e93;&#x5b58;&#x5e93;&#xd;&#xa;                       MAX&#x28; A1.W_UPDATE_DT&#x29;                             AS W_UPDATE_DT&#xd;&#xa;            FROM        DMK.dmk_tp_shipqty_d A1  --&#x8fd0;&#x8f93;&#x8fd0;&#x8425;&#x91cf;&#x7edf;&#x8ba1;&#xd;&#xa;            LEFT JOIN   &#x28;SELECT     BASE_ID,&#xd;&#xa;                                    CITY_ID,&#xd;&#xa;                                    CITY_NAME&#xd;&#xa;                         FROM       DIM.DIM_ORG&#xd;&#xa;                         WHERE      ORG_TYPE&#x3d;&#x27;S&#x27;&#xd;&#xa;                           AND      RGN_NAME &#x3c;&#x3e; &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;                           AND      STR_TYPE NOT IN &#x28;&#x27;&#x5728;&#x9014;&#x5e93;&#x27;,&#x27;&#x865a;&#x62df;&#x5e93;&#x27;&#x29;&#xd;&#xa;                         GROUP  BY  BASE_ID,&#xd;&#xa;                                    CITY_ID,&#xd;&#xa;                                    CITY_NAME&#x29; A2&#xd;&#xa;                   ON   A1.BASE_ID &#x3d; A2.BASE_ID&#xd;&#xa;            WHERE       SUBSTR&#x28;A1.PERIOD_WID,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                AND A1.tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;                AND A1.brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;            GROUP BY    A2.CITY_ID&#xd;&#xa;&#xd;&#xa;            UNION ALL&#xd;&#xa;&#xd;&#xa;            SELECT      A2.CITY_ID                                       AS CITY_ID,             --&#x57fa;&#x5730;ID&#xd;&#xa;                        0                                                AS CUR_M_SHIP_QTY,      --&#x53d1;&#x8fd0;&#x91cf;&#x6b21;&#xd;&#xa;                        SUM&#x28;A1.instock_qty&#x29;                             AS CUR_M_STOCK_IN_QTY,  --&#x5165;&#x5e93;&#x91cf;&#xd;&#xa;                        0                                                AS IN_STOCK,            --&#x5728;&#x5e93;&#x5b58;&#x5e93;&#xd;&#xa;                        MAX&#x28;A1.W_UPDATE_DT&#x29;                              AS W_UPDATE_DT&#xd;&#xa;&#xd;&#xa;            FROM        DMK.dmk_str_operqty_d A1 --&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x91cf;&#x7edf;&#x8ba1;&#xd;&#xa;       LEFT JOIN      &#x28;SELECT     BASE_ID,&#xd;&#xa;                                    CITY_ID,&#xd;&#xa;                                    CITY_NAME&#xd;&#xa;                         FROM       DIM.DIM_ORG&#xd;&#xa;                         WHERE      ORG_TYPE&#x3d;&#x27;S&#x27;&#xd;&#xa;                           AND      RGN_NAME &#x3c;&#x3e; &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;                           AND      STR_TYPE NOT IN &#x28;&#x27;&#x5728;&#x9014;&#x5e93;&#x27;,&#x27;&#x865a;&#x62df;&#x5e93;&#x27;&#x29;&#xd;&#xa;                         GROUP  BY  BASE_ID,&#xd;&#xa;                                    CITY_ID,&#xd;&#xa;                                    CITY_NAME&#x29; A2&#xd;&#xa;                   ON   A1.BASE_ID &#x3d; A2.BASE_ID&#xd;&#xa;            WHERE       SUBSTR&#x28;A1.PERIOD_WID,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            GROUP BY    A2.CITY_ID&#x29; T1&#xd;&#xa;&#xd;&#xa;LEFT JOIN  &#x28;SELECT BASE_ID,&#xd;&#xa;                   BASE_NAME,&#xd;&#xa;                   RGN_NAME,&#xd;&#xa;                   CITY_ID,&#xd;&#xa;                   CITY_NAME,&#xd;&#xa;                   &#x28; CASE WHEN  BASE_TYPE&#x3d;&#x27;VDC&#x27; THEN 1&#xd;&#xa;                          WHEN  BASE_TYPE&#x3d;&#x27;VSC&#x27; THEN 2&#xd;&#xa;                          WHEN  BASE_TYPE&#x3d;&#x27;VTC&#x27; THEN 3&#xd;&#xa;                   END&#x29;                             AS BASE_TYPE_IND,&#xd;&#xa;                   ROW_NUMBER&#x28;&#x29; OVER &#x28; PARTITION BY CITY_ID ORDER BY &#x28; CASE WHEN  BASE_TYPE&#x3d;&#x27;VDC&#x27; THEN 1&#xd;&#xa;                                                                            WHEN  BASE_TYPE&#x3d;&#x27;VSC&#x27; THEN 2&#xd;&#xa;                                                                            WHEN  BASE_TYPE&#x3d;&#x27;VTC&#x27; THEN 3&#xd;&#xa;                                                                            END&#x29; ASC&#x29;  AS NUM&#xd;&#xa;            FROM  DIM.DIM_ORG&#xd;&#xa;            WHERE ORG_TYPE&#x3d;&#x27;S&#x27;&#xd;&#xa;              AND RGN_NAME &#x3c;&#x3e; &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;              AND STR_TYPE NOT IN &#x28;&#x27;&#x5728;&#x9014;&#x5e93;&#x27;,&#x27;&#x865a;&#x62df;&#x5e93;&#x27;&#x29; &#x29; T2&#xd;&#xa;       ON  T1.CITY_ID &#x3d; T2.CITY_ID&#xd;&#xa;      AND  T2.NUM &#x3d; 1&#xd;&#xa;&#xd;&#xa;GROUP BY T2.BASE_TYPE_IND,T2.BASE_NAME,T2.CITY_NAME&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;INSERT INTO TABLE DMR.DMR_OV_BASE_BIZ_INF PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;&#xd;&#xa;SELECT       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;     AS DATE_ID,          --&#x671f;&#x95f4;ID  8&#x4f4d;&#x65e5;&#x671f;  ETL&#x53c2;&#x6570;&#xd;&#xa;             &#x27;3&#x27;                                   AS BASE_TYPE_IND,    --&#x57fa;&#x5730;&#x7c7b;&#x578b;&#x6807;&#x5fd7;&#xff08;1-&#x751f;&#x4ea7;&#x57fa;&#x5730;&#xff0c;2-&#x4e2d;&#x8f6c;&#x7ad9;&#x3001;3-&#x57ce;&#x5e02;&#x914d;&#x9001;&#x4e2d;&#x5fc3;&#xff09;&#xd;&#xa;             &#x27;&#x5317;&#x4eac;&#x914d;&#x9001;&#x4e2d;&#x5fc3;&#x27;                                       AS ST_BASE_NAME,     --&#x59cb;&#x53d1;&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;             &#x27;&#x5317;&#x4eac;&#x27;                                               AS ST_CITY_NAME,     --&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;&#xd;&#xa;             &#x27;&#x27;                                                   AS CUR_M_SHIP_QTY,         --&#x5f53;&#x6708;&#x53d1;&#x8fd0;&#x91cf;&#xd;&#xa;             &#x27;&#x27;                                                   AS CUR_M_STOCK_IN_QTY,     --&#x5f53;&#x6708;&#x5165;&#x5e93;&#x91cf;&#xd;&#xa;             &#x27;&#x27;                                                   AS IN_STOCK,               --&#x5728;&#x5e93;&#x5b58;&#x5e93;&#xd;&#xa;             FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;                                AS UPDATE_DT           -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;             ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;             ,SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;     AS PART_DT&#xd;&#xa; UNION ALL&#xd;&#xa;&#xd;&#xa;SELECT       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;     AS DATE_ID,          --&#x671f;&#x95f4;ID  8&#x4f4d;&#x65e5;&#x671f;  ETL&#x53c2;&#x6570;&#xd;&#xa;             &#x27;3&#x27;                                   AS BASE_T