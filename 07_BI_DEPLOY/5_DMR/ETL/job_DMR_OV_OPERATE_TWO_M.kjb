      </entry>
    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_FIN_TPBUDGET_D	&#x8fd0;&#x8f93;&#x8d22;&#x52a1;&#x9884;&#x7b97;&#x65e5;&#x660e;&#x7ec6;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;05_DMK&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_OPERATE_TWO_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.MONTH_ID --&#x6708;&#x4efd;ID&#xd;&#xa;      ,T.ORG_ID --&#x7ec4;&#x7ec7;ID&#xd;&#xa;      ,T.ORG_TYPE --&#x7ec4;&#x7ec7;&#x7c7b;&#x578b;&#xd;&#xa;      ,T.ORG_NAME --&#x7ec4;&#x7ec7;&#x540d;&#x79f0;&#xd;&#xa;      ,T.CUR_M_MKT_EXP_INCOME_AMT --&#x5f53;&#x6708;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;      ,T.LST_TQ_M_MKT_EXP_INCOME_AMT --&#x53bb;&#x5e74;&#x540c;&#x671f;&#x6708;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;      ,T.CUR_M_PLAN_MKT_EXP_INCOME_AMT --&#x5f53;&#x6708;&#x8ba1;&#x5212;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;      ,T.CUR_M_BIZ_INCOME_AMT --&#x5f53;&#x6708;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;      ,T.CUR_M_HR_FEE_RATE --&#x5f53;&#x6708;&#x4eba;&#x4e8b;&#x8d39;&#x7528;&#x7387;&#xd;&#xa;      ,T.CUR_M_HR_FEE_RATE_TARGT --&#x5f53;&#x6708;&#x4eba;&#x4e8b;&#x8d39;&#x7528;&#x7387;&#x76ee;&#x6807;&#x503c;&#xd;&#xa;      ,T.CUR_M_BIZ_QTY --&#x5f53;&#x6708;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,T.CUR_M_BIZ_INCOME_HB_RATE --&#x5f53;&#x6708;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x73af;&#x6bd4;&#xd;&#xa;      ,T.CUR_M_PROFIT_AMT --&#x5f53;&#x6708;&#x5229;&#x6da6;&#x91d1;&#x989d;&#xd;&#xa;      ,T.LST_TQ_M_PROFIT_AMT --&#x53bb;&#x5e74;&#x540c;&#x671f;&#x6708;&#x5229;&#x6da6;&#x91d1;&#x989d;&#xd;&#xa;      ,T.CUR_M_PROFIT_GROW_RATE --&#x5f53;&#x6708;&#x5229;&#x6da6;&#x589e;&#x957f;&#x7387;&#xd;&#xa;      ,T.CUR_M_CA_ONTIME_RATE --&#x5f53;&#x6708;&#x957f;&#x5b89;&#x51c6;&#x5230;&#x7387;&#xd;&#xa;      ,T.CUR_M_CA_ONTIME_RATE_TARGT --&#x5f53;&#x6708;&#x957f;&#x5b89;&#x51c6;&#x5230;&#x7387;&#x76ee;&#x6807;&#xd;&#xa;      ,T.CUR_M_FT_ONTIME_RATE --&#x5f53;&#x6708;&#x798f;&#x7279;&#x51c6;&#x5230;&#x7387;&#xd;&#xa;      ,T.CUR_M_FT_ONTIME_RATE_TARGT --&#x5f53;&#x6708;&#x798f;&#x7279;&#x51c6;&#x5230;&#x7387;&#x76ee;&#x6807;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_INSERT_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_UPDATE_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,T.MONTH_ID AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_OV_OPERATE_TWO_M_BASE T1&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_OV_OPERATE_TWO_M_RGN T2&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_OV_OPERATE_TWO_M_OC T3&#x29; AS T&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
