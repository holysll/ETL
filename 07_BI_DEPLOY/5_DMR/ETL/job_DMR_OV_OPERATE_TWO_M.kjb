    <entry>
      <name>TEMP_OC</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_OC&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_OPERATE_TWO_M_OC&#xd;&#xa;--&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;&#xd;&#xa;SELECT M.PERIOD_WID AS MONTH_ID --&#x6708;&#x4efd;ID&#xd;&#xa;      ,M.ORG_ID AS ORG_ID --&#x7ec4;&#x7ec7;ID&#xd;&#xa;      ,M.ORG_TYPE AS ORG_TYPE --&#x7ec4;&#x7ec7;&#x7c7b;&#x578b;&#xd;&#xa;      ,M.ORG_NAME AS ORG_NAME --&#x7ec4;&#x7ec7;&#x540d;&#x79f0;&#xd;&#xa;      ,M1.MARKET_EXPAND_INCOME_AMT AS CUR_M_MKT_EXP_INCOME_AMT --&#x5f53;&#x6708;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;      ,TQ1.MARKET_EXPAND_INCOME_AMT AS LST_TQ_M_MKT_EXP_INCOME_AMT --&#x53bb;&#x5e74;&#x540c;&#x671f;&#x6708;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;      ,M1.MARKET_EXPAND_INCOME_TGT_AMT AS CUR_M_PLAN_MKT_EXP_INCOME_AMT --&#x5f53;&#x6708;&#x8ba1;&#x5212;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;      ,M1.BIZ_INCOME_AMT AS CUR_M_BIZ_INCOME_AMT --&#x5f53;&#x6708;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;HR_FEE_TGT_RATE, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;M1.HUMAN_COST_AMT&#x2f;M1.BIZ_INCOME_AMT&#x29; &#x2a;100 --HR1.HR_FEE_TGT_RATE    --update 20180706&#xd;&#xa;       END AS CUR_M_HR_FEE_RATE --&#x5f53;&#x6708;&#x4eba;&#x4e8b;&#x8d39;&#x7528;&#x7387;&#xd;&#xa;      ,HR1.HR_FEE_TGT_RATE &#x2a;100 AS CUR_M_HR_FEE_RATE_TARGT --&#x5f53;&#x6708;&#x4eba;&#x4e8b;&#x8d39;&#x7528;&#x7387;&#x76ee;&#x6807;&#x503c;&#xd;&#xa;      ,QTY1.BIZ_QTY AS CUR_M_BIZ_QTY --&#x5f53;&#x6708;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;LM1.BIZ_INCOME_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;&#x28;M1.BIZ_INCOME_AMT - LM1.BIZ_INCOME_AMT&#x29; &#x2f; LM1.BIZ_INCOME_AMT&#x29; &#x2a;100&#xd;&#xa;       END AS CUR_M_BIZ_INCOME_HB_RATE --&#x5f53;&#x6708;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x73af;&#x6bd4;&#xd;&#xa;      ,M1.PROFIT_AMT AS CUR_M_PROFIT_AMT --&#x5f53;&#x6708;&#x5229;&#x6da6;&#x91d1;&#x989d;&#xd;&#xa;      ,TQ1.PROFIT_AMT LST_TQ_M_PROFIT_AMT --&#x53bb;&#x5e74;&#x540c;&#x671f;&#x6708;&#x5229;&#x6da6;&#x91d1;&#x989d;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;LM1.PROFIT_AMT, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          0&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;&#x28;M1.PROFIT_AMT - LM1.PROFIT_AMT&#x29; &#x2f; LM1.PROFIT_AMT&#x29; &#x2a;100&#xd;&#xa;       END AS CUR_M_PROFIT_GROW_RATE --&#x5f53;&#x6708;&#x5229;&#x6da6;&#x589e;&#x957f;&#x7387;&#xd;&#xa;      ,CA1.ONTIME_RATE&#x2a;100 AS CUR_M_CA_ONTIME_RATE --&#x5f53;&#x6708;&#x957f;&#x5b89;&#x51c6;&#x5230;&#x7387;&#xd;&#xa;      ,92.5 AS CUR_M_CA_ONTIME_RATE_TARGT --&#x5f53;&#x6708;&#x957f;&#x5b89;&#x51c6;&#x5230;&#x7387;&#x76ee;&#x6807;&#xd;&#xa;      ,FT1.ONTIME_RATE&#x2a;100 AS CUR_M_FT_ONTIME_RATE --&#x5f53;&#x6708;&#x798f;&#x7279;&#x51c6;&#x5230;&#x7387;&#xd;&#xa;      ,90 AS CUR_M_FT_ONTIME_RATE_TARGT --&#x5f53;&#x6708;&#x798f;&#x7279;&#x51c6;&#x5230;&#x7387;&#x76ee;&#x6807; &#xd;&#xa;  FROM TEMP.TEMP_DMR_OV_OPERATE_TWO_M_ORG M&#xd;&#xa;  LEFT JOIN &#x28;SELECT SUBSTRING&#x28;T.PERIOD_WID, 1, 6&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.OC_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.MARKET_EXPAND_INCOME_AMT&#x29; AS MARKET_EXPAND_INCOME_AMT --&#x5f53;&#x6708;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.MARKET_EXPAND_INCOME_TGT_AMT&#x29; AS MARKET_EXPAND_INCOME_TGT_AMT --&#x5f53;&#x6708;&#x8ba1;&#x5212;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.VCL_INCOME_AMT&#x29; AS BIZ_INCOME_AMT --&#x5f53;&#x6708;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.HUMAN_COST_AMT&#x29; AS HUMAN_COST_AMT --&#x5f53;&#x6708;&#x4eba;&#x4e8b;&#x8d39;&#x7528;&#xd;&#xa;                   ,SUM&#x28;T.PROFIT_AMT&#x29; AS PROFIT_AMT --&#x5f53;&#x6708;&#x5229;&#x6da6;&#x91d1;&#x989d;&#xd;&#xa;               FROM DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;              WHERE SUBSTRING&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY SUBSTRING&#x28;T.PERIOD_WID, 1, 6&#x29;, T.OC_ID&#x29; AS M1&#xd;&#xa;    ON M.ORG_ID &#x3d; M1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; M1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.OC_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.MARKET_EXPAND_INCOME_AMT&#x29; AS MARKET_EXPAND_INCOME_AMT --&#x5f53;&#x6708;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.MARKET_EXPAND_INCOME_TGT_AMT&#x29; AS MARKET_EXPAND_INCOME_TGT_AMT --&#x5f53;&#x6708;&#x8ba1;&#x5212;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.VCL_INCOME_AMT&#x29; AS BIZ_INCOME_AMT --&#x5f53;&#x6708;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.HUMAN_COST_AMT&#x29; AS HUMAN_COST_AMT --&#x5f53;&#x6708;&#x4eba;&#x4e8b;&#x8d39;&#x7528;&#xd;&#xa;                   ,SUM&#x28;T.PROFIT_AMT&#x29; AS PROFIT_AMT --&#x5f53;&#x6708;&#x5229;&#x6da6;&#x91d1;&#x989d;&#xd;&#xa;               FROM DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;              WHERE SUBSTRING&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             7&#x29;,&#xd;&#xa;                                   &#x27;-&#x27;,&#xd;&#xa;                                   &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T.OC_ID&#x29; AS LM1&#xd;&#xa;    ON M.ORG_ID &#x3d; LM1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; LM1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,T.OC_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T.MARKET_EXPAND_INCOME_AMT&#x29; AS MARKET_EXPAND_INCOME_AMT --&#x53bb;&#x5e74;&#x540c;&#x671f;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.MARKET_EXPAND_INCOME_TGT_AMT&#x29; AS MARKET_EXPAND_INCOME_TGT_AMT --&#x53bb;&#x5e74;&#x540c;&#x671f;&#x8ba1;&#x5212;&#x5e02;&#x573a;&#x62d3;&#x5c55;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.VCL_INCOME_AMT&#x29; AS BIZ_INCOME_AMT --&#x53bb;&#x5e74;&#x540c;&#x671f;&#x8425;&#x4e1a;&#x6536;&#x5165;&#x91d1;&#x989d;&#xd;&#xa;                   ,SUM&#x28;T.HUMAN_COST_AMT&#x29; AS HUMAN_COST_AMT --&#x53bb;&#x5e74;&#x540c;&#x671f;&#x4eba;&#x4e8b;&#x8d39;&#x7528;&#xd;&#xa;                   ,SUM&#x28;T.PROFIT_AMT&#x29; AS PROFIT_AMT --&#x53bb;&#x5e74;&#x540c;&#x671f;&#x5229;&#x6da6;&#x91d1;&#x989d;&#xd;&#xa;               FROM DMK.DMK_FIN_INCOMECOST_M T&#xd;&#xa;              WHERE SUBSTRING&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             7&#x29;,&#xd;&#xa;                                   &#x27;-&#x27;,&#xd;&#xa;                                   &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T.OC_ID&#x29; AS TQ1&#xd;&#xa;    ON M.ORG_ID &#x3d; TQ1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; TQ1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,MAX&#x28;A.HR_FEE_TGT_RATE&#x29; AS HR_FEE_TGT_RATE&#xd;&#xa;                   ,B.OC_ID AS ORG_ID&#xd;&#xa;               FROM DMK.DMK_FIN_OCHRFEERATE_M A&#xd;&#xa;               JOIN DIM.DIM_ORG B&#xd;&#xa;                 ON B.ORG_TYPE &#x3d; &#x27;O&#x27;&#xd;&#xa;                AND B.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;                AND A.OC_ID &#x3d; B.OC_ID&#xd;&#xa;              WHERE A.PART_DT &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY B.OC_ID&#x29; AS HR1&#xd;&#xa;    ON M.ORG_ID &#x3d; HR1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; HR1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT T4.PART_DT AS PERIOD_WID&#xd;&#xa;                   ,T4.OC_ID AS ORG_ID&#xd;&#xa;                   ,SUM&#x28;T4.BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_TP_PROJBIZQTY_M T4&#xd;&#xa;              WHERE T4.PART_DT &#x3d;&#xd;&#xa;                    REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;              GROUP BY T4.PART_DT, T4.OC_ID&#x29; AS QTY1&#xd;&#xa;    ON M.ORG_ID &#x3d; QTY1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; QTY1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT substr&#x28;T5.period_wid,1,6&#x29; AS PERIOD_WID&#xd;&#xa;                   ,coalesce&#x28;T5.oc_id,&#x27;-1&#x27;&#x29; AS ORG_ID&#xd;&#xa;                    ,sum&#x28;T5.arrive_ontime_qty&#x29;&#x2f;SUM&#x28;T5.arrive_plan_qty&#x29; AS ONTIME_RATE&#xd;&#xa;               FROM DMK.dmk_tp_shipqty_d T5&#xd;&#xa;              WHERE T5.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;                AND substr&#x28;T5.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;              GROUP BY substr&#x28;T5.period_wid,1,6&#x29;&#xd;&#xa;                ,coalesce&#x28;T5.oc_id,&#x27;-1&#x27;&#x29;&#x29; AS CA1&#xd;&#xa;    ON M.ORG_ID &#x3d; CA1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; CA1.PERIOD_WID&#xd;&#xa;  LEFT JOIN &#x28;SELECT substr&#x28;T5.period_wid,1,6&#x29; AS PERIOD_WID&#xd;&#xa;                   ,coalesce&#x28;T5.oc_id,&#x27;-1&#x27;&#x29; AS ORG_ID&#xd;&#xa;                    ,sum&#x28;T5.arrive_ontime_qty&#x29;&#x2f;SUM&#x28;T5.arrive_plan_qty&#x29; AS ONTIME_RATE&#xd;&#xa;               FROM DMK.dmk_tp_shipqty_d T5&#xd;&#xa;              WHERE T5.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;                AND substr&#x28;T5.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;              GROUP BY substr&#x28;T5.period_wid,1,6&#x29;&#xd;&#xa;                ,coalesce&#x28;T5.oc_id,&#x27;-1&#x27;&#x29;&#x29; AS FT1&#xd;&#xa;    ON M.ORG_ID &#x3d; FT1.ORG_ID&#xd;&#xa;   AND M.PERIOD_WID &#x3d; FT1.PERIOD_WID&#xd;&#xa; WHERE M.ORG_TYPE &#x3d; &#x27;O&#x27;&#x3b;&#xd;&#xa;</sql>
