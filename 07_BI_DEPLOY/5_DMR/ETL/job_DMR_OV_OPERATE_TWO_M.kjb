    <entry>
      <name>TEMP_PERIOD_ORG</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TEMP_PERIOD_ORG&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.mapred.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_OV_OPERATE_TWO_M_ORG&#xd;&#xa;  SELECT D.PERIOD_WID, A.ORG_TYPE, A.ORG_ID, A.ORG_NAME&#xd;&#xa;    FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;            FROM DIM.DIM_PERIOD_D D&#xd;&#xa;           WHERE D.PERIOD_TYPE &#x3d; &#x27;M&#x27;&#xd;&#xa;             AND SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;             AND D.PERIOD_WID &#x3c;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyyMMdd&#x27;&#x29;&#x29; D&#xd;&#xa;    JOIN &#x28;SELECT T.ORG_TYPE,T.ORG_ID,CASE WHEN T.ORG_TYPE &#x3d; &#x27;O&#x27; THEN&#xd;&#xa;                                                   O.OC_NAME&#xd;&#xa;                                              WHEN T.ORG_TYPE &#x3d; &#x27;R&#x27; THEN&#xd;&#xa;                                                   O.RGN_NAME&#xd;&#xa;                                              WHEN T.ORG_TYPE &#x3d; &#x27;B&#x27; THEN&#xd;&#xa;                                                   O.BASE_NAME&#xd;&#xa;                                              WHEN T.ORG_TYPE &#x3d; &#x27;S&#x27; THEN&#xd;&#xa;                                                   O.STR_NAME&#xd;&#xa;                                              ELSE&#xd;&#xa;                                                   NULL&#xd;&#xa;                                              END AS ORG_NAME&#xd;&#xa;            FROM &#x28;&#xd;&#xa;            SELECT &#x27;B&#x27; AS ORG_TYPE,NVL&#x28;IF&#x28;A.BASE_ID&#x3d;&#x27;-1&#x27;,&#x27;BASE&#x2f;-1&#x27;,A.BASE_ID&#x29;, &#x27;BASE&#x2f;-1&#x27;&#x29;  AS ORG_ID&#xd;&#xa;              FROM DMK.DMK_FIN_INCOMECOST_M A&#xd;&#xa;             WHERE SUBSTR&#x28;A.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;            UNION ALL&#xd;&#xa;            SELECT &#x27;B&#x27; AS ORG_TYPE,NVL&#x28;IF&#x28;C.BASE_ID&#x3d;&#x27;-1&#x27;,&#x27;BASE&#x2f;-1&#x27;,C.BASE_ID&#x29;, &#x27;BASE&#x2f;-1&#x27;&#x29; AS ORG_ID&#xd;&#xa;              FROM DMK.DMK_TP_PROJBIZQTY_M C&#xd;&#xa;             WHERE SUBSTR&#x28;C.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;            UNION ALL&#xd;&#xa;            SELECT &#x27;B&#x27; AS ORG_TYPE,NVL&#x28;IF&#x28;D.BASE_ID&#x3d;&#x27;-1&#x27;,&#x27;BASE&#x2f;-1&#x27;,D.BASE_ID&#x29;, &#x27;BASE&#x2f;-1&#x27;&#x29; AS ORG_ID&#xd;&#xa;              FROM DMK.DMK_TP_SHIPQTY_D D&#xd;&#xa;             WHERE SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;            UNION ALL&#xd;&#xa;            SELECT &#x27;R&#x27; AS ORG_TYPE,A.RGN_ID AS ORG_ID&#xd;&#xa;              FROM DMK.DMK_FIN_INCOMECOST_M A&#xd;&#xa;             WHERE SUBSTR&#x28;A.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;            UNION ALL&#xd;&#xa;            SELECT &#x27;R&#x27; AS ORG_TYPE,C.RGN_ID AS ORG_ID&#xd;&#xa;              FROM DMK.DMK_TP_PROJBIZQTY_M C&#xd;&#xa;             WHERE SUBSTR&#x28;C.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;            UNION ALL&#xd;&#xa;            SELECT &#x27;R&#x27; AS ORG_TYPE,D.RGN_ID AS ORG_ID&#xd;&#xa;              FROM DMK.DMK_TP_SHIPQTY_D D&#xd;&#xa;             WHERE SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;            UNION ALL&#xd;&#xa;            SELECT &#x27;O&#x27; AS ORG_TYPE,A.OC_ID AS ORG_ID&#xd;&#xa;              FROM DMK.DMK_FIN_INCOMECOST_M A&#xd;&#xa;             WHERE SUBSTR&#x28;A.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;            UNION ALL&#xd;&#xa;            SELECT &#x27;O&#x27; AS ORG_TYPE,C.OC_ID AS ORG_ID&#xd;&#xa;              FROM DMK.DMK_TP_PROJBIZQTY_M C&#xd;&#xa;             WHERE SUBSTR&#x28;C.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;            UNION ALL&#xd;&#xa;            SELECT &#x27;O&#x27; AS ORG_TYPE,D.OC_ID AS ORG_ID&#xd;&#xa;              FROM DMK.DMK_TP_SHIPQTY_D D&#xd;&#xa;             WHERE SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x29; AS T&#xd;&#xa;              JOIN DIM.DIM_ORG O &#xd;&#xa;                ON T.ORG_ID &#x3d; O.ORG_ID&#xd;&#xa;            GROUP BY T.ORG_TYPE,T.ORG_ID,CASE WHEN T.ORG_TYPE &#x3d; &#x27;O&#x27; THEN&#xd;&#xa;                                                   O.OC_NAME&#xd;&#xa;                                              WHEN T.ORG_TYPE &#x3d; &#x27;R&#x27; THEN&#xd;&#xa;                                                   O.RGN_NAME&#xd;&#xa;                                              WHEN T.ORG_TYPE &#x3d; &#x27;B&#x27; THEN&#xd;&#xa;                                                   O.BASE_NAME&#xd;&#xa;                                              WHEN T.ORG_TYPE &#x3d; &#x27;S&#x27; THEN&#xd;&#xa;                                                   O.STR_NAME&#xd;&#xa;                                              ELSE&#xd;&#xa;                                                   NULL&#xd;&#xa;                                              END&#x29; AS A&#xd;&#xa;      ON 1 &#x3d; 1&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>HIVE</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
