      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMR&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_ORD_OVR_D PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT M.PERIOD_WID AS DATE_ID&#xd;&#xa;      ,M.RGN_ID&#xd;&#xa;      ,COALESCE&#x28;M1.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;      ,M.BASE_ID&#xd;&#xa;      ,COALESCE&#x28;M2.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME&#xd;&#xa;      ,M.BRAND_ID&#xd;&#xa;      ,COALESCE&#x28;M3.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME&#xd;&#xa;      ,M.SUPPLIER_ID&#xd;&#xa;      ,COALESCE&#x28;M4.SERVPROV_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS SUPPLIER_NAME&#xd;&#xa;      ,M.TPMODE_ID&#xd;&#xa;      ,COALESCE&#x28;M5.TPMODE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS TPMODE_NAME&#xd;&#xa;      ,M.OVRTM_TYPE_ID&#xd;&#xa;      ,M.OVRTM_TYPE_NAME&#xd;&#xa;      ,M.OVRTM_ORD_QTY&#xd;&#xa;      ,M.ORD_QTY&#xd;&#xa;      ,M.AGE_ONE_QTY&#xd;&#xa;      ,M.AGE_TWO_QTY&#xd;&#xa;      ,M.AGE_THREE_QTY&#xd;&#xa;      ,M.AGE_FOUR_QTY&#xd;&#xa;      ,M.AGE_FIVE_QTY&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DATE --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;M.PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT T.PERIOD_WID&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,T.BASE_ID&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,T.SUPPLIER_ID&#xd;&#xa;              ,T.TPMODE_ID&#xd;&#xa;              ,&#x27;OT_TYPE_01&#x27; AS OVRTM_TYPE_ID&#xd;&#xa;              ,&#x27;&#x672a;&#x5230;&#x8fbe;&#x27; AS OVRTM_TYPE_NAME&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS OVRTM_ORD_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS ORD_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3e; 0 AND T.OUTTIME_TIME &#x3c;&#x3d; 3600 &#x2a; 24 &#x2a; 3 THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS AGE_ONE_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3e; 3600 &#x2a; 24 &#x2a; 3 AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3c;&#x3d; 3600 &#x2a; 24 &#x2a; 5 THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS AGE_TWO_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3e; 3600 &#x2a; 24 &#x2a; 5 AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3c;&#x3d; 3600 &#x2a; 24 &#x2a; 10 THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS AGE_THREE_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3e; 3600 &#x2a; 24 &#x2a; 10 AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3c;&#x3d; 3600 &#x2a; 24 &#x2a; 15 THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS AGE_FOUR_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;N&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3e; 3600 &#x2a; 24 &#x2a; 15 THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS AGE_FIVE_QTY&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_TP_ORD_OVR_D_2 T&#xd;&#xa;         WHERE T.IS_ARRIVE &#x3d; &#x27;N&#x27;&#xd;&#xa;         GROUP BY T.PERIOD_WID&#xd;&#xa;                 ,T.RGN_ID&#xd;&#xa;                 ,T.BASE_ID&#xd;&#xa;                 ,T.BRAND_ID&#xd;&#xa;                 ,T.SUPPLIER_ID&#xd;&#xa;                 ,T.TPMODE_ID&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT T.PERIOD_WID&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,T.BASE_ID&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,T.SUPPLIER_ID&#xd;&#xa;              ,T.TPMODE_ID&#xd;&#xa;              ,&#x27;OT_TYPE_02&#x27; AS OVRTM_TYPE_ID&#xd;&#xa;              ,&#x27;&#x5df2;&#x5230;&#x8fbe;&#x27; AS OVRTM_TYPE_NAME&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS OVRTM_ORD_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS ORD_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3e; 0 AND T.OUTTIME_TIME &#x3c;&#x3d; 3600 &#x2a; 24 &#x2a; 3 THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS AGE_ONE_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3e; 3600 &#x2a; 24 &#x2a; 3 AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3c;&#x3d; 3600 &#x2a; 24 &#x2a; 5 THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS AGE_TWO_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3e; 3600 &#x2a; 24 &#x2a; 5 AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3c;&#x3d; 3600 &#x2a; 24 &#x2a; 10 THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS AGE_THREE_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3e; 3600 &#x2a; 24 &#x2a; 10 AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3c;&#x3d; 3600 &#x2a; 24 &#x2a; 15 THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS AGE_FOUR_QTY&#xd;&#xa;              ,COUNT&#x28;CASE&#xd;&#xa;                       WHEN T.IS_ARRIVE &#x3d; &#x27;Y&#x27; AND T.IS_OUTTIME &#x3d; &#x27;Y&#x27; AND&#xd;&#xa;                            T.OUTTIME_TIME &#x3e; 3600 &#x2a; 24 &#x2a; 15 THEN&#xd;&#xa;                        T.ORDER_RELEASE_GID&#xd;&#xa;                     END&#x29; AS AGE_FIVE_QTY&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_TP_ORD_OVR_D_2 T&#xd;&#xa;         WHERE T.IS_ARRIVE &#x3d; &#x27;Y&#x27;&#xd;&#xa;         GROUP BY T.PERIOD_WID&#xd;&#xa;                 ,T.RGN_ID&#xd;&#xa;                 ,T.BASE_ID&#xd;&#xa;                 ,T.BRAND_ID&#xd;&#xa;                 ,T.SUPPLIER_ID&#xd;&#xa;                 ,T.TPMODE_ID&#x29; M&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; M1&#xd;&#xa;    ON M.RGN_ID &#x3d; M1.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; M2&#xd;&#xa;    ON M.BASE_ID &#x3d; M2.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; M3&#xd;&#xa;    ON M.BRAND_ID &#x3d; M3.BRAND_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT SERVPROV_ID&#xd;&#xa;                   ,SERVPROV_NAME&#xd;&#xa;               FROM DIM.DIM_SERVPROV&#xd;&#xa;              GROUP BY SERVPROV_ID&#xd;&#xa;                      ,SERVPROV_NAME&#x29; M4&#xd;&#xa;    ON M.SUPPLIER_ID &#x3d; M4.SERVPROV_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT TPMODE_ID&#xd;&#xa;                   ,TPMODE_NAME&#xd;&#xa;               FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;              GROUP BY TPMODE_ID&#xd;&#xa;                      ,TPMODE_NAME&#x29; M5&#xd;&#xa;    ON M.TPMODE_ID &#x3d; M5.TPMODE_ID&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
