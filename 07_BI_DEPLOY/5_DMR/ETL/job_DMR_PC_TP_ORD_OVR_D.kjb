    <entry>
      <name>TO_TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_OVR_D_2&#xd;&#xa;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;T.ARRIVAL_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS PERIOD_WID&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,T.SUPPLIER_ID&#xd;&#xa;      ,T.TRANSPORTMODE_ID AS TPMODE_ID&#xd;&#xa;      ,T.ORDER_RELEASE_GID&#xd;&#xa;      ,T.VIN&#xd;&#xa;      ,T.FACTORY_DAY&#xd;&#xa;      ,T.ARRIVAL_DAY&#xd;&#xa;      ,T.ON_PASSAGE_DAYS&#xd;&#xa;      ,&#x27;Y&#x27; AS IS_ARRIVE&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN unix_timestamp&#x28;NVL&#x28;T.dlvr_day,T.arrival_day&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;T.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;T.on_passage_days,0&#x29;&#xd;&#xa;         THEN&#xd;&#xa;          &#x27;Y&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;N&#x27;&#xd;&#xa;       END AS IS_OUTTIME&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN unix_timestamp&#x28;nvl&#x28;T.dlvr_day,T.arrival_day&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;T.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;T.on_passage_days,0&#x29;&#xd;&#xa;         THEN&#xd;&#xa;          UNIX_TIMESTAMP&#x28;NVL&#x28;T.DLVR_DAY,T.ARRIVAL_DAY&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; -&#xd;&#xa;          UNIX_TIMESTAMP&#x28;T.FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; -&#xd;&#xa;          24 &#x2a; 3600 &#x2a; NVL&#x28;T.ON_PASSAGE_DAYS,0&#x29;&#xd;&#xa;         ELSE&#xd;&#xa;          0&#xd;&#xa;       END AS OUTTIME_TIME&#xd;&#xa;  FROM DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa; WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;NVL&#x28;T.dlvr_day,T.ARRIVAL_DAY&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;    AND SUBSTR&#x28;REGEXP_REPLACE&#x28;NVL&#x28;T.dlvr_day,T.ARRIVAL_DAY&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT D.PERIOD_WID&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,SUPPLIER_ID&#xd;&#xa;      ,TRANSPORTMODE_ID AS TPMODE_ID&#xd;&#xa;      ,ORDER_RELEASE_GID&#xd;&#xa;      ,VIN&#xd;&#xa;      ,FACTORY_DAY&#xd;&#xa;      ,ARRIVAL_DAY&#xd;&#xa;      ,ON_PASSAGE_DAYS&#xd;&#xa;      ,&#x27;N&#x27; AS IS_ARRIVE&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN unix_timestamp&#x28;concat&#x28;D.period_date,&#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;T.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;T.on_passage_days,0&#x29;&#xd;&#xa;         THEN&#xd;&#xa;          &#x27;Y&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;N&#x27;&#xd;&#xa;       END AS IS_OUTTIME&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN unix_timestamp&#x28;concat&#x28;D.period_date,&#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;-unix_timestamp&#x28;T.factory_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; &#x3e; 24&#x2a;3600&#x2a;NVL&#x28;T.on_passage_days,0&#x29;&#xd;&#xa;         THEN&#xd;&#xa;          UNIX_TIMESTAMP&#x28;CONCAT&#x28;D.PERIOD_DATE, &#x27; 00&#x3a;00&#x3a;01&#x27;&#x29;,&#xd;&#xa;                         &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; -&#xd;&#xa;          UNIX_TIMESTAMP&#x28;FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; -&#xd;&#xa;          24 &#x2a; 3600 &#x2a; NVL&#x28;T.ON_PASSAGE_DAYS,0&#x29;&#xd;&#xa;         ELSE&#xd;&#xa;          0&#xd;&#xa;       END AS OUTTIME_TIME&#xd;&#xa;  FROM DIM.DIM_PERIOD_D D&#xd;&#xa;  JOIN DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;    ON 1 &#x3d; 1&#xd;&#xa; WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;FACTORY_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d; D.PERIOD_WID&#xd;&#xa;   AND NVL&#x28;SUBSTR&#x28;REGEXP_REPLACE&#x28;NVL&#x28;T.dlvr_day,T.arrival_day&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;, &#x27;29991231&#x27;&#x29; &#x3e;&#xd;&#xa;       D.PERIOD_WID&#xd;&#xa;   AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;   AND SUBSTR&#x28;D.PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>HIVE</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>224</xloc>
      <yloc>208</yloc>
      </entry>
    <entry>
      <name>CREATE_TEMP_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_OVR_D_2&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_OVR_D_2&#x28;&#xd;&#xa;PERIOD_WID BIGINT&#xd;&#xa;,RGN_ID	STRING	COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID STRING    COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID STRING   COMMENT&#x27;&#x54c1;&#x724c;&#x27;&#xd;&#xa;,SUPPLIER_ID STRING		COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	STRING	COMMENT&#x27;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;ID&#x27;&#xd;&#xa;,ORDER_RELEASE_GID STRING   COMMENT&#x27;OTM&#x5355;&#x53f7;&#x27;&#xd;&#xa;,VIN	STRING	COMMENT&#x27;VIN&#x27;&#xd;&#xa;,FACTORY_DAY	STRING	COMMENT&#x27;&#x53d1;&#x8fd0;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,ARRIVAL_DAY	STRING	COMMENT&#x27;&#x5230;&#x8fbe;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,ON_PASSAGE_DAYS	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x6807;&#x51c6;&#x5728;&#x9014;&#x5929;&#x6570;&#x27;&#xd;&#xa;,IS_ARRIVE	STRING	COMMENT&#x27;&#x662f;&#x5426;&#x5230;&#x8fbe;&#x27;&#xd;&#xa;,IS_OUTTIME	STRING	COMMENT&#x27;&#x662f;&#x5426;&#x8d85;&#x65f6;&#x27;&#xd;&#xa;,OUTTIME_TIME	BIGINT	COMMENT&#x27;&#x8d85;&#x65f6;&#x65f6;&#x957f;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8d85;&#x65f6;&#x8ba2;&#x5355;&#x660e;&#x7ec6;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;</sql>
