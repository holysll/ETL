      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_CARRIER_KPI_MON PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; AS PERIOD_WID,&#xd;&#xa;       DIM.SERVPROV_ID,&#xd;&#xa;       DIM.SERVPROV_NAME,&#xd;&#xa;       AVG&#x28;CASE MONTH&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29; &#xd;&#xa;             WHEN 1  THEN SRV.JAN_AMT&#xd;&#xa;             WHEN 2  THEN SRV.FEB_AMT&#xd;&#xa;             WHEN 3  THEN SRV.MAR_AMT&#xd;&#xa;             WHEN 4  THEN SRV.APR_AMT&#xd;&#xa;             WHEN 5  THEN SRV.MAY_AMT&#xd;&#xa;             WHEN 6  THEN SRV.JUNE_AMT&#xd;&#xa;             WHEN 7  THEN SRV.JULY_AMT&#xd;&#xa;             WHEN 8  THEN SRV.AUG_AMT&#xd;&#xa;             WHEN 9  THEN SRV.SEPT_AMT&#xd;&#xa;             WHEN 10 THEN SRV.OCT_AMT&#xd;&#xa;             WHEN 11 THEN SRV.NOV_AMT&#xd;&#xa;             ELSE SRV.DEC_AMT END&#x29; AS KPI_SCORE,&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;&#x29;,&#xd;&#xa;       &#x27;DW&#x27;,&#xd;&#xa;       REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;  FROM ODS_BLPT.T_HAND_SERVPROV_KPI SRV&#xd;&#xa;    LEFT JOIN DIM.DIM_SERVPROV DIM&#xd;&#xa;           ON SRV.SERVPROV_NAME &#x3d; DIM.SERVPROV_NAME&#xd;&#xa;    WHERE SRV.YEAR_ID &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;      GROUP BY DIM.SERVPROV_ID,&#xd;&#xa;               DIM.SERVPROV_NAME&#xd;&#xa;&#x3b;</sql>
