     WHERE SUBSTR&#x28;PERIOD_WID, 1, 4&#x29; &#x3e;&#x3d;&#xd;&#xa;                               SUBSTR&#x28;ADD_MONTHS&#x28;TO_DATE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;, -12&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      4&#x29;&#xd;&#xa;                           AND TRIM&#x28;BIZTYPE&#x29; &#x3d; &#x27;&#x8fd0;&#x8f93;&#x27;&#xd;&#xa;                         GROUP BY COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                 ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                 ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;&#x29; B1&#xd;&#xa;                 GROUP BY B1.RGN_ID&#xd;&#xa;                         ,B1.BASE_ID&#xd;&#xa;                         ,B1.BRAND_ID WITH CUBE&#x29; T1&#xd;&#xa;          FULL JOIN &#x28;SELECT COALESCE&#x28;COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                          ,COALESCE&#x28;COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                          ,COALESCE&#x28;COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                          ,SUM&#x28;COALESCE&#x28;BIZ_QTY, 0&#x29;&#x29; AS CUR_M_TRANS_PLAN_QTY&#xd;&#xa;                      FROM DMK.DMK_TP_BIZQTYTGT_M&#xd;&#xa;                     WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29;&#xd;&#xa;                     GROUP BY COALESCE&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                             ,COALESCE&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                             ,COALESCE&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; T2&#xd;&#xa;            ON T1.RGN_ID &#x3d; T2.RGN_ID&#xd;&#xa;           AND T1.BASE_ID &#x3d; T2.BASE_ID&#xd;&#xa;           AND T1.BRAND_ID &#x3d; T2.BRAND_ID&#x29; A1&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG A2 --&#x5927;&#x533a;&#xd;&#xa;    ON A1.RGN_ID &#x3d; A2.RGN_ID&#xd;&#xa;   AND A2.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG A3 --&#x57fa;&#x5730;&#xd;&#xa;    ON A1.BASE_ID &#x3d; A3.BASE_ID&#xd;&#xa;   AND A3.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_BRAND A4 --&#x54c1;&#x724c;&#xd;&#xa;    ON A1.BRAND_ID &#x3d; A4.BRAND_ID&#xd;&#xa; GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;         ,A1.RGN_ID&#xd;&#xa;         ,A2.RGN_NAME&#xd;&#xa;         ,A1.BASE_ID&#xd;&#xa;         ,A3.BASE_NAME&#xd;&#xa;         ,A1.BRAND_ID&#xd;&#xa;         ,A4.BRAND_NAME&#x3b;&#xd;&#xa;</sql>
