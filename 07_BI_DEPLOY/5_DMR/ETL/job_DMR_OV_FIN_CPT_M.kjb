    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMR_OV_FIN_CPT_M&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;DMR&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set hive.strict.checks.type.safety&#x3d;false&#x3b;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_OV_FIN_CPT_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; AS MONTH_ID --&#x6708;&#x4efd;ID&#xd;&#xa;      ,A1.RGN_ID AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN A1.RGN_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x5927;&#x533a;&#x27;&#xd;&#xa;         WHEN A1.RGN_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          A2.RGN_NAME&#xd;&#xa;       END AS RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;      ,A1.BASE_ID AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN A1.BASE_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x57fa;&#x5730;&#x27;&#xd;&#xa;         WHEN A1.BASE_ID IN &#x28;&#x27;-1&#x27;, &#x27;CMAL&#x2f;ZC.-1&#x27;&#x29; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          A3.BASE_NAME&#xd;&#xa;       END AS BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;      ,A1.BRAND_ID AS BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN A1.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x54c1;&#x724c;&#x27;&#xd;&#xa;         WHEN A1.BRAND_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          A4.BRAND_NAME&#xd;&#xa;       END AS BRAND_NAME --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_QP_QTY&#x29; AS CUR_M_QP_QTY --&#x5f53;&#x6708;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_QP_PLAN_QTY&#x29; AS CUR_M_QP_PLAN_QTY --&#x5f53;&#x6708;&#x542f;&#x7968;&#x8ba1;&#x5212;&#x91cf;&#xd;&#xa;      ,GREATEST&#x28;ROUND&#x28;SUM&#x28;A1.CUR_M_QP_FNSH_RATE&#x29;, 2&#x29;, 0.00&#x29; AS CUR_M_QP_FNSH_RATE --&#x5f53;&#x6708;&#x542f;&#x7968;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_ONTIME_ARRIVE_QTY&#x29; AS CUR_M_ONTIME_ARRIVE_QTY --&#x5f53;&#x6708;&#x51c6;&#x65f6;&#x5230;&#x8fbe;&#x91cf;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_DLV_QTY&#x29; AS CUR_M_DLV_QTY --&#x5f53;&#x6708;&#x4ea4;&#x4ed8;&#x91cf;&#xd;&#xa;      ,ROUND&#x28;SUM&#x28;A1.CUR_M_ONTIME_DLV_RATE&#x29;, 2&#x29; AS CUR_M_ONTIME_DLV_RATE --&#x5f53;&#x6708;&#x51c6;&#x65f6;&#x4ea4;&#x4ed8;&#x7387;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_TRANS_BIZ_QTY&#x29; AS CUR_M_TRANS_BIZ_QTY --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x6b21;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_TRANS_PLAN_QTY&#x29; AS CUR_M_TRANS_PLAN_QTY --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x8ba1;&#x5212;&#x91cf;&#x6b21;&#xd;&#xa;      ,GREATEST&#x28;ROUND&#x28;SUM&#x28;A1.CUR_M_TRANS_FNSH_RATE&#x29;, 2&#x29;, 0.00&#x29; AS CUR_M_TRANS_FNSH_RATE --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x91cf;&#x6b21;&#x5b8c;&#x6210;&#x7387;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_TP_BIZ_NUM&#x29; AS CUR_M_TRANS_BIZ_QTY --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,SUM&#x28;A1.CUR_M_TP_PLAN_NUM&#x29; AS CUR_M_TRANS_PLAN_QTY --&#x5f53;&#x6708;&#x8fd0;&#x8f93;&#x8ba1;&#x5212;&#x91cf;&#xd;&#xa;      ,GREATEST&#