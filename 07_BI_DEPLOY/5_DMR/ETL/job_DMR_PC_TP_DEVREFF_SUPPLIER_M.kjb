    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--target table &#x3a; DMR_PC_TP_DEVREFF_SUPPLIER_M &#x4ea4;&#x4ed8;&#x4e0e;&#x6548;&#x7387;&#x7ba1;&#x7406;&#xff08;&#x6708;&#xff09;-&#x627f;&#x8fd0;&#x5546;&#x7ba1;&#x7406;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_DEVREFF_SUPPLIER_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;    SELECT T.PERIOD_WID&#xd;&#xa;          ,T.SOURCE_CITY_ID&#xd;&#xa;          ,T.SOURCE_CITY_NAME&#xd;&#xa;          ,T.DEST_CITY_ID&#xd;&#xa;          ,T.DEST_CITY_NAME&#xd;&#xa;          ,T.BRAND_ID&#xd;&#xa;          ,T.BRAND_NAME&#xd;&#xa;          ,T.RGN_ID&#xd;&#xa;          ,T.RGN_NAME&#xd;&#xa;          ,T.SUPPLIER_ID&#xd;&#xa;          ,T.SUPPLIER_NAME&#xd;&#xa;          ,T.RETURN_ORDER_NUM&#xd;&#xa;          ,T.NOT_RETURN_ORDER_NUM&#xd;&#xa;          ,T.NOT_RETURN_AMOUNT_COST AS NOT_RETURN_ORDER_COST_AMT&#xd;&#xa;          ,NULL AS TP_CAP_NEED_NUM&#xd;&#xa;          ,T2.KPI AS KPI_NUM&#xd;&#xa;          ,NULL AS DLR_SATISFACTION&#xd;&#xa;          ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;          ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;YYYY-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;          ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;          ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;      FROM TEMP.TEMP_DMR_PC_TP_DEVREFF_SUPPLIER_M_ORDER_NUM T&#xd;&#xa;      LEFT JOIN TEMP.TEMP_DMR_PC_TP_DEVREFF_SUPPLIER_M_KPI T2&#xd;&#xa;        ON T.RGN_NAME &#x3d; T2.RGN_NAME&#xd;&#xa;       AND T.SUPPLIER_NAME &#x3d; T2.SERVPROV_NAME</sql>
