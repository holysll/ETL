      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_DEVREFF_SUPPLIER_M_ORDER_NUM&#xd;&#xa;SELECT PERIOD_WID&#xd;&#xa;      ,SOURCE_CITY_ID&#xd;&#xa;      ,SOURCE_CITY_NAME&#xd;&#xa;      ,DEST_CITY_ID&#xd;&#xa;      ,DEST_CITY_NAME&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,BRAND.BRAND_NAME AS BRAND_NAME&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,RGN.RGN_NAME AS RGN_NAME&#xd;&#xa;      ,SUPPLIER_ID&#xd;&#xa;      ,SP.SERVPROV_NAME AS SUPPLIER_NAME&#xd;&#xa;      ,SUM&#x28;RETURN_ORDER_NUM&#x29; AS RETURN_ORDER_NUM&#xd;&#xa;      ,SUM&#x28;RETURN_ORDER_AMOUNT_COST&#x29; AS RETURN_ORDER_AMOUNT_COST&#xd;&#xa;      ,SUM&#x28;NOT_RETURN_ORDER_NUM&#x29; AS NOT_RETURN_ORDER_NUM&#xd;&#xa;      ,SUM&#x28;NOT_RETURN_AMOUNT_COST&#x29; AS NOT_RETURN_AMOUNT_COST&#xd;&#xa;  FROM &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,T.SOURCE_CTY_ID AS SOURCE_CITY_ID&#xd;&#xa;              ,T.SOURCE_CTY_ID AS SOURCE_CITY_NAME&#xd;&#xa;              ,T.DEST_CTY_ID AS DEST_CITY_ID&#xd;&#xa;              ,T.DEST_CTY_ID AS DEST_CITY_NAME&#xd;&#xa;              ,T.BRAND_ID AS BRAND_ID&#xd;&#xa;              ,T.RGN_ID AS RGN_ID&#xd;&#xa;              ,T.SUPPLIER_ID AS SUPPLIER_ID&#xd;&#xa;              ,COUNT&#x28;T.ORDER_RELEASE_GID&#x29; AS RETURN_ORDER_NUM&#xd;&#xa;              ,SUM&#x28;OM.AMOUNT_COST&#x29; AS RETURN_ORDER_AMOUNT_COST&#xd;&#xa;              ,0 AS NOT_RETURN_ORDER_NUM&#xd;&#xa;              ,0 AS NOT_RETURN_AMOUNT_COST&#xd;&#xa;          FROM DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;          LEFT JOIN DW.DW_TP_ORDERMOVEMENT OM&#xd;&#xa;            ON T.ORDER_RELEASE_GID &#x3d; OM.ORDER_RELEASE_GID&#xd;&#xa;         WHERE OM.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND SUBSTRING&#x28;T.RECEIPT_DATE, 1, 7&#x29; &#x3d; SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29; -- IS NOT NULL&#xd;&#xa;         GROUP BY T.SOURCE_CTY_ID&#xd;&#xa;                 ,T.SOURCE_CTY_ID&#xd;&#xa;                 ,T.DEST_CTY_ID&#xd;&#xa;                 ,T.DEST_CTY_ID&#xd;&#xa;                 ,T.BRAND_ID&#xd;&#xa;                 ,T.RGN_ID&#xd;&#xa;                 ,T.SUPPLIER_ID&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,T.SOURCE_CTY_ID AS SOURCE_CITY_ID&#xd;&#xa;              ,T.SOURCE_CTY_ID AS SOURCE_CITY_NAME&#xd;&#xa;              ,T.DEST_CTY_ID AS DEST_CITY_ID&#xd;&#xa;              ,T.DEST_CTY_ID AS DEST_CITY_NAME&#xd;&#xa;              ,T.BRAND_ID AS BRAND_ID&#xd;&#xa;              ,T.RGN_ID AS RGN_ID&#xd;&#xa;              ,T.SUPPLIER_ID AS SUPPLIER_ID&#xd;&#xa;              ,0 AS RETURN_ORDER_NUM&#xd;&#xa;              ,0 AS RETURN_ORDER_AMOUNT_COST&#xd;&#xa;              ,COUNT&#x28;T.ORDER_RELEASE_GID&#x29; AS NOT_RETURN_ORDER_NUM&#xd;&#xa;              ,SUM&#x28;OM.AMOUNT_COST&#x29; AS NOT_RETURN_AMOUNT_COST&#xd;&#xa;          FROM DMK.DMK_TP_DTD_PROG_TIME_D T&#xd;&#xa;          LEFT JOIN DW.DW_TP_ORDERMOVEMENT OM&#xd;&#xa;            ON T.ORDER_RELEASE_GID &#x3d; OM.ORDER_RELEASE_GID&#xd;&#xa;         WHERE OM.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND T.RECEIPT_DATE IS NULL --&#x672a;&#x56de;&#x5355;&#x6570;&#xd;&#xa;         GROUP BY T.SOURCE_CTY_ID&#xd;&#xa;                 ,T.SOURCE_CTY_ID&#xd;&#xa;                 ,T.DEST_CTY_ID&#xd;&#xa;                 ,T.DEST_CTY_ID&#xd;&#xa;                 ,T.BRAND_ID&#xd;&#xa;                 ,T.RGN_ID&#xd;&#xa;                 ,T.SUPPLIER_ID&#x29; S&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID, RGN_NAME FROM DIM.DIM_ORG WHERE ORG_TYPE &#x3d; &#x27;R&#x27; AND ENABLE_FLAG &#x3d; &#x27;T&#x27;&#x29; RGN&#xd;&#xa;    ON S.RGN_ID &#x3d; RGN.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID, BRAND_NAME FROM DIM.DIM_BRAND WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27;&#x29; BRAND&#xd;&#xa;    ON S.BRAND_ID &#x3d; BRAND.BRAND_ID&#xd;&#xa;  LEFT JOIN DIM.DIM_SERVPROV SP&#xd;&#xa;    ON S.SUPPLIER_ID &#x3d; SP.SERVPROV_CODE&#xd;&#xa;   AND SP.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa; GROUP BY S.PERIOD_WID&#xd;&#xa;         ,S.SOURCE_CITY_ID&#xd;&#xa;         ,S.SOURCE_CITY_NAME&#xd;&#xa;         ,S.DEST_CITY_ID&#xd;&#xa;         ,S.DEST_CITY_NAME&#xd;&#xa;         ,S.BRAND_ID&#xd;&#xa;         ,BRAND.BRAND_NAME&#xd;&#xa;         ,S.RGN_ID&#xd;&#xa;         ,RGN.RGN_NAME&#xd;&#xa;         ,S.SUPPLIER_ID&#xd;&#xa;         ,SP.SERVPROV_NAME</sql>
