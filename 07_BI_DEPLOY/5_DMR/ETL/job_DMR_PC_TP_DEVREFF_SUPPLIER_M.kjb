    <entry>
      <name>CREATE_TEMP</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_DEVREFF_SUPPLIER_M_KPI&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_DEVREFF_SUPPLIER_M_KPI&#xd;&#xa;&#x28;&#xd;&#xa;    PERIOD_WID                   STRING                     COMMENT  &#x27;&#x6708;&#x4efd;ID&#x27;,&#xd;&#xa;    RGN_NAME                     STRING                     COMMENT  &#x27;&#x5927;&#x533a;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;    SERVPROV_NAME                STRING                     COMMENT  &#x27;&#x627f;&#x8fd0;&#x5546;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;    KPI                          DECIMAL&#x28;32, 8&#x29;             COMMENT  &#x27;KPI&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;COMMENT &#x27;temporary table for DMR_PC_TP_DEVREFF_SUPPLIER_M - KPI&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27; STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x4ee5;&#x4e0b;query&#x5f97;&#x51fa;&#x56de;&#x5355;&#x91cf;&#xff0c;&#x672a;&#x56de;&#x5355;&#x91cf;&#x548c;&#x672a;&#x56de;&#x5355;&#x4ea7;&#x503c;&#x6210;&#x672c;&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_DEVREFF_SUPPLIER_M_ORDER_NUM&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_DEVREFF_SUPPLIER_M_ORDER_NUM&#xd;&#xa;&#x28;&#xd;&#xa;    PERIOD_WID                   STRING                   COMMENT &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;    SOURCE_CITY_ID               STRING                   COMMENT &#x27;&#x51fa;&#x53d1;&#x57ce;&#x5e02;ID&#x27;,&#xd;&#xa;    SOURCE_CITY_NAME             STRING                   COMMENT &#x27;&#x51fa;&#x53d1;&#x57ce;&#x5e02;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;    DEST_CITY_ID                 STRING                   COMMENT &#x27;&#x76ee;&#x7684;&#x57ce;&#x5e02;ID&#x27;,&#xd;&#xa;    DEST_CITY_NAME               STRING                   COMMENT &#x27;&#x76ee;&#x7684;&#x57ce;&#x5e02;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;    BRAND_ID                     STRING                   COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;    BRAND_NAME                   STRING                   COMMENT &#x27;&#x54c1;&#x724c;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;    RGN_ID                       STRING                   COMMENT &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;    RGN_NAME                     STRING                   COMMENT &#x27;&#x5927;&#x533a;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;    SUPPLIER_ID                  STRING                   COMMENT &#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;,&#xd;&#xa;    SUPPLIER_NAME                STRING                   COMMENT &#x27;&#x627f;&#x8fd0;&#x5546;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;    RETURN_ORDER_NUM             BIGINT                   COMMENT &#x27;&#x56de;&#x5355;&#x91cf;&#x27;,&#xd;&#xa;    RETURN_ORDER_AMOUNT_COST     DECIMAL&#x28;32, 8&#x29;           COMMENT &#x27;&#x56de;&#x5355;&#x6210;&#x672c;&#x27;,&#xd;&#xa;    NOT_RETURN_ORDER_NUM         BIGINT                   COMMENT &#x27;&#x672a;&#x56de;&#x5355;&#x91cf;&#x27;,&#xd;&#xa;    NOT_RETURN_AMOUNT_COST       DECIMAL&#x28;32, 8&#x29;           COMMENT &#x27;&#x672a;&#x56de;&#x5355;&#x4ea7;&#x503c;&#x6210;&#x672c;&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;COMMENT &#x27;temporary table for DMR_PC_TP_DEVREFF_SUPPLIER_M - ORDER NUM&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27; STORED AS PARQUET&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
