    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMR&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_QAT_MANAGE_M PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT T.month_id&#xd;&#xa;    ,T.rgn_id&#xd;&#xa;    ,T1.RGN_NAME&#xd;&#xa;    ,T.base_id&#xd;&#xa;    ,T2.BASE_NAME&#xd;&#xa;    ,T.brand_id&#xd;&#xa;    ,T3.BRAND_NAME&#xd;&#xa;    ,&#x27;-1&#x27; AS supplier_id&#xd;&#xa;    ,&#x27;-1&#x27; AS SUPPLIER_NAME&#xd;&#xa;    ,T.tpmode_id&#xd;&#xa;    ,T5.TPMODE_NAME&#xd;&#xa;    ,T.MASSLOSE_QTY&#xd;&#xa;    ,T.INTRAINT_QTY&#xd;&#xa;    ,T.los_qat_RATE&#xd;&#xa;    ,T.TIME_SCHEDULE&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DATE  --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT&#xd;&#xa;FROM TEMP.TEMP_DMR_PC_TP_QAT_MANAGE_M_1 T&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.DIM_ORG&#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29;T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT base_id,base_name&#xd;&#xa;    FROM DIM.DIM_ORG&#xd;&#xa;    WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY base_id,base_name&#xd;&#xa;    &#x29;T2&#xd;&#xa;ON T.base_id &#x3d; T2.base_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.DIM_BRAND&#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29;T3&#xd;&#xa;ON T.brand_id &#x3d; T3.brand_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT tpmode_id,tpmode_name&#xd;&#xa;    FROM DIM.dim_transport_mode&#xd;&#xa;    GROUP BY tpmode_id,tpmode_name&#xd;&#xa;    &#x29;T5&#xd;&#xa;ON T.tpmode_id &#x3d; T5.tpmode_id</sql>
