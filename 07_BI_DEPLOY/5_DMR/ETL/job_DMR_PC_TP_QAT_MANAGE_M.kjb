      </entry>
    <entry>
      <name>CREATE_TEMP_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_QAT_MANAGE_M_1&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_QAT_MANAGE_M_1&#x28;&#xd;&#xa;MONTH_ID STRING    COMMENT&#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID	STRING	COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID STRING    COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID STRING   COMMENT&#x27;&#x54c1;&#x724c;&#x27;&#xd;&#xa;,TPMODE_ID	STRING	COMMENT&#x27;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;ID&#x27;&#xd;&#xa;,MASSLOSE_QTY BIGINT COMMENT &#x27;&#x5728;&#x9014;&#x8d28;&#x635f;&#x6570;&#x27;&#xd;&#xa;,INTRAINT_QTY BIGINT COMMENT &#x27;&#x603b;&#x5728;&#x9014;&#x6570;&#x27;&#xd;&#xa;,LOS_QAT_RATE DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x5728;&#x9014;&#x8d28;&#x635f;&#x7387;&#x27;&#xd;&#xa;,TIME_SCHEDULE	DECIMAL&#x28;32,8&#x29;	COMMENT&#x27;&#x65f6;&#x95f4;&#x8fdb;&#x5ea6;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;PC&#x8d28;&#x91cf;&#x6708;&#x627f;&#x8fd0;&#x5546;&#x7ba1;&#x7406;-&#x8d28;&#x91cf;&#x7ba1;&#x7406;&#x4e34;&#x65f6;&#x8868;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;</sql>
