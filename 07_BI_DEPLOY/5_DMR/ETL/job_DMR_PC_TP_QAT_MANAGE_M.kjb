    <entry>
      <name>TO_TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_QAT_MANAGE_M_1 &#xd;&#xa;SELECT B.PERIOD_WID AS MONTH_ID&#xd;&#xa;      ,B.RGN_ID&#xd;&#xa;      ,B.BASE_ID&#xd;&#xa;      ,B.BRAND_ID&#xd;&#xa;      ,B.TPMODE_ID&#xd;&#xa;      ,B.MASSLOSE_QTY&#xd;&#xa;      ,B.INTRAINT_QTY&#xd;&#xa;      ,NVL&#x28;B.MASSLOSE_RATE, 0&#x29; AS LOS_QAT_RATE&#xd;&#xa;      ,CAST&#x28;SUBSTRING&#x28;B.PERIOD_WID, 5, 2&#x29; AS INT&#x29; &#x2f; 12 AS TIME_SCHEDULE&#xd;&#xa;  FROM &#x28;SELECT COALESCE&#x28;A.PERIOD_WID, P.PERIOD_WID&#x29; AS PERIOD_WID&#xd;&#xa;              ,COALESCE&#x28;A.RGN_ID, P.RGN_ID&#x29; AS RGN_ID&#xd;&#xa;              ,COALESCE&#x28;A.BASE_ID, P.BASE_ID&#x29; AS BASE_ID&#xd;&#xa;              ,COALESCE&#x28;A.BRAND_ID, P.BRAND_ID&#x29; AS BRAND_ID&#xd;&#xa;              ,COALESCE&#x28;A.TPMODE_ID, P.TPMODE_ID&#x29; AS TPMODE_ID&#xd;&#xa;              ,A.MASSLOSE_QTY AS MASSLOSE_QTY&#xd;&#xa;              ,P.PLAN_QTY_MON AS INTRAINT_QTY&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN NVL&#x28;P.PLAN_QTY_MON, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;                  0&#xd;&#xa;                 ELSE&#xd;&#xa;                  A.MASSLOSE_QTY &#x2f; P.PLAN_QTY_MON&#xd;&#xa;               END AS MASSLOSE_RATE&#xd;&#xa;          FROM &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;T.RECEIVING_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PERIOD_WID --&#x671f;&#x95f4;&#xd;&#xa;                      ,T.RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;                      ,T.BASE_ID&#xd;&#xa;                      ,T.BRAND_ID&#xd;&#xa;                      ,TP.TPMODE_ID AS TPMODE_ID&#xd;&#xa;                      ,COUNT&#x28;1&#x29; AS MASSLOSE_QTY --&#x5728;&#x9014;&#x8d28;&#x635f;&#x6570;&#xd;&#xa;                  FROM DW.DW_TP_INROAD_LOSS T&#xd;&#xa;                  LEFT JOIN &#x28;SELECT TPMODE_ID&#xd;&#xa;                                  ,TPMODE_NAME&#xd;&#xa;                              FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;                             GROUP BY TPMODE_ID&#xd;&#xa;                                     ,TPMODE_NAME&#x29; TP&#xd;&#xa;                    ON SUBSTRING&#x28;T.TRANSPORT_MODE, 1, 2&#x29; &#x3d; TP.TPMODE_NAME&#xd;&#xa;                 WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;T.RECEIVING_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;                 GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;T.RECEIVING_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                         ,T.RGN_ID&#xd;&#xa;                         ,T.BASE_ID&#xd;&#xa;                         ,T.BRAND_ID&#xd;&#xa;                         ,TP.TPMODE_ID&#x29; AS A&#xd;&#xa;          FULL JOIN &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;PLN.MONTH_ID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29; AS PERIOD_WID&#xd;&#xa;                          ,RGN.RGN_ID&#xd;&#xa;                          ,BASE.BASE_ID&#xd;&#xa;                          ,BRAND.BRAND_ID&#xd;&#xa;                          ,TP.TPMODE_ID&#xd;&#xa;                          ,SUM&#x28;PLN.REV_PLAN&#x29; AS PLAN_QTY_MON&#xd;&#xa;                      FROM ODS_BLPT.T_HAND_REV_TICKET_PLAN PLN&#xd;&#xa;                      LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;                        ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                       AND PLN.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;                      LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                                      ,BASE_NAME&#xd;&#xa;                                  FROM DIM.DIM_ORG&#xd;&#xa;                                 WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                                 GROUP BY BASE_ID&#xd;&#xa;                                         ,BASE_NAME&#x29; BASE&#xd;&#xa;                        ON PLN.BASE_NAME &#x3d; BASE.BASE_NAME&#xd;&#xa;                      LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                                      ,BRAND_NAME&#xd;&#xa;                                  FROM DIM.DIM_BRAND&#xd;&#xa;                                 GROUP BY BRAND_ID&#xd;&#xa;                                         ,BRAND_NAME&#x29; BRAND&#xd;&#xa;                        ON PLN.BRAND_NAME &#x3d; BRAND.BRAND_NAME&#xd;&#xa;                      LEFT JOIN &#x28;SELECT TPMODE_ID&#xd;&#xa;                                      ,TPMODE_NAME&#xd;&#xa;                                  FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;                                 GROUP BY TPMODE_ID&#xd;&#xa;                                         ,TPMODE_NAME&#x29; TP&#xd;&#xa;                        ON PLN.TP_MODE &#x3d; TP.TPMODE_NAME&#xd;&#xa;                     WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;PLN.MONTH_ID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29;&#xd;&#xa;                     GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;PLN.MONTH_ID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                     1,&#xd;&#xa;                                     6&#x29;&#xd;&#xa;                             ,RGN.RGN_ID&#xd;&#xa;                             ,BASE.BASE_ID&#xd;&#xa;                             ,BRAND.BRAND_ID&#xd;&#xa;                             ,TP.TPMODE_ID&#x29; AS P&#xd;&#xa;            ON A.PERIOD_WID &#x3d; P.PERIOD_WID&#xd;&#xa;           AND A.RGN_ID &#x3d; P.RGN_ID&#xd;&#xa;           AND A.BASE_ID &#x3d; P.BASE_ID&#xd;&#xa;           AND A.BRAND_ID &#x3d; P.BRAND_ID&#xd;&#xa;           AND A.TPMODE_ID &#x3d; P.TPMODE_ID&#x29; AS B&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
