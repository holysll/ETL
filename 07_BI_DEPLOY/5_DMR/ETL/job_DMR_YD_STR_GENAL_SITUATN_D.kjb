#xd;&#xa;CUR_D_STR_INV_QTY	DECIMAL&#x28;32,8&#x29;			COMMENT	&#x27;&#x73b0;&#x6709;&#x5e93;&#x5b58;&#x603b;&#x91cf;&#x27;,&#xd;&#xa;CUR_M_STR_BIZ_QTY	BIGINT			COMMENT	&#x27;&#x27;,&#xd;&#xa;LST_M_STR_BIZ_QTY	BIGINT			COMMENT	&#x27;&#x27;,&#xd;&#xa;LST_Y_M_STR_BIZ_QTY	BIGINT			COMMENT	&#x27;&#x27;,&#xd;&#xa;CUR_Y_STR_BIZ_QTY	BIGINT			COMMENT	&#x27;&#x27;,&#xd;&#xa;LST_Y_Y_STR_BIZ_QTY	BIGINT			COMMENT	&#x27;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4ed3;&#x50a8;&#x6982;&#x51b5;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_STR_GENAL_SITUATN_D_02&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_STR_GENAL_SITUATN_D_02&#x28;&#xd;&#xa;RGN_ID           STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID          STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BIZ_QTY_M        BIGINT        COMMENT  &#x27;&#x672c;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_LM       BIGINT        COMMENT  &#x27;&#x4e0a;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_LYM      BIGINT        COMMENT  &#x27;&#x53bb;&#x5e74;&#x540c;&#x671f;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_Y        BIGINT        COMMENT  &#x27;&#x672c;&#x5e74;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_LY       BIGINT        COMMENT  &#x27;&#x53bb;&#x5e74;&#x540c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4ed3;&#x50a8;&#x6982;&#x51b5;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_STR_GENAL_SITUATN_D_03&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_STR_GENAL_SITUATN_D_03&#x28;&#xd;&#xa;RGN_ID           STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID          STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;USE_QTY          BIGINT        COMMENT  &#x27;&#x4f7f;&#x7528;&#x5e93;&#x4f4d;&#x91cf;&#x27;,&#xd;&#xa;ALL_QTY          BIGINT        COMMENT  &#x27;&#x603b;&#x5e93;&#x4f4d;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_M        BIGINT        COMMENT  &#x27;&#x672c;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_LM       BIGINT        COMMENT  &#x27;&#x4e0a;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_LYM      BIGINT        COMMENT  &#x27;&#x53bb;&#x5e74;&#x540c;&#x671f;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_Y        BIGINT        COMMENT  &#x27;&#x672c;&#x5e74;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_LY       BIGINT        COMMENT  &#x27;&#x53bb;&#x5e74;&#x540c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4ed3;&#x50a8;&#x6982;&#x51b5;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_STR_GENAL_SITUATN_D_04&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_STR_GENAL_SITUATN_D_04&#x28;&#xd;&#xa;RGN_ID           STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;RGN_NAME         STRING        COMMENT  &#x27;&#x5927;&#x533a;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;BASE_ID          STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BASE_NAME        STRING        COMMENT  &#x27;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;BRAND_ID         STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;BRAND_NAME       STRING        COMMENT  &#x27;&#x54c1;&#x724c;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ALL_QTY          BIGINT        COMMENT  &#x27;&#x603b;&#x5e93;&#x4f4d;&#x91cf;&#x27;,&#xd;&#xa;USE_QTY          BIGINT        COMMENT  &#x27;&#x4f7f;&#x7528;&#x5e93;&#x4f4d;&#x91cf;&#x27;,&#xd;&#xa;USE_RATE         DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x5e93;&#x623f;&#x5229;&#x7528;&#x7387;&#x27;, &#xd;&#xa;BIZ_QTY_M        BIGINT        COMMENT  &#x27;&#x672c;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_LM       BIGINT        COMMENT  &#x27;&#x4e0a;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_LYM      BIGINT        COMMENT  &#x27;&#x53bb;&#x5e74;&#x540c;&#x671f;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_Y        BIGINT        COMMENT  &#x27;&#x672c;&#x5e74;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_QTY_LY       BIGINT        COMMENT  &#x27;&#x53bb;&#x5e74;&#x540c;&#x671f;&#x5e74;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;CHAIN_RATE_M     DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x73af;&#x6bd4;&#x27;,&#xd;&#xa;YOY_RATE_M       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x6708;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x540c;&#x6bd4;&#x27;,&#xd;&#xa;YOY_RATE_Y       DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x5e74;&#x7d2f;&#x8ba1;&#x4e1a;&#x52a1;&#x91cf;&#x540c;&#x6bd4;&#x27;,&#xd;&#xa;COLOR_FLAG1      STRING        COMMENT  &#x27;&#x5e93;&#x623f;&#x5229;&#x7528;&#x7387;&#x989c;&#x8272;&#x6807;&#x8bb0;&#x27;,&#xd;&#xa;COLOR_FLAG2      STRING        COMMENT  &#x27;&#x672c;&#x6708;&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x91cf;&#x73af;&#x6bd4;&#x989c;&#x8272;&#x6807;&#x8bb0;&#x27;,&#xd;&#xa;COLOR_FLAG3      STRING        COMMENT  &#x27;&#x672c;&#x6708;&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x91cf;&#x540c;&#x6bd4;&#x989c;&#x8272;&#x6807;&#x8bb0;&#x27;,&#xd;&#xa;COLOR_FLAG4      STRING        COMMENT  &#x27;&#x672c;&#x5e74;&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x91cf;&#x540c;&#x6bd4;&#x989c;&#x8272;&#x6807;&#x8bb0;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4ed3;&#x50a8;&#x6982;&#x51b5;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#x2a;&#x2f;</sql>
