    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_STR_GENAL_SITUATN_D PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID AS DATE_ID&#xd;&#xa;      ,T.RGN_ID AS RNG_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.RGN_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T1.RGN_NAME&#xd;&#xa;       END AS RNG_NAME&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.BASE_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.BASE_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T2.BASE_NAME&#xd;&#xa;       END AS BASE_NAME&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN&#xd;&#xa;          &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;-1&#x27; THEN&#xd;&#xa;          &#x27;&#x672a;&#x77e5;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          T3.BRAND_NAME&#xd;&#xa;       END AS BRAND_NAME&#xd;&#xa;      ,SUM&#x28;T.TOTAL_STR_LOCATION_QTY&#x29; AS TOTAL_STR_LOCATION_QTY --&#x5e93;&#x623f;&#x5e93;&#x4f4d;&#x603b;&#x91cf;&#xd;&#xa;      ,SUM&#x28;T.CUR_D_STR_INV_QTY&#x29; AS CUR_D_STR_INV_QTY --&#x73b0;&#x6709;&#x5e93;&#x5b58;&#x603b;&#x91cf;&#xd;&#xa;      ,SUM&#x28;T.CUR_D_STR_INV_QTY&#x29; &#x2f; SUM&#x28;T.TOTAL_STR_LOCATION_QTY&#x29; &#x2a; 100 AS STR_LOCATION_USE_RATE --&#x5e93;&#x623f;&#x5229;&#x7528;&#x7387;&#xd;&#xa;      ,SUM&#x28;T.CUR_M_STR_BIZ_QTY&#x29; AS CUR_M_STR_BIZ_QTY --&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;      ,SUM&#x28;T.LST_M_STR_BIZ_QTY&#x29; AS LST_M_STR_BIZ_QTY --&#x4e0a;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;      ,SUM&#x28;T.LST_Y_M_STR_BIZ_QTY&#x29; AS LST_Y_M_STR_BIZ_QTY --&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#xd;&#xa;      ,SUM&#x28;T.CUR_Y_STR_BIZ_QTY&#x29; AS CUR_Y_STR_BIZ_QTY --&#x5f53;&#x6708;&#x5e74;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;      ,SUM&#x28;LST_Y_Y_STR_BIZ_QTY&#x29; AS LST_Y_Y_STR_BIZ_QTY --&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x5e74;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;      ,&#x28;SUM&#x28;CUR_M_STR_BIZ_QTY&#x29; &#x2f; SUM&#x28;T.LST_M_STR_BIZ_QTY&#x29; - 1&#x29; &#x2a; 100 AS STR_BIZ_M_MOM --&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x91cf;&#x73af;&#x6bd4;&#xd;&#xa;      ,&#x28;SUM&#x28;CUR_M_STR_BIZ_QTY&#x29; &#x2f; SUM&#x28;T.LST_Y_M_STR_BIZ_QTY&#x29; - 1&#x29; &#x2a; 100 AS STR_BIZ_M_YOY --&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x91cf;&#x540c;&#x6bd4;&#xd;&#xa;      ,&#x28;SUM&#x28;T.CUR_Y_STR_BIZ_QTY&#x29; &#x2f; SUM&#x28;LST_Y_Y_STR_BIZ_QTY&#x29; - 1&#x29; &#x2a; 100 AS STR_BIZ_Y_YOY --&#x5f53;&#x5e74;&#x4ed3;&#x50a8;&#x4e1a;&#x52a1;&#x91cf;&#x540c;&#x6bd4;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN SUM&#x28;T.CUR_D_STR_INV_QTY&#x29; &#x2f; SUM&#x28;T.TOTAL_STR_LOCATION_QTY&#x29; &#x2a; 100 &#x3c; 80 THEN&#xd;&#xa;          1&#xd;&#xa;         WHEN SUM&#x28;T.CUR_D_STR_INV_QTY&#x29; &#x2f; SUM&#x28;T.TOTAL_STR_LOCATION_QTY&#x29; &#x2a; 100 &#x3e;&#x3d; 80 AND&#xd;&#xa;              SUM&#x28;T.CUR_D_STR_INV_QTY&#x29; &#x2f; SUM&#x28;T.TOTAL_STR_LOCATION_QTY&#x29; &#x2a; 100 &#x3c;&#x3d; 90 THEN&#xd;&#xa;          0&#xd;&#xa;         WHEN SUM&#x28;T.CUR_D_STR_INV_QTY&#x29; &#x2f; SUM&#x28;T.TOTAL_STR_LOCATION_QTY&#x29; &#x2a; 100 &#x3e; 90 THEN&#xd;&#xa;          -1&#xd;&#xa;         ELSE&#xd;&#xa;          NULL&#xd;&#xa;       END AS USE_WRN_FLAG --&#x5229;&#x7528;&#x7387;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,IF&#x28;&#x28;SUM&#x28;CUR_M_STR_BIZ_QTY&#x29; &#x2f; SUM&#x28;T.LST_M_STR_BIZ_QTY&#x29; - 1&#x29; &#x2a; 100 &#x3e;&#x3d; 0,&#xd;&#xa;          1,&#xd;&#xa;          -1&#x29; AS MOM_WRN_FLAG --&#x6708;&#x73af;&#x6bd4;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,IF&#x28;&#x28;SUM&#x28;CUR_M_STR_BIZ_QTY&#x29; &#x2f; SUM&#x28;T.LST_Y_M_STR_BIZ_QTY&#x29; - 1&#x29; &#x2a; 100 &#x3e;&#x3d; 0,&#xd;&#xa;          1,&#xd;&#xa;          -1&#x29; AS YOY_WRN_FLAG --&#x6708;&#x540c;&#x6bd4;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,IF&#x28;&#x28;SUM&#x28;T.CUR_Y_STR_BIZ_QTY&#x29; &#x2f; SUM&#x28;LST_Y_Y_STR_BIZ_QTY&#x29; - 1&#x29; &#x2a; 100 &#x3e;&#x3d; 0,&#xd;&#xa;          1,&#xd;&#xa;          -1&#x29; AS YOY_Y_WRN_FLAG --&#x5e74;&#x540c;&#x6bd4;&#x9884;&#x8b66;&#x6807;&#x5fd7;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_YD_STR_GENAL_SITUATN_D_01&#xd;&#xa;         WHERE 1&#x3d;1&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_YD_STR_GENAL_SITUATN_D_02&#xd;&#xa;         WHERE 1&#x3d;1&#x29; T&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; T1&#xd;&#xa;    ON T.RGN_ID &#x3d; T1.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; T2&#xd;&#xa;    ON T.BASE_ID &#x3d; T2.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; T3&#xd;&#xa;    ON T.BRAND_ID &#x3d; T3.BRAND_ID&#xd;&#xa; GROUP BY T.PERIOD_WID&#xd;&#xa;         ,T.RGN_ID&#xd;&#xa;         ,T.BASE_ID&#xd;&#xa;         ,T.BRAND_ID&#xd;&#xa;         ,T1.RGN_NAME&#xd;&#xa;         ,T2.BASE_NAME&#xd;&#xa;         ,T3.BRAND_NAME&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>400</xloc>
      <yloc>272</yloc>
      </entry>
    <entry>
      <name>TO_TMP1</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_STR_GENAL_SITUATN_D_01&#xd;&#xa;SELECT B.PERIOD_WID&#xd;&#xa;      ,B.RGN_ID&#xd;&#xa;      ,B.BASE_ID&#xd;&#xa;      ,B.BRAND_ID&#xd;&#xa;      ,B.CUR_D_STR_CAPACITY AS TOTAL_STR_LOCATION_QTY --&#x5e93;&#x623f;&#x5bb9;&#x91cf;&#xd;&#xa;      ,B.CUR_D_STR_INV_QTY  AS CUR_D_STR_INV_QTY --&#x73b0;&#x6709;&#x5e93;&#x5b58;&#x91cf; &#xd;&#xa;      ,0                    AS CUR_M_STR_BIZ_QTY --&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;      ,0                    AS LST_M_STR_BIZ_QTY --&#x4e0a;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;      ,0                    AS LST_Y_M_STR_BIZ_QTY --&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#xd;&#xa;      ,0                    AS CUR_Y_STR_BIZ_QTY --&#x5f53;&#x5e74;&#x5e74;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;      ,0                    AS LST_Y_Y_STR_BIZ_QTY --&#x4e0a;&#x5e74;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;  FROM &#x28;SELECT A.PERIOD_WID&#xd;&#xa;              ,A.RGN_ID&#xd;&#xa;              ,A.BASE_ID&#xd;&#xa;              ,A.BRAND_ID&#xd;&#xa;              ,SUM&#x28;A.CUR_D_STR_CAPACITY&#x29; AS CUR_D_STR_CAPACITY --&#x5e93;&#x623f;&#x5bb9;&#x91cf;&#xd;&#xa;              ,SUM&#x28;A.CUR_D_STR_INV_QTY&#x29; AS CUR_D_STR_INV_QTY --&#x73b0;&#x6709;&#x5e93;&#x5b58;&#x91cf; &#xd;&#xa;          FROM &#x28;SELECT X1.PERIOD_WID&#xd;&#xa;                      ,X1.RGN_ID&#xd;&#xa;                      ,X1.BASE_ID&#xd;&#xa;                      ,X1.BRAND_ID&#xd;&#xa;                      ,0 AS CUR_D_STR_CAPACITY --&#x5e93;&#x623f;&#x5bb9;&#x91cf;&#xd;&#xa;                      ,X2.CUR_D_STR_INV_QTY --&#x73b0;&#x6709;&#x5e93;&#x5b58;&#x91cf;&#xd;&#xa;                  FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                              ,T.RGN_ID&#xd;&#xa;                              ,T.BASE_ID&#xd;&#xa;                              ,T.BRAND_ID&#xd;&#xa;                          FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                          JOIN &#x28;SELECT NVL&#x28;NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                     ,NVL&#x28;NVL&#x28;B.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                     ,NVL&#x28;NVL&#x28;P.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                 FROM DMK.DMK_STR_INVENTORY_D A&#xd;&#xa;                                 LEFT JOIN DIM.DIM_PROJ_STR_R B&#xd;&#xa;                                   ON A.STR_ID &#x3d; B.STR_ID&#xd;&#xa;                                 LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;                                   ON NVL&#x28;B.PROJ_ID, &#x27;-1&#x27;&#x29; &#x3d; P.PROJ_ID&#xd;&#xa;                                WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;PERIOD_WID,&#xd;&#xa;                                                            &#x27;-&#x27;,&#xd;&#xa;                                                            &#x27;&#x27;&#x29;,&#xd;&#xa;                                             1,&#xd;&#xa;                                             6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                        &#x27;-&#x27;,&#xd;&#xa;                                                                        &#x27;&#x27;&#x29;,&#xd;&#xa;                                                         1,&#xd;&#xa;                                                         6&#x29;&#xd;&#xa;                                GROUP BY NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                        ,NVL&#x28;B.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                        ,NVL&#x28;P.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; T&#xd;&#xa;                            ON 1 &#x3d; 1&#xd;&#xa;                         WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                           AND SUBSTR&#x28;REGEXP_REPLACE&#x28;D.PERIOD_WID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      6&#x29; &#x3d;&#xd;&#xa;                               SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                      1,&#xd;&#xa;                                      6&#x29;&#x29; AS X1&#xd;&#xa;                  LEFT JOIN &#x28;SELECT PERIOD_WID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;B.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                                  ,NVL&#x28;NVL&#x28;P.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                                  ,SUM&#x28;INVENTORY_QTY&#x29; AS CUR_D_STR_INV_QTY --&#x73b0;&#x6709;&#x5e93;&#x5b58;&#x91cf;&#xd;&#xa;                              FROM DMK.DMK_STR_INVENTORY_D A&#xd;&#xa;                              LEFT JOIN DIM.DIM_PROJ_STR_R B&#xd;&#xa;                                ON A.STR_ID &#x3d; B.STR_ID&#xd;&#xa;                              LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;                                ON NVL&#x28;B.PROJ_ID, &#x27;-1&#x27;&#x29; &#x3d; P.PROJ_ID&#xd;&#xa;                             WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;PERIOD_WID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                          1,&#xd;&#xa;                                          6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                                     &#x27;-&#x27;,&#xd;&#xa;                                                                     &#x27;&#x27;&#x29;,&#xd;&#xa;                                                      1,&#xd;&#xa;                                                      6&#x29;&#xd;&#xa;                             GROUP BY PERIOD_WID&#xd;&#xa;                                     ,NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                     ,NVL&#x28;B.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                     ,NVL&#x28;P.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS X2&#xd;&#xa;                    ON X1.PERIOD_WID &#x3d; X2.PERIOD_WID&#xd;&#xa;                   AND X1.RGN_ID &#x3d; X2.RGN_ID&#xd;&#xa;                   AND X1.BASE_ID &#x3d; X2.BASE_ID&#xd;&#xa;                   AND X1.BRAND_ID &#x3d; X2.BRAND_ID&#xd;&#xa;                UNION ALL&#xd;&#xa;                SELECT D.PERIOD_WID&#xd;&#xa;                      ,X.RGN_ID&#xd;&#xa;                      ,X.BASE_ID&#xd;&#xa;                      ,X.BRAND_ID&#xd;&#xa;                      ,X.CUR_D_STR_CAPACITY --&#x5e93;&#x623f;&#x5bb9;&#x91cf;&#xd;&#xa;                      ,X.CUR_D_STR_INV_QTY --&#x73b0;&#x6709;&#x5e93;&#x5b58;&#x91cf;&#xd;&#xa;                  FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                  JOIN &#x28;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;PERIOD_WID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                     1,&#xd;&#xa;                                     6&#x29; AS PERIOD_WID&#xd;&#xa;                             ,NVL&#x28;NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                             ,NVL&#x28;NVL&#x28;B.BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                             ,NVL&#x28;NVL&#x28;P.BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                             ,SUM&#x28;A.MAX_STORE_QTY&#x29; AS CUR_D_STR_CAPACITY --&#x5e93;&#x623f;&#x5bb9;&#x91cf;&#xd;&#xa;                             ,0 AS CUR_D_STR_INV_QTY --&#x73b0;&#x6709;&#x5e93;&#x5b58;&#x91cf;&#xd;&#xa;                         FROM DMK.DMK_STR_MAXSTORE_M A&#xd;&#xa;                         LEFT JOIN DIM.DIM_PROJ_STR_R B&#xd;&#xa;                           ON A.STR_ID &#x3d; B.STR_ID&#xd;&#xa;                         LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;                           ON NVL&#x28;B.PROJ_ID, &#x27;-1&#x27;&#x29; &#x3d; P.PROJ_ID&#xd;&#xa;                        WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;A.PERIOD_WID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                     1,&#xd;&#xa;                                     6&#x29; &#x3d;&#xd;&#xa;                              SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                     1,&#xd;&#xa;                                     6&#x29;&#xd;&#xa;                        GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;PERIOD_WID, &#x27;-&#x27;, &#x27;&#x27;&#x29;,&#xd;&#xa;                                        1,&#xd;&#xa;                                        6&#x29;&#xd;&#xa;                                ,NVL&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                ,NVL&#x28;B.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                ,NVL&#x28;P.BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; X&#xd;&#xa;                    ON SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d; X.PERIOD_WID&#xd;&#xa;                 WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                   AND SUBSTR&#x28;REGEXP_REPLACE&#x28;D.PERIOD_WID, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#x29; AS A&#xd;&#xa;         GROUP BY A.PERIOD_WID&#xd;&#xa;                 ,A.RGN_ID&#xd;&#xa;                 ,A.BASE_ID&#xd;&#xa;                 ,A.BRAND_ID&#x29; AS B&#xd;&#xa; WHERE SUBSTR&#x28;B.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#x3b;&#xd;&#xa;</sql>
