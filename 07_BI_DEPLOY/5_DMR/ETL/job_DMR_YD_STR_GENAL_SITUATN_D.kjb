      <name>TO_TMP2</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_STR_GENAL_SITUATN_D_02&#xd;&#xa;SELECT X.PERIOD_WID&#xd;&#xa;      ,X.RGN_ID&#xd;&#xa;      ,X.BASE_ID&#xd;&#xa;      ,X.BRAND_ID&#xd;&#xa;      ,X.TOTAL_STR_LOCATION_QTY --&#x5e93;&#x623f;&#x5e93;&#x4f4d;&#x603b;&#x91cf;&#xd;&#xa;      ,X.CUR_D_STR_INV_QTY --&#x73b0;&#x6709;&#x5e93;&#x5b58;&#x603b;&#x91cf;&#xd;&#xa;      ,X.CUR_M_STR_BIZ_QTY --&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;      ,X.LST_M_STR_BIZ_QTY --&#x4e0a;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;      ,X.LST_Y_M_STR_BIZ_QTY --&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#xd;&#xa;      ,X.CUR_Y_STR_BIZ_QTY --&#x5f53;&#x5e74;&#x5e74;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;      ,X.LST_Y_Y_STR_BIZ_QTY --&#x4e0a;&#x5e74;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;  FROM &#x28;SELECT X1.PERIOD_WID&#xd;&#xa;              ,X1.RGN_ID&#xd;&#xa;              ,X1.BASE_ID&#xd;&#xa;              ,X1.BRAND_ID&#xd;&#xa;              ,0 AS TOTAL_STR_LOCATION_QTY --&#x5e93;&#x623f;&#x5e93;&#x4f4d;&#x603b;&#x91cf;&#xd;&#xa;              ,0 AS CUR_D_STR_INV_QTY --&#x73b0;&#x6709;&#x5e93;&#x5b58;&#x603b;&#x91cf;&#xd;&#xa;              ,SUM&#x28;X2.INSTOCK_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 6&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_M_STR_BIZ_QTY --&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;              ,X3.INSTOCK_QTY AS LST_M_STR_BIZ_QTY --&#x4e0a;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;              ,X4.INSTOCK_QTY AS LST_Y_M_STR_BIZ_QTY --&#x4e0a;&#x5e74;&#x5f53;&#x6708;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#xd;&#xa;              ,SUM&#x28;X2.INSTOCK_QTY&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID, 1, 4&#x29;, X1.RGN_ID, X1.BASE_ID, X1.BRAND_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_Y_STR_BIZ_QTY --&#x5f53;&#x5e74;&#x5e74;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;              ,X5.INSTOCK_QTY AS LST_Y_Y_STR_BIZ_QTY --&#x4e0a;&#x5e74;&#x4ed3;&#x50a8;&#x6536;&#x8f66;&#x91cf;&#xd;&#xa;          FROM &#x28;SELECT D.PERIOD_WID&#xd;&#xa;                      ,T.RGN_ID&#xd;&#xa;                      ,T.BASE_ID&#xd;&#xa;                      ,T.BRAND_ID&#xd;&#xa;                  FROM DIM.DIM_PERIOD_D D&#xd;&#xa;                  JOIN &#x28;SELECT NVL&#x28;NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                             ,NVL&#x28;NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                             ,NVL&#x28;NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                         FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;                        WHERE 1 &#x3d; 1&#xd;&#xa;                          AND SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3e;&#x3d;&#xd;&#xa;                              SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                               -0&#x29;,&#xd;&#xa;                                                    &#x27;-&#x27;,&#xd;&#xa;                                                    &#x27;&#x27;&#x29;,&#xd;&#xa;                                     1,&#xd;&#xa;                                     4&#x29;&#xd;&#xa;                          AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                              SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                               -0&#x29;,&#xd;&#xa;                                                    &#x27;-&#x27;,&#xd;&#xa;                                                    &#x27;&#x27;&#x29;,&#xd;&#xa;                                     1,&#xd;&#xa;                                     6&#x29;&#xd;&#xa;                        GROUP BY NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                ,NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                                ,NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS T&#xd;&#xa;                    ON 1 &#x3d; 1&#xd;&#xa;                 WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                   AND SUBSTR&#x28;D.PERIOD_WID, 1, 4&#x29; &#x3e;&#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -0&#x29;,&#xd;&#xa;                                             &#x27;-&#x27;,&#xd;&#xa;                                             &#x27;&#x27;&#x29;,&#xd;&#xa;                              1,&#xd;&#xa;                              4&#x29;&#xd;&#xa;                   AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                       SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -0&#x29;,&#xd;&#xa;                                             &#x27;-&#x27;,&#xd;&#xa;                                             &#x27;&#x27;&#x29;,&#xd;&#xa;                              1,&#xd;&#xa;                              6&#x29;&#x29; AS X1&#xd;&#xa;          LEFT JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                          ,SUM&#x28;INSTOCK_QTY&#x29; AS INSTOCK_QTY&#xd;&#xa;                      FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;                     WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; &#x3e;&#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                            -0&#x29;,&#xd;&#xa;                                                 &#x27;-&#x27;,&#xd;&#xa;                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  4&#x29;&#xd;&#xa;                       AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3c;&#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                            -0&#x29;,&#xd;&#xa;                                                 &#x27;-&#x27;,&#xd;&#xa;                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29;&#xd;&#xa;                     GROUP BY T.PERIOD_WID&#xd;&#xa;                             ,NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS X2&#xd;&#xa;            ON X1.PERIOD_WID &#x3d; X2.PERIOD_WID&#xd;&#xa;           AND X1.RGN_ID &#x3d; X2.RGN_ID&#xd;&#xa;           AND X1.BASE_ID &#x3d; X2.BASE_ID&#xd;&#xa;           AND X1.BRAND_ID &#x3d; X2.BRAND_ID&#xd;&#xa;          LEFT JOIN &#x28;SELECT NVL&#x28;NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                          ,SUM&#x28;INSTOCK_QTY&#x29; AS INSTOCK_QTY&#xd;&#xa;                      FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;                     WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                           SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#xd;&#xa;                                                            -1&#x29;,&#xd;&#xa;                                                 &#x27;-&#x27;,&#xd;&#xa;                                                 &#x27;&#x27;&#x29;,&#xd;&#xa;                                  1,&#xd;&#xa;                                  6&#x29;&#xd;&#xa;                     GROUP BY NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;                             ,NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29; WITH CUBE&#x29; AS X3&#xd;&#xa;            ON X1.RGN_ID &#x3d; X3.RGN_ID&#xd;&#xa;           AND X1.BASE_ID &#x3d; X3.BASE_ID&#xd;&#xa;           AND X1.BRAND_ID &#x3d; X3.BRAND_ID&#xd;&#xa;          LEFT JOIN &#x28;SELECT NVL&#x28;NVL&#x28;RGN_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;BASE_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                          ,NVL&#x28;NVL&#x28;BRAND_ID, &#x27;-1&#x27;&#x29;, &#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                          ,SUM&#x28;INSTOCK_QTY&#x29; AS INSTOCK_QTY&#xd;&#xa;                      FROM DMK.DMK_STR_OPERQTY_D T&#xd;&#xa;                     WHERE SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                