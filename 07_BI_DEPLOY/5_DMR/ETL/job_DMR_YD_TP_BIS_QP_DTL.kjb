      </entry>
    <entry>
      <name>TO_TMP</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_TP_BIS_QP_DTL_01&#xd;&#xa;SELECT COALESCE&#x28;ACT.RGN_ID,PLN.RGN_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;ACT.BASE_ID,PLN.BASE_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;ACT.BRAND_ID,PLN.BRAND_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       COALESCE&#x28;ACT.PROJ_ID,PLN.PROJ_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;       SUM&#x28;ACT.TKT_QTY_MON&#x29;,&#xd;&#xa;       SUM&#x28;PLN.PLAN_QTY_MON&#x29;,&#xd;&#xa;       SUM&#x28;ACT.TKT_QTY_YEAR&#x29;,&#xd;&#xa;       SUM&#x28;PLN.PLAN_QTY_YEAR&#x29;&#xd;&#xa;  FROM &#x28;SELECT RGN_ID              AS RGN_ID,        -- &#x5927;&#x533a;ID&#xd;&#xa;               BASE_ID             AS BASE_ID,       -- &#x57fa;&#x5730;ID&#xd;&#xa;               BRAND_ID            AS BRAND_ID,      -- &#x54c1;&#x724c;ID&#xd;&#xa;               PROJ_ID             AS PROJ_ID,       -- &#x9879;&#x76ee;ID&#xd;&#xa;               SUM&#x28;CASE WHEN SUBSTR&#x28;EARLY_PICKUP_DATE,1,10&#x29; &#x3d; &#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#xd;&#xa;                          THEN 1 ELSE 0 END&#x29; AS TKT_QTY_DAY,  -- &#x6628;&#x65e5;&#x542f;&#x7968;&#x6570;&#xd;&#xa;               SUM&#x28;CASE WHEN SUBSTR&#x28;EARLY_PICKUP_DATE,1,7&#x29; &#x3d; SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,7&#x29;&#xd;&#xa;                          THEN 1 ELSE 0 END&#x29; AS TKT_QTY_MON,  -- &#x6708;&#x7d2f;&#x8ba1;&#x542f;&#x7968;&#x6570;&#xd;&#xa;               SUM&#x28;CASE WHEN SUBSTR&#x28;EARLY_PICKUP_DATE,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                          THEN 1 ELSE 0 END&#x29; AS TKT_QTY_YEAR  -- &#x5e74;&#x7d2f;&#x8ba1;&#x542f;&#x7968;&#x6570;&#xd;&#xa;          FROM DW.DW_TP_ORDERRELEASE&#xd;&#xa;            WHERE SUBSTR&#x28;EARLY_PICKUP_DATE,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;              AND OR_PLAN_TYPE &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;              AND BRAND_ID IN&#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;              GROUP BY RGN_ID,&#xd;&#xa;                       BASE_ID,&#xd;&#xa;                       BRAND_ID,&#xd;&#xa;                       PROJ_ID&#x29; ACT&#xd;&#xa;    FULL JOIN &#x28;SELECT RGN.RGN_ID,&#xd;&#xa;                      BAS.BASE_ID,&#xd;&#xa;                      BRD.BRAND_ID,&#xd;&#xa;                      PRJ.PROJ_ID,&#xd;&#xa;                      SUM&#x28;CASE WHEN PLN.MONTH_ID &#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;                                 THEN PLN.REV_PLAN ELSE 0 END&#x29; AS PLAN_QTY_MON,  -- &#x6708;&#x8ba1;&#x5212;&#x542f;&#x7968;&#x6570;&#xd;&#xa;                      SUM&#x28;CASE WHEN PLN.MONTH_ID &#x3c;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;                                 THEN PLN.REV_PLAN ELSE 0 END&#x29; AS PLAN_QTY_YEAR  -- &#x5e74;&#x8ba1;&#x5212;&#x542f;&#x7968;&#x6570;&#xd;&#xa;                 FROM ODS_BLPT.T_HAND_REV_TICKET_PLAN PLN&#xd;&#xa;                   LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;                          ON RGN.ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;                         AND PLN.RGN_NAME &#x3d; RGN.RGN_NAME&#xd;&#xa;                   LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;                          ON BAS.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;                         AND PLN.BASE_NAME &#x3d; BAS.BASE_NAME&#xd;&#xa;                   LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;                          ON PLN.BRAND_NAME &#x3d; BRD.BRAND_NAME&#xd;&#xa;                   LEFT JOIN DIM.DIM_PROJ PRJ&#xd;&#xa;                          ON PLN.PRO_NAME &#x3d; PRJ.PROJ_NAME&#xd;&#xa;                     WHERE SUBSTR&#x28;PLN.MONTH_ID,1,4&#x29; &#x3d; YEAR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;&#xd;&#xa;                       GROUP BY RGN.RGN_ID,&#xd;&#xa;                                BAS.BASE_ID,&#xd;&#xa;                                BRD.BRAND_ID,&#xd;&#xa;                                PRJ.PROJ_ID&#xd;&#xa;              &#x29; PLN ON ACT.RGN_ID &#x3d; PLN.RGN_ID&#xd;&#xa;                   AND ACT.BASE_ID &#x3d; PLN.BASE_ID&#xd;&#xa;                   AND ACT.BRAND_ID &#x3d; PLN.BRAND_ID&#xd;&#xa;                   AND ACT.PROJ_ID &#x3d; PLN.PROJ_ID&#xd;&#xa;      GROUP BY COALESCE&#x28;ACT.RGN_ID,PLN.RGN_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;               COALESCE&#x28;ACT.BASE_ID,PLN.BASE_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;               COALESCE&#x28;ACT.BRAND_ID,PLN.BRAND_ID,&#x27;ALL&#x27;&#x29;,&#xd;&#xa;               COALESCE&#x28;ACT.PROJ_ID,PLN.PROJ_ID,&#x27;ALL&#x27;&#x29;&#xd;&#xa;        WITH CUBE&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_YD_TP_BIS_QP_DTL_02&#xd;&#xa;SELECT TMP.RGN_ID,&#xd;&#xa;       CASE WHEN TMP.RGN_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       TMP.BASE_ID,&#xd;&#xa;       CASE WHEN TMP.BASE_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       TMP.BRAND_ID,&#xd;&#xa;       CASE WHEN TMP.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;BRD.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       &#x27;&#x542f;&#x7968;&#x27; AS TP_PROCESS,&#xd;&#xa;       TMP.PROJ_ID,&#xd;&#xa;       CASE WHEN TMP.PROJ_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;            ELSE NVL&#x28;PRJ.PROJ_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; END,&#xd;&#xa;       ROUND&#x28;TMP.TKT_QTY_MON &#x2f; TMP.PLAN_QTY_MON &#x2a; 100,2&#x29;,&#xd;&#xa;       ROUND&#x28;TMP.TKT_QTY_YEAR &#x2f; TMP.PLAN_QTY_YEAR &#x2a; 100,2&#x29;,&#xd;&#xa;       NULL AS COLOR_FLAG1,&#xd;&#xa;       NULL AS COLOR_FLAG2&#xd;&#xa;  FROM TEMP.TEMP_DMR_YD_TP_BIS_QP_DTL_01 TMP&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;           ON TMP.RGN_ID &#x3d; RGN.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;           ON TMP.BASE_ID &#x3d; BAS.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_BRAND BRD&#xd;&#xa;           ON TMP.BRAND_ID &#x3d; BRD.BRAND_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_PROJ PRJ&#xd;&#xa;           ON TMP.PROJ_ID &#x3d; PRJ.PROJ_ID&#xd;&#xa;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
