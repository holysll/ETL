      <name>TO_DMR 2</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_YD_TP_BIS_QP_DTL PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.period_wid AS DATE_ID&#xd;&#xa;    ,T.RGN_ID AS RNG_ID&#xd;&#xa;    ,CASE WHEN T.RGN_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.RGN_ID &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.RGN_NAME END AS RNG_NAME&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,CASE WHEN T.BASE_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.BASE_ID &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.BASE_NAME END AS BASE_NAME&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,CASE WHEN T.BRAND_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.BRAND_ID &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.BRAND_NAME END AS BRAND_NAME&#xd;&#xa;    ,&#x27;&#x542f;&#x7968;&#x27; AS TP_PROCESS&#xd;&#xa;    ,T.PROJ_ID&#xd;&#xa;    ,CASE WHEN T.PROJ_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.PROJ_ID &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T4.PROJ_NAME END AS PROJ_NAME&#xd;&#xa;    ,T.CUR_M_QP_QTY&#x2f;T.CUR_M_TGT_QP_QTY AS CUR_M_QP_CMP_RATE&#xd;&#xa;    ,T.CUR_Y_QP_QTY&#x2f;T.CUR_Y_TGT_QP_QTY AS CUR_Y_QP_CMP_RATE&#xd;&#xa;    ,NULL AS WRN_M_FLAG  --&#xd;&#xa;    ,NULL AS WRN_Y_FLAG  --&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;T.PERIOD_WID,1,6&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT X.PERIOD_WID&#xd;&#xa;        ,X.RGN_ID&#xd;&#xa;        ,X.BASE_ID&#xd;&#xa;        ,X.BRAND_ID&#xd;&#xa;        ,X.PROJ_ID&#xd;&#xa;        ,X.CUR_M_QP_QTY&#xd;&#xa;        ,B.CUR_M_TGT_QP_QTY&#xd;&#xa;        ,X.CUR_Y_QP_QTY&#xd;&#xa;        ,B.CUR_Y_TGT_QP_QTY&#xd;&#xa;    FROM&#x28;&#xd;&#xa;        SELECT X1.PERIOD_WID&#xd;&#xa;            ,X1.RGN_ID&#xd;&#xa;            ,X1.BASE_ID&#xd;&#xa;            ,X1.BRAND_ID&#xd;&#xa;            ,X1.PROJ_ID&#xd;&#xa;            ,SUM&#x28;QP_QTY&#x29; OVER &#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID,1,6&#x29;,X1.RGN_ID,X1.BASE_ID,X1.BRAND_ID,X1.PROJ_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_M_QP_QTY&#xd;&#xa;            ,SUM&#x28;QP_QTY&#x29; OVER &#x28;PARTITION BY SUBSTR&#x28;X1.PERIOD_WID,1,4&#x29;,X1.RGN_ID,X1.BASE_ID,X1.BRAND_ID,X1.PROJ_ID ORDER BY X1.PERIOD_WID&#x29; AS CUR_Y_QP_QTY&#xd;&#xa;        FROM&#x28;&#xd;&#xa;            SELECT T.PERIOD_WID&#xd;&#xa;                ,NVL&#x28;NVL&#x28;T.RGN_ID,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                ,NVL&#x28;NVL&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                ,NVL&#x28;NVL&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                ,NVL&#x28;NVL&#x28;T.PROJ_ID,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;                ,sum&#x28;T.ticket_qty&#x29; AS QP_QTY&#xd;&#xa;            FROM DMK.dmk_tp_orderqty_d T&#xd;&#xa;            WHERE SUBSTR&#x28;T.PERIOD_WID,1,4&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29;&#xd;&#xa;                AND SUBSTR&#x28;T.PERIOD_WID,1,6&#x29; &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            GROUP BY T.PERIOD_WID,NVL&#x28;T.RGN_ID,&#x27;-1&#x27;&#x29;,NVL&#x28;T.BASE_ID,&#x27;-1&#x27;&#x29;,NVL&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29;,NVL&#x28;T.PROJ_ID,&#x27;-1&#x27;&#x29; WITH CUBE&#xd;&#xa;                &#x29; AS X1&#xd;&#xa;        &#x29; X&#xd;&#xa;        LEFT JOIN &#x28;&#xd;&#xa;            SELECT NVL&#x28;coalesce&#x28;T1.rgn_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;                ,NVL&#x28;coalesce&#x28;T2.base_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;                ,NVL&#x28;coalesce&#x28;T3.brand_id,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;                ,NVL&#x28;coalesce&#x28;T4.proj_id ,&#x27;-1&#x27;&#x29;,&#x27;ALL&#x27;&#x29; AS PROJ_ID &#xd;&#xa;                ,sum&#x28;CASE WHEN substr&#x28;T.month_id,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.rev_plan ELSE 0 END&#x29; AS CUR_M_TGT_QP_QTY  --&#xd;&#xa;                ,sum&#x28;CASE WHEN substr&#x28;T.month_id,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29; THEN T.rev_plan ELSE 0 END&#x29; AS CUR_Y_TGT_QP_QTY  --&#xd;&#xa;            FROM ods_blpt.t_hand_rev_ticket_plan T  &#xd;&#xa;            LEFT JOIN&#x28;&#xd;&#xa;                SELECT rgn_id,rgn_name&#xd;&#xa;                FROM DIM.dim_org &#xd;&#xa;                WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;                GROUP BY rgn_id,rgn_name&#xd;&#xa;                &#x29; T1&#xd;&#xa;            ON T.rgn_name &#x3d; T1.rgn_name&#xd;&#xa;            LEFT JOIN&#x28;&#xd;&#xa;                SELECT base_id,base_name&#xd;&#xa;                FROM DIM.dim_org &#xd;&#xa;                WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;                GROUP BY base_id,base_name&#xd;&#xa;                &#x29; T2&#xd;&#xa;            ON T.base_name &#x3d; T2.base_name&#xd;&#xa;            LEFT JOIN&#x28;&#xd;&#xa;                SELECT brand_id,brand_name&#xd;&#xa;                FROM DIM.dim_brand &#xd;&#xa;                GROUP BY brand_id,brand_name&#xd;&#xa;                &#x29; T3&#xd;&#xa;            ON T.brand_name &#x3d; T3.brand_name&#xd;&#xa;            LEFT JOIN&#x28;&#xd;&#xa;                SELECT proj_id,proj_name&#xd;&#xa;                FROM DIM.dim_proj&#xd;&#xa;                GROUP BY proj_id,proj_name&#xd;&#xa;                &#x29; T4&#xd;&#xa;            ON T.PRO_NAME &#x3d; T4.proj_name&#xd;&#xa;            WHERE substr&#x28;T.month_id,1,4&#x29; &#x3d; substr&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,1,4&#x29;&#xd;&#xa;                AND substr&#x28;T.month_id,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            GROUP BY coalesce&#x28;T1.rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;                ,coalesce&#x28;T2.base_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;                ,coalesce&#x28;T3.brand_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;                ,coalesce&#x28;T4.proj_id ,&#x27;-1&#x27;&#x29;  WITH CUBE&#xd;&#xa;            &#x29; AS B&#xd;&#xa;        ON X.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;        AND X.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;        AND X.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;        AND X.PROJ_ID &#x3d; B.PROJ_ID&#xd;&#xa;    WHERE SUBSTR&#x28;X.PERIOD_WID,1,6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND SUBSTR&#x28;X.PERIOD_WID,1,8&#x29; &#x3c;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    &#x29; T&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; T1&#xd;&#xa;    ON T.RGN_ID &#x3d; T1.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; T2&#xd;&#xa;    ON T.BASE_ID &#x3d; T2.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; T3&#xd;&#xa;    ON T.BRAND_ID &#x3d; T3.BRAND_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT PROJ_ID&#xd;&#xa;                   ,PROJ_NAME&#xd;&#xa;               FROM DIM.DIM_PROJ&#xd;&#xa;              GROUP BY PROJ_ID&#xd;&#xa;                      ,PROJ_NAME&#x29; T4&#xd;&#xa;    ON T.PROJ_ID &#x3d; T4.PROJ_ID&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT T.period_wid AS DATE_ID&#xd;&#xa;    ,T.rgn_id AS RNG_ID&#xd;&#xa;    ,CASE WHEN T.rgn_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x6574;&#x8f66;&#x4e8b;&#x4e1a;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.rgn_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T1.rgn_name END AS RNG_NAME&#xd;&#xa;    ,T.base_id&#xd;&#xa;    ,CASE WHEN T.base_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.base_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T2.base_name END AS BASE_NAME&#xd;&#xa;    ,T.brand_id&#xd;&#xa;    ,CASE WHEN T.brand_id &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.brand_id &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T3.brand_name END AS BRAND_NAME&#xd;&#xa;    ,&#x27;&#x542f;&#x7968;&#x27; AS TP_PROCESS&#xd;&#xa;    ,T.PROJ_ID&#xd;&#xa;    ,CASE WHEN T.PROJ_ID &#x3d; &#x27;ALL&#x27; THEN &#x27;&#x5168;&#x90e8;&#x27;&#xd;&#xa;        WHEN T.PROJ_ID &#x3d; &#x27;-1&#x27; THEN &#x27;&#x672a;&#x77e5;&#x27; &#xd;&#xa;        ELSE T4.PROJ_NAME END AS PROJ_NAME&#xd;&#xa;    ,sum&#x28;CUR_M_QP_QTY&#x29;&#x2f;sum&#x28;CUR_M_TGT_QP_QTY&#x29; AS CUR_M_QP_CMP_RATE&#xd;&#xa;    ,sum&#x28;CUR_Y_QP_QTY&#x29;&#x2f;sum&#x28;CUR_Y_TGT_QP_QTY&#x29; AS CUR_Y_QP_CMP_RATE&#xd;&#xa;    ,NULL AS WRN_M_FLAG  --&#xd;&#xa;    ,NULL AS WRN_Y_FLAG  --&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS UPDATE_TIME                     --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;    ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;    ,substr&#x28;T.period_wid,1,6&#x29; AS PART_DT&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT A.period_wid  &#xd;&#xa;        ,coalesce&#x28;A.RGN_ID,&#x27;ALL&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;A.BASE_ID,&#x27;ALL&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;A.BRAND_ID,&#x27;ALL&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;A.PROJ_ID,&#x27;ALL&#x27;&#x29; AS PROJ_ID&#xd;&#xa;        ,sum&#x28;CUR_M_QP_QTY&#x29; AS CUR_M_QP_QTY  --&#x5f53;&#x6708;&#x542f;&#x7968;&#x6570;&#xd;&#xa;        ,sum&#x28;CUR_M_TGT_QP_QTY&#x29; AS CUR_M_TGT_QP_QTY  --&#x5f53;&#x6708;&#x8ba1;&#x5212;&#x542f;&#x7968;&#x6570;&#xd;&#xa;        ,sum&#x28;CUR_Y_QP_QTY&#x29; AS CUR_Y_QP_QTY  --&#x5f53;&#x5e74;&#x542f;&#x7968;&#x6570;&#xd;&#xa;        ,sum&#x28;CUR_Y_TGT_QP_QTY&#x29; AS CUR_Y_TGT_QP_QTY  --&#x5f53;&#x5e74;&#x8ba1;&#x5212;&#x542f;&#x7968;&#x6570;&#xd;&#xa;    FROM&#x28;&#xd;&#xa;        SELECT P.period_wid&#xd;&#xa;            ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;            ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;            ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;P.period_wid,1,6&#x29; AND substr&#x28;T.period_wid,1,8&#x29; &#x3c;&#x3d; substr&#x28;P.period_wid,1,8&#x29; THEN ticket_qty ELSE 0 END&#x29; AS CUR_M_QP_QTY  --&#xd;&#xa;            ,0 AS CUR_M_TGT_QP_QTY  --&#xd;&#xa;            ,sum&#x28;CASE WHEN substr&#x28;T.period_wid,1,4&#x29; &#x3d; substr&#x28;P.period_wid,1,4&#x29; AND substr&#x28;T.period_wid,1,8&#x29; &#x3c;&#x3d; substr&#x28;P.period_wid,1,8&#x29; THEN ticket_qty ELSE 0 END&#x29; AS CUR_Y_QP_QTY  --&#xd;&#xa;            ,0 AS CUR_Y_TGT_QP_QTY  --&#xd;&#xa;        FROM &#x28;SELECT period_wid FROM DIM.dim_period_d WHERE period_type &#x3d; &#x27;D&#x27;&#x29; P&#xd;&#xa;        LEFT JOIN &#x28;SELECT period_wid,rgn_id,base_id,brand_id,proj_id,ticket_qty FROM DMK.dmk_tp_orderqty_d &#xd;&#xa;                    WHERE substr&#x28;period_wid,1,4&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,4&#x29;&#xd;&#xa;                        AND substr&#x28;period_wid,1,6&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#x29; T  &#xd;&#xa;        WHERE substr&#x28;P.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            AND substr&#x28;P.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;        GROUP BY P.period_wid&#xd;&#xa;            ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;            ,coalesce&#x28;proj_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT P.period_wid&#xd;&#xa;            ,coalesce&#x28;T1.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;            ,coalesce&#x28;T2.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;            ,coalesce&#x28;T3.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;            ,coalesce&#x28;T4.proj_id ,&#x27;-1&#x27;&#x29; AS PROJ_ID&#xd;&#xa;            ,0 AS CUR_M_QP_QTY  --&#xd;&#xa;            ,sum&#x28;CASE WHEN substr&#x28;T.month_id,1,6&#x29; &#x3d; substr&#x28;P.period_wid,1,6&#x29; THEN T.rev_plan ELSE 0 END&#x29; AS CUR_M_TGT_QP_QTY  --&#xd;&#xa;            ,0 CUR_Y_QP_QTY  --&#xd;&#xa;            ,sum&#x28;CASE WHEN substr&#x28;T.month_id,1,4&#x29; &#x3d; substr&#x28;P.period_wid,1,4&#x29; THEN T.rev_plan ELSE 0 END&#x29; AS CUR_Y_TGT_QP_QTY  --&#xd;&#xa;        FROM &#x28;SELECT period_wid FROM DIM.dim_period_d WHERE period_type &#x3d; &#x27;D&#x27;&#x29; P&#xd;&#xa;        LEFT JOIN &#x28;SELECT month_id,rgn_name,base_name,brand_name,pro_name,rev_plan FROM ods_blpt.t_hand_rev_ticket_plan&#x29; T  &#xd;&#xa;        LEFT JOIN&#x28;&#xd;&#xa;            SELECT rgn_id,rgn_name&#xd;&#xa;            FROM DIM.dim_org &#xd;&#xa;            WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;            GROUP BY rgn_id,rgn_name&#xd;&#xa;            &#x29; T1&#xd;&#xa;        ON T.rgn_name &#x3d; T1.rgn_name&#xd;&#xa;        LEFT JOIN&#x28;&#xd;&#xa;            SELECT base_id,base_name&#xd;&#xa;            FROM DIM.dim_org &#xd;&#xa;            WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;            GROUP BY base_id,base_name&#xd;&#xa;            &#x29; T2&#xd;&#xa;        ON T.base_name &#x3d; T2.base_name&#xd;&#xa;        LEFT JOIN&#x28;&#xd;&#xa;            SELECT brand_id,brand_name&#xd;&#xa;            FROM DIM.dim_brand &#xd;&#xa;            GROUP BY brand_id,brand_name&#xd;&#xa;            &#x29; T3&#xd;&#xa;        ON T.brand_name &#x3d; T3.brand_name&#xd;&#xa;        LEFT JOIN&#x28;&#xd;&#xa;            SELECT proj_id,proj_name&#xd;&#xa;            FROM DIM.dim_proj&#xd;&#xa;            GROUP BY proj_id,proj_name&#xd;&#xa;            &#x29; T4&#xd;&#xa;        ON T.PRO_NAME &#x3d; T4.proj_name&#xd;&#xa;        WHERE substr&#x28;P.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            AND substr&#x28;P.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;        GROUP BY P.period_wid&#xd;&#xa;            ,coalesce&#x28;T1.rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;            ,coalesce&#x28;T2.base_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;            ,coalesce&#x28;T3.brand_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;            ,coalesce&#x28;T4.proj_id ,&#x27;-1&#x27;&#x29; &#xd;&#xa;        &#x29; A&#xd;&#xa;    GROUP BY A.period_wid,A.RGN_ID,A.BASE_ID,A.BRAND_ID,A.PROJ_ID WITH CUBE&#xd;&#xa;    &#x29; T&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT rgn_id,rgn_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id,rgn_name&#xd;&#xa;    &#x29; T1&#xd;&#xa;ON T.rgn_id &#x3d; T1.rgn_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT base_id,base_name&#xd;&#xa;    FROM DIM.dim_org &#xd;&#xa;    WHERE org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    GROUP BY base_id,base_name&#xd;&#xa;    &#x29; T2&#xd;&#xa;ON T.base_id &#x3d; T2.base_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT brand_id,brand_name&#xd;&#xa;    FROM DIM.dim_brand &#xd;&#xa;    GROUP BY brand_id,brand_name&#xd;&#xa;    &#x29; T3&#xd;&#xa;ON T.brand_id &#x3d; T3.brand_id&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;    SELECT proj_id,proj_name&#xd;&#xa;    FROM DIM.dim_proj&#xd;&#xa;    GROUP BY proj_id,proj_name&#xd;&#xa;&#x29; T4&#xd;&#xa;ON T.PROJ_ID &#x3d; T4.PROJ_ID&#xd;&#xa;WHERE CASE WHEN T.BASE_ID &#x3c;&#x3e; &#x27;ALL&#x27; THEN T.RGN_ID &#x3c;&#x3e; &#x27;ALL&#x27; AND T.PROJ_ID &#x3c;&#x3e; &#x27;ALL&#x27; AND T.period_wid IS NOT NULL&#xd;&#xa;        ELSE &#x28;T.RGN_ID &#x3c;&#x3e; &#x27;ALL&#x27; OR T.RGN_ID &#x3d; &#x27;ALL&#x27;&#x29; AND T.PROJ_ID &#x3c;&#x3e; &#x27;ALL&#x27; AND T.period_wid IS NOT NULL END&#xd;&#xa;GROUP BY T.period_wid,T.RGN_ID, T1.rgn_name,T.BASE_ID, T2.base_name,T.BRAND_ID, T3.brand_name,T.PROJ_ID,T4.proj_name&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2a;&#x2f;</sql>
