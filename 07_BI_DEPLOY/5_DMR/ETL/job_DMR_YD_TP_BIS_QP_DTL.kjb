    <entry>
      <name>CREATE_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_TP_BIS_QP_DTL_01&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_TP_BIS_QP_DTL_01&#x28;&#xd;&#xa;RGN_ID           STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID          STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BRAND_ID         STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;PROJ_ID          STRING        COMMENT  &#x27;&#x9879;&#x76ee;ID&#x27;,&#xd;&#xa;TKT_QTY_MON      BIGINT        COMMENT  &#x27;&#x6708;&#x7d2f;&#x8ba1;&#x542f;&#x7968;&#x6570;&#x27;,&#xd;&#xa;PLAN_QTY_MON     BIGINT        COMMENT  &#x27;&#x6708;&#x8ba1;&#x5212;&#x542f;&#x7968;&#x6570;&#x27;,&#xd;&#xa;TKT_QTY_YEAR     BIGINT        COMMENT  &#x27;&#x5e74;&#x7d2f;&#x8ba1;&#x542f;&#x7968;&#x6570;&#x27;,&#xd;&#xa;PLAN_QTY_YEAR    BIGINT        COMMENT  &#x27;&#x5e74;&#x8ba1;&#x5212;&#x542f;&#x7968;&#x6570;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4e1a;&#x52a1;&#x91cf;&#x7b80;&#x62a5;-&#x8d77;&#x7968;&#x8be6;&#x60c5;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_YD_TP_BIS_QP_DTL_02&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_YD_TP_BIS_QP_DTL_02&#x28;&#xd;&#xa;RGN_ID           STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;RGN_NAME         STRING        COMMENT  &#x27;&#x5927;&#x533a;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;BASE_ID          STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BASE_NAME        STRING        COMMENT  &#x27;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;BRAND_ID         STRING        COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;BRAND_NAME       STRING        COMMENT  &#x27;&#x54c1;&#x724c;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;TP_PROCESS       STRING        COMMENT  &#x27;&#x8fd0;&#x8f93;&#x8fc7;&#x7a0b;&#x27;,&#xd;&#xa;PROJ_ID          STRING        COMMENT  &#x27;&#x9879;&#x76ee;ID&#x27;,&#xd;&#xa;PROJ_NAME        STRING        COMMENT  &#x27;&#x9879;&#x76ee;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACMP_RATE_MON    DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x6708;&#x5ea6;&#x542f;&#x7968;&#x5b8c;&#x6210;&#x7387;&#x27;,&#xd;&#xa;ACMP_RATE_YEAR   DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x5e74;&#x5ea6;&#x542f;&#x7968;&#x5b8c;&#x6210;&#x7387;&#x27;,&#xd;&#xa;COLOR_FLAG_MON   STRING        COMMENT  &#x27;&#x6708;&#x5ea6;&#x542f;&#x7968;&#x5b8c;&#x6210;&#x7387;&#x989c;&#x8272;&#x6807;&#x8bb0;&#x27;,&#xd;&#xa;COLOR_FLAG_YEAR  STRING        COMMENT  &#x27;&#x5e74;&#x5ea6;&#x542f;&#x7968;&#x5b8c;&#x6210;&#x7387;&#x989c;&#x8272;&#x6807;&#x8bb0;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4e1a;&#x52a1;&#x91cf;&#x7b80;&#x62a5;-&#x8d77;&#x7968;&#x8be6;&#x60c5;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;</sql>
