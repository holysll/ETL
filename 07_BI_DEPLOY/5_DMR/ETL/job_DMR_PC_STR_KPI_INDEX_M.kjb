      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;--07 &#x878d;&#x8d44;&#x91cf;&#xd;&#xa;--orderrelease &#x8ba1;&#x5212;&#x65e5; &#x3d; &#x672c;&#x6708; &#x53d1;&#x8fd0;&#x8bf4;&#x660e;OR_SHIPDESC &#x4e3a;&#x878d;&#x8d44;&#x8f66;&#x7684;&#x6570;&#x91cf;&#xd;&#xa;--&#x878d;&#x8d44;&#x8f66;&#x5360;&#x6bd4; &#x3d; &#x878d;&#x8d44;&#x8f66;&#x6570;&#x91cf;&#x2f;&#x603b;&#x542f;&#x7968;&#x6570; &#x542f;&#x7968;&#x6570;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_STR_KPI_INDEX_M_7&#xd;&#xa;SELECT T.MONTH_ID AS MONTH_ID&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,T.BASE_ID AS BASE_ID&#xd;&#xa;      ,T.BRAND_ID AS BRAND_ID&#xd;&#xa;      ,T.MODEL_ID AS MODEL_ID&#xd;&#xa;      ,&#x27;07&#x27; AS INDEX_SEQ&#xd;&#xa;      ,&#x27;&#x878d;&#x8d44;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,T.BIZ_QTY AS BIZ_QTY&#xd;&#xa;      ,T.BIZ_QTY &#x2f; T.CNT AS BIZ_RATE&#xd;&#xa;      ,TB.TQ_BIZ_QTY AS BIZ_QTY_YOY&#xd;&#xa;      ,HB.LST_M_BIZ_QTY AS BIZ_QTY_MOM&#xd;&#xa;      ,T.BIZ_QTY &#x2f; TB.TQ_BIZ_QTY - 1 AS TB_RATE&#xd;&#xa;      ,T.BIZ_QTY &#x2f; HB.LST_M_BIZ_QTY - 1 AS HB_RATE&#xd;&#xa;  FROM &#x28;SELECT SUBSTRING&#x28;PERIOD_WID, 1, 6&#x29; AS MONTH_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,SUM&#x28;OUTSTOCK_QTY&#x29; AS CNT&#xd;&#xa;              ,SUM&#x28;CASE WHEN FINANCING_FLAG &#x3d; &#x27;1&#x27; THEN OUTSTOCK_QTY ELSE 0 END&#x29; AS BIZ_QTY&#xd;&#xa;          FROM DMK.DMK_TP_SHIPQTY_D&#xd;&#xa;         WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;         GROUP BY SUBSTRING&#x28;PERIOD_WID, 1, 6&#x29;&#xd;&#xa;                 ,RGN_ID&#xd;&#xa;                 ,BASE_ID&#xd;&#xa;                 ,BRAND_ID&#xd;&#xa;                 ,MODEL_ID&#x29;T&#xd;&#xa; LEFT JOIN &#x28;SELECT RGN_ID ,BASE_ID ,BRAND_ID ,MODEL_ID ,SUM&#x28;OUTSTOCK_QTY&#x29; AS TQ_BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_TP_SHIPQTY_D&#xd;&#xa;              WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                AND FINANCING_FLAG &#x3d; &#x27;1&#x27; AND BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;              GROUP BY RGN_ID, BASE_ID, BRAND_ID, MODEL_ID&#x29; TB&#xd;&#xa;    ON T.RGN_ID &#x3d; TB.RGN_ID&#xd;&#xa;   AND T.BASE_ID &#x3d; TB.BASE_ID&#xd;&#xa;   AND T.BRAND_ID &#x3d; TB.BRAND_ID&#xd;&#xa;   AND T.MODEL_ID &#x3d; TB.MODEL_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID, BASE_ID, BRAND_ID, MODEL_ID, SUM&#x28;OUTSTOCK_QTY&#x29; AS LST_M_BIZ_QTY&#xd;&#xa;               FROM DMK.DMK_TP_SHIPQTY_D&#xd;&#xa;              WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                AND FINANCING_FLAG &#x3d; &#x27;1&#x27; AND BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;              GROUP BY RGN_ID, BASE_ID, BRAND_ID, MODEL_ID&#x29; HB&#xd;&#xa;    ON T.RGN_ID &#x3d; HB.RGN_ID&#xd;&#xa;   AND T.BASE_ID &#x3d; HB.BASE_ID&#xd;&#xa;   AND T.BRAND_ID &#x3d; HB.BRAND_ID&#xd;&#xa;   AND T.MODEL_ID &#x3d; HB.MODEL_ID&#x3b;&#xd;&#xa;</sql>
