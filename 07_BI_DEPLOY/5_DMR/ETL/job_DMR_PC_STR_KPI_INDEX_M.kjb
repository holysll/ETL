    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;--target table &#x3a; DMR.DMR_PC_STR_KPI_INDEX_M DMRPC&#x4ed3;&#x50a8;-&#x4ed3;&#x50a8;&#x6982;&#x89c8;-&#x4ed3;&#x50a8;&#x6982;&#x89c8;&#x28;&#x6708;&#x29;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_STR_KPI_INDEX_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT T.MONTH_ID AS MONTH_ID --&#x6708;&#x4efd;ID&#xd;&#xa;      ,NVL&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;      ,NVL&#x28;RGN.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RNG_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;      ,NVL&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,NVL&#x28;BASE.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;      ,NVL&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;      ,NVL&#x28;BRAND.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;      ,NVL&#x28;T.MODEL_ID, &#x27;-1&#x27;&#x29; AS MODEL_ID --&#x8f66;&#x578b;ID&#xd;&#xa;      ,NVL&#x28;MODEL.MODEL_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS MODEL_NAME --&#x8f66;&#x578b;&#x540d;&#x79f0;&#xd;&#xa;      ,T.INDEX_SEQ AS INDEX_SEQ --&#x6307;&#x6807;&#x5e8f;&#x53f7;&#xd;&#xa;      ,T.INDEX_NAME AS INDEX_NAME --&#x6307;&#x6807;&#x540d;&#x79f0;&#xd;&#xa;      ,T.BIZ_QTY AS BIZ_QTY --&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,T.BIZ_RATE AS BIZ_RATE --&#x4e1a;&#x52a1;&#x5360;&#x6bd4;&#xd;&#xa;      ,T.BIZ_QTY_YOY AS BIZ_QTY_YOY --&#x4e1a;&#x52a1;&#x91cf;&#x540c;&#x671f;&#xd;&#xa;      ,T.BIZ_QTY_MOM AS BIZ_QTY_MOM --&#x4e1a;&#x52a1;&#x91cf;&#x4e0a;&#x671f;&#xd;&#xa;      ,T.TB_RATE AS TB_RATE --&#x540c;&#x6bd4;&#xd;&#xa;      ,T.HB_RATE AS HB_RATE --&#x73af;&#x6bd4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,T.MONTH_ID AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT MONTH_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,INDEX_SEQ&#xd;&#xa;              ,INDEX_NAME&#xd;&#xa;              ,BIZ_QTY&#xd;&#xa;              ,BIZ_RATE&#xd;&#xa;              ,BIZ_QTY_YOY&#xd;&#xa;              ,BIZ_QTY_MOM&#xd;&#xa;              ,TB_RATE&#xd;&#xa;              ,HB_RATE&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_M_1&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT MONTH_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,INDEX_SEQ&#xd;&#xa;              ,INDEX_NAME&#xd;&#xa;              ,BIZ_QTY&#xd;&#xa;              ,BIZ_RATE&#xd;&#xa;              ,BIZ_QTY_YOY&#xd;&#xa;              ,BIZ_QTY_MOM&#xd;&#xa;              ,TB_RATE&#xd;&#xa;              ,HB_RATE&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_M_2&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT MONTH_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,INDEX_SEQ&#xd;&#xa;              ,INDEX_NAME&#xd;&#xa;              ,BIZ_QTY&#xd;&#xa;              ,BIZ_RATE&#xd;&#xa;              ,BIZ_QTY_YOY&#xd;&#xa;              ,BIZ_QTY_MOM&#xd;&#xa;              ,TB_RATE&#xd;&#xa;              ,HB_RATE&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_M_3&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT MONTH_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,INDEX_SEQ&#xd;&#xa;              ,INDEX_NAME&#xd;&#xa;              ,BIZ_QTY&#xd;&#xa;              ,BIZ_RATE&#xd;&#xa;              ,BIZ_QTY_YOY&#xd;&#xa;              ,BIZ_QTY_MOM&#xd;&#xa;              ,TB_RATE&#xd;&#xa;              ,HB_RATE&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_M_4&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT MONTH_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,INDEX_SEQ&#xd;&#xa;              ,INDEX_NAME&#xd;&#xa;              ,BIZ_QTY&#xd;&#xa;              ,BIZ_RATE&#xd;&#xa;              ,BIZ_QTY_YOY&#xd;&#xa;              ,BIZ_QTY_MOM&#xd;&#xa;              ,TB_RATE&#xd;&#xa;              ,HB_RATE&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_M_6&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT MONTH_ID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,INDEX_SEQ&#xd;&#xa;              ,INDEX_NAME&#xd;&#xa;              ,BIZ_QTY&#xd;&#xa;              ,BIZ_RATE&#xd;&#xa;              ,BIZ_QTY_YOY&#xd;&#xa;              ,BIZ_QTY_MOM&#xd;&#xa;              ,TB_RATE&#xd;&#xa;              ,HB_RATE&#xd;&#xa;          FROM TEMP.TEMP_DMR_PC_STR_KPI_INDEX_M_7&#x29; T&#xd;&#xa;--SHARED DIMENSION JOINS&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; RGN&#xd;&#xa;    ON T.RGN_ID &#x3d; RGN.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; BASE&#xd;&#xa;    ON T.BASE_ID &#x3d; BASE.BASE_ID&#xd;&#xa;  LEFT JOIN DIM.DIM_BRAND BRAND&#xd;&#xa;    ON T.BRAND_ID &#x3d; BRAND.BRAND_ID&#xd;&#xa;   AND BRAND.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_MODEL MODEL&#xd;&#xa;    ON T.MODEL_ID &#x3d; MODEL.MODEL_ID&#xd;&#xa;   AND MODEL.ENABLE_FLAG &#x3d; &#x27;Y&#x27; &#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>864</xloc>
      <yloc>272</yloc>
      </entry>
    <entry>
      <name>TP_TEMP1</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_STR_KPI_INDEX_M_1&#xd;&#xa;SELECT MONTH_ID&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,MODEL_ID&#xd;&#xa;      ,&#x27;01&#x27;             AS INDEX_SEQ&#xd;&#xa;      ,&#x27;&#x6536;&#x8f66;&#x91cf;&#x27;         AS INDEX_NAME&#xd;&#xa;      ,BIZ_QTY          AS BIZ_QTY&#xd;&#xa;      ,NULL             AS BIZ_RATE&#xd;&#xa;      ,TB_QTY           AS BIZ_QTY_YOY&#xd;&#xa;      ,HB_QTY           AS BIZ_QTY_MOM&#xd;&#xa;      ,BIZ_QTY &#x2f; TB_QTY - 1 AS TB_RATE&#xd;&#xa;      ,BIZ_QTY &#x2f; HB_QTY - 1 AS HB_RATE&#xd;&#xa;  FROM &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS MONTH_ID&#xd;&#xa;              ,T.RGN_ID&#xd;&#xa;              ,T.BASE_ID&#xd;&#xa;              ,T.BRAND_ID&#xd;&#xa;              ,T.MODEL_ID&#xd;&#xa;              ,MAX&#x28;T.BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;              ,MAX&#x28;T.TB_QTY&#x29; AS TB_QTY&#xd;&#xa;              ,MAX&#x28;T.HB_QTY&#x29; AS HB_QTY&#xd;&#xa;          FROM &#x28;SELECT RGN_ID&#xd;&#xa;                      ,BASE_ID&#xd;&#xa;                      ,BRAND_ID&#xd;&#xa;                      ,MODEL_ID&#xd;&#xa;                      ,SUM&#x28;INSTOCK_QTY&#x29; AS BIZ_QTY&#xd;&#xa;                      ,NULL AS TB_QTY&#xd;&#xa;                      ,NULL AS HB_QTY&#xd;&#xa;                  FROM DMK.DMK_STR_OPERQTY_D&#xd;&#xa;                 WHERE SUBSTRING&#x28;PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                 GROUP BY RGN_ID, BASE_ID, BRAND_ID, MODEL_ID&#xd;&#xa;                &#xd;&#xa;                UNION ALL&#xd;&#xa;                &#xd;&#xa;                SELECT RGN_ID&#xd;&#xa;                      ,BASE_ID&#xd;&#xa;                      ,BRAND_ID&#xd;&#xa;                      ,MODEL_ID&#xd;&#xa;                      ,NULL AS BIZ_QTY&#xd;&#xa;                      ,SUM&#x28;INSTOCK_QTY&#x29; AS TB_QTY&#xd;&#xa;                      ,NULL AS HB_QTY&#xd;&#xa;                  FROM DMK.DMK_STR_OPERQTY_D&#xd;&#xa;                 WHERE SUBSTRING&#x28;PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                 GROUP BY RGN_ID, BASE_ID, BRAND_ID, MODEL_ID&#xd;&#xa;                &#xd;&#xa;                UNION ALL&#xd;&#xa;                &#xd;&#xa;                SELECT RGN_ID&#xd;&#xa;                      ,BASE_ID&#xd;&#xa;                      ,BRAND_ID&#xd;&#xa;                      ,MODEL_ID&#xd;&#xa;                      ,NULL AS BIZ_QTY&#xd;&#xa;                      ,NULL AS TB_QTY&#xd;&#xa;                      ,SUM&#x28;INSTOCK_QTY&#x29; AS HB_QTY&#xd;&#xa;                  FROM DMK.DMK_STR_OPERQTY_D&#xd;&#xa;                 WHERE SUBSTRING&#x28;PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                 GROUP BY RGN_ID, BASE_ID, BRAND_ID, MODEL_ID&#x29; T&#xd;&#xa;         GROUP BY T.RGN_ID, T.BASE_ID, T.BRAND_ID, T.MODEL_ID&#x29; A</sql>
