    <entry>
      <name>TO_TEMP2</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_STR_KPI_INDEX_M_2&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS MONTH_ID&#xd;&#xa;      ,A.RGN_ID AS RGN_ID&#xd;&#xa;      ,A.BASE_ID AS BASE_ID&#xd;&#xa;      ,A.BRAND_ID AS BRAND_ID&#xd;&#xa;      ,A.MODEL_ID AS MODEL_ID&#xd;&#xa;      ,&#x27;02&#x27; AS INDEX_SEQ&#xd;&#xa;      ,&#x27;&#x51fa;&#x5e93;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,A.BIZ_QTY AS BIZ_QTY&#xd;&#xa;      ,A.BIZ_RATE AS BIZ_RATE&#xd;&#xa;      ,B.BIZ_QTY           AS BIZ_QTY_YOY&#xd;&#xa;      ,C.BIZ_QTY           AS BIZ_QTY_MOM&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;B.BIZ_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          NULL&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;A.BIZ_QTY - B.BIZ_QTY&#x29; &#x2f; B.BIZ_QTY&#xd;&#xa;       END AS TB_RATE&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN NVL&#x28;C.BIZ_QTY, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;          NULL&#xd;&#xa;         ELSE&#xd;&#xa;          &#x28;A.BIZ_QTY - C.BIZ_QTY&#x29; &#x2f; C.BIZ_QTY&#xd;&#xa;       END AS HB_RATE&#xd;&#xa;  FROM &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,BRAND_ID&#xd;&#xa;              ,MODEL_ID&#xd;&#xa;              ,&#x27;02&#x27; AS INDEX_SEQ&#xd;&#xa;              ,&#x27;&#x51fa;&#x5e93;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;              ,SUM&#x28;OUTSTOCK_QTY&#x29; AS BIZ_QTY&#xd;&#xa;              ,NULL AS BIZ_RATE&#xd;&#xa;              ,NULL AS TB_RATE&#xd;&#xa;              ,NULL AS HB_RATE&#xd;&#xa;          FROM DMK.DMK_STR_OPERQTY_D&#xd;&#xa;         WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -0&#x29;,&#xd;&#xa;                                     &#x27;-&#x27;,&#xd;&#xa;                                     &#x27;&#x27;&#x29;,&#xd;&#xa;                      1,&#xd;&#xa;                      6&#x29;&#xd;&#xa;         GROUP BY SUBSTR&#x28;PERIOD_WID, 1, 6&#x29;&#xd;&#xa;                 ,RGN_ID&#xd;&#xa;                 ,BASE_ID&#xd;&#xa;                 ,BRAND_ID&#xd;&#xa;                 ,MODEL_ID&#x29; A&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,RGN_ID&#xd;&#xa;                   ,BASE_ID&#xd;&#xa;                   ,BRAND_ID&#xd;&#xa;                   ,MODEL_ID&#xd;&#xa;                   ,&#x27;02&#x27; AS INDEX_SEQ&#xd;&#xa;                   ,&#x27;&#x51fa;&#x5e93;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;                   ,SUM&#x28;OUTSTOCK_QTY&#x29; AS BIZ_QTY&#xd;&#xa;                   ,NULL AS BIZ_RATE&#xd;&#xa;                   ,NULL AS TB_RATE&#xd;&#xa;                   ,NULL AS HB_RATE&#xd;&#xa;               FROM DMK.DMK_STR_OPERQTY_D&#xd;&#xa;              WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                           1,&#xd;&#xa;                           6&#x29;&#xd;&#xa;              GROUP BY SUBSTR&#x28;PERIOD_WID, 1, 6&#x29;&#xd;&#xa;                      ,RGN_ID&#xd;&#xa;                      ,BASE_ID&#xd;&#xa;                      ,BRAND_ID&#xd;&#xa;                      ,MODEL_ID&#x29; B&#xd;&#xa;    ON A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;   AND A.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;   AND A.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;   AND A.MODEL_ID &#x3d; B.MODEL_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                   ,RGN_ID&#xd;&#xa;                   ,BASE_ID&#xd;&#xa;                   ,BRAND_ID&#xd;&#xa;                   ,MODEL_ID&#xd;&#xa;                   ,&#x27;02&#x27; AS INDEX_SEQ&#xd;&#xa;                   ,&#x27;&#x51fa;&#x5e93;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;                   ,SUM&#x28;OUTSTOCK_QTY&#x29; AS BIZ_QTY&#xd;&#xa;                   ,NULL AS BIZ_RATE&#xd;&#xa;                   ,NULL AS TB_RATE&#xd;&#xa;                   ,NULL AS HB_RATE&#xd;&#xa;               FROM DMK.DMK_STR_OPERQTY_D&#xd;&#xa;              WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;                    SUBSTR&#x28;REGEXP_REPLACE&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                           1,&#xd;&#xa;                           6&#x29;&#xd;&#xa;              GROUP BY SUBSTR&#x28;PERIOD_WID, 1, 6&#x29;&#xd;&#xa;                      ,RGN_ID&#xd;&#xa;                      ,BASE_ID&#xd;&#xa;                      ,BRAND_ID&#xd;&#xa;                      ,MODEL_ID&#x29; C&#xd;&#xa;    ON A.RGN_ID &#x3d; C.RGN_ID&#xd;&#xa;   AND A.BASE_ID &#x3d; C.BASE_ID&#xd;&#xa;   AND A.BRAND_ID &#x3d; C.BRAND_ID&#xd;&#xa;   AND A.MODEL_ID &#x3d; C.MODEL_ID&#x3b;&#xd;&#xa;</sql>
