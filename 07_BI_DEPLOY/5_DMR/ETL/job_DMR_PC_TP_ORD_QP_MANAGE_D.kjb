LDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_QP_MANAGE_D_2&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_QP_MANAGE_D_2&#x28;&#xd;&#xa;DATE_ID	STRING	COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID	STRING	COMMENT &#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,RGN_NAME	STRING	COMMENT &#x27;&#x5927;&#x533a;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,BASE_ID	STRING	COMMENT &#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BASE_NAME	STRING	COMMENT &#x27;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,BRAND_ID	STRING	COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,BRAND_NAME	STRING	COMMENT &#x27;&#x54c1;&#x724c;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,TPMODE_ID    STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;&#xd;&#xa;,TPMODE_NAME    STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,SUPPLIER_ID	STRING	COMMENT &#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,SUPPLIER_NAME	STRING	COMMENT &#x27;&#x627f;&#x8fd0;&#x5546;&#x540d;&#x79f0;&#x27;&#xd;&#xa;,CUR_TGT_QP_QTY		DECIMAL&#x28;32,8&#x29;	COMMENT &#x27;&#x5f53;&#x65e5;&#x8ba1;&#x5212;&#x542f;&#x7968;&#x91cf;&#x27;&#xd;&#xa;,CUR_TGT_ALLOC_QTY		DECIMAL&#x28;32,8&#x29;	COMMENT &#x27;&#x5f53;&#x65e5;&#x8ba1;&#x5212;&#x5206;&#x914d;&#x91cf;&#x27;&#xd;&#xa;,CUR_QP_QTY		DECIMAL&#x28;32,8&#x29;	COMMENT &#x27;&#x5f53;&#x65e5;&#x542f;&#x7968;&#x91cf;&#x27;&#xd;&#xa;,CUR_ALLOC_QTY		DECIMAL&#x28;32,8&#x29;	COMMENT &#x27;&#x5f53;&#x65e5;&#x5206;&#x914d;&#x91cf;&#x27;&#xd;&#xa;&#x29;COMMENT &#x27;&#x5b9e;&#x9645;&#x542f;&#x7968;&#x4e34;&#x65f6;&#x8868;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_QP_MANAGE_D_3&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_QP_MANAGE_D_3&#x28;&#xd;&#xa;DATE_ID	STRING	COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,R