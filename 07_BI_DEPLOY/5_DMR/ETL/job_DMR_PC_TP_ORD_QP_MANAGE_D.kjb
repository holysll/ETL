      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_3&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_QP_MANAGE_D_3&#xd;&#xa;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;A.period_wid, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS DATE_ID&#xd;&#xa;      ,coalesce&#x28;A.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,coalesce&#x28;B.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;      ,coalesce&#x28;A.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,coalesce&#x28;C.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,coalesce&#x28;A.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,coalesce&#x28;D.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME&#xd;&#xa;      ,coalesce&#x28;A.tpmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;      ,coalesce&#x28;E.TPMODE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS TPMODE_NAME&#xd;&#xa;      ,coalesce&#x28;A.supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;      ,coalesce&#x28;F.SERVPROV_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS SUPPLIER_NAME&#xd;&#xa;	  ,NULL&#xd;&#xa;	  ,NULL&#xd;&#xa;	  ,NULL&#xd;&#xa;      ,SUM&#x28;allot_qty&#x29; AS CUR_ALLOC_QTY  --&#x5f53;&#x6708;&#x5206;&#x914d;&#x91cf;&#xd;&#xa;  FROM dmk.dmk_tp_orderqty_d A&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; B&#xd;&#xa;    ON A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; C&#xd;&#xa;    ON A.BASE_ID &#x3d; C.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; D&#xd;&#xa;    ON A.BRAND_ID &#x3d; D.BRAND_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT TPMODE_ID&#xd;&#xa;                   ,TPMODE_NAME&#xd;&#xa;               FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;              GROUP BY TPMODE_ID&#xd;&#xa;                      ,TPMODE_NAME&#x29; E&#xd;&#xa;    ON A.TPMODE_ID &#x3d; E.TPMODE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT SERVPROV_ID&#xd;&#xa;                   ,SERVPROV_NAME&#xd;&#xa;               FROM DIM.DIM_SERVPROV&#xd;&#xa;              GROUP BY SERVPROV_ID&#xd;&#xa;                      ,SERVPROV_NAME&#x29; F&#xd;&#xa;    ON A.SUPPLIER_ID &#x3d; F.SERVPROV_ID&#xd;&#xa; WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;A.period_wid, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;       SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa; GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;A.period_wid, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;&#xd;&#xa;      ,coalesce&#x28;A.rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;      ,coalesce&#x28;B.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; &#xd;&#xa;      ,coalesce&#x28;A.BASE_ID,&#x27;-1&#x27;&#x29; &#xd;&#xa;      ,coalesce&#x28;C.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; &#xd;&#xa;      ,coalesce&#x28;A.brand_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;      ,coalesce&#x28;D.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;      ,coalesce&#x28;A.tpmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;      ,coalesce&#x28;E.TPMODE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; &#xd;&#xa;      ,coalesce&#x28;A.supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;      ,coalesce&#x28;F.SERVPROV_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; </sql>
