    <entry>
      <name>TO_TEMP_1</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_1&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_QP_MANAGE_D_1&#xd;&#xa;--&#x542f;&#x7968;,&#x5206;&#x914d;&#xd;&#xa;SELECT P.PERIOD_WID AS DATE_ID --&#x65e5;&#x671f;ID&#xd;&#xa;      ,NVL&#x28;B.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;      ,NVL&#x28;A.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;      ,NVL&#x28;C.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,NVL&#x28;A.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;      ,NVL&#x28;D.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID --&#x54c1;&#x724c;ID&#xd;&#xa;      ,NVL&#x28;A.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME --&#x54c1;&#x724c;&#x540d;&#x79f0;&#xd;&#xa;      ,NVL&#x28;E.TPMODE_ID, &#x27;-1&#x27;&#x29; AS TPMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;      ,NVL&#x28;A.TP_MODE, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS TP_MODE --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x540d;&#x79f0;&#xd;&#xa;      ,&#x27;ALL&#x27; AS SUPPLIER_ID&#xd;&#xa;      ,&#x27;ALL&#x27; AS SUPPLIER_NAME&#xd;&#xa;      ,SUM&#x28;A.REV_PLAN&#x29; &#x2f; MAX&#x28;P.CNT&#x29; AS CUR_TGT_QP_QTY --&#x5f53;&#x65e5;&#x8ba1;&#x5212;&#x542f;&#x7968;&#x91cf;&#xd;&#xa;      ,SUM&#x28;A.REV_PLAN&#x29; &#x2f; MAX&#x28;P.CNT&#x29; AS CUR_TGT_ALLOC_QTY --&#x5f53;&#x65e5;&#x8ba1;&#x5212;&#x5206;&#x914d;&#x91cf;&#xd;&#xa;      ,NULL&#xd;&#xa;      ,NULL&#xd;&#xa;  FROM ODS_BLPT.T_HAND_REV_TICKET_PLAN A&#xd;&#xa; INNER JOIN &#x28;SELECT PERIOD_WID&#xd;&#xa;                   ,COUNT&#x28;1&#x29; OVER&#x28;PARTITION BY SUBSTR&#x28;PERIOD_WID, 1, 6&#x29;&#x29; CNT&#xd;&#xa;               FROM DIM.DIM_PERIOD_D&#xd;&#xa;              WHERE PERIOD_TYPE &#x3d; &#x27;D&#x27;&#x29; P&#xd;&#xa;    ON A.MONTH_ID &#x3d; SUBSTR&#x28;P.PERIOD_WID, 1, 6&#x29;&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; B&#xd;&#xa;    ON A.RGN_NAME &#x3d; B.RGN_NAME&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; C&#xd;&#xa;    ON A.BASE_NAME &#x3d; C.BASE_NAME&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; D&#xd;&#xa;    ON A.BRAND_NAME &#x3d; D.BRAND_NAME&#xd;&#xa;  LEFT JOIN &#x28;SELECT TPMODE_ID&#xd;&#xa;                   ,TPMODE_NAME&#xd;&#xa;               FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;              GROUP BY TPMODE_ID&#xd;&#xa;                      ,TPMODE_NAME&#x29; E&#xd;&#xa;    ON A.TP_MODE &#x3d; E.TPMODE_NAME&#xd;&#xa; WHERE A.MONTH_ID &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#xd;&#xa; GROUP BY P.PERIOD_WID&#xd;&#xa;         ,NVL&#x28;B.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;A.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;C.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;A.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;D.BRAND_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;A.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;E.TPMODE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,NVL&#x28;A.TP_MODE, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
