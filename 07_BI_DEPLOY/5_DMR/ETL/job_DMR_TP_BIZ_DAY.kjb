      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_DAY_02&#xd;&#xa;SELECT PERIOD_WID&#xd;&#xa;    ,RGN_ID&#xd;&#xa;    ,BASE_ID&#xd;&#xa;    ,BRAND_ID&#xd;&#xa;    ,tpmode_id&#xd;&#xa;    ,INDEX_ID&#xd;&#xa;    ,INDEX_NAME&#xd;&#xa;    ,sum&#x28;BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;    ,sum&#x28;INVOICE_COMP_QTY&#x29; AS INVOICE_COMP_QTY&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT A.PERIOD_WID&#xd;&#xa;          ,A.RGN_ID&#xd;&#xa;          ,A.BASE_ID&#xd;&#xa;          ,A.BRAND_ID&#xd;&#xa;          ,A.tpmode_id&#xd;&#xa;          ,&#x27;K02&#x27;AS INDEX_ID&#xd;&#xa;          ,&#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x6b21;&#x27; AS INDEX_NAME&#xd;&#xa;          ,sum&#x28;A.shipping_qty&#x29; AS BIZ_QTY&#xd;&#xa;          ,0 AS INVOICE_COMP_QTY&#xd;&#xa;    FROM DMK.dmk_tp_shipqty_d A  &#xd;&#xa;    LEFT JOIN DIM.dim_org B&#xd;&#xa;    ON A.base_id &#x3d; B.base_id AND B.org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    WHERE substr&#x28;A.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND substr&#x28;A.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;        --AND B.base_type &#x3d; &#x27;VDC&#x27;&#xd;&#xa;        AND A.brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;        AND A.tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;    GROUP BY A.PERIOD_WID&#xd;&#xa;          ,A.RGN_ID&#xd;&#xa;          ,A.BASE_ID&#xd;&#xa;          ,A.BRAND_ID&#xd;&#xa;          ,A.tpmode_id&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT P.period_wid AS PERIOD_WID&#xd;&#xa;        ,coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,&#x27;ALL&#x27; AS TPMODE_ID&#xd;&#xa;        ,&#x27;K02&#x27; AS INDEX_ID&#xd;&#xa;        ,&#x27;&#x8fd0;&#x8f93;&#x603b;&#x91cf;&#x6b21;&#x27; AS INDEX_NAME&#xd;&#xa;        ,0 AS BIZ_QTY&#xd;&#xa;        ,sum&#x28;T.biz_qty&#x29;&#x2f;30 AS INVOICE_COMP_QTY&#xd;&#xa;    FROM &#x28;&#xd;&#xa;    SELECT period_wid&#xd;&#xa;    FROM DIM.dim_period_d&#xd;&#xa;    WHERE period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;        &#x29; P&#xd;&#xa;    LEFT JOIN DMK.dmk_tp_bizqtytgt_m T&#xd;&#xa;    ON T.period_wid &#x3d; substr&#x28;P.period_wid,1,6&#x29;&#xd;&#xa;    WHERE T.period_wid &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;		AND SUBSTR&#x28;T.PERIOD_WID,1,8&#x29; &#x3c;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;        AND T.rgn_id NOT IN &#x28;&#x27;BOYU&#x27;,&#x27;CENTER&#x27;&#x29;&#xd;&#xa;    GROUP BY P.period_wid &#xd;&#xa;        ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;        ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY PERIOD_WID&#xd;&#xa;    ,RGN_ID&#xd;&#xa;    ,BASE_ID&#xd;&#xa;    ,BRAND_ID&#xd;&#xa;    ,tpmode_id&#xd;&#xa;    ,INDEX_ID&#xd;&#xa;    ,INDEX_NAME&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;      ,A.RGN_ID&#xd;&#xa;      ,A.BASE_ID&#xd;&#xa;      ,A.BRAND_ID&#xd;&#xa;      ,A.SEGMETN_TRANSPORTMODE&#xd;&#xa;      ,&#x27;K02&#x27;&#xd;&#xa;      ,&#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x6b21;&#x27;&#xd;&#xa;      ,A.BIZ_QTY&#xd;&#xa;      ,NULL AS INVOICE_COMP_QTY&#xd;&#xa;  FROM &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;F.UNLOAD_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,F.RGN_ID&#xd;&#xa;              ,F.BASE_ID&#xd;&#xa;              ,F.BRAND_ID&#xd;&#xa;              ,F.SEGMETN_TRANSPORTMODE&#xd;&#xa;              ,COUNT&#x28;1&#x29; AS BIZ_QTY&#xd;&#xa;          FROM DW.DW_TP_ORDERMOVEMENT F --&#x539f;&#x8868; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;  &#xd;&#xa;         WHERE F.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND F.SEGMETN_TRANSPORTMODE NOT IN &#x28;&#x27;ZT&#x27;&#x29; --&#x81ea;&#x63d0;&#x4e0d;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTR&#x28;F.UNLOAD_DAY, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND F.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;         GROUP BY REGEXP_REPLACE&#x28;SUBSTR&#x28;F.UNLOAD_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                 ,F.RGN_ID&#xd;&#xa;                 ,F.BASE_ID&#xd;&#xa;                 ,F.BRAND_ID&#xd;&#xa;                 ,F.SEGMETN_TRANSPORTMODE&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,F.RGN_ID&#xd;&#xa;              ,F.BASE_ID&#xd;&#xa;              ,F.BRAND_ID&#xd;&#xa;              ,F.SEGMETN_TRANSPORTMODE&#xd;&#xa;              ,COUNT&#x28;1&#x29; AS BIZ_QTY&#xd;&#xa;          FROM DW.DW_TP_ORDERMOVEMENT F --&#x539f;&#x8868; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29; &#xd;&#xa;         WHERE F.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND F.SEGMETN_TRANSPORTMODE NOT IN &#x28;&#x27;ZT&#x27;&#x29; --&#x81ea;&#x63d0;&#x4e0d;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND F.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;         GROUP BY REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                 ,F.RGN_ID&#xd;&#xa;                 ,F.BASE_ID&#xd;&#xa;                 ,F.BRAND_ID&#xd;&#xa;                 ,F.SEGMETN_TRANSPORTMODE&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;F.EARLY_PICKUP_DATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,F.RGN_ID&#xd;&#xa;              ,F.BASE_ID&#xd;&#xa;              ,F.BRAND_ID&#xd;&#xa;              ,F.SEGMETN_TRANSPORTMODE&#xd;&#xa;              ,COUNT&#x28;1&#x29; AS BIZ_QTY&#xd;&#xa;          FROM DW.DW_TP_ORDERMOVEMENT F --&#x539f;&#x8868; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29; &#xd;&#xa;         WHERE F.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND F.SEGMETN_TRANSPORTMODE NOT IN &#x28;&#x27;ZT&#x27;&#x29; --&#x81ea;&#x63d0;&#x4e0d;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTR&#x28;F.EARLY_PICKUP_DATE, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND F.BRAND_ID NOT IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;         GROUP BY REGEXP_REPLACE&#x28;SUBSTR&#x28;F.EARLY_PICKUP_DATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                 ,F.RGN_ID&#xd;&#xa;                 ,F.BASE_ID&#xd;&#xa;                 ,F.BRAND_ID&#xd;&#xa;                 ,F.SEGMETN_TRANSPORTMODE&#x29; A&#x3b;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;</sql>
