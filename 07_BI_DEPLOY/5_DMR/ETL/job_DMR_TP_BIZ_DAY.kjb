      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_DAY_04&#xd;&#xa;SELECT PERIOD_WID&#xd;&#xa;      ,RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,BRAND_ID&#xd;&#xa;      ,tpmode_id&#xd;&#xa;      ,&#x27;K04&#x27; AS INDEX_ID&#xd;&#xa;      ,&#x27;&#x8fd0;&#x8f93;&#x603b;&#x91cc;&#x7a0b;&#x27; AS INDEX_NAME&#xd;&#xa;      ,SUM&#x28;MILEGE&#x29; AS MILEGE&#xd;&#xa;      ,NULL AS MILEGE_PLAN&#xd;&#xa;  FROM DMK.dmk_tp_shipqty_d&#xd;&#xa; WHERE SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;       REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;    AND SUBSTR&#x28;PERIOD_WID, 1, 8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    AND tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;    AND brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa; GROUP BY PERIOD_WID&#xd;&#xa;         ,RGN_ID&#xd;&#xa;         ,BASE_ID&#xd;&#xa;         ,BRAND_ID&#xd;&#xa;         ,tpmode_id&#x3b;&#xd;&#xa;</sql>
