      <name>CREATE_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_BIZ_DAY_01&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_BIZ_DAY_01&#x28;&#xd;&#xa;PERIOD_WID             BIGINT        COMMENT  &#x27;&#x671f;&#x95f4;&#x27;,&#xd;&#xa;RGN_ID                 STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID                STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BRAND_ID               STRING , &#xd;&#xa;TPMODE_ID              STRING        COMMENT  &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;, &#xd;&#xa;INDEX_ID               STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME             STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M            DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5b9e;&#x9645;&#x27;,&#xd;&#xa;TAR_VALUE_M            DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;&#x29;COMMENT &#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x5206;&#x6790;&#xff08;&#x65e5;&#xff09;-&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x7ba1;&#x7406;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_BIZ_DAY_02&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_BIZ_DAY_02&#x28;&#xd;&#xa;PERIOD_WID             BIGINT        COMMENT  &#x27;&#x671f;&#x95f4;&#x27;,&#xd;&#xa;RGN_ID                 STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID                STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BRAND_ID               STRING , &#xd;&#xa;TPMODE_ID              STRING        COMMENT  &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;, &#xd;&#xa;INDEX_ID               STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME             STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M            DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5b9e;&#x9645;&#x27;,&#xd;&#xa;TAR_VALUE_M            DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;&#x29;COMMENT &#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x5206;&#x6790;&#xff08;&#x65e5;&#xff09;-&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x7ba1;&#x7406;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_BIZ_DAY_03&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_BIZ_DAY_03&#x28;&#xd;&#xa;PERIOD_WID             BIGINT        COMMENT  &#x27;&#x671f;&#x95f4;&#x27;,&#xd;&#xa;RGN_ID                 STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID                STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BRAND_ID               STRING , &#xd;&#xa;TPMODE_ID              STRING        COMMENT  &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;, &#xd;&#xa;INDEX_ID               STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME             STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M            DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5b9e;&#x9645;&#x27;,&#xd;&#xa;TAR_VALUE_M            DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;&#x29;COMMENT &#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x5206;&#x6790;&#xff08;&#x65e5;&#xff09;-&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x7ba1;&#x7406;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_BIZ_DAY_04&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_BIZ_DAY_04&#x28;&#xd;&#xa;PERIOD_WID             BIGINT        COMMENT  &#x27;&#x671f;&#x95f4;&#x27;,&#xd;&#xa;RGN_ID                 STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID                STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BRAND_ID               STRING , &#xd;&#xa;TPMODE_ID              STRING        COMMENT  &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;, &#xd;&#xa;INDEX_ID               STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME             STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M            DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5b9e;&#x9645;&#x27;,&#xd;&#xa;TAR_VALUE_M            DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;&#x29;COMMENT &#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x5206;&#x6790;&#xff08;&#x65e5;&#xff09;-&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x7ba1;&#x7406;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_TP_BIZ_DAY_05&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_TP_BIZ_DAY_05&#x28;&#xd;&#xa;PERIOD_WID             BIGINT        COMMENT  &#x27;&#x671f;&#x95f4;&#x27;,&#xd;&#xa;RGN_ID                 STRING        COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID                STRING        COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BRAND_ID               STRING , &#xd;&#xa;TPMODE_ID              STRING        COMMENT  &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;, &#xd;&#xa;INDEX_ID               STRING        COMMENT  &#x27;&#x6307;&#x6807;ID&#x27;,&#xd;&#xa;INDEX_NAME             STRING        COMMENT  &#x27;&#x6307;&#x6807;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;ACT_VALUE_M            DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x5b9e;&#x9645;&#x27;,&#xd;&#xa;TAR_VALUE_M            DECIMAL&#x28;32,8&#x29; COMMENT  &#x27;&#x672c;&#x671f;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;&#x29;COMMENT &#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x5206;&#x6790;&#xff08;&#x65e5;&#xff09;-&#x5355;&#x8f66;&#x5e73;&#x5747;&#x91cc;&#x7a0b;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;</sql>
