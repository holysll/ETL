    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_TP_BIZ_DAY PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT TMP.PERIOD_WID,&#xd;&#xa;       NVL&#x28;RGN.RGN_ID,&#x27;-1&#x27;&#x29; AS RGN_ID,&#xd;&#xa;       NVL&#x28;RGN.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME,&#xd;&#xa;       NVL&#x28;BAS.BASE_ID,&#x27;-1&#x27;&#x29; AS BASE_ID,&#xd;&#xa;       NVL&#x28;BAS.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME,&#xd;&#xa;       NVL&#x28;B.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID,&#xd;&#xa;       NVL&#x28;B.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME, &#xd;&#xa;       NVL&#x28;TPT.TPMODE_ID,&#x27;-1&#x27;&#x29;,&#xd;&#xa;       NVL&#x28;TPT.TPMODE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS TPMODE_NAME, &#xd;&#xa;       TMP.INDEX_ID,&#xd;&#xa;       TMP.INDEX_NAME,&#xd;&#xa;       TMP.ACT_VALUE_M,&#xd;&#xa;       TMP.TAR_VALUE_M,&#xd;&#xa;       FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;&#x29; AS W_INSERT_DT, --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;       &#x27;DW&#x27;                            AS PART_SC,     --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;       SUBSTR&#x28;TMP.PERIOD_WID,1,6&#x29;  AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;  FROM &#x28;SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_DAY_01&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_DAY_02&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_DAY_03&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_DAY_04&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a;&#xd;&#xa;          FROM TEMP.TEMP_DMR_TP_BIZ_DAY_05&#xd;&#xa;       &#x29; TMP&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG RGN&#xd;&#xa;           ON TMP.RGN_ID &#x3d; RGN.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_ORG BAS&#xd;&#xa;           ON TMP.BASE_ID &#x3d; BAS.ORG_ID&#xd;&#xa;    LEFT JOIN DIM.DIM_TRANSPORT_MODE TPT&#xd;&#xa;           ON TMP.TPMODE_ID &#x3d; TPT.TPMODE_ID &#xd;&#xa;    LEFT JOIN DIM.DIM_BRAND B&#xd;&#xa;           ON TMP.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>544</xloc>
      <yloc>416</yloc>
      </entry>
    <entry>
      <name>TMP1</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_DAY_01&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.TPMODE_ID&#xd;&#xa;    ,T.INDEX_ID&#xd;&#xa;    ,T.INDEX_NAME&#xd;&#xa;    ,sum&#x28;BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;    ,sum&#x28;INVOICE_COMP_QTY&#x29; AS INVOICE_COMP_QTY&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT P.period_wid AS PERIOD_WID&#xd;&#xa;        ,coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,&#x27;ALL&#x27; AS TPMODE_ID&#xd;&#xa;        ,&#x27;K01&#x27; AS INDEX_ID&#xd;&#xa;        ,&#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;        ,0 AS BIZ_QTY&#xd;&#xa;        ,sum&#x28;T.biz_qty&#x29;&#x2f;30 AS INVOICE_COMP_QTY&#xd;&#xa;    FROM &#x28;&#xd;&#xa;    SELECT period_wid&#xd;&#xa;    FROM DIM.dim_period_d&#xd;&#xa;    WHERE period_type &#x3d; &#x27;D&#x27;&#x29; P&#xd;&#xa;    LEFT JOIN DMK.dmk_tp_bizqtytgt_m T&#xd;&#xa;    ON substr&#x28;P.period_wid,1,6&#x29; &#x3d; T.period_wid&#xd;&#xa;    LEFT JOIN DIM.dim_org T2&#xd;&#xa;    ON T.base_name &#x3d; T2.base_name AND T2.org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    WHERE T.period_wid &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND T2.base_type &#x3d; &#x27;VDC&#x27;&#xd;&#xa;        AND T.rgn_id NOT IN &#x28;&#x27;BOYU&#x27;,&#x27;CENTER&#x27;&#x29;&#xd;&#xa;    GROUP BY P.period_wid&#xd;&#xa;        ,coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT A.PERIOD_WID&#xd;&#xa;          ,A.RGN_ID&#xd;&#xa;          ,A.BASE_ID&#xd;&#xa;          ,A.BRAND_ID&#xd;&#xa;          ,A.tpmode_id&#xd;&#xa;          ,&#x27;K01&#x27; AS INDEX_ID&#xd;&#xa;          ,&#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;          ,sum&#x28;A.shipping_qty&#x29; AS BIZ_QTY&#xd;&#xa;          ,0 AS INVOICE_COMP_QTY&#xd;&#xa;    FROM DMK.dmk_tp_shipqty_d A  &#xd;&#xa;    LEFT JOIN DIM.dim_org B&#xd;&#xa;    ON A.base_id &#x3d; B.base_id AND B.org_type &#x3d; &#x27;B&#x27;&#xd;&#xa;    WHERE substr&#x28;A.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        AND substr&#x28;A.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;        AND B.base_type &#x3d; &#x27;VDC&#x27;&#xd;&#xa;        AND A.brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa;        AND A.tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;    GROUP BY A.PERIOD_WID&#xd;&#xa;          ,A.RGN_ID&#xd;&#xa;          ,A.BASE_ID&#xd;&#xa;          ,A.BRAND_ID&#xd;&#xa;          ,A.tpmode_id&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.PERIOD_WID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.TPMODE_ID&#xd;&#xa;    ,T.INDEX_ID&#xd;&#xa;    ,T.INDEX_NAME&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT A.PERIOD_WID&#xd;&#xa;      ,A.RGN_ID&#xd;&#xa;      ,A.BASE_ID&#xd;&#xa;      ,A.BRAND_ID&#xd;&#xa;      ,A.SEGMETN_TRANSPORTMODE&#xd;&#xa;      ,&#x27;K01&#x27;&#xd;&#xa;      ,&#x27;&#x8fd0;&#x8f93;&#x4e1a;&#x52a1;&#x91cf;&#x27;&#xd;&#xa;      ,A.BIZ_QTY&#xd;&#xa;      ,NULL AS INVOICE_COMP_QTY&#xd;&#xa;  FROM &#x28;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;F.UNLOAD_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,R.RGN_ID&#xd;&#xa;              ,R.BASE_ID&#xd;&#xa;              ,R.BRAND_ID&#xd;&#xa;              ,R.TRANSPORTMODE AS SEGMETN_TRANSPORTMODE&#xd;&#xa;              ,COUNT&#x28;DISTINCT F.VIN&#x29; AS BIZ_QTY&#xd;&#xa;          FROM DW.DW_TP_ORDERMOVEMENT F --&#x539f;&#x8868; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29; &#xd;&#xa;         INNER JOIN DW.DW_TP_ORDERRELEASE R&#xd;&#xa;            ON F.ORDER_RELEASE_GID &#x3d; R.ORDER_RELEASE_GID&#xd;&#xa;         JOIN DIM.DIM_ORG B&#xd;&#xa;              ON F.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;             AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;           WHERE F.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;             AND B.BASE_TYPE &#x3d; &#x27;VDC&#x27; --&#x53ea;&#x53d6;&#x57fa;&#x5730;&#xff0c;&#x4e2d;&#x8f6c;&#x7ad9;&#x4e0d;&#x8ba1;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND F.SEGMETN_TRANSPORTMODE NOT IN &#x28;&#x27;ZT&#x27;&#x29; --&#x81ea;&#x63d0;&#x4e0d;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTR&#x28;F.UNLOAD_DAY, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND F.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;         GROUP BY REGEXP_REPLACE&#x28;SUBSTR&#x28;F.UNLOAD_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                 ,R.RGN_ID&#xd;&#xa;                 ,R.BASE_ID&#xd;&#xa;                 ,R.BRAND_ID&#xd;&#xa;                 ,R.TRANSPORTMODE&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,R.RGN_ID&#xd;&#xa;              ,R.BASE_ID&#xd;&#xa;              ,R.BRAND_ID&#xd;&#xa;              ,R.TRANSPORTMODE AS SEGMETN_TRANSPORTMODE&#xd;&#xa;              ,COUNT&#x28;DISTINCT F.VIN&#x29; AS BIZ_QTY&#xd;&#xa;          FROM DW.DW_TP_ORDERMOVEMENT F --&#x539f;&#x8868; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;         INNER JOIN DW.DW_TP_ORDERRELEASE R&#xd;&#xa;            ON F.ORDER_RELEASE_GID &#x3d; R.ORDER_RELEASE_GID&#xd;&#xa;         JOIN DIM.DIM_ORG B&#xd;&#xa;              ON F.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;             AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;           WHERE F.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;             AND B.BASE_TYPE &#x3d; &#x27;VDC&#x27; --&#x53ea;&#x53d6;&#x57fa;&#x5730;&#xff0c;&#x4e2d;&#x8f6c;&#x7ad9;&#x4e0d;&#x8ba1;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND F.SEGMETN_TRANSPORTMODE NOT IN &#x28;&#x27;ZT&#x27;&#x29; --&#x81ea;&#x63d0;&#x4e0d;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND F.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;         GROUP BY REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                 ,R.RGN_ID&#xd;&#xa;                 ,R.BASE_ID&#xd;&#xa;                 ,R.BRAND_ID&#xd;&#xa;                 ,R.TRANSPORTMODE&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;F.EARLY_PICKUP_DATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;              ,R.RGN_ID&#xd;&#xa;              ,R.BASE_ID&#xd;&#xa;              ,R.BRAND_ID&#xd;&#xa;              ,R.TRANSPORTMODE AS SEGMETN_TRANSPORTMODE&#xd;&#xa;              ,COUNT&#x28;DISTINCT F.VIN&#x29; AS BIZ_QTY&#xd;&#xa;          FROM DW.DW_TP_ORDERMOVEMENT F --&#x539f;&#x8868; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29; &#xd;&#xa;         INNER JOIN DW.DW_TP_ORDERRELEASE R&#xd;&#xa;            ON F.ORDER_RELEASE_GID &#x3d; R.ORDER_RELEASE_GID&#xd;&#xa;         JOIN DIM.DIM_ORG B&#xd;&#xa;              ON F.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;             AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;           WHERE F.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;             AND B.BASE_TYPE &#x3d; &#x27;VDC&#x27; --&#x53ea;&#x53d6;&#x57fa;&#x5730;&#xff0c;&#x4e2d;&#x8f6c;&#x7ad9;&#x4e0d;&#x8ba1;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND F.SEGMETN_TRANSPORTMODE NOT IN &#x28;&#x27;ZT&#x27;&#x29; --&#x81ea;&#x63d0;&#x4e0d;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;           AND REGEXP_REPLACE&#x28;SUBSTR&#x28;F.EARLY_PICKUP_DATE, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           AND F.BRAND_ID NOT IN &#x28;&#x27;CA&#x27;, &#x27;FT&#x27;&#x29;&#xd;&#xa;         GROUP BY REGEXP_REPLACE&#x28;SUBSTR&#x28;F.EARLY_PICKUP_DATE, 1, 10&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                 ,R.RGN_ID&#xd;&#xa;                 ,R.BASE_ID&#xd;&#xa;                 ,R.BRAND_ID&#xd;&#xa;                 ,R.TRANSPORTMODE&#x29; A&#x3b;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;&#xd;&#xa;</sql>
