      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_DAY_05&#xd;&#xa;  SELECT A.PERIOD_WID&#xd;&#xa;        ,A.RGN_ID&#xd;&#xa;        ,A.BASE_ID&#xd;&#xa;        ,A.BRAND_ID&#xd;&#xa;        ,A.TPMODE_ID&#xd;&#xa;        ,&#x27;K05&#x27;&#xd;&#xa;        ,&#x27;&#x5355;&#x8f66;&#x5e73;&#x5747;&#x91cc;&#x7a0b;&#x27;&#xd;&#xa;        ,CASE&#xd;&#xa;           WHEN NVL&#x28;A.ACT_VALUE_M, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;            NULL&#xd;&#xa;           ELSE&#xd;&#xa;            B.ACT_VALUE_M &#x2f; A.ACT_VALUE_M&#xd;&#xa;         END AS ACT_VALUE_M&#xd;&#xa;        ,NULL AS TAR_VALUE_M&#xd;&#xa;    FROM TEMP.TEMP_DMR_TP_BIZ_DAY_01 A&#xd;&#xa;    JOIN TEMP.TEMP_DMR_TP_BIZ_DAY_04 B&#xd;&#xa;      ON A.PERIOD_WID &#x3d; B.PERIOD_WID&#xd;&#xa;     AND A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;     AND A.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;     AND A.BRAND_ID &#x3d; B.BRAND_ID&#xd;&#xa;     AND A.TPMODE_ID &#x3d; B.TPMODE_ID&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
