      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DIM.DIM_ORG &#xd;&#xa;--Target Table in Hive&#x3a; DMR.DMR_OV_RGN_PROVINCE_RELA&#xd;&#xa;--Reference&#x3a; &#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_TP_BIZ_DAY_03&#xd;&#xa;SELECT T.period_wid&#xd;&#xa;    ,coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID &#xd;&#xa;    ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID &#xd;&#xa;    ,coalesce&#x28;T.tpmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;    ,&#x27;K03&#x27; AS INDEX_ID&#xd;&#xa;    ,&#x27;&#x79fb;&#x5e93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;    ,sum&#x28;T.shipping_qty&#x29; AS MOV_QTY&#xd;&#xa;    ,NULL AS MOV_PLAN_QTY&#xd;&#xa;FROM DMK.dmk_tp_orderqty_d T &#xd;&#xa;WHERE substr&#x28;T.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    AND substr&#x28;T.period_wid,1,8&#x29; &#x3c;&#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    AND T.plan_type &#x3d; &#x27;2&#x27;&#xd;&#xa;GROUP BY T.period_wid&#xd;&#xa;    ,coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;    ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;    ,coalesce&#x28;T.tpmode_id,&#x27;-1&#x27;&#x29;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT P.period_wid&#xd;&#xa;    ,coalesce&#x28;R.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;O.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;T.segmetn_transportmode,&#x27;-1&#x27;&#x29; AS TPMODE&#xd;&#xa;    ,&#x27;K03&#x27; AS INDEX_ID&#xd;&#xa;    ,&#x27;&#x79fb;&#x5e93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;    ,count&#x28;O.vin&#x29; AS MOV_QTY&#xd;&#xa;    ,NULL AS MOV_PLAN_QTY&#xd;&#xa;FROM DW.dw_tp_ordermovement T&#xd;&#xa;INNER JOIN DW.dw_tp_orderrelease O&#xd;&#xa;ON T.order_release_gid &#x3d; O.order_release_gid&#xd;&#xa;INNER JOIN &#x28;&#xd;&#xa;    SELECT rgn_id&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id&#xd;&#xa;    &#x29; R&#xd;&#xa;ON O.RGN_ID &#x3d; R.RGN_ID&#xd;&#xa;INNER JOIN DIM.dim_period_d P  &#xd;&#xa;ON FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;O.FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - 28800,&#x27;yyyyMMdd&#x27;&#x29; &#x3d; P.period_wid AND P.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;WHERE &#x28;substr&#x28;P.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    &#x29;&#xd;&#xa;    AND O.or_plan_type &#x3d; &#x27;&#x79fb;&#x5e93;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;    AND O.brand_id &#x3d; &#x27;FT&#x27;&#xd;&#xa;    AND O.is_known_shipper &#x3d; &#x27;N&#x27;&#xd;&#xa;    AND O.vin IS NOT NULL&#xd;&#xa;    AND O.arrive_day IS NOT NULL&#xd;&#xa;GROUP BY P.period_wid&#xd;&#xa;    ,coalesce&#x28;R.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;O.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.segmetn_transportmode,&#x27;-1&#x27;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT P.period_wid&#xd;&#xa;    ,coalesce&#x28;R.rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;    ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;    ,coalesce&#x28;T.segmetn_transportmode,&#x27;-1&#x27;&#x29; AS TPMODE&#xd;&#xa;    ,&#x27;K03&#x27; AS INDEX_ID&#xd;&#xa;    ,&#x27;&#x79fb;&#x5e93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;    ,count&#x28;O.vin&#x29; AS MOV_QTY&#xd;&#xa;    ,NULL AS MOV_PLAN_QTY&#xd;&#xa;FROM DW.dw_tp_ordermovement T&#xd;&#xa;INNER JOIN DW.dw_tp_orderrelease O&#xd;&#xa;ON T.order_release_gid &#x3d; O.order_release_gid&#xd;&#xa;INNER JOIN &#x28;&#xd;&#xa;    SELECT rgn_id&#xd;&#xa;    FROM DIM.dim_org&#xd;&#xa;    WHERE org_type &#x3d; &#x27;R&#x27;&#xd;&#xa;    GROUP BY rgn_id&#xd;&#xa;    &#x29; R&#xd;&#xa;ON T.RGN_ID &#x3d; R.RGN_ID&#xd;&#xa;INNER JOIN DIM.dim_period_d P  &#xd;&#xa;ON FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.unload_day, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; - 28800,&#x27;yyyyMMdd&#x27;&#x29; &#x3d; P.period_wid AND P.period_type &#x3d; &#x27;D&#x27;&#xd;&#xa;WHERE &#x28;substr&#x28;P.period_wid,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    &#x29;&#xd;&#xa;    AND O.or_plan_type &#x3d; &#x27;&#x79fb;&#x5e93;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;    AND T.brand_id &#x3d; &#x27;CA&#x27;&#xd;&#xa;    AND O.is_known_shipper &#x3d; &#x27;N&#x27;&#xd;&#xa;    AND O.vin IS NOT NULL&#xd;&#xa;    AND O.arrive_day IS NOT NULL&#xd;&#xa;GROUP BY P.period_wid&#xd;&#xa;    ,coalesce&#x28;R.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.segmetn_transportmode,&#x27;-1&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;SELECT D.PERIOD_WID&#xd;&#xa;      ,M.RGN_ID&#xd;&#xa;      ,M.BASE_ID&#xd;&#xa;      ,M.BRAND_ID&#xd;&#xa;      ,M.SEGMETN_TRANSPORTMODE&#xd;&#xa;      ,&#x27;K03&#x27; AS INDEX_ID&#xd;&#xa;      ,&#x27;&#x79fb;&#x5e93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,COUNT&#x28;DISTINCT T.VIN&#x29; AS MOV_QTY&#xd;&#xa;      ,NULL AS MOV_PLAN_QTY&#xd;&#xa;  FROM DW.DW_TP_ORDERMOVEMENT M&#xd;&#xa; INNER JOIN DW.DW_TP_ORDERRELEASE T&#xd;&#xa;    ON M.ORDER_RELEASE_GID &#x3d; T.ORDER_RELEASE_GID&#xd;&#xa; INNER JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.DEST_LOCATION_ID &#x3d; O.ORG_ID&#xd;&#xa; INNER JOIN DIM.DIM_PERIOD_D D&#xd;&#xa;    ON FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - 28800, &#x27;yyyyMMdd&#x27;&#x29; &#x3d; D.PERIOD_WID&#xd;&#xa; WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND T.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;   AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;   AND O.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;   AND T.OR_PLAN_TYPE &#x3d; &#x27;&#x79fb;&#x5e93;&#x8ba1;&#x5212;&#x27;&#xd;&#xa;   AND T.IS_KNOWN_SHIPPER &#x3d; &#x27;N&#x27; --&#x53bb;&#x9664;&#x5728;&#x9014;&#x542f;&#x7968;&#xd;&#xa;   AND T.VIN IS NOT NULL&#xd;&#xa;   AND T.FACTORY_DAY IS NOT NULL&#xd;&#xa; GROUP BY D.PERIOD_WID&#xd;&#xa;         ,M.RGN_ID&#xd;&#xa;         ,M.BASE_ID&#xd;&#xa;         ,M.BRAND_ID&#xd;&#xa;         ,M.SEGMETN_TRANSPORTMODE&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT D.PERIOD_WID&#xd;&#xa;      ,OM.RGN_ID&#xd;&#xa;      ,OM.BASE_ID&#xd;&#xa;      ,OM.BRAND_ID&#xd;&#xa;      ,OM.SEGMETN_TRANSPORTMODE&#xd;&#xa;      ,&#x27;K03&#x27; AS INDEX_ID&#xd;&#xa;      ,&#x27;&#x79fb;&#x5e93;&#x4e1a;&#x52a1;&#x91cf;&#x27; AS INDEX_NAME&#xd;&#xa;      ,COUNT&#x28;DISTINCT T.VIN&#x29; AS MOV_QTY&#xd;&#xa;      ,NULL AS MOV_PLAN_QTY&#xd;&#xa;  FROM DW.DW_TP_ORDERRELEASE T&#xd;&#xa; INNER JOIN DW.DW_TP_ORDERMOVEMENT OM&#xd;&#xa;    ON T.ORDER_RELEASE_GID &#x3d; OM.ORDER_RELEASE_GID&#xd;&#xa; INNER JOIN DIM.DIM_ORG O&#xd;&#xa;    ON T.DEST_LOCATION_ID &#x3d; O.ORG_ID&#xd;&#xa; INNER JOIN DIM.DIM_PERIOD_D D&#xd;&#xa;    ON FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;OM.UNLOAD_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29; - 28800, &#x27;yyyyMMdd&#x27;&#x29; &#x3d; D.PERIOD_WID&#xd;&#xa; WHERE D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;   AND T.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;   AND SUBSTR&#x28;D.PERIOD_WID, 1, 6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;   AND O.BASE_TYPE &#x3d; &#x27;VDC&#x27;&#xd;&#xa;   AND T.ORDER_RELEASE_NAME LIKE &#x27;5&#x25;&#x27;&#xd;&#xa;   AND T.IS_KNOWN_SHIPPER &#x3d; &#x27;N&#x27; --&#x53bb;&#x9664;&#x5728;&#x9014;&#x542f;&#x7968;&#xd;&#xa;   AND T.VIN IS NOT NULL&#xd;&#xa;   AND OM.UNLOAD_DAY IS NOT NULL&#xd;&#xa; GROUP BY D.PERIOD_WID&#xd;&#xa;         ,OM.RGN_ID&#xd;&#xa;         ,OM.BASE_ID&#xd;&#xa;         ,OM.SEGMETN_TRANSPORTMODE&#xd;&#xa;         ,OM.BRAND_ID&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;</sql>
