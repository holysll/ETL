      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_4&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x65f6;&#x95f4;dtd&#x5e73;&#x5747;&#x65f6;&#x957f;&#x8868;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_4&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;    ,B.rgn_id AS RGN_ID&#xd;&#xa;    ,B.base_id AS BASE_ID&#xd;&#xa;    ,B.brand_id AS BRAND_ID&#xd;&#xa;    ,C.supplier_id AS SUPPLIER_ID&#xd;&#xa;    ,C.segmetn_transportmode AS TPMODE_ID&#xd;&#xa;    ,count&#x28;DISTINCT CASE WHEN substr&#x28;regexp_replace&#x28;A.cainputdate,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN B.order_release_name END&#x29; AS CUR_DELETE_QTY  --&#xd;&#xa;    ,count&#x28;DISTINCT CASE WHEN substr&#x28;regexp_replace&#x28;A.cainputdate,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN B.order_release_name END&#x29; AS LST_DELETE_QTY  --&#xd;&#xa;    ,count&#x28;DISTINCT CASE WHEN substr&#x28;regexp_replace&#x28;A.cainputdate,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN B.order_release_name END&#x29; AS LST_Y_DELETE_QTY  --&#xd;&#xa;FROM ODS_ZCOTM.XX_CA_ORD_DETAIL_DELETE A&#xd;&#xa;INNER JOIN dw.dw_tp_orderrelease B&#xd;&#xa;ON A.otmseqno  &#x3d; B.otmseqno&#xd;&#xa;LEFT JOIN DW.DW_TP_ORDERMOVEMENT C&#xd;&#xa;ON B.ORDER_RELEASE_GID&#x3d;C.ORDER_RELEASE_GID&#xd;&#xa;WHERE substr&#x28;regexp_replace&#x28;A.cainputdate,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR substr&#x28;regexp_replace&#x28;A.cainputdate,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;    OR substr&#x28;regexp_replace&#x28;A.cainputdate,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;GROUP BY B.rgn_id&#xd;&#xa;    ,B.base_id&#xd;&#xa;    ,B.brand_id&#xd;&#xa;    ,C.supplier_id &#xd;&#xa;    ,C.segmetn_transportmode &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
