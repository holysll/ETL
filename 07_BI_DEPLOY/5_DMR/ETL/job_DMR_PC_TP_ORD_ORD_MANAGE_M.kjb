      <name>TO_TEMP_3</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_3&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x53d1;&#x8fd0;&#x6807;&#x51c6;&#x65f6;&#x957f;&#x8868;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_3&#xd;&#xa;SELECT T.MONTH_ID	--&#x6708;&#x4efd;ID&#xd;&#xa;    ,T.RGN_ID	--&#x5927;&#x533a;ID&#xd;&#xa;    ,T.BASE_ID	--&#x6025;&#x6b7b;ID&#xd;&#xa;    ,T.BRAND_ID	--&#x54c1;&#x724c;ID&#xd;&#xa;    ,T.SUPPLIER_ID	--&#x627f;&#x8fd0;&#x5546;ID&#xd;&#xa;    ,T.TPMODE_ID	--&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;    ,sum&#x28;T.CUR_ONWAY_QTY&#x29; AS CUR_ONWAY_QTY  --&#x5f53;&#x6708;&#x5728;&#x9014;&#x91cf;&#xd;&#xa;    ,sum&#x28;T.LST_ONWAY_QTY&#x29; AS LST_ONWAY_QTY  --&#x4e0a;&#x6708;&#x5728;&#x9014;&#x91cf;&#xd;&#xa;    ,sum&#x28;T.LST_Y_ONWAY_QTY&#x29; AS LST_Y_ONWAY_QTY  --&#x53bb;&#x5e74;&#x5f53;&#x6708;&#x5728;&#x9014;&#x91cf;&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;  AS MONTH_ID&#xd;&#xa;        ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,count&#x28;DISTINCT order_release_name&#x29; AS CUR_ONWAY_QTY  --&#xd;&#xa;        ,0 AS LST_ONWAY_QTY --&#xd;&#xa;        ,0 AS LST_Y_ONWAY_QTY   --&#xd;&#xa;    FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;FACTORY_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; &#xd;&#xa;        CASE WHEN substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;        THEN &#xd;&#xa;        from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; &#xd;&#xa;        ELSE &#xd;&#xa;        substr&#x28;regexp_replace&#x28;LAST_DAY&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;        END&#xd;&#xa;        AND NVL&#x28;substr&#x28;regexp_replace&#x28;NVL&#x28;dlvr_day,arrival_day&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;,&#x27;29991231&#x27;&#x29; &#x3e; &#xd;&#xa;        CASE WHEN substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;        THEN &#xd;&#xa;        from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; &#xd;&#xa;        ELSE &#xd;&#xa;        substr&#x28;regexp_replace&#x28;LAST_DAY&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;        END&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;  AS MONTH_ID&#xd;&#xa;        ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,0 AS CUR_ONWAY_QTY  --&#xd;&#xa;        ,count&#x28;DISTINCT order_release_name&#x29; AS LST_ONWAY_QTY --&#xd;&#xa;        ,0 AS LST_Y_ONWAY_QTY   --&#xd;&#xa;    FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;FACTORY_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;last_day&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;        AND &#x28;substr&#x28;regexp_replace&#x28;NVL&#x28;dlvr_day,nvl&#x28;arrival_day,&#x27;29991231&#x27;&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3e; substr&#x28;regexp_replace&#x28;last_day&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;            OR arrival_day IS NULL&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;  AS MONTH_ID&#xd;&#xa;        ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,0 AS CUR_ONWAY_QTY  --&#xd;&#xa;        ,0 AS LST_ONWAY_QTY --&#xd;&#xa;        ,count&#x28;DISTINCT order_release_name&#x29; AS LST_Y_ONWAY_QTY   --&#xd;&#xa;    FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;FACTORY_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; substr&#x28;regexp_replace&#x28;last_day&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;        AND &#x28;substr&#x28;regexp_replace&#x28;NVL&#x28;dlvr_day,NVL&#x28;arrival_day,&#x27;29991231&#x27;&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3e; substr&#x28;regexp_replace&#x28;last_day&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;            OR arrival_day IS NULL&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.MONTH_ID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.SUPPLIER_ID&#xd;&#xa;    ,T.TPMODE_ID&#xd;&#xa;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; as MONTH_ID  --&#xd;&#xa;    ,A.RGN_ID&#xd;&#xa;    ,A.BASE_ID&#xd;&#xa;    ,A.BRAND_ID&#xd;&#xa;    ,A.SUPLLIER_ID&#xd;&#xa;    ,A.TPMODE_ID&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;PERIOD_WID,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN A.ON_PASSAGE_NUM ELSE 0 END&#x29; AS CUR_ONWAY_QTY&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;PERIOD_WID,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN A.ON_PASSAGE_NUM ELSE 0 END&#x29; AS LST_ONWAY_QTY&#xd;&#xa;    ,SUM&#x28;CASE WHEN substr&#x28;regexp_replace&#x28;PERIOD_WID,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN A.ON_PASSAGE_NUM ELSE 0 END&#x29; AS LST_Y_ONWAY_QTY&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT D.PERIOD_WID AS PERIOD_WID --From dw&#x3a;FK to DIM_PERIOD_D.PERIOD_WID&#xd;&#xa;          ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;          ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;          ,COALESCE&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID  --&#xd;&#xa;          ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;          ,COALESCE&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29; AS SUPLLIER_ID  --&#xd;&#xa;          ,T.transportmode_id AS TPMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x28;&#x4ec5;&#x542b;&#x7b2c;&#x4e00;&#x6bb5;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#x29; &#xd;&#xa;          ,COUNT&#x28;T.order_release_gid&#x29; AS ON_PASSAGE_NUM --&#x5728;&#x9014;&#x91cf;&#xd;&#xa;      FROM DIM.DIM_PERIOD_D D&#xd;&#xa;      JOIN DMK.dmk_tp_dtd_prog_time_d T --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;        ON 1 &#x3d; 1&#xd;&#xa;     WHERE 1 &#x3d; 1&#xd;&#xa;       AND D.PERIOD_WID &#x3e;&#x3d;&#xd;&#xa;           FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.FACTORY_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;       AND D.PERIOD_WID &#x3c;&#xd;&#xa;           NVL&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.DLVR_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;,&#x27;29991231&#x27;&#x29;&#xd;&#xa;       AND T.BRAND_ID &#x3c;&#x3e; &#x27;CA&#x27;&#xd;&#xa;       AND &#x28;SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;            OR SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;            OR SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;            &#x29;&#xd;&#xa;       &#xd;&#xa;     GROUP BY D.PERIOD_WID&#xd;&#xa;          ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;          ,T.transportmode_id &#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT D.PERIOD_WID AS PERIOD_WID --From dw&#x3a;FK to DIM_PERIOD_D.PERIOD_WID&#xd;&#xa;          ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;          ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;          ,COALESCE&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID  --&#xd;&#xa;          ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;          ,COALESCE&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29; AS SUPLLIER_ID  --&#xd;&#xa;          ,T.transportmode_id AS TPMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x28;&#x4ec5;&#x542b;&#x7b2c;&#x4e00;&#x6bb5;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#x29; &#xd;&#xa;          ,COUNT&#x28;T.order_release_gid&#x29; AS ON_PASSAGE_NUM --&#x5728;&#x9014;&#x91cf;&#xd;&#xa;      FROM DIM.DIM_PERIOD_D D&#xd;&#xa;      JOIN DMK.dmk_tp_dtd_prog_time_d T --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;        ON 1 &#x3d; 1&#xd;&#xa;     WHERE 1 &#x3d; 1&#xd;&#xa;       AND D.PERIOD_WID &#x3e;&#x3d;&#xd;&#xa;           FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.OUT_STORE_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;       AND D.PERIOD_WID &#x3c;&#xd;&#xa;           NVL&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.ARRIVAL_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;,&#x27;29991231&#x27;&#x29;&#xd;&#xa;       AND T.BRAND_ID &#x3d; &#x27;CA&#x27; --&#xd;&#xa;       AND &#x28;SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;            OR SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;            OR SUBSTR&#x28;D.PERIOD_WID,1,6&#x29; &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;BEFORE_YEAR&#x7d;&#x27;, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;            &#x29;&#xd;&#xa;     GROUP BY D.PERIOD_WID&#xd;&#xa;          ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;          ,COALESCE&#x28;T.SUPPLIER_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;          ,T.transportmode_id&#xd;&#xa;&#x29; A&#xd;&#xa;GROUP BY A.RGN_ID&#xd;&#xa;    ,A.BASE_ID&#xd;&#xa;    ,A.BRAND_ID&#xd;&#xa;    ,A.SUPLLIER_ID&#xd;&#xa;    ,A.TPMODE_ID&#xd;&#xa;&#x2a;&#x2f;</sql>
