      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMR&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_ORD_ORD_MANAGE_M PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT T.MONTH_ID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,coalesce&#x28;A.RGN_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,coalesce&#x28;B.BASE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,coalesce&#x28;C.BRAND_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME&#xd;&#xa;    ,T.SUPPLIER_ID &#xd;&#xa;    ,coalesce&#x28;D.SERVPROV_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29;  AS SUPPLIER_NAME&#xd;&#xa;    ,T.TPMODE_ID&#xd;&#xa;    ,coalesce&#x28;E.TPMODE_NAME,&#x27;&#x672a;&#x77e5;&#x27;&#x29; AS TPMODE_NAME          &#xd;&#xa;    ,SUM&#x28;T.CUR_ORD_QTY&#x29;           AS CUR_ORD_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_ORD_QTY&#x29;           AS LST_ORD_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_Y_ORD_QTY&#x29;         AS LST_Y_ORD_QTY			&#xd;&#xa;    ,SUM&#x28;T.CUR_SHIPED_QTY&#x29;        AS CUR_SHIPED_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_SHIPED_QTY&#x29;        AS LST_SHIPED_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_Y_SHIPED_QTY&#x29;      AS LST_Y_SHIPED_QTY		&#xd;&#xa;    ,SUM&#x28;T.CUR_TGT_SHIPED_QTY&#x29;    AS CUR_TGT_SHIPED_QTY		&#xd;&#xa;    ,SUM&#x28;T.LST_TGT_SHIPED_QTY&#x29;    AS LST_TGT_SHIPED_QTY		&#xd;&#xa;    ,SUM&#x28;T.LST_Y_TGT_SHIPED_QTY&#x29;  AS LST_Y_TGT_SHIPED_QTY	&#xd;&#xa;    ,SUM&#x28;T.CUR_WAIT_SHIP_QTY&#x29;     AS CUR_WAIT_SHIP_QTY		&#xd;&#xa;    ,SUM&#x28;T.LST_WAIT_SHIP_QTY&#x29;     AS LST_WAIT_SHIP_QTY		&#xd;&#xa;    ,SUM&#x28;T.LST_Y_WAIT_SHIP_QTY&#x29;   AS LST_Y_WAIT_SHIP_QTY	&#xd;&#xa;    ,SUM&#x28;T.CUR_CHANGE_WAY_QTY&#x29;    AS CUR_CHANGE_WAY_QTY		&#xd;&#xa;    ,SUM&#x28;T.LST_CHANGE_WAY_QTY&#x29;    AS LST_CHANGE_WAY_QTY		&#xd;&#xa;    ,SUM&#x28;T.LST_Y_CHANGE_WAY_QTY&#x29;  AS LST_Y_CHANGE_WAY_QTY	&#xd;&#xa;    ,SUM&#x28;T.CUR_ONWAY_QTY&#x29;         AS CUR_ONWAY_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_ONWAY_QTY&#x29;         AS LST_ONWAY_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_Y_ONWAY_QTY&#x29;       AS LST_Y_ONWAY_QTY		&#xd;&#xa;    ,SUM&#x28;T.CUR_HOLD_QTY&#x29;          AS CUR_HOLD_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_HOLD_QTY&#x29;          AS LST_HOLD_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_Y_HOLD_QTY&#x29;        AS LST_Y_HOLD_QTY			&#xd;&#xa;    ,SUM&#x28;T.CUR_DELAY_QTY&#x29;         AS CUR_DELAY_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_DELAY_QTY&#x29;         AS LST_DELAY_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_Y_DELAY_QTY&#x29;       AS LST_Y_DELAY_QTY		&#xd;&#xa;    ,SUM&#x28;T.CUR_DELETE_QTY&#x29;        AS CUR_DELETE_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_DELETE_QTY&#x29;        AS LST_DELETE_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_Y_DELETE_QTY&#x29;      AS LST_Y_DELETE_QTY		&#xd;&#xa;    ,SUM&#x28;T.CUR_ONTIME_QTY&#x29;        AS CUR_ONTIME_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_ONTIME_QTY&#x29;        AS LST_ONTIME_QTY			&#xd;&#xa;    ,SUM&#x28;T.LST_Y_ONTIME_QTY&#x29;      AS LST_Y_ONTIME_QTY	&#xd;&#xa;    ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;	,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DATE  --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;	,&#x27;OTM&#x27; AS PART_SC&#xd;&#xa;	,substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS PART_DT  --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;FROM&#x28;&#xd;&#x