ON_PASSAGE_DAYS, 0&#x29; &#x3c;&#x3d; 0 THEN 1 ELSE 0 END&#x29; AS LST_Y_ONTIME_QTY  --&#xd;&#xa;FROM DW.DW_TP_ORDERRELEASE T --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;LEFT JOIN DW.DW_TP_ORDERMOVEMENT T1&#xd;&#xa;ON T.ORDER_RELEASE_GID &#x3d; T1.ORDER_RELEASE_GID&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;   AND T.OR_PLAN_TYPE &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27;  --ODS ORF&#x6ca1;&#x6570;&#x636e;&#xff0c;&#x8be5;&#x6761;&#x4ef6;&#x5148;&#x53bb;&#x6389;&#xd;&#xa;   AND T.BRAND_ID &#x3c;&#x3e; &#x27;CA&#x27;&#xd;&#xa;   AND T.TRANSPORTSTATUS&#x3d;&#x27;&#x4ea4;&#x8f66;&#x27; --ADD 20180705&#xd;&#xa;   AND &#x28;SUBSTR&#x28;regexp_replace&#x28;T.DLVR_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        OR SUBSTR&#x28;regexp_replace&#x28;T.DLVR_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        OR SUBSTR&#x28;regexp_replace&#x28;T.DLVR_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;GROUP BY T.RGN_ID  --&#x5927;&#x533a;ID&#xd;&#xa;      ,T.BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,T.BRAND_ID  --&#xd;&#xa;      ,T1.supplier_id --&#xd;&#xa;      ,T1.segmetn_transportmode&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID  --&#xd;&#xa;      ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;      ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,COALESCE&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID  --&#xd;&#xa;      ,COALESCE&#x28;T.supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID  --&#xd;&#xa;      ,COALESCE&#x28;T.segmetn_transportmode,&#x27;-1&#x27;&#x29; AS TPMODE_ID  --&#xd;&#xa;      ,COUNT&#x28;CASE WHEN SUBSTR&#x28;regexp_replace&#x28;T.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.order_release_gid ELSE 0 END&#x29; AS CUR_TOTAL_ARRIVE_QTY --&#xd;&#xa;      ,COUNT&#x28;CASE WHEN SUBSTR&#x28;regexp_replace&#x28;T.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.order_release_gid ELSE 0 END&#x29; AS LST_TOTAL_ARRIVE_QTY --&#xd;&#xa;      ,COUNT&#x28;CASE WHEN SUBSTR&#x28;regexp_replace&#x28;T.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T.order_release_gid ELSE 0 END&#x29; AS LST_Y_TOTAL_ARRIVE_QTY --&#xd;&#xa;      ,SUM&#x28;CASE WHEN SUBSTR&#x28;regexp_replace&#x28;T.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            AND DATEDIFF&#x28;T.DLVR_DAY, T.FACTORY_DAY&#x29;  - NVL&#x28;T.ON_PASSAGE_DAYS, 0&#x29; &#x3c;&#x3d; 0 THEN 1 ELSE 0 END&#x29; AS CUR_ONTIME_QTY  --&#xd;&#xa;      ,SUM&#x28;CASE WHEN SUBSTR&#x28;regexp_replace&#x28;T.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            AND DATEDIFF&#x28;T.DLVR_DAY, T.FACTORY_DAY&#x29;  - NVL&#x28;T.ON_PASSAGE_DAYS, 0&#x29; &#x3c;&#x3d; 0 THEN 1 ELSE 0 END&#x29; AS LST_ONTIME_QTY  --&#xd;&#xa;      ,SUM&#x28;CASE WHEN SUBSTR&#x28;regexp_replace&#x28;T.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;            AND DATEDIFF&#x28;T.DLVR_DAY, T.FACTORY_DAY&#x29;  - NVL&#x28;T.ON_PASSAGE_DAYS, 0&#x29; &#x3c;&#x3d; 0 THEN 1 ELSE 0 END&#x29; AS LST_Y_ONTIME_QTY  --&#xd;&#xa;FROM DW.DW_TP_ORDERMOVEMENT T --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;LEFT JOIN DW.DW_TP_ORDERRELEASE T1&#xd;&#xa;ON T.ORDER_RELEASE_GID &#x3d; T1.ORDER_RELEASE_GID&#xd;&#xa;WHERE 1&#x3d;1&#xd;&#xa;   AND T1.OR_PLAN_TYPE &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27;  --ODS ORF&#x6ca1;&#x6570;&#x636e;&#xff0c;&#x8be5;&#x6761;&#x4ef6;&#x5148;&#x53bb;&#x6389;&#xd;&#xa;   AND T.BRAND_ID &#x3d; &#x27;CA&#x27; --&#x798f;&#x7279;&#xd;&#xa;   AND T1.TRANSPORTSTATUS&#x3d;&#x27;&#x4ea4;&#x8f66;&#x27; --ADD 20180705&#xd;&#xa;   AND &#x28;SUBSTR&#x28;regexp_replace&#x28;T.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        OR SUBSTR&#x28;regexp_replace&#x28;T.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;BEFORE_MONTH&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        OR SUBSTR&#x28;regexp_replace&#x28;T.ARRIVAL_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;ADD_MONTHS&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, -12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;GROUP BY T.RGN_ID  --&#x5927;&#x533a;ID&#xd;&#xa;      ,T.BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,T.BRAND_ID  --&#xd;&#xa;      ,T.supplier_id --&#xd;&#xa;      ,T.segmetn_transportmode&#xd;&#xa;&#x2a;&#x2f;</sql>
