      <name>TO_TEMP_5</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_5&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x5f85;&#x53d1;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_5&#xd;&#xa;SELECT T.MONTH_ID	--&#x6708;&#x4efd;ID&#xd;&#xa;    ,T.RGN_ID	--&#x5927;&#x533a;ID&#xd;&#xa;    ,T.BASE_ID	--&#x6025;&#x6b7b;ID&#xd;&#xa;    ,T.BRAND_ID	--&#x54c1;&#x724c;ID&#xd;&#xa;    ,T.SUPPLIER_ID	--&#x627f;&#x8fd0;&#x5546;ID&#xd;&#xa;    ,T.TPMODE_ID	--&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;    ,sum&#x28;T.CUR_WAIT_SHIP_QTY&#x29;   AS CUR_WAIT_SHIP_QTY    --&#x672c;&#x671f;&#x5f85;&#x53d1;&#x91cf;&#xd;&#xa;    ,sum&#x28;T.LST_WAIT_SHIP_QTY&#x29;   AS LST_WAIT_SHIP_QTY    --&#x4e0a;&#x671f;&#x5f85;&#x53d1;&#x91cf;&#xd;&#xa;    ,sum&#x28;T.LST_Y_WAIT_SHIP_QTY&#x29; AS LST_Y_WAIT_SHIP_QTY  --&#x540c;&#x671f;&#x5f85;&#x53d1;&#x91cf;&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;  AS MONTH_ID&#xd;&#xa;        ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,count&#x28;DISTINCT order_release_name&#x29; AS CUR_WAIT_SHIP_QTY&#xd;&#xa;        ,0 AS LST_WAIT_SHIP_QTY&#xd;&#xa;        ,0 AS LST_Y_WAIT_SHIP_QTY&#xd;&#xa;    FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; &#xd;&#xa;        CASE WHEN substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;        THEN &#xd;&#xa;        from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; &#xd;&#xa;        ELSE &#xd;&#xa;        substr&#x28;regexp_replace&#x28;last_day&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;        END&#xd;&#xa;      AND NVL&#x28;substr&#x28;regexp_replace&#x28;FACTORY_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;,&#x27;29991231&#x27;&#x29; &#x3e; &#xd;&#xa;        CASE WHEN substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMM&#x27;&#x29; &#xd;&#xa;        THEN &#xd;&#xa;        from_unixtime&#x28;unix_timestamp&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; &#xd;&#xa;        ELSE &#xd;&#xa;        substr&#x28;regexp_replace&#x28;last_day&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;        END&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;  AS MONTH_ID&#xd;&#xa;        ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,0 AS CUR_WAIT_SHIP_QTY&#xd;&#xa;        ,count&#x28;DISTINCT order_release_name&#x29; AS LST_WAIT_SHIP_QTY&#xd;&#xa;        ,0 AS LST_Y_WAIT_SHIP_QTY&#xd;&#xa;    FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;last_day&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;      AND NVL&#x28;substr&#x28;regexp_replace&#x28;FACTORY_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;,&#x27;29991231&#x27;&#x29; &#x3e; substr&#x28;regexp_replace&#x28;last_day&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    UNION ALL&#xd;&#xa;    SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;  AS MONTH_ID&#xd;&#xa;        ,coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;        ,0 AS CUR_WAIT_SHIP_QTY&#xd;&#xa;        ,0 AS LST_WAIT_SHIP_QTY&#xd;&#xa;        ,count&#x28;DISTINCT order_release_name&#x29; AS LST_Y_WAIT_SHIP_QTY&#xd;&#xa;    FROM DMK.dmk_tp_dtd_prog_time_d&#xd;&#xa;    WHERE substr&#x28;regexp_replace&#x28;EARLY_PICKUP_DATE,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; &#x3c;&#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;last_day&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;      AND NVL&#x28;substr&#x28;regexp_replace&#x28;FACTORY_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;,&#x27;29991231&#x27;&#x29; &#x3e; substr&#x28;regexp_replace&#x28;last_day&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;    GROUP BY coalesce&#x28;rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;base_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;        ,coalesce&#x28;transportmode_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    &#x29; T&#xd;&#xa;GROUP BY T.MONTH_ID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.SUPPLIER_ID&#xd;&#xa;    ,T.TPMODE_ID&#xd;&#xa;&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
