      <name>TO_TEMP_6</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_6&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x5728;&#x9014;&#x8d28;&#x635f;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_6&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID&#xd;&#xa;    ,T.RGN_ID&#xd;&#xa;    ,T.BASE_ID&#xd;&#xa;    ,T.BRAND_ID&#xd;&#xa;    ,T.SUPPLIER_ID&#xd;&#xa;    ,T.TPMODE_ID&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.PERIOD_WID,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN CUR_D_TOTAL_QTY ELSE 0 END&#x29; AS CUR_M_TOTAL_QTY --&#x603b;&#x5230;&#x6570;&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.PERIOD_WID,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN CUR_D_TOTAL_QTY ELSE 0 END&#x29; AS LST_M_TOTAL_QTY --&#x603b;&#x5230;&#x6570;&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.PERIOD_WID,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN CUR_D_TOTAL_QTY ELSE 0 END&#x29; AS LST_Y_M_TOTAL_QTY --&#x603b;&#x5230;&#x6570;&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.PERIOD_WID,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN CUR_D_ONTIME_QTY ELSE 0 END&#x29; AS CUR_M_ONTIME_QTY --&#x51c6;&#x5230;&#x6570;&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.PERIOD_WID,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN CUR_D_ONTIME_QTY ELSE 0 END&#x29; AS LST_M_ONTIME_QTY --&#x51c6;&#x5230;&#x6570;&#xd;&#xa;    ,sum&#x28;CASE WHEN substr&#x28;T.PERIOD_WID,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN CUR_D_ONTIME_QTY ELSE 0 END&#x29; AS LST_Y_M_ONTIME_QTY --&#x51c6;&#x5230;&#x6570;&#xd;&#xa;FROM&#x28;&#xd;&#xa;    SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DLVR_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; AS PERIOD_WID&#xd;&#xa;          ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;          ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;          ,COALESCE&#x28;T.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;          ,COALESCE&#x28;M.SUPPLIER_ID, &#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;          ,COALESCE&#x28;M.SEGMETN_TRANSPORTMODE, &#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;          ,COUNT&#x28;DISTINCT T.ORDER_RELEASE_NAME&#x29; AS CUR_D_TOTAL_QTY --&#x603b;&#x5230;&#x6570;&#xd;&#xa;          ,COUNT&#x28;DISTINCT CASE&#xd;&#xa;                   WHEN DATEDIFF&#x28;T.DLVR_DAY, T.FACTORY_DAY&#x29; -&#xd;&#xa;                        NVL&#x28;T.ON_PASSAGE_DAYS, 0&#x29; &#x3c;&#x3d; 0 THEN&#xd;&#xa;                    T.ORDER_RELEASE_NAME&#xd;&#xa;                   ELSE&#xd;&#xa;                    NULL&#xd;&#xa;                 END&#x29; AS CUR_D_ONTIME_QTY --&#x51c6;&#x5230;&#x6570;&#xd;&#xa;      FROM DW.DW_TP_ORDERRELEASE T --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;      LEFT JOIN DW.DW_TP_ORDERMOVEMENT M&#xd;&#xa;        ON T.ORDER_RELEASE_GID &#x3d; M.ORDER_RELEASE_GID&#xd;&#xa;       AND NVL&#x28;M.SEQ_NO, 1&#x29; &#x3d; &#x27;1&#x27;&#xd;&#xa;       AND M.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;      LEFT JOIN &#x28;SELECT SERVPROV_NAME AS SUPPLIER_ID&#xd;&#xa;                   FROM DIM.DIM_SERVPROV&#xd;&#xa;                  WHERE SERVPROV_ID LIKE &#x27;&#x25;&#x7279;&#x9510;&#x25;&#x27;&#xd;&#xa;                     OR SERVPROV_ID LIKE &#x27;&#x25;&#x957f;&#x4e45;&#x25;&#x27;&#xd;&#xa;                     OR SERVPROV_ID LIKE &#x27;&#x25;&#x5b89;&#x76db;&#x25;&#x27;&#xd;&#xa;                     OR SERVPROV_ID LIKE &#x27;&#x25;&#x7f8e;&#x8054;&#x25;&#x27;&#xd;&#xa;                     OR SERVPROV_ID LIKE &#x27;&#x25;&#x4f17;&#x5347;&#x25;&#x27;&#x29; P&#xd;&#xa;        ON M.LSP &#x3d; P.SUPPLIER_ID&#xd;&#xa;     WHERE 1 &#x3d; 1&#xd;&#xa;       AND T.OR_PLAN_TYPE &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27; --ODS ORF&#x6ca1;&#x6570;&#x636e;&#xff0c;&#x8be5;&#x6761;&#x4ef6;&#x5148;&#x53bb;&#x6389;&#xd;&#xa;       AND T.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;       AND T.TRANSPORTSTATUS &#x3d; &#x27;&#x4ea4;&#x8f66;&#x27; --ADD 20180705&#xd;&#xa;       AND &#x28;SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DLVR_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;           SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;           OR SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DLVR_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;           SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;           OR SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DLVR_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29; &#x3d;&#xd;&#xa;           SUBSTR&#x28;REGEXP_REPLACE&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;           &#x29;&#xd;&#xa;       AND P.SUPPLIER_ID IS NULL&#xd;&#xa;     GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;T.DLVR_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29;&#xd;&#xa;             ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;           