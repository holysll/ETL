      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_1&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_1&#x28;&#xd;&#xa;RGN_ID STRING    COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID STRING    COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID STRING   COMMENT&#x27;&#x54c1;&#x724c;&#x27;&#xd;&#xa;,PROJ_ID STRING   COMMENT&#x27;&#x9879;&#x76ee;ID&#x27;&#xd;&#xa;,SUPPLIER_ID	STRING	COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,VIN STRING   COMMENT&#x27;VIN&#x27;&#xd;&#xa;,ORDER_RELEASE_GID STRING   COMMENT&#x27;OTM&#x5355;&#x53f7;&#x27;&#xd;&#xa;,SEQ_NO STRING   COMMENT&#x27;&#x8fd0;&#x8f93;&#x6bb5;&#x27;&#xd;&#xa;,ORDER_MOVEMENT_GID STRING COMMENT&#x27;&#x5206;&#x6bb5;&#x8def;&#x7531;GID&#x27;&#xd;&#xa;,TRANSPORTMODE_ID STRING   COMMENT&#x27;&#x8fd0;&#x8f93;&#x6bb5;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,EARLY_PICKUP_DATE STRING   COMMENT&#x27;&#x63a5;&#x5355;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,ALLOT_PLN_DAY STRING   COMMENT&#x27;&#x5206;&#x914d;&#x65f6;&#x95f4; &#x82e5;&#x6ca1;&#x6709;&#x5219;&#x53d6;&#x627f;&#x8fd0;&#x5546;&#x6700;&#x65b0;&#x786e;&#x8ba4;&#x65f6;&#x95f4;  &#x27;&#xd;&#xa;,OUT_STORE_DAY STRING   COMMENT&#x27;&#x51fa;&#x5e93;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,FACTORY_DAY STRING   COMMENT&#x27;&#x6253;&#x5355;&#x65f6;&#x95f4;  &#x51fa;&#x573a;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,DLVR_DAY	STRING	COMMENT&#x27;&#x4e2d;&#x8f6c;&#x5165;&#x5e93;&#x65e5;&#x2f;&#x4ea4;&#x8f66;&#x65e5;&#x2f;&#x5230;&#x8fbe;&#x65e5;&#x27;&#xd;&#xa;,ARRIVAL_DAY STRING   COMMENT&#x27;&#x4ea4;&#x4ed8;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,RECEIPT_DATE STRING   COMMENT&#x27;&#x56de;&#x5355;&#x65f6;&#x95f4;&#x27;&#xd;&#xa;,BILL_DATE STRING   COMMENT&#x27;&#x7ed3;&#x7b97;&#x65f6;&#x95f4;&#x28;&#x4e0e;&#x627f;&#x8fd0;&#x5546;&#xff09;&#x27;&#xd;&#xa;,DETAIL_FLAG STRING   COMMENT&#x27;&#x6563;&#x5355;&#x6807;&#x5fd7;&#x28;&#x6563;&#x5355;&#x7684;&#x5224;&#x65ad;&#x903b;&#x8f91;&#x29;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;DTD&#x603b;&#x65f6;&#x957f;TEMP&#x8868;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_2&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_2&#x28;&#xd;&#xa;MONTH_ID	STRING	COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID  STRING  COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID  STRING  COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	STRING	COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID  STRING  COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CUR_ORD_QTY	BIGINT	COMMENT	&#x27;&#x672c;&#x671f;&#x8ba2;&#x5355;&#x91cf;&#x27;&#xd;&#xa;,LST_ORD_QTY	BIGINT	COMMENT	&#x27;&#x4e0a;&#x671f;&#x8ba2;&#x5355;&#x91cf;&#x27;&#xd;&#xa;,LST_Y_ORD_QTY	BIGINT	COMMENT	&#x27;&#x540c;&#x671f;&#x8ba2;&#x5355;&#x91cf;&#x27;&#xd;&#xa;,CUR_SHIPED_QTY	BIGINT	COMMENT	&#x27;&#x672c;&#x671f;&#x5df2;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;,LST_SHIPED_QTY	BIGINT	COMMENT	&#x27;&#x4e0a;&#x671f;&#x5df2;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;,LST_Y_SHIPED_QTY	BIGINT	COMMENT	&#x27;&#x540c;&#x671f;&#x5df2;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x8ba2;&#x5355;&#x91cf;&#xff0c;&#x5df2;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_3&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_3&#x28;&#xd;&#xa;MONTH_ID	STRING	COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID  STRING  COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID  STRING  COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	STRING	COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID  STRING  COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CUR_ONWAY_QTY	BIGINT	COMMENT	&#x27;&#x672c;&#x671f;&#x5728;&#x9014;&#x91cf;&#x27;&#xd;&#xa;,LST_ONWAY_QTY	BIGINT	COMMENT	&#x27;&#x4e0a;&#x671f;&#x5728;&#x9014;&#x91cf;&#x27;&#xd;&#xa;,LST_Y_ONWAY_QTY	BIGINT	COMMENT	&#x27;&#x540c;&#x671f;&#x5728;&#x9014;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x5728;&#x9014;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_4&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_4&#x28;&#xd;&#xa;MONTH_ID	STRING	COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID  STRING  COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID  STRING  COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	STRING	COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID  STRING  COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CUR_DELETE_QTY	BIGINT	COMMENT	&#x27;&#x672c;&#x671f;&#x5220;&#x5355;&#x91cf;&#x27;&#xd;&#xa;,LST_DELETE_QTY	BIGINT	COMMENT	&#x27;&#x4e0a;&#x671f;&#x5220;&#x5355;&#x91cf;&#x27;&#xd;&#xa;,LST_Y_DELETE_QTY	BIGINT	COMMENT	&#x27;&#x540c;&#x671f;&#x5220;&#x5355;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x5220;&#x5355;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_5&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_5&#x28;&#xd;&#xa;MONTH_ID	STRING	COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID  STRING  COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID  STRING  COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	STRING	COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID  STRING  COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CUR_WAIT_SHIP_QTY  BIGINT COMMENT &#x27;&#x672c;&#x671f;&#x5f85;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;,LST_WAIT_SHIP_QTY BIGINT COMMENT &#x27;&#x4e0a;&#x671f;&#x5f85;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;,LST_Y_WAIT_SHIP_QTY BIGINT COMMENT &#x27;&#x540c;&#x671f;&#x5f85;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#x5f85;&#x53d1;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;&#xd;&#xa;DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_6&#x3b;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_ORD_ORD_MANAGE_M_6&#x28;&#xd;&#xa;MONTH_ID	STRING	COMMENT &#x27;&#x6708;&#x4efd;ID&#x27;&#xd;&#xa;,RGN_ID  STRING  COMMENT&#x27;&#x5927;&#x533a;ID&#x27;&#xd;&#xa;,BASE_ID  STRING  COMMENT&#x27;&#x57fa;&#x5730;ID&#x27;&#xd;&#xa;,BRAND_ID	STRING	COMMENT &#x27;&#x54c1;&#x724c;ID&#x27;&#xd;&#xa;,SUPPLIER_ID  STRING  COMMENT&#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;&#xd;&#xa;,TPMODE_ID	STRING	COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;&#xd;&#xa;,CUR_TOTAL_ARRIVE_QTY	BIGINT	COMMENT	&#x27;&#x672c;&#x671f;&#x5230;&#x8fbe;&#x91cf;&#x27;&#xd;&#xa;,LST_TOTAL_ARRIVE_QTY	BIGINT	COMMENT	&#x27;&#x4e0a;&#x671f;&#x5230;&#x8fbe;&#x91cf;&#x27;&#xd;&#xa;,LST_Y_TOTAL_ARRIVE_QTY	BIGINT	COMMENT	&#x27;&#x540c;&#x671f;&#x5230;&#x8fbe;&#x91cf;&#x27;&#xd;&#xa;,CUR_ONTIME_QTY	BIGINT	COMMENT	&#x27;&#x672c;&#x671f;&#x51c6;&#x5230;&#x91cf;&#x27;&#xd;&#xa;,LST_ONTIME_QTY	BIGINT	COMMENT	&#x27;&#x4e0a;&#x671f;&#x51c6;&#x5230;&#x91cf;&#x27;&#xd;&#xa;,LST_Y_ONTIME_QTY	BIGINT	COMMENT	&#x27;&#x540c;&#x671f;&#x51c6;&#x5230;&#x91cf;&#x27;&#xd;&#xa;&#x29; COMMENT &#x27;&#x51c6;&#x5230;&#xff0c;&#x603b;&#x5230;&#x8fbe;&#x91cf;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
