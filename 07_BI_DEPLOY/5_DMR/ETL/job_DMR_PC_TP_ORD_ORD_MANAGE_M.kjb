r&#x28;regexp_replace&#x28;T.dlvr_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#xd;&#xa;                            AND DATEDIFF&#x28;T.DLVR_DAY, T.FACTORY_DAY&#x29;  - NVL&#x28;T.ON_PASSAGE_DAYS, 0&#x29; &#x3c;&#x3d; 0 &#xd;&#xa;                        THEN T.order_release_name ELSE NULL END&#x29; AS LST_M_ONTIME_QTY --&#x51c6;&#x5230;&#x6570;&#xd;&#xa;    ,COUNT&#x28;DISTINCT CASE WHEN substr&#x28;regexp_replace&#x28;T.dlvr_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;                            AND DATEDIFF&#x28;T.DLVR_DAY, T.FACTORY_DAY&#x29;  - NVL&#x28;T.ON_PASSAGE_DAYS, 0&#x29; &#x3c;&#x3d; 0 &#xd;&#xa;                        THEN T.order_release_name ELSE NULL END&#x29; AS LST_Y_M_ONTIME_QTY --&#x51c6;&#x5230;&#x6570;&#xd;&#xa;  FROM DW.DW_TP_ORDERMOVEMENT T&#xd;&#xa;  JOIN DW.DW_TP_ORDERRELEASE R --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;    ON T.ORDER_RELEASE_GID &#x3d; R.ORDER_RELEASE_GID&#xd;&#xa; LEFT JOIN &#x28;&#xd;&#xa;    SELECT servprov_name AS SUPPLIER_ID FROM dim.dim_servprov WHERE servprov_id LIKE &#x27;&#x25;&#x7279;&#x9510;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x957f;&#x4e45;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x5b89;&#x76db;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x7f8e;&#x8054;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x4f17;&#x5347;&#x25;&#x27;&#xd;&#xa;    &#x29; P&#xd;&#xa; ON  T.lsp &#x3d; P.SUPPLIER_ID&#xd;&#xa; WHERE 1&#x3d;1&#xd;&#xa;   AND R.OR_PLAN_TYPE &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27;  --ODS ORF&#x6ca1;&#x6570;&#x636e;&#xff0c;&#x8be5;&#x6761;&#x4ef6;&#x5148;&#x53bb;&#x6389;&#xd;&#xa;   AND R.TRANSPORTSTATUS&#x3d;&#x27;&#x4ea4;&#x8f66;&#x27; --ADD 20180705&#xd;&#xa;   AND T.BRAND_ID &#x3d; &#x27;CA&#x27; --&#x957f;&#x5b89;&#xd;&#xa;   AND &#x28;substr&#x28;regexp_replace&#x28;T.dlvr_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        OR substr&#x28;regexp_replace&#x28;T.dlvr_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-1&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        OR substr&#x28;regexp_replace&#x28;T.dlvr_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;add_months&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,-12&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29;&#xd;&#xa;        &#x29;&#xd;&#xa;   AND P.SUPPLIER_ID IS NULL&#xd;&#xa;GROUP BY coalesce&#x28;T.rgn_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.base_id,&#x27;-1&#x27;&#x29; &#xd;&#xa;    ,coalesce&#x28;T.brand_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.supplier_id,&#x27;-1&#x27;&#x29;&#xd;&#xa;    ,coalesce&#x28;T.segmetn_transportmode,&#x27;-1&#x27;&#x29; &#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; AS MONTH_ID  --&#xd;&#xa;      ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;      ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,COALESCE&#x28;T.BRAND_ID,&#x27;-1&#x27;&#x29; AS BRAND_ID  --&#xd;&#xa;      ,COALESCE&#x28;T1.supplier_id,&#x27;-1&#x27;&#x29; AS SUPPLIER_ID  --&#xd;&#xa;      ,COALESCE&#x28;T1.segmetn_transportmode,&#x27;-1&#x27;&#x29; AS TPMODE_ID  --&#xd;&#xa;      ,COUNT&#x28;CASE WHEN SUBSTR&#x28;regexp_replace&#x28;T.DLVR_DAY,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; &#x3d; substr&#x28;regexp_replace&#x28;&#