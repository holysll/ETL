    <entry>
      <name>TO_DMK</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMK&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;INSERT OVERWRITE TABLE DMK.DMK_STR_PROJBIZQTY_M PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT D.PERIOD_WID&#xd;&#xa;      ,D.YEAR&#xd;&#xa;      ,D.MONTH&#xd;&#xa;      ,COALESCE&#x28;D.OC_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;      ,COALESCE&#x28;D.RGN_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;      ,COALESCE&#x28;D.BASE_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;      ,COALESCE&#x28;D.BRAND_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;      ,COALESCE&#x28;D.PROJ_ID,&#x27;-1&#x27;&#x29;&#xd;&#xa;      ,SUM&#x28;BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;      ,SUM&#x28;D.BIZ_NUM&#x29; AS BIZ_NUM&#xd;&#xa;      ,&#x27;DW&#x27; AS SOURCE_CODE	--DMK_&#x5b;&#x6e90;&#x8868;&#x5d;&#xd;&#xa;      ,&#x27;-1&#x27; AS INTEGRATION_ID	--SOURCE_CODE &#x7c;&#x7c; _ &#x7c;&#x7c; ORDER_ID&#xd;&#xa;      ,NULL AS AUX1_CHANGED_ON_DT	--TABLE_NAME1&#x5b;yyyy-MM-dd HH&#x3a;mm&#x3a;dd&#x5d;,&#x2026;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_INSERT_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_UPDATE_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTRING&#x28;D.PERIOD_WID,1,6&#x29;  AS PART_DT &#xd;&#xa;  FROM TEMP.TEMP_DMK_STR_PROJBIZQTY_M D&#xd;&#xa; GROUP BY D.PERIOD_WID&#xd;&#xa;         ,D.YEAR&#xd;&#xa;         ,D.MONTH&#xd;&#xa;         ,D.OC_ID&#xd;&#xa;         ,D.RGN_ID&#xd;&#xa;         ,D.BASE_ID&#xd;&#xa;         ,D.BRAND_ID&#xd;&#xa;         ,D.PROJ_ID&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
