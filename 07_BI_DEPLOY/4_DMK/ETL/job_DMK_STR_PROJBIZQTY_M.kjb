    <entry>
      <name>CREATE_TEMP_TAB</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP_TAB&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Udf&#xd;&#xa;&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMK_STR_PROJBIZQTY_M&#x28;&#xd;&#xa;PERIOD_WID                    BIGINT                             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;YEAR                          BIGINT,&#xd;&#xa;MONTH                         BIGINT,&#xd;&#xa;OC_ID                         STRING                             COMMENT  &#x27;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#x27;,&#xd;&#xa;RGN_ID                        STRING                             COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;BASE_ID                       STRING                             COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;BRAND_ID                      STRING                             COMMENT  &#x27;&#x54c1;&#x724c;ID&#x27;,&#xd;&#xa;PROJ_ID                       STRING                             COMMENT  &#x27;&#x9879;&#x76ee;ID&#x27;,&#xd;&#xa;BIZ_QTY                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x4e1a;&#x52a1;&#x91cf;&#x27;,&#xd;&#xa;BIZ_NUM                       decimal&#x28;32,8&#x29;                      COMMENT  &#x27;&#x4e1a;&#x52a1;&#x8f86;&#x6b21;&#x27;&#xd;&#xa;&#x29; &#xd;&#xa;COMMENT &#x27;&#x4ed3;&#x50a8;&#x9879;&#x76ee;&#x7c92;&#x5ea6;&#x4e1a;&#x52a1;&#x91cf;&#x6708;&#x4efd;&#x7edf;&#x8ba1;&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27;&#xd;&#xa;STORED AS PARQUET&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
