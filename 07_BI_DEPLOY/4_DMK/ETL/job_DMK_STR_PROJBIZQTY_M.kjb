      </entry>
    <entry>
      <name>TO_TEMP</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMK_STR_PROJBIZQTY_M&#xd;&#xa;--&#x957f;&#x5b89;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; AS YEAR&#xd;&#xa;      ,T.MONTH_ID AS MONTH&#xd;&#xa;      ,T.OC_ID&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,T.PROJ_ID&#xd;&#xa;      ,SUM&#x28;T.BASE_BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;      ,0 AS BIZ_NUM&#xd;&#xa;  FROM DW.DW_FIN_MANAGE_FEE T&#xd;&#xa; WHERE T.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa; GROUP BY T.PERIOD_WID&#xd;&#xa;         ,SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;&#xd;&#xa;         ,T.MONTH_ID&#xd;&#xa;         ,T.OC_ID&#xd;&#xa;         ,T.RGN_ID&#xd;&#xa;         ,T.BASE_ID&#xd;&#xa;         ,T.BRAND_ID&#xd;&#xa;         ,T.PROJ_ID&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;INSERT INTO TEMP.TEMP_DMK_STR_PROJBIZQTY_M&#xd;&#xa;--&#x957f;&#x5b89;&#x798f;&#x7279;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,T.YEAR&#xd;&#xa;      ,T.MONTH&#xd;&#xa;      ,T.OC_ID&#xd;&#xa;      ,T.RGN_ID&#xd;&#xa;      ,T.BASE_ID&#xd;&#xa;      ,T.BRAND_ID&#xd;&#xa;      ,T.PROJ_ID&#xd;&#xa;      ,SUM&#x28;T.IN_STORE_QTY&#x29; AS BIZ_QTY&#xd;&#xa;      ,0 AS BIZ_NUM&#xd;&#xa;  FROM DMK.DMK_STR_OPERATE_D T&#xd;&#xa; WHERE T.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa; GROUP BY T.PERIOD_WID&#xd;&#xa;         ,T.YEAR&#xd;&#xa;         ,T.MONTH&#xd;&#xa;         ,T.OC_ID&#xd;&#xa;         ,T.RGN_ID&#xd;&#xa;         ,T.BASE_ID&#xd;&#xa;         ,T.BRAND_ID&#xd;&#xa;         ,T.PROJ_ID&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;INSERT INTO TEMP.TEMP_DMK_STR_PROJBIZQTY_M&#xd;&#xa;--&#x624b;&#x5de5;&#x6a21;&#x677f;&#x3010;P6 &#x975e;&#x7cfb;&#x7edf;&#x4e1a;&#x52a1;&#x3011;&#x4e2d;&#x3010;&#x4e1a;&#x52a1;&#x7c7b;&#x522b;&#x3011;&#x4e3a;&#x201c;&#x8fd0;&#x8f93;&#x201c;&#x7684;&#x4e1a;&#x52a1;&#xd;&#xa;SELECT T.PERIOD_WID&#xd;&#xa;      ,T.YEAR&#xd;&#xa;      ,T.MONTH&#xd;&#xa;      ,P.OC_ID&#xd;&#xa;      ,P.RGN_ID&#xd;&#xa;      ,P.BASE_ID&#xd;&#xa;      ,P.BRAND_ID&#xd;&#xa;      ,P.PROJ_ID&#xd;&#xa;      ,SUM&#x28;T.BIZ_QTY&#x29; AS BIZ_QTY&#xd;&#xa;      ,SUM&#x28;T.BIZ_QTY_PER_CAR&#x29; AS BIZ_NUM&#xd;&#xa;  FROM DW.DW_TP_BOYUBIZ T&#xd;&#xa;  LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;    ON T.PROJ_NAME &#x3d; P.PROJ_NAME&#xd;&#xa; WHERE T.REMARK &#x3d; &#x27;&#x65b0;&#x4e1a;&#x52a1;&#x27;&#xd;&#xa; GROUP BY T.PERIOD_WID&#xd;&#xa;         ,T.YEAR&#xd;&#xa;         ,T.MONTH&#xd;&#xa;         ,P.OC_ID&#xd;&#xa;         ,P.RGN_ID&#xd;&#xa;         ,P.BASE_ID&#xd;&#xa;         ,P.BRAND_ID&#xd;&#xa;         ,P.PROJ_ID&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
