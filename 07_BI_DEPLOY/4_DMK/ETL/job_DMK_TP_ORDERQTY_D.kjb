      <type>SQL</type>
      <sql>--&#x53d6;&#x7cfb;&#x7edf;&#x6570;&#x636e;&#xd;&#xa;--&#x5230;&#x5929;&#xd;&#xa;--&#x798f;&#x7279;&#xff1a;DW.DW_STR_STOCKDEPART&#xd;&#xa;--&#x957f;&#x5b89;&#xff1a;&#x8fd8;&#x6ca1;&#x505a;dw&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMK&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMK_TP_ORDERQTY_D_3&#xd;&#xa;--&#x8ba2;&#x5355;&#x5230;&#x8fbe;&#x91cf;&#xd;&#xa;SELECT substr&#x28;regexp_replace&#x28;T.arrive_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29; AS PERIOD_WID --From dw&#x3a;FK to DIM_PERIOD_D.PERIOD_WID &#xd;&#xa;      ,COALESCE&#x28;T.OC_ID, &#x27;-1&#x27;&#x29; AS OC_ID --&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;      ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;      ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;      ,T.SOURCE_LOCATION_TYPE AS SOURCE_LOCATION_TYPE --&#x59cb;&#x53d1;&#x5730;&#x7c7b;&#x578b;  &#xd;&#xa;      ,T.SOURCE_LOCATION_ID AS SOURCE_LOCATION_ID --&#x59cb;&#x53d1;&#x5730;ID    &#xd;&#xa;      ,T.SRC_PRVN_ID AS SRC_PRVN_ID --&#x59cb;&#x53d1;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;      ,T.SRC_CTY_ID AS SRC_CITY_ID --&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;      ,T.DEST_LOCATION_TYPE AS DEST_LOCATION_TYPE --&#x76ee;&#x7684;&#x5730;&#x7c7b;&#x578b;  &#xd;&#xa;      ,T.DEST_LOCATION_ID AS DEST_LOCATION_ID --&#x76ee;&#x7684;&#x5730;ID    &#xd;&#xa;      ,T.DEST_PRVN_ID AS DEST_PRVN_ID --&#x76ee;&#x7684;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;      ,T.DEST_CTY_ID AS DEST_CITY_ID --&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;      ,T.CUST_ID AS CUST_ID --&#x5ba2;&#x6237;&#xd;&#xa;      ,T.BRAND_ID AS BRAND_ID --&#x54c1;&#x724c;&#xd;&#xa;      ,T.MODEL_ID AS MODEL_ID --&#x8f66;&#x578b;&#xd;&#xa;      ,M.SEGMETN_TRANSPORTMODE AS TPMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x28;&#x4ec5;&#x542b;&#x7b2c;&#x4e00;&#x6bb5;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#x29; &#xd;&#xa;      ,M.LSP AS SUPPLIER_ID --&#x627f;&#x8fd0;&#x5546;&#x28;&#x7b2c;&#x4e00;&#x6bb5;&#x29;&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN &#x28;T.OR_SHIPDESC LIKE &#x27;&#x25;&#x4e09;&#x65b9;&#x4fe1;&#x8d37;&#x25;&#x27; OR T.OR_SHIPDESC LIKE &#x27;&#x25;&#x5175;&#x8d22;&#x5b58;&#x8d27;&#x878d;&#x8d44;&#x25;&#x27; OR&#xd;&#xa;              T.OR_SHIPDESC LIKE &#x27;&#x25;&#x8bc1;&#x9700;&#x76d1;&#x7ba1;&#x25;&#x27;&#x29; THEN&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;0&#x27;&#xd;&#xa;       END AS FINANCING_FLAG&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN FIXED_ITINERARY_GID LIKE &#x27;&#x25;&#x8054;&#x8fd0;&#x25;&#x27; THEN&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;0&#x27;&#xd;&#xa;       END AS CNT_FLAG&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;CA&#x27; AND T.ORDER_RELEASE_NAME LIKE &#x27;5&#x25;&#x27; THEN&#xd;&#xa;          &#x27;2&#x27;&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;FT&#x27; AND T.OR_PLAN_TYPE &#x3d; &#x27;&#x79fb;&#x5e93;&#x8ba1;&#x5212;&#x27;  THEN&#xd;&#xa;          &#x27;2&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;       END AS PLAN_TYPE&#xd;&#xa;      ,NULL AS TICKET_QTY --&#x8d77;&#x7968;&#x91cf;&#xd;&#xa;      ,NULL AS ALLOT_QTY --&#x5df2;&#x5206;&#x914d;&#x8ba2;&#x5355;&#x91cf; &#xd;&#xa;      ,NULL AS SHIPPING_QTY --&#x53d1;&#x8fd0;&#x91cf;&#xd;&#xa;      ,COUNT&#x28;1&#x29; AS ARRIVE_QTY --&#x5230;&#x8fbe;&#x91cf;&#xd;&#xa;      ,NULL AS TICKET_ORD_QTY --&#x8d77;&#x7968;&#x91cf;&#xd;&#xa;      ,NULL AS ALLOT_ORD_QTY --&#x5df2;&#x5206;&#x914d;&#x8ba2;&#x5355;&#x91cf; &#xd;&#xa;      ,NULL AS SHIPPING_ORD_QTY --&#x53d1;&#x8fd0;&#x91cf;&#xd;&#xa;      ,COUNT&#x28;DISTINCT T.order_release_name&#x29; AS ARRIVE_ORD_QTY --&#x5230;&#x8fbe;&#x91cf;&#xd;&#xa;  FROM DW.DW_TP_ORDERRELEASE T --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;  JOIN DW.DW_TP_ORDERMOVEMENT M&#xd;&#xa;    ON T.ORDER_RELEASE_GID &#x3d; M.ORDER_RELEASE_GID&#xd;&#xa;   AND M.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;   AND T.DEST_LOCATION_ID &#x3d; M.DEST_LOCATION_ID --&#x76ee;&#x7684;&#x5730;&#x76f8;&#x540c;&#x7684;&#x8def;&#x7531;&#x4e3a;&#x6700;&#x7ec8;&#x8def;&#x7531;&#xd;&#xa;  LEFT JOIN &#x28;&#xd;&#xa;    SELECT servprov_name AS SUPPLIER_ID FROM dim.dim_servprov WHERE servprov_id LIKE &#x27;&#x25;&#x7279;&#x9510;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x957f;&#x4e45;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x5b89;&#x76db;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x7f8e;&#x8054;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x4f17;&#x5347;&#x25;&#x27;&#xd;&#xa;    &#x29; P&#xd;&#xa;  ON M.LSP &#x3d; P.SUPPLIER_ID&#xd;&#xa; WHERE REGEXP_REPLACE&#x28;SUBSTRING&#x28;T.arrive_day, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;       REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;	AND P.SUPPLIER_ID IS NULL&#xd;&#xa; GROUP BY substr&#x28;regexp_replace&#x28;T.arrive_day,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,8&#x29;&#xd;&#xa;         ,COALESCE&#x28;T.OC_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,T.SOURCE_LOCATION_TYPE&#xd;&#xa;         ,T.SOURCE_LOCATION_ID&#xd;&#xa;         ,T.SRC_PRVN_ID&#xd;&#xa;         ,T.SRC_CTY_ID&#xd;&#xa;         ,T.DEST_LOCATION_TYPE&#xd;&#xa;         ,T.DEST_LOCATION_ID&#xd;&#xa;         ,T.DEST_PRVN_ID&#xd;&#xa;         ,T.DEST_CTY_ID&#xd;&#xa;         ,T.CUST_ID&#xd;&#xa;         ,T.BRAND_ID&#xd;&#xa;         ,T.MODEL_ID&#xd;&#xa;         ,M.SEGMETN_TRANSPORTMODE&#xd;&#xa;         ,M.LSP&#xd;&#xa;         ,CASE&#xd;&#xa;            WHEN &#x28;T.OR_SHIPDESC LIKE &#x27;&#x25;&#x4e09;&#x65b9;&#x4fe1;&#x8d37;&#x25;&#x27; OR&#xd;&#xa;                 T.OR_SHIPDESC LIKE &#x27;&#x25;&#x5175;&#x8d22;&#x5b58;&#x8d27;&#x878d;&#x8d44;&#x25;&#x27; OR&#xd;&#xa;                 T.OR_SHIPDESC LIKE &#x27;&#x25;&#x8bc1;&#x9700;&#x76d1;&#x7ba1;&#x25;&#x27;&#x29; THEN&#xd;&#xa;             &#x27;1&#x27;&#xd;&#xa;            ELSE&#xd;&#xa;             &#x27;0&#x27;&#xd;&#xa;          END&#xd;&#xa;         ,CASE&#xd;&#xa;            WHEN FIXED_ITINERARY_GID LIKE &#x27;&#x25;&#x8054;&#x8fd0;&#x25;&#x27; THEN&#xd;&#xa;             &#x27;1&#x27;&#xd;&#xa;            ELSE&#xd;&#xa;             &#x27;0&#x27;&#xd;&#xa;          END&#xd;&#xa;         ,CASE&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;CA&#x27; AND T.ORDER_RELEASE_NAME LIKE &#x27;5&#x25;&#x27; THEN&#xd;&#xa;          &#x27;2&#x27;&#xd;&#xa;         WHEN T.BRAND_ID &#x3d; &#x27;FT&#x27; AND T.OR_PLAN_TYPE &#x3d; &#x27;&#x79fb;&#x5e93;&#x8ba1;&#x5212;&#x27;  THEN&#xd;&#xa;          &#x27;2&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          &#x27;1&#x27;&#xd;&#xa;       END&#x3b;</sql>
