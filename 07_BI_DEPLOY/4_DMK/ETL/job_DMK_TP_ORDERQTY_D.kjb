    <entry>
      <name>TO_DMK</name>
      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMK&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMK.DMK_TP_ORDERQTY_D PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT PERIOD_WID          &#xd;&#xa;      ,OC_ID               &#xd;&#xa;      ,RGN_ID              &#xd;&#xa;      ,BASE_ID             &#xd;&#xa;      ,PROJ_ID             &#xd;&#xa;      ,SOURCE_LOCATION_TYPE&#xd;&#xa;      ,SOURCE_LOCATION_ID  &#xd;&#xa;      ,SRC_PRVN_ID         &#xd;&#xa;      ,SRC_CITY_ID         &#xd;&#xa;      ,DEST_LOCATION_TYPE  &#xd;&#xa;      ,DEST_LOCATION_ID    &#xd;&#xa;      ,DEST_PRVN_ID        &#xd;&#xa;      ,DEST_CITY_ID        &#xd;&#xa;      ,CUST_ID             &#xd;&#xa;      ,BRAND_ID            &#xd;&#xa;      ,MODEL_ID            &#xd;&#xa;      ,TPMODE_ID           &#xd;&#xa;      ,SUPPLIER_ID         &#xd;&#xa;      ,FINANCING_FLAG      &#xd;&#xa;      ,CNT_FLAG            &#xd;&#xa;      ,PLAN_TYPE           &#xd;&#xa;      ,SUM&#x28;TICKET_QTY&#x29;   AS TICKET_QTY          &#xd;&#xa;      ,SUM&#x28;ALLOT_QTY&#x29;    AS ALLOT_QTY&#xd;&#xa;      ,SUM&#x28;SHIPPING_QTY&#x29; AS SHIPPING_QTY        &#xd;&#xa;      ,SUM&#x28;ARRIVE_QTY&#x29;   AS ARRIVE_QTY       &#xd;&#xa;      ,SUM&#x28;TICKET_ORD_QTY&#x29;   AS TICKET_ORD_QTY          &#xd;&#xa;      ,SUM&#x28;ALLOT_ORD_QTY&#x29;    AS ALLOT_ORD_QTY&#xd;&#xa;      ,SUM&#x28;SHIPPING_ORD_QTY&#x29; AS SHIPPING_ORD_QTY        &#xd;&#xa;      ,SUM&#x28;ARRIVE_ORD_QTY&#x29;   AS ARRIVE_ORD_QTY      &#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_DATE --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_DATE --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;      ,SUBSTR&#x28;PERIOD_WID, 1, 6&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;&#xd;&#xa;        SELECT &#x2a; FROM TEMP.TEMP_DMK_TP_ORDERQTY_D_1&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a; FROM TEMP.TEMP_DMK_TP_ORDERQTY_D_2&#xd;&#xa;        UNION ALL&#xd;&#xa;        SELECT &#x2a; FROM TEMP.TEMP_DMK_TP_ORDERQTY_D_3&#xd;&#xa;		UNION ALL&#xd;&#xa;        SELECT &#x2a; FROM TEMP.TEMP_DMK_TP_ORDERQTY_D_4&#x29; as A&#xd;&#xa; GROUP BY PERIOD_WID          &#xd;&#xa;      ,OC_ID               &#xd;&#xa;      ,RGN_ID              &#xd;&#xa;      ,BASE_ID             &#xd;&#xa;      ,PROJ_ID             &#xd;&#xa;      ,SOURCE_LOCATION_TYPE&#xd;&#xa;      ,SOURCE_LOCATION_ID  &#xd;&#xa;      ,SRC_PRVN_ID         &#xd;&#xa;      ,SRC_CITY_ID         &#xd;&#xa;      ,DEST_LOCATION_TYPE  &#xd;&#xa;      ,DEST_LOCATION_ID    &#xd;&#xa;      ,DEST_PRVN_ID        &#xd;&#xa;      ,DEST_CITY_ID        &#xd;&#xa;      ,CUST_ID             &#xd;&#xa;      ,BRAND_ID            &#xd;&#xa;      ,MODEL_ID            &#xd;&#xa;      ,TPMODE_ID           &#xd;&#xa;      ,SUPPLIER_ID         &#xd;&#xa;      ,FINANCING_FLAG      &#xd;&#xa;      ,CNT_FLAG            &#xd;&#xa;      ,PLAN_TYPE&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
