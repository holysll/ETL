      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP3&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;--&#x957f;&#x5b89;&#x5230;&#x8fbe;&#x91cf;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMK_TP_ARRIVE_D3&#xd;&#xa;SELECT FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.dlvr_day, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; AS PERIOD_WID --From dw&#x3a;FK to DIM_PERIOD_D.PERIOD_WID&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.dlvr_day, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; AS PERIOD_DT --&#x65e5;&#xd;&#xa;      ,COALESCE&#x28;T.OC_ID, &#x27;-1&#x27;&#x29; AS OC_ID --&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;      ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;      ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; AS PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;      ,T.SOURCE_LOCATION_TYPE AS SOURCE_LOCATION_TYPE --&#x59cb;&#x53d1;&#x5730;&#x7c7b;&#x578b;  &#xd;&#xa;      ,T.SOURCE_LOCATION_ID AS SOURCE_LOCATION_ID --&#x59cb;&#x53d1;&#x5730;ID    &#xd;&#xa;      ,T.SOURCE_PRVN_ID AS SRC_PRVN_ID --&#x59cb;&#x53d1;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;      ,T.SOURCE_CTY_ID AS SRC_CITY_ID --&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;      ,T.DEST_LOCATION_TYPE AS DEST_LOCATION_TYPE --&#x76ee;&#x7684;&#x5730;&#x7c7b;&#x578b;  &#xd;&#xa;      ,T.DEST_LOCATION_ID AS DEST_LOCATION_ID --&#x76ee;&#x7684;&#x5730;ID    &#xd;&#xa;      ,T.DEST_PRVN_ID AS DEST_PRVN_ID --&#x76ee;&#x7684;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;      ,T.DEST_CTY_ID AS DEST_CITY_ID --&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;      ,R.CUST_ID AS CUST_ID --&#x5ba2;&#x6237;&#xd;&#xa;      ,R.BRAND_ID AS BRAND_ID --&#x54c1;&#x724c;&#xd;&#xa;      ,T.MODEL_ID AS MODEL_ID --&#x8f66;&#x578b;&#xd;&#xa;      ,T.SEGMETN_TRANSPORTMODE AS TRANSPORTMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x28;&#x4ec5;&#x542b;&#x7b2c;&#x4e00;&#x6bb5;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#x29; &#xd;&#xa;      ,0 AS BIZ_QTY --&#x4e1a;&#x52a1;&#x91cf;&#xff0c;&#x6709;&#x591a;&#x5c11;&#x5355;&#x53f7;&#x5c31;&#x6709;&#x591a;&#x5c11;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,COUNT&#x28;DISTINCT T.ORDER_RELEASE_NAME&#x29; AS TOTAL_ARRIVE_NUM --&#x603b;&#x5230;&#x6570;&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE&#xd;&#xa;             --WHEN DATEDIFF&#x28;T.ARRIVAL_DAY, T.OUT_STORE_DAY&#x29; - NVL&#x28;R.ON_PASSAGE_DAYS, 0&#x29; &#x3d; 0 THEN UPDATE 20180705&#xd;&#xa;             WHEN DATEDIFF&#x28;T.dlvr_day, T.unload_day&#x29; - NVL&#x28;T.ON_PASSAGE_DAYS, 0&#x29; &#x3d; 0 THEN&#xd;&#xa;              T.ORDER_RELEASE_NAME&#xd;&#xa;             ELSE&#xd;&#xa;              NULL&#xd;&#xa;           END&#x29; AS ONTIME_ARRIVE_NUM --&#x51c6;&#x5230;&#x6570;&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE&#xd;&#xa;             WHEN DATEDIFF&#x28;T.dlvr_day, T.unload_day&#x29; - NVL&#x28;T.ON_PASSAGE_DAYS, 0&#x29; &#x3e; 0 THEN&#xd;&#xa;              T.ORDER_RELEASE_NAME&#xd;&#xa;             ELSE&#xd;&#xa;              NULL&#xd;&#xa;           END&#x29; AS LATE_ARRIVE_NUM --&#x665a;&#x5230;&#x6570;&#xd;&#xa;      ,COUNT&#x28;DISTINCT CASE&#xd;&#xa;             WHEN DATEDIFF&#x28;T.dlvr_day, T.unload_day&#x29; - NVL&#x28;T.ON_PASSAGE_DAYS, 0&#x29; &#x3c; 0 THEN&#xd;&#xa;              T.ORDER_RELEASE_NAME&#xd;&#xa;             ELSE&#xd;&#xa;              NULL&#xd;&#xa;           END&#x29; AS ADVANCE_ARRIVE_NUM --&#x65e9;&#x5230;&#x6570;&#xd;&#xa;      ,0 AS ON_PASSAGE_NUM --&#x5728;&#x9014;&#x91cf;&#xd;&#xa;      ,0 AS ON_PASSAGE_OUTTIME_NUM --&#x5728;&#x9014;&#x8d85;&#x65f6;&#x91cf;&#xd;&#xa;  FROM DW.DW_TP_ORDERMOVEMENT T&#xd;&#xa;  JOIN DW.DW_TP_ORDERRELEASE R --&#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;    ON T.ORDER_RELEASE_GID &#x3d; R.ORDER_RELEASE_GID&#xd;&#xa; LEFT JOIN &#x28;&#xd;&#xa;    SELECT servprov_name AS SUPPLIER_ID FROM dim.dim_servprov WHERE servprov_id LIKE &#x27;&#x25;&#x7279;&#x9510;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x957f;&#x4e45;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x5b89;&#x76db;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x7f8e;&#x8054;&#x25;&#x27; &#xd;&#xa;    	OR servprov_id LIKE &#x27;&#x25;&#x4f17;&#x5347;&#x25;&#x27;&#xd;&#xa;    &#x29; P&#xd;&#xa; ON  T.lsp &#x3d; P.SUPPLIER_ID&#xd;&#xa; WHERE 1&#x3d;1&#xd;&#xa;   AND R.OR_PLAN_TYPE &#x3d; &#x27;&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x27;  --ODS ORF&#x6ca1;&#x6570;&#x636e;&#xff0c;&#x8be5;&#x6761;&#x4ef6;&#x5148;&#x53bb;&#x6389;&#xd;&#xa;   AND R.TRANSPORTSTATUS&#x3d;&#x27;&#x4ea4;&#x8f66;&#x27; --ADD 20180705&#xd;&#xa;   AND T.BRAND_ID &#x3d; &#x27;CA&#x27; --&#x957f;&#x5b89;&#xd;&#xa;   AND SUBSTR&#x28;FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.dlvr_day, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;,1, 6&#x29; &#xd;&#xa;       &#x3d; REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; &#xd;&#xa;   AND P.SUPPLIER_ID IS NULL&#xd;&#xa; GROUP BY FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;         ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T.dlvr_day,&#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;,&#x27;yyyyMMdd&#x27;&#x29; --&#x65e5;&#xd;&#xa;         ,COALESCE&#x28;T.OC_ID, &#x27;-1&#x27;&#x29; --&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;         ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; --&#x5927;&#x533a;ID&#xd;&#xa;         ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; --&#x57fa;&#x5730;ID&#xd;&#xa;         ,COALESCE&#x28;T.PROJ_ID, &#x27;-1&#x27;&#x29; --&#x9879;&#x76ee;ID&#xd;&#xa;         ,T.SOURCE_LOCATION_TYPE --&#x59cb;&#x53d1;&#x5730;&#x7c7b;&#x578b;  &#xd;&#xa;         ,T.SOURCE_LOCATION_ID --&#x59cb;&#x53d1;&#x5730;ID    &#xd;&#xa;         ,T.SOURCE_PRVN_ID --&#x59cb;&#x53d1;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;         ,T.SOURCE_CTY_ID --&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;         ,T.DEST_LOCATION_TYPE --&#x76ee;&#x7684;&#x5730;&#x7c7b;&#x578b;  &#xd;&#xa;         ,T.DEST_LOCATION_ID --&#x76ee;&#x7684;&#x5730;ID    &#xd;&#xa;         ,T.DEST_PRVN_ID --&#x76ee;&#x7684;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;         ,T.DEST_CTY_ID --&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;         ,R.CUST_ID&#xd;&#xa;         ,R.BRAND_ID&#xd;&#xa;         ,T.MODEL_ID&#xd;&#xa;         ,T.SEGMETN_TRANSPORTMODE&#x3b;</sql>
