    <entry>
      <name>TO_DMK</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW_TP_ORDERRELEASE &#x8fd0;&#x8f93;&#x8ba2;&#x5355;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_TP_ARRIVE_D &#x8fd0;&#x8f93;&#x5230;&#x8fbe;&#x91cf;&#x7edf;&#x8ba1;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;02_&#x9700;&#x6c42;&#x5206;&#x6790;&#x5c;02_&#x4fe1;&#x606f;&#x7cfb;&#x7edf;&#x5206;&#x6790;&#x5c;01_&#x6e90;&#x7cfb;&#x7edf;&#x8d44;&#x6599;&#x5c;OTM&#x5c;&#x5176;&#x4ed6;OTM&#x524d;&#x53f0;&#x53c2;&#x8003;&#x5c;&#x957f;&#x5b89;&#x798f;&#x7279;&#x516c;&#x8def;&#x5728;&#x9014;&#x8ddf;&#x8e2a;&#x8868;.sql&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMK.DMK_TP_ARRIVE_D PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT T.PERIOD_WID --  &#x671f;&#x95f4;ID&#xd;&#xa;      ,T.PERIOD_DT --  &#x65e5;&#xd;&#xa;      ,T.OC_ID --  &#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;      ,T.RGN_ID --  &#x5927;&#x533a;ID&#xd;&#xa;      ,T.BASE_ID --  &#x57fa;&#x5730;ID&#xd;&#xa;      ,T.PROJ_ID --  &#x9879;&#x76ee;ID&#xd;&#xa;      ,T.SOURCE_LOCATION_TYPE --  &#x59cb;&#x53d1;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;      ,T.SOURCE_LOCATION_ID --  &#x59cb;&#x53d1;&#x5730;ID&#xd;&#xa;      ,T.SRC_PRVN_ID --  &#x59cb;&#x53d1;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;      ,T.SRC_CITY_ID --  &#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;      ,T.DEST_LOCATION_TYPE --  &#x76ee;&#x7684;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;      ,T.DEST_LOCATION_ID --  &#x76ee;&#x7684;&#x5730;ID&#xd;&#xa;      ,T.DEST_PRVN_ID --  &#x76ee;&#x7684;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;      ,T.DEST_CITY_ID --  &#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;      ,T.CUST_ID --  &#x5ba2;&#x6237;&#xd;&#xa;      ,T.BRAND_ID --  &#x54c1;&#x724c;&#xd;&#xa;      ,T.MODEL_ID --  &#x8f66;&#x578b;&#xd;&#xa;      ,T.TRANSPORTMODE_ID --  &#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x28;&#x4ec5;&#x542b;&#x7b2c;&#x4e00;&#x6bb5;&#x53d1;&#x8fd0;&#x65b9;&#x5f0f;&#x29;&#xd;&#xa;      ,SUM&#x28;T.BIZ_QTY&#x29; AS BIZ_QTY --  &#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;      ,SUM&#x28;T.TOTAL_ARRIVE_NUM&#x29; AS TOTAL_ARRIVE_NUM --  &#x603b;&#x5230;&#x6570;&#xd;&#xa;      ,SUM&#x28;T.ONTIME_ARRIVE_NUM&#x29; AS ONTIME_ARRIVE_NUM --  &#x51c6;&#x5230;&#x6570;&#xd;&#xa;      ,SUM&#x28;T.LATE_ARRIVE_NUM&#x29; AS LATE_ARRIVE_NUM --  &#x665a;&#x5230;&#x6570;&#xd;&#xa;      ,SUM&#x28;T.ADVANCE_ARRIVE_NUM&#x29; AS ADVANCE_ARRIVE_NUM --  &#x65e9;&#x5230;&#x6570;&#xd;&#xa;      ,SUM&#x28;T.ON_PASSAGE_NUM&#x29; AS ON_PASSAGE_NUM --  &#x5728;&#x9014;&#x91cf;&#xd;&#xa;      ,SUM&#x28;T.ON_PASSAGE_OUTTIME_NUM&#x29; AS ON_PASSAGE_OUTTIME_NUM --  &#x5728;&#x9014;&#x8d85;&#x65f6;&#x91cf;&#xd;&#xa;      ,&#x27;DMK_DW_TP_ORDERRELEASE&#x27; AS SOURCE_CODE --&#x6e90;&#x7cfb;&#x7edf;&#x6807;&#x8bc6;&#xff0c;format&#x3a; DMK_&#x5b;&#x6e90;&#x8868;&#x5d;&#xd;&#xa;      ,&#x27;-1&#x27; AS INTEGRATION_ID --&#x8bb0;&#x5f55;&#x552f;&#x4e00;&#x6807;&#x8bc6;, format&#x3a; SOURCE_CODE &#x7c;&#x7c; _ &#x7c;&#x7c; ORDER_ID&#xd;&#xa;      ,&#x27;-1&#x27; AS AUX_CHANGED_ON_DT --&#x539f;&#x7cfb;&#x7edf;&#x8bb0;&#x5f55;&#x521b;&#x5efa;&#x65f6;&#x95f4;, format&#x3a; TABLE_NAME1&#x5b;yyyy-MM-dd HH&#x3a;mm&#x3a;dd&#x5d;,...&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_INSERT_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_UPDATE_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27