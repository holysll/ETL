    <entry>
      <name>TO_DMK</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_FIN_TPBUDGET_D	&#x8fd0;&#x8f93;&#x8d22;&#x52a1;&#x9884;&#x7b97;&#x65e5;&#x660e;&#x7ec6;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;05_DMK&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;--2018-10-15 &#x5254;&#x9664;&#x2018;&#x7279;&#x9510;&#x3001;&#x957f;&#x4e45;&#x3001;&#x5b89;&#x76db;&#x3001;&#x7f8e;&#x8054;&#x3001;&#x4f17;&#x5347;&#x2019;&#x627f;&#x8fd0;&#x5546;&#x6570;&#x636e;&#xd;&#xa;INSERT OVERWRITE TABLE DMK.DMK_TP_OPERATE_D PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY,1,10&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; AS PERIOD_WID --&#x671f;&#x95f4;ID&#xd;&#xa;      ,F.OC_ID --&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;      ,F.RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;      ,F.BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;      ,F.PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;      ,F.SOURCE_LOCATION_TYPE --&#x59cb;&#x53d1;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;      ,F.SOURCE_LOCATION_ID --&#x59cb;&#x53d1;&#x5730;ID&#xd;&#xa;      ,F.DEST_LOCATION_TYPE --&#x76ee;&#x7684;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;      ,F.DEST_LOCATION_ID --&#x76ee;&#x7684;&#x5730;ID&#xd;&#xa;      ,F.SOURCE_PRVN_ID --&#x59cb;&#x53d1;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;      ,F.SOURCE_CTY_ID --&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;      ,F.DEST_PRVN_ID --&#x76ee;&#x7684;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;      ,F.DEST_CTY_ID --&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;      ,F.LSP AS SUPPLIER_ID --&#x627f;&#x8fd0;&#x5546;ID&#xd;&#xa;      ,F.SEGMETN_TRANSPORTMODE AS TRANSPORTMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;  SEGMETN_TRANSPORTMODE AS TRANSPORTMODE_ID&#xd;&#xa;      ,D.CUST_ID --&#x5ba2;&#x6237;   DIM.DIM_MODEL.CUST_ID WHERE MODEL_ID&#x3d;MODEL_ID&#xd;&#xa;      ,F.BRAND_ID --&#x54c1;&#x724c;   DIM.DIM_MODEL.BRAND_ID WHERE MODEL_ID&#x3d;MODEL_ID&#xd;&#xa;      ,F.MODEL_ID --&#x8f66;&#x578b;&#xd;&#xa;      ,F.MDLGRP_ID --&#x8f66;&#x578b;&#x7ec4;&#xd;&#xa;      ,SUM&#x28;F.MILEGE&#x29; AS MILEGE --&#x91cc;&#x7a0b;&#xd;&#xa;      ,COUNT&#x28;F.vin&#x29; AS SHIPPING_NUM --&#x53d1;&#x8fd0;&#x91cf;&#x6b21;&#xd;&#xa;      ,count&#x28;CASE WHEN B.base_type &#x3d; &#x27;VDC&#x27; THEN F.vin ELSE NULL END&#x29; AS SHIPPING_QTY  --&#x53d1;&#x8fd0;&#x91cf;&#xd;&#xa;      ,&#x27;DW&#x27; AS SOURCE_CODE --DMK_&#x5b;&#x6e90;&#x8868;&#x5d;&#xd;&#xa;      ,&#x27;-1&#x27; AS INTEGRATION_ID --SOURCE_CODE &#x7c;&#x7c; _ &#x7c;&#x7c; ORDER_ID&#xd;&#xa;      ,NULL AS AUX1_CHANGED_ON_DT --TABLE_NAME1&#x5b;yyyy-MM-dd HH&#x3a;mm&#x3a;dd&#x5d;,&#x2026;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_INSERT_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_UPDATE_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;      ,REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;FROM DW.DW_TP_ORDERMOVEMENT F --&#x539f;&#x8868; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;LEFT JOIN &#x28;&#xd;&#xa;	SELECT servprov_id AS SUPPLIER_ID FROM dim.dim_servprov WHERE servprov_id LIKE &#x27;&#x25;&#x7279;&#x9510;&#x25;&#x27; &#xd;&#xa;		OR servprov_id LIKE &#x27;&#x25;&#x957f;&#x4e45;&#x25;&#x27; &#xd;&#xa;		OR servprov_id LIKE &#x27;&#x25;&#x5b89;&#x76db;&#x25;&#x27; &#xd;&#xa;		OR servprov_id LIKE &#x27;&#x25;&#x7f8e;&#x8054;&#x25;&#x27; &#xd;&#xa;		OR servprov_id LIKE &#x27;&#x25;&#x4f17;&#x5347;&#x25;&#x27;&#xd;&#xa;		&#x29; C&#xd;&#xa;	ON F.LSP &#x3d; C.SUPPLIER_ID&#xd;&#xa;LEFT JOIN DIM.DIM_BRAND D &#xd;&#xa;  ON F.BRAND_ID &#x3d; D.BRAND_ID&#xd;&#xa;LEFT JOIN DIM.dim_org B  &#xd;&#xa;ON F.base_id &#x3d; B.base_id AND B.org_type &#x3d; &#x27;B&#x27;&#xd;&#xa; WHERE F.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;   AND F.SEGMETN_TRANSPORTMODE NOT IN &#x28;&#x27;ZT&#x27;&#x29; --&#x81ea;&#x63d0;&#x4e0d;&#x7b97;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;   AND REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; BETWEEN REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;       AND REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;END_DATE&#x7d;&#x27;, 1, 7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;   AND F.BRAND_ID &#x3d; &#x27;FT&#x27;	 &#xd;&#xa;   AND C.SUPPLIER_ID IS NULL&#xd;&#xa;GROUP BY REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY,1,10&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; --&#x671f;&#x95f4;ID&#xd;&#xa;         ,F.OC_ID --&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;         ,F.RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;         ,F.BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;         ,F.PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;         ,F.SOURCE_LOCATION_TYPE --&#x59cb;&#x53d1;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;         ,F.SOURCE_LOCATION_ID --&#x59cb;&#x53d1;&#x5730;ID&#xd;&#xa;         ,F.DEST_LOCATION_TYPE --&#x76ee;&#x7684;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;         ,F.DEST_LOCATION_ID --&#x76ee;&#x7684;&#x5730;ID&#xd;&#xa;         ,F.SOURCE_PRVN_ID --&#x59cb;&#x53d1;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;         ,F.SOURCE_CTY_ID --&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;         ,F.DEST_PRVN_ID --&#x76ee;&#x7684;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;         ,F.DEST_CTY_ID --&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;         ,F.LSP --&#x627f;&#x8fd0;&#x5546;ID&#xd;&#xa;         ,F.SEGMETN_TRANSPORTMODE --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;  SEGMETN_TRANSPORTMODE AS TRANSPORTMODE_ID&#xd;&#xa;         ,D.CUST_ID --&#x5ba2;&#x6237;   DIM.DIM_MODEL.CUST_ID WHERE MODEL_ID&#x3d;MODEL_ID&#xd;&#xa;         ,F.BRAND_ID --&#x54c1;&#x724c;   DIM.DIM_MODEL.BRAND_ID WHERE MODEL_ID&#x3d;MODEL_ID&#xd;&#xa;         ,F.MODEL_ID --&#x8f66;&#x578b;&#xd;&#xa;         ,F.MDLGRP_ID --&#x8f66;&#x578b;&#x7ec4; &#xd;&#xa;         ,REGEXP_REPLACE&#x28;SUBSTR&#x28;F.FACTORY_DAY,1,7&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; &#xd;&#xa;UNION ALL&#xd;&#xa;SELECT REGEXP_REPLACE&#x28;SUBSTR&#x28;F.UNLOAD_DAY,1,10&#x29;,&#x27;-&#x27;,&#x27;&#x27;&#x29; AS PERIOD_WID --&#x671f;&#x95f4;ID&#xd;&#xa;    ,F.OC_ID --&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;    ,F.RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;    ,F.BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;    ,F.PROJ_ID --&#x9879;&#x76ee;ID&#xd;&#xa;    ,F.SOURCE_LOCATION_TYPE --&#x59cb;&#x53d1;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;    ,F.SOURCE_LOCATION_ID --&#x59cb;&#x53d1;&#x5730;ID&#xd;&#xa;    ,F.DEST_LOCATION_TYPE --&#x76ee;&#x7684;&#x5730;&#x7c7b;&#x578b;&#xd;&#xa;    ,F.DEST_LOCATION_ID --&#x76ee;&#x7684;&#x5730;ID&#xd;&#xa;    ,F.SOURCE_PRVN_ID --&#x59cb;&#x53d1;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;    ,F.SOURCE_CTY_ID --&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;    ,F.DEST_PRVN_ID --&#x76ee;&#x7684;&#x5730;&#x7701;&#x4efd;ID&#xd;&#xa;    ,F.DEST_CTY_ID --&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#xd;&#xa;    ,F.LSP AS SUPPLIER_ID --&#x627f;&#x8fd0;&#x5546;ID&#xd;&#xa;    ,F.SEGMETN_TRANSPORTMODE AS TRANSPORTMODE_ID --&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;  SEGMETN_TRANSPORTMODE AS TRANSPORTMODE_ID&#xd;&#xa;    ,D.CUST_ID --&#x5ba2;&#x6237;   DIM.DIM_MODEL.CUST_ID WHERE MODEL_ID&#x3d;MODEL_ID&#xd;&#xa;    ,F.BRAND_ID --&#x54c1;&#x724c;  