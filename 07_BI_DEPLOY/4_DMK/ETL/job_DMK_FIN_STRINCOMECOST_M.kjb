    <entry>
      <name>TO_DMK</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DMK&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;INSERT OVERWRITE TABLE DMK.DMK_FIN_STRINCOMECOST_M PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;  SELECT T.PERIOD_WID --  &#x671f;&#x95f4;ID&#xd;&#xa;        ,T.YEAR --  &#x5e74;&#xd;&#xa;        ,T.MONTH --  &#x6708;&#xd;&#xa;        ,COALESCE&#x28;T.OC_ID, &#x27;CMAL&#x2f;ZC&#x27;&#x29; --  &#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;        ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; --  &#x5927;&#x533a;ID&#xd;&#xa;        ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; --  &#x57fa;&#x5730;ID&#xd;&#xa;        ,COALESCE&#x28;P.PROJ_ID, &#x27;-1&#x27;&#x29; --  &#x9879;&#x76ee;ID&#xd;&#xa;        ,SUM&#x28;T.VCL_MANAGER_INCOME_AMT&#x29; AS VCL_MANAGER_INCOME_AMT --  &#x6574;&#x8f66;&#x7ba1;&#x7406;&#x8d39;&#x6536;&#x5165;&#xd;&#xa;        ,SUM&#x28;T.VCL_RENT_INCOME_AMT&#x29; AS VCL_RENT_INCOME_AMT --  &#x6574;&#x8f66;&#x79df;&#x91d1;&#x6536;&#x5165;&#xd;&#xa;        ,SUM&#x28;T.SECURE_CLEAN_INCOME_AMT&#x29; AS SECURE_CLEAN_INCOME_AMT --  &#x6574;&#x8f66;&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6536;&#x5165;&#xd;&#xa;        ,SUM&#x28;T.STORE_MANAGER_COST_AMT&#x29; AS STORE_MANAGER_COST_AMT --  &#x4ed3;&#x50a8;&#x7ba1;&#x7406;&#x6210;&#x672c;&#xd;&#xa;        ,SUM&#x28;T.RENT_COST_AMT&#x29; AS RENT_COST_AMT --  &#x79df;&#x91d1;&#x6210;&#x672c;&#xd;&#xa;        ,SUM&#x28;T.SECURE_CLEAN_COST_AMT&#x29; AS SECURE_CLEAN_COST_AMT --  &#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6210;&#x672c;&#xd;&#xa;        ,&#x27;DW&#x27; AS SOURCE_CODE --DMK_&#x5b;&#x6e90;&#x8868;&#x5d;&#xd;&#xa;        ,&#x27;-1&#x27; AS INTEGRATION_ID --SOURCE_CODE &#x7c;&#x7c; _ &#x7c;&#x7c; ORDER_ID&#xd;&#xa;        ,NULL AS AUX1_CHANGED_ON_DT --TABLE_NAME1&#x5b;yyyy-MM-dd HH&#x3a;mm&#x3a;dd&#x5d;,&#x2026;&#xd;&#xa;        ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_INSERT_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;        ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_UPDATE_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;        ,&#x27;DW&#x27; AS PART_SC&#xd;&#xa;        ,T.PERIOD_WID AS PART_DT&#xd;&#xa;    FROM &#x28;SELECT &#x2a;&#xd;&#xa;            FROM TEMP.TEMP_DMK_FIN_STRINCOMECOST_M_CA A&#xd;&#xa;          UNION ALL&#xd;&#xa;          SELECT &#x2a;&#xd;&#xa;            FROM TEMP.TEMP_DMK_FIN_STRINCOMECOST_M_FT B&#xd;&#xa;          UNION ALL&#xd;&#xa;          SELECT &#x2a;&#xd;&#xa;            FROM TEMP.TEMP_DMK_FIN_STRINCOMECOST_M_BY C&#x29; AS T&#xd;&#xa;    LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;      ON T.PROJ_NAME &#x3d; P.PROJ_NAME&#xd;&#xa;   GROUP BY T.PERIOD_WID --  &#x671f;&#x95f4;ID&#xd;&#xa;           ,T.YEAR --  &#x5e74;&#xd;&#xa;           ,T.MONTH --  &#x6708;&#xd;&#xa;           ,COALESCE&#x28;T.OC_ID, &#x27;CMAL&#x2f;ZC&#x27;&#x29; --  &#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#xd;&#xa;           ,COALESCE&#x28;T.RGN_ID, &#x27;-1&#x27;&#x29; --  &#x5927;&#x533a;ID&#xd;&#xa;           ,COALESCE&#x28;T.BASE_ID, &#x27;-1&#x27;&#x29; --  &#x57fa;&#x5730;ID&#xd;&#xa;           ,COALESCE&#x28;P.PROJ_ID, &#x27;-1&#x27;&#x29; --  &#x9879;&#x76ee;ID&#xd;&#xa;  &#x3b;&#xd;&#xa;</sql>
