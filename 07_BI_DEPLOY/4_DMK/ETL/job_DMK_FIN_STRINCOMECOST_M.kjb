    <entry>
      <name>TO_TEMP_CA</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_CA&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMK_FIN_STRINCOMECOST_M_CA&#xd;&#xa;--&#x957f;&#x5b89;&#xd;&#xa;  SELECT T.PERIOD_WID&#xd;&#xa;        ,T.YEAR&#xd;&#xa;        ,T.MONTH&#xd;&#xa;        ,T.OC_ID&#xd;&#xa;        ,T.RGN_ID&#xd;&#xa;        ,T.BASE_ID&#xd;&#xa;        ,NVL&#x28;P.PROJ_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS PROJ_NAME&#xd;&#xa;        ,T.VCL_MANAGER_INCOME_AMT &#x2f; &#x28;1 &#x2b; R.TAX_RATE&#x29; AS VCL_MANAGER_INCOME_AMT&#xd;&#xa;        ,T.VCL_RENT_INCOME_AMT&#xd;&#xa;        ,T.SECURE_CLEAN_INCOME_AMT&#xd;&#xa;        ,T.STORE_MANAGER_COST_AMT&#xd;&#xa;        ,T.RENT_COST_AMT&#xd;&#xa;        ,T.SECURE_CLEAN_COST_AMT&#xd;&#xa;    FROM &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                ,SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; AS YEAR&#xd;&#xa;                ,SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29; AS MONTH&#xd;&#xa;                ,T.OC_ID&#xd;&#xa;                ,T.RGN_ID&#xd;&#xa;                ,T.BASE_ID&#xd;&#xa;                ,T.PROJ_ID&#xd;&#xa;                ,SUM&#x28;T.BASE_BIZ_QTY &#x2a; BASE_DOWNLINE_INCOME_PRICE&#x29; &#x2b;&#xd;&#xa;                 SUM&#x28;TRANS_ALLOCA_BIZ_QTY &#x2a; TRANS_ALLOCA_INCOME_PRICE&#x29; AS VCL_MANAGER_INCOME_AMT --&#x6574;&#x8f66;&#x7ba1;&#x7406;&#x6536;&#x5165;&#xd;&#xa;                ,SUM&#x28;RENT_INCOME&#x29; AS VCL_RENT_INCOME_AMT --&#x6574;&#x8f66;&#x79df;&#x91d1;&#x6536;&#x5165;&#xd;&#xa;                ,SUM&#x28;SECURE_CLEAN_INCOME&#x29; AS SECURE_CLEAN_INCOME_AMT --&#x6574;&#x8f66;&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6536;&#x5165;&#xd;&#xa;                ,SUM&#x28;MANPOWER_COST &#x2b; FOUR_ITM_FEE &#x2b; DEPRECIATION &#x2b;&#xd;&#xa;                     OTHER_FEE&#x29; AS STORE_MANAGER_COST_AMT --&#x4ed3;&#x50a8;&#x7ba1;&#x7406;&#x6210;&#x672c;&#xd;&#xa;                ,SUM&#x28;RENT_COST&#x29; AS RENT_COST_AMT --&#x79df;&#x91d1;&#x6210;&#x672c;&#xd;&#xa;                ,SUM&#x28;SECURE_CLEAN_COST&#x29; AS SECURE_CLEAN_COST_AMT --&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6210;&#x672c;&#xd;&#xa;            FROM DW.DW_FIN_MANAGE_FEE T&#xd;&#xa;           WHERE T.BRAND_ID &#x3d; &#x27;CA&#x27;&#xd;&#xa;             AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; BETWEEN&#xd;&#xa;                 REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AND&#xd;&#xa;                 REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;END_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;           GROUP BY T.PERIOD_WID&#xd;&#xa;                   ,SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;&#xd;&#xa;                   ,SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;&#xd;&#xa;                   ,T.OC_ID&#xd;&#xa;                   ,T.RGN_ID&#xd;&#xa;                   ,T.BASE_ID&#xd;&#xa;                   ,T.PROJ_ID&#x29; AS T&#xd;&#xa;    JOIN DW.DW_FIN_TAX_RATE R&#xd;&#xa;      ON T.PERIOD_WID &#x3d; R.PERIOD_WID&#xd;&#xa;    LEFT JOIN DIM.DIM_PROJ P&#xd;&#xa;      ON T.PROJ_ID &#x3d; P.PROJ_ID&#xd;&#xa;   WHERE R.BIZ_CAT IN &#x28;&#x27;&#x4ed3;&#x50a8;&#x27;&#x29;&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
