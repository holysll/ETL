    <entry>
      <name>TO_TEMP_FT</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_FT&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMK_FIN_STRINCOMECOST_M_FT&#xd;&#xa;--&#x798f;&#x7279;&#xd;&#xa;  SELECT NVL&#x28;C.PERIOD_WID, D.PERIOD_WID&#x29; AS PERIOD_WID&#xd;&#xa;        ,NVL&#x28;C.YEAR, D.YEAR&#x29; AS YEAR&#xd;&#xa;        ,NVL&#x28;C.MONTH, D.MONTH&#x29; AS MONTH&#xd;&#xa;        ,NVL&#x28;C.OC_ID, D.OC_ID&#x29; AS OC_ID&#xd;&#xa;        ,NVL&#x28;C.RGN_ID, D.RGN_ID&#x29; AS RGN_ID&#xd;&#xa;        ,NVL&#x28;C.BASE_ID, D.BASE_ID&#x29; AS BASE_ID&#xd;&#xa;        ,NVL&#x28;C.PROJ_NAME, D.PROJ_NAME&#x29; AS PROJ_NAME&#xd;&#xa;        ,SUM&#x28;C.VCL_MANAGER_INCOME_AMT&#x29; AS VCL_MANAGER_INCOME_AMT&#xd;&#xa;        ,SUM&#x28;D.VCL_RENT_INCOME_AMT&#x29; AS VCL_RENT_INCOME_AMT&#xd;&#xa;        ,SUM&#x28;D.SECURE_CLEAN_INCOME_AMT&#x29; AS SECURE_CLEAN_INCOME_AMT&#xd;&#xa;        ,SUM&#x28;D.STORE_MANAGER_COST_AMT&#x29; AS STORE_MANAGER_COST_AMT&#xd;&#xa;        ,SUM&#x28;D.RENT_COST_AMT&#x29; AS RENT_COST_AMT&#xd;&#xa;        ,SUM&#x28;D.SECURE_CLEAN_COST_AMT&#x29; AS SECURE_CLEAN_COST_AMT&#xd;&#xa;    FROM &#x28;SELECT A.PERIOD_WID&#xd;&#xa;                ,SUBSTR&#x28;A.PERIOD_WID, 1, 4&#x29; AS YEAR&#xd;&#xa;                ,SUBSTR&#x28;A.PERIOD_WID, 5, 2&#x29; AS MONTH&#xd;&#xa;                ,A.OC_ID&#xd;&#xa;                ,A.RGN_ID&#xd;&#xa;                ,A.BASE_ID&#xd;&#xa;                ,A.PROJ_NAME&#xd;&#xa;                ,SUM&#x28;&#x28;A.BIZ_QTY &#x2a; B.PRICE&#x29; &#x2f; &#x28;1 &#x2b; R.TAX_RATE&#x29;&#x29; AS VCL_MANAGER_INCOME_AMT -- &#x4e0d;&#x542b;&#x7a0e;&#x6536;&#x5165;&#xd;&#xa;            FROM &#x28;SELECT D.FIN_IN_MONTH AS PERIOD_WID&#xd;&#xa;                        ,D.FIN_IN_MONTH AS MONTH&#xd;&#xa;                        ,T.OC_ID&#xd;&#xa;                        ,T.RGN_ID&#xd;&#xa;                        ,O.BASE_TYPE&#xd;&#xa;                        ,T.BASE_ID&#xd;&#xa;                        ,P.PROJ_NAME&#xd;&#xa;                        ,SUM&#x28;T.IN_STORE_QTY&#x29; AS BIZ_QTY&#xd;&#xa;                    FROM DMK.DMK_STR_OPERATE_D T&#xd;&#xa;                   INNER JOIN DIM.DIM_PROJ P&#xd;&#xa;                      ON T.PROJ_ID &#x3d; P.PROJ_ID&#xd;&#xa;                   INNER JOIN DIM.DIM_ORG O&#xd;&#xa;                      ON T.BASE_ID &#x3d; O.ORG_ID&#xd;&#xa;                   INNER JOIN DIM.DIM_PERIOD_D D&#xd;&#xa;                      ON T.PERIOD_WID &#x3d; D.PERIOD_WID&#xd;&#xa;                   WHERE 1 &#x3d; 1&#xd;&#xa;                     AND D.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;                     AND T.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;                     AND D.FIN_IN_MONTH BETWEEN&#xd;&#xa;                         REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                        &#x27;-&#x27;,&#xd;&#xa;                                        &#x27;&#x27;&#x29; AND&#xd;&#xa;                         REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;END_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                   GROUP BY D.FIN_IN_MONTH&#xd;&#xa;                           ,T.OC_ID&#xd;&#xa;                           ,T.RGN_ID&#xd;&#xa;                           ,O.BASE_TYPE&#xd;&#xa;                           ,T.BASE_ID&#xd;&#xa;                           ,P.PROJ_NAME&#x29; AS A --&#x5f97;&#x5230;&#x798f;&#x7279;&#x57fa;&#x5730;&#x4e0e;&#x4e2d;&#x8f6c;&#x7ad9;&#x4e1a;&#x52a1;&#x91cf;&#xd;&#xa;            JOIN &#x28;SELECT T.PERIOD_WID AS MONTH&#xd;&#xa;                       ,T.OC_ID&#xd;&#xa;                       ,T.RGN_ID&#xd;&#xa;                       ,&#x27;VDC&#x27; AS BASE_TYPE&#xd;&#xa;                       ,T.BASE_ID&#xd;&#xa;                       ,T.PROJ_NAME&#xd;&#xa;                       ,T.BASE_DOWNLINE_INCOME_PRICE AS PRICE&#xd;&#xa;                   FROM DW.DW_FIN_MANAGE_FEE T&#xd;&#xa;                  WHERE T.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;                    AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; BETWEEN&#xd;&#xa;                        REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                       &#x27;-&#x27;,&#xd;&#xa;                                       &#x27;&#x27;&#x29; AND&#xd;&#xa;                        REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;END_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                 UNION ALL&#xd;&#xa;                 SELECT T.PERIOD_WID AS MONTH&#xd;&#xa;                       ,T.OC_ID&#xd;&#xa;                       ,T.RGN_ID&#xd;&#xa;                       ,&#x27;VSC&#x27; AS BASE_TYPE&#xd;&#xa;                       ,T.BASE_ID&#xd;&#xa;                       ,T.PROJ_NAME&#xd;&#xa;                       ,T.TRANS_ALLOCA_INCOME_PRICE AS PRICE&#xd;&#xa;                   FROM DW.DW_FIN_MANAGE_FEE T&#xd;&#xa;                  WHERE T.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;                    AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; BETWEEN&#xd;&#xa;                        REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;,&#xd;&#xa;                                       &#x27;-&#x27;,&#xd;&#xa;                                       &#x27;&#x27;&#x29; AND&#xd;&#xa;                        REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;END_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; AS B --&#x798f;&#x7279;&#x5355;&#x4ef7;&#xd;&#xa;              ON A.MONTH &#x3d; B.MONTH&#xd;&#xa;             AND A.OC_ID &#x3d; B.OC_ID&#xd;&#xa;             AND A.BASE_TYPE &#x3d; B.BASE_TYPE&#xd;&#xa;             AND A.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;             AND A.PROJ_NAME &#x3d; B.PROJ_NAME&#xd;&#xa;            JOIN DW.DW_FIN_TAX_RATE R&#xd;&#xa;              ON A.PERIOD_WID &#x3d; R.PERIOD_WID&#xd;&#xa;           WHERE R.BIZ_CAT IN &#x28;&#x27;&#x4ed3;&#x50a8;&#x27;&#x29;&#xd;&#xa;           GROUP BY A.PERIOD_WID&#xd;&#xa;                   ,SUBSTR&#x28;A.PERIOD_WID, 1, 4&#x29;&#xd;&#xa;                   ,SUBSTR&#x28;A.PERIOD_WID, 5, 2&#x29;&#xd;&#xa;                   ,A.OC_ID&#xd;&#xa;                   ,A.RGN_ID&#xd;&#xa;                   ,A.BASE_ID&#xd;&#xa;                   ,A.PROJ_NAME&#x29; AS C&#xd;&#xa;    FULL JOIN &#x28;SELECT T.PERIOD_WID&#xd;&#xa;                     ,SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; AS YEAR&#xd;&#xa;                     ,SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29; AS MONTH&#xd;&#xa;                     ,T.OC_ID&#xd;&#xa;                     ,T.RGN_ID&#xd;&#xa;                     ,T.BASE_ID&#xd;&#xa;                     ,T.PROJ_NAME&#xd;&#xa;                     ,SUM&#x28;RENT_INCOME&#x29; AS VCL_RENT_INCOME_AMT --&#x6574;&#x8f66;&#x79df;&#x91d1;&#x6536;&#x5165;&#xd;&#xa;                     ,SUM&#x28;SECURE_CLEAN_INCOME&#x29; AS SECURE_CLEAN_INCOME_AMT --&#x6574;&#x8f66;&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6536;&#x5165;&#xd;&#xa;                     ,SUM&#x28;MANPOWER_COST &#x2b; FOUR_ITM_FEE &#x2b; DEPRECIATION &#x2b;&#xd;&#xa;                          OTHER_FEE&#x29; AS STORE_MANAGER_COST_AMT --&#x4ed3;&#x50a8;&#x7ba1;&#x7406;&#x6210;&#x672c;&#xd;&#xa;                     ,SUM&#x28;RENT_COST&#x29; AS RENT_COST_AMT --&#x79df;&#x91d1;&#x6210;&#x672c;&#xd;&#xa;                     ,SUM&#x28;SECURE_CLEAN_COST&#x29; AS SECURE_CLEAN_COST_AMT --&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6210;&#x672c;&#xd;&#xa;                 FROM DW.DW_FIN_MANAGE_FEE T&#xd;&#xa;                WHERE T.BRAND_ID &#x3d; &#x27;FT&#x27;&#xd;&#xa;                  AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; BETWEEN&#xd;&#xa;                      REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AND&#xd;&#xa;                      REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;END_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;                GROUP BY T.PERIOD_WID&#xd;&#xa;                        ,SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;&#xd;&#xa;                        ,SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;&#xd;&#xa;                        ,T.OC_ID&#xd;&#xa;                        ,T.RGN_ID&#xd;&#xa;                        ,T.BASE_ID&#xd;&#xa;                        ,T.PROJ_NAME&#x29; AS D --&#x798f;&#x7279;&#x5176;&#x4ed6;&#x6536;&#x5165;&#x3001;&#x6210;&#x672c; D&#xd;&#xa;      ON C.PERIOD_WID &#x3d; D.PERIOD_WID&#xd;&#xa;     AND C.OC_ID &#x3d; D.OC_ID&#xd;&#xa;     AND C.RGN_ID &#x3d; D.RGN_ID&#xd;&#xa;     AND C.BASE_ID &#x3d; D.BASE_ID&#xd;&#xa;     AND C.PROJ_NAME &#x3d; D.PROJ_NAME&#xd;&#xa;   GROUP BY NVL&#x28;C.PERIOD_WID, D.PERIOD_WID&#x29;&#xd;&#xa;           ,NVL&#x28;C.YEAR, D.YEAR&#x29;&#xd;&#xa;           ,NVL&#x28;C.MONTH, D.MONTH&#x29;&#xd;&#xa;           ,NVL&#x28;C.OC_ID, D.OC_ID&#x29;&#xd;&#xa;           ,NVL&#x28;C.RGN_ID, D.RGN_ID&#x29;&#xd;&#xa;           ,NVL&#x28;C.BASE_ID, D.BASE_ID&#x29;&#xd;&#xa;           ,NVL&#x28;C.PROJ_NAME, D.PROJ_NAME&#x29;&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
      <connection>Hive</connection>
      <parallel>N</parallel>
      <draw>Y</draw>
