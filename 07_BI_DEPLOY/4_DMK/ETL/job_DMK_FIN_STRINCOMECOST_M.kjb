      <name>TO_TEMP_BY</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_TEMP_BY&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--Insert Data&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMK_FIN_STRINCOMECOST_M_BY&#xd;&#xa;--&#x535a;&#x5b87;&#x548c;&#x672c;&#x90e8;&#xd;&#xa;  SELECT T.PERIOD_WID&#xd;&#xa;        ,SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29; AS YEAR&#xd;&#xa;        ,SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29; AS MONTH&#xd;&#xa;        ,T.OC_ID&#xd;&#xa;        ,T.RGN_ID&#xd;&#xa;        ,T.BASE_ID&#xd;&#xa;        ,T.PROJ_NAME&#xd;&#xa;        ,0 AS VCL_MANAGER_INCOME_AMT --&#x6574;&#x8f66;&#x7ba1;&#x7406;&#x6536;&#x5165;&#xd;&#xa;        ,SUM&#x28;COALESCE&#x28;RENT_INCOME, 0&#x29;&#x29; AS VCL_RENT_INCOME_AMT --&#x6574;&#x8f66;&#x79df;&#x91d1;&#x6536;&#x5165;&#xd;&#xa;        ,SUM&#x28;COALESCE&#x28;SECURE_CLEAN_INCOME, 0&#x29;&#x29; AS SECURE_CLEAN_INCOME_AMT --&#x6574;&#x8f66;&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6536;&#x5165;&#xd;&#xa;        ,SUM&#x28;COALESCE&#x28;MANPOWER_COST, 0&#x29; &#x2b; COALESCE&#x28;FOUR_ITM_FEE, 0&#x29; &#x2b;&#xd;&#xa;             COALESCE&#x28;DEPRECIATION, 0&#x29; &#x2b; COALESCE&#x28;OTHER_FEE, 0&#x29;&#x29; AS STORE_MANAGER_COST_AMT --&#x4ed3;&#x50a8;&#x7ba1;&#x7406;&#x6210;&#x672c;&#xd;&#xa;        ,SUM&#x28;COALESCE&#x28;RENT_COST, 0&#x29;&#x29; AS RENT_COST_AMT --&#x79df;&#x91d1;&#x6210;&#x672c;&#xd;&#xa;        ,SUM&#x28;COALESCE&#x28;SECURE_CLEAN_COST, 0&#x29;&#x29; AS SECURE_CLEAN_COST_AMT --&#x4fdd;&#x5b89;&#x4fdd;&#x6d01;&#x6210;&#x672c;&#xd;&#xa;    FROM DW.DW_FIN_MANAGE_FEE T&#xd;&#xa;   WHERE TRIM&#x28;T.RGN_ID&#x29; IN &#x28;&#x27;&#x535a;&#x5b87;&#x516c;&#x53f8;&#x27;, &#x27;&#x672c;&#x90e8;&#x27;&#x29;&#xd;&#xa;     AND SUBSTR&#x28;T.PERIOD_WID, 1, 6&#x29; BETWEEN&#xd;&#xa;         REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AND&#xd;&#xa;         REGEXP_REPLACE&#x28;SUBSTR&#x28;&#x27;&#x24;&#x7b;END_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#xd;&#xa;   GROUP BY T.PERIOD_WID&#xd;&#xa;           ,SUBSTR&#x28;T.PERIOD_WID, 1, 4&#x29;&#xd;&#xa;           ,SUBSTR&#x28;T.PERIOD_WID, 5, 2&#x29;&#xd;&#xa;           ,T.OC_ID&#xd;&#xa;           ,T.RGN_ID&#xd;&#xa;           ,T.BASE_ID&#xd;&#xa;           ,T.PROJ_NAME&#x3b;&#xd;&#xa;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
