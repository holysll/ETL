    <entry>
      <name>TO_DMK</name>
      <description/>
      <type>SQL</type>
      <sql>--Source Table in Hive&#x3a; DW.DW_TP_ORDERMOVEMENT&#x28;&#x8fd0;&#x8f93;&#x8def;&#x7531;&#x29;&#xd;&#xa;--Target Table in Hive&#x3a; DMK_FIN_TPBUDGET_D	&#x8fd0;&#x8f93;&#x8d22;&#x52a1;&#x9884;&#x7b97;&#x65e5;&#x660e;&#x7ec6;&#xd;&#xa;--Reference&#x3a; DDOM&#x5c;04_&#x7cfb;&#x7edf;&#x8bbe;&#x8ba1;&#x5c;05_DMK&#x6570;&#x636e;&#x5b57;&#x5178;&#xd;&#xa;&#xd;&#xa;--hive conf settings&#xd;&#xa;set hive.execution.engine&#x3d;mr&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;set spark.app.name &#x3d; &#x24;&#x7b;Internal.Job.Name&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE DMK.DMK_TP_DTD_PROG_TIME_D PARTITION&#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;SELECT&#xd;&#xa;       T1.PERIOD_WID                    --STRING             COMMENT  &#x27;&#x671f;&#x95f4;ID&#x27;,&#xd;&#xa;      ,T1.ORDER_RELEASE_GID             --STRING             COMMENT  &#x27;OTM&#x5355;&#x53f7;&#x27;,&#xd;&#xa;      ,T1.ORDER_MOVEMENT_GID            --STRING             COMMENT  &#x27;&#x5206;&#x6bb5;&#x8def;&#x7531;GID&#x27;,&#xd;&#xa;      ,T1.SEQ_NO                        --STRING             COMMENT  &#x27;&#x8def;&#x7531;&#x5e8f;&#x5217;&#x53f7;&#x27;,&#xd;&#xa;      ,T1.BUDGET_SHIPMENT_GID           --STRING             COMMENT  &#x27;&#x4ea4;&#x63a5;&#x5355;id&#x27;,&#xd;&#xa;      ,T1.OC_ID                         --STRING             COMMENT  &#x27;&#x8fd0;&#x8425;&#x4e2d;&#x5fc3;ID&#x27;,&#xd;&#xa;      ,T1.RGN_ID                        --STRING             COMMENT  &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;      ,T1.BASE_ID                       --STRING             COMMENT  &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;      ,T1.BASE_NAME                     --STRING             COMMENT  &#x27;&#x57fa;&#x5730;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;      ,T1.PROJ_ID                       --STRING             COMMENT  &#x27;&#x9879;&#x76ee;ID&#x27;,&#xd;&#xa;      ,T1.PROJ_NAME                     --STRING             COMMENT  &#x27;&#x9879;&#x76ee;&#x540d;&#x79f0;&#x27;,&#xd;&#xa;      ,T1.SOURCE_LOCATION_TYPE          --STRING             COMMENT  &#x27;&#x59cb;&#x53d1;&#x5730;&#x7c7b;&#x578b;&#x27;,&#xd;&#xa;      ,T1.SOURCE_LOCATION_ID            --STRING             COMMENT  &#x27;&#x59cb;&#x53d1;&#x5730;ID&#x27;,&#xd;&#xa;      ,T1.DEST_LOCATION_TYPE            --STRING             COMMENT  &#x27;&#x76ee;&#x7684;&#x5730;&#x7c7b;&#x578b;&#x27;,&#xd;&#xa;      ,T1.DEST_LOCATION_ID              --STRING             COMMENT  &#x27;&#x76ee;&#x7684;&#x5730;ID&#x27;,&#xd;&#xa;      ,T1.SOURCE_PRVN_ID                --STRING             COMMENT  &#x27;&#x59cb;&#x53d1;&#x5730;&#x7701;&#x4efd;ID&#x27;,&#xd;&#xa;      ,T1.SOURCE_CTY_ID                 --STRING             COMMENT  &#x27;&#x59cb;&#x53d1;&#x5730;&#x57ce;&#x5e02;ID&#x27;,&#xd;&#xa;      ,T1.DEST_PRVN_ID                  --STRING             COMMENT  &#x27;&#x76ee;&#x7684;&#x5730;&#x7701;&#x4efd;ID&#x27;,&#xd;&#xa;      ,T1.DEST_CTY_ID                   --STRING             COMMENT  &#x27;&#x76ee;&#x7684;&#x5730;&#x57ce;&#x5e02;ID&#x27;,&#xd;&#xa;      ,T1.LSP                           --STRING             COMMENT  &#x27;&#x627f;&#x8fd0;&#x5546;ID&#x27;,&#xd;&#xa;      ,T1.SEGMETN_TRANSPORTMODE AS TRANSPORTMODE_ID            --STRING             COMMENT  &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x27;,&#xd;&#xa;      ,T2.CUST_ID                       --STRING             COMMENT  &#x27;&#x5ba2;&#x6237;&#x27;,&#xd;&#xa;      ,T1.BRAND_ID                      --STRING             COMMENT  &#x27;&#x54c1;&#x724c;&#x27;,&#xd;&#xa;      ,T1.MODEL_ID                      --STRING             COMMENT  &#x27;&#x8f66;&#x578b;&#x27;,&#xd;&#xa;      ,T1.MDLGRP_ID                     --STRING             COMMENT  &#x27;&#x8f66;&#x578b;&#x7ec4;&#x27;,&#xd;&#xa;      ,T1.MILEGE                        --decimal&#x28;32,8&#x29;      COMMENT  &#x27;&#x91cc;&#x7a0b;&#x27;,&#xd;&#xa;      ,T1.VIN                           --STRING             COMMENT  &#x27;VIN&#x7801;&#x27;,&#xd;&#xa;      ,CASE WHEN TRIM&#x28;T1.BRAND_ID&#x29; &#x3d;&#x27;FT&#x27; THEN T2.ORDER_RELEASE_NAME &#xd;&#xa;        ELSE T1.ORDER_RELEASE_NAME END          AS ORDER_RELEASE_NAME  --&#x8ba2;&#x5355;&#x53f7;      &#xd;&#xa;      ,T1.REST_REASON                   --STRING             COMMENT  &#x27;&#x534f;&#x529b;&#x539f;&#x56e0;&#x28;&#x6563;&#x5355;&#x6807;&#x5fd7;&#x28;&#x6563;&#x5355;&#x7684;&#x5224;&#x65ad;&#x903b;&#x8f91;DETAIL_FLAG&#x29;&#x29;&#x27;,&#xd;&#xa;      ,CASE WHEN T1.REST_REASON LIKE &#x27;&#x25;&#x62a2;&#x5355;&#x5206;&#x914d;&#x25;&#x27; THEN 1 &#xd;&#xa;           ELSE 0 END        AS DETAIL_FLAG --&#x6563;&#x5355;&#x6807;&#x5fd7;&#x28;&#x6563;&#x5355;&#x7684;&#x5224;&#x65ad;&#x903b;&#x8f91;&#x29;&#xd;&#xa;      ,COALESCE&#x28; &#x28;CASE WHEN T1.BRAND_ID&#x3d;&#x27;FT&#x27; THEN T2.ERP_IMPORT_DATE ELSE T1.EARLY_PICKUP_DATE END&#x29;,T8.INSERT_DATE&#x29;              AS EARLY_PICKUP_DATE  --&#x63a5;&#x5355;&#x65f6;&#x95f4;      &#xd;&#xa;      ,CASE WHEN T1.BRAND_ID&#x3d;&#x27;FT&#x27; THEN T1.FACTORY_DAY ELSE T1.UNLOAD_DAY  END AS     FACTORY_DAY             --STRING             COMMENT  &#x27;&#x51fa;&#x573a;&#x65e5;-&#x6253;&#x5355;&#x65f6;&#x95f4;-&#x51fa;&#x573a;&#x65f6;&#x95f4;-ATTRIBUTE_DATE8-FT&#x27;,&#xd;&#xa;      ,T1.ARRANGE_DAY AS  ARRANGE_DAY               --STRING             COMMENT  &#x27;&#x5b89;&#x6392;&#x65e5;&#x28;&#x6d3e;&#x8f66;&#x786e;&#x8ba4;&#x65e5;&#x29;&#x27;,&#xd;&#xa;      ,T1.OUT_STORE_DAY                 --STRING             COMMENT  &#x27;&#x51fa;&#x5e93;&#x65e5;&#x2f;&#x4e2d;&#x8f6c;&#x51fa;&#x5e93;&#x65e5;&#x27;,&#xd;&#xa;      ,CASE WHEN TRIM&#x28;T1.BRAND_ID&#x29; &#x3d;&#x27;CA&#x27; THEN T1.SHIPMENT_DAY&#xd;&#xa;                  ELSE T1.ALLOT_PLN_DAY &#xd;&#xa;       END                                                     AS ALLOT_PLN_DAY  --&#x5206;&#x914d;&#x65f6;&#x95f4; &#x82e5;&#x6ca1;&#x6709;&#x5219;&#x53d6;&#x627f;&#x8fd0;&#x5546;&#x6700;&#x65b0;&#x786e;&#x8ba4;&#x65f6;&#x95f4;  --T5.INSERT_DATE     &#xd;&#xa;      ,T1.DLVR_DAY  AS  DLVR_DAY                    --STRING             COMMENT  &#x27;&#x5230;&#x8fbe;&#x27;,&#xd;&#xa;      --,CASE WHEN FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T1.ARRIVAL_DAY, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T1.ARRIVAL_DAY&#xd;&#xa;      --       ELSE NULL END               AS ARRIVAL_DAY   --&#x4ea4;&#x4ed8;&#x65f6;&#x95f4;   &#xd;&#xa;      ,T1.ARRIVAL_DAY AS  ARRIVAL_DAY   --&#x4ea4;&#x8f66;&#x65f6;&#x95f4;   &#xd;&#xa;      ,T1.SHIPMENT_DAY                  --STRING             COMMENT  &#x27;&#x88c5;&#x8239;&#x65e5;&#x671f;&#x27;,&#xd;&#xa;      ,T1.UNLOAD_DAY                    --STRING             COMMENT  &#x27;&#x5378;&#x8239;&#x65e5;&#x671f;-CA&#x27;,&#xd;&#xa;      ,T1.LOAD_DAY                      --STRING             COMMENT  &#x27;&#x88c5;&#x8f66;&#x65e5;&#x671f;&#x27;,&#xd;&#xa;      ,T1.LEAVE_BASE_DAY                --STRING             COMMENT  &#x27;&#x79bb;&#x5f00;&#x57fa;&#x5730;&#x548c;&#x4e2d;&#x8f6c;&#x7ad9;&#x65e5;&#x671f;&#x27;,&#xd;&#xa;      ,T3.RECEIPT_DATE                  --STRING             COMMENT  &#x27;&#x56de;&#x5355;&#x65f6;&#x95f4;-DW_TP_SHIPMENT&#x27;,&#xd;&#xa;     ,--CASE WHEN FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;T3.INSERT_DATE, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;sss&#x27;&#x29;, &#x27;yyyyMM&#x27;&#x29; &#x3d;  SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;,&#x27;-&#x27;,&#x27;&#x27;&#x29;,1,6&#x29; THEN T3.INSERT_DATE&#xd;&#xa;      --            ELSE NULL&#xd;&#xa;      --      END                                                     AS BILL_DATE     --&#x7ed3;&#x7b97;&#x65f6;&#x95f4;&#x28;&#x4e0e;&#x627f;&#x8fd0;&#x5546;&#xff09;&#xd;&#xa;       T3.INSERT_DATE AS BILL_DATE   --&#x7ed3;&#x7b97;&#x65f6;&#x95f4;&#x28;&#x4e0e;&#x627f;&#x8fd0;&#x5546;&#xff09;&#xd;&#xa;      ,T3.INSERT_DATE			        --STRING             COMMENT   &#x8fd0;&#x8f93;&#x53d1;&#x7968;&#x5165;&#x5e93;&#x65f6;&#x95f4;&#xd;&#xa;      ,T2.LATE_DELIVERY_DATE            --STRING             COMMENT  &#x27;&#x8003;&#x6838;&#x65e5;DW_TP_ORDERRELEASE&#x27;,&#xd;&#xa;      ,T2.LEAVE_CQ_DAY                  --STRING             COMMENT  &#x27;&#x79bb;&#x6e1d;&#x65e5;DW_TP_ORDERRELEASE&#x27;,&#xd;&#xa;      ,T2.DMS_PICKUP_DAY                --STRING             COMMENT  &#x27;DMS&#x6536;&#x8f66;&#x65e5;DW_TP_ORDERRELEASE&#x27;,&#xd;&#xa;      ,T2.CHECK_DAY                     --STRING             COMMENT  &#x27;&#x8003;&#x6838;&#x65e5;DW_TP_ORDERRELEASE&#x27;,&#xd;&#xa;      ,T2.VDRNO_VDC_DAY                 --STRING             COMMENT  &#x27;VDRNO&#x53d1;&#x9001;VDC&#x65e5;&#x671f;DW_TP_ORDERRELEASE&#x27;,&#xd;&#xa;      ,T2.ALLOCATEDATE                  --STRING             COMMENT  &#x27;&#x798f;&#x7279;&#x9500;&#x552e;&#x5206;&#x914d;&#x65e5;&#x671f;ALLOCATEDATE&#x27;DW_TP_ORDERRELEASE,&#xd;&#xa;      ,&#x27;DW&#x27; AS SOURCE_CODE --&#x6e90;&#x7cfb;&#x7edf;&#x6807;&#x8bc6;&#xd;&#xa;      ,NULL AS INTEGRATION_ID --&#x8bb0;&#x5f55;&#x552f;&#x4e00;&#x6807;&#x8bc6;&#xd;&#xa;      ,NULL AS AUX1_CHANGED_ON_DT --&#x539f;&#x7cfb;&#x7edf;&#x8bb0;&#x5f55;&#x521b;&#x5efa;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_INSERT_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_UPDATE_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x4fee;&#x6539;&#x65f6;&#x95f4;&#xd;&#xa;      ,T1.ON_PASSAGE_DAYS AS ON_PASSAGE_DAYS--&#x5728;&#x9014;&#x5929;&#x6570;&#xd;&#xa;      ,T2.OR_PLAN_TYPE AS --&#x53d1;&#x8fd0;&#x8ba1;&#x5212;&#x2f;&#x7559;&#x5e93;&#x8ba1;&#x5212;&#x2f;&#x79fb;&#x5e93;&#x8ba1;&#x5212; DW_TP_ORDERRELEASE&#xd;&#xa;      ,&#x27;DW&#x27; AS PART_SC --&#x6570;&#x636e;&#x6765;&#x6e90;&#xd;&#xa;      ,SUBSTR&#x28;T1.PERIOD_WID, 1, 6&#x29; AS PART_DT --&#x5206;&#x533a;&#x65f6;&#x95f4;&#xd;&#xa;FROM DW.DW_TP_ORDERMOVEMENT T1  --&#x8def;&#x7531;&#x8868;&#xd;&#xa;LEFT JOIN  DW.DW_TP_ORDERRELEASE T2   --&#x8ba2;&#x5355;&#x8868;&#xd;&#xa;       ON  T1.ORDER_RELEASE_GID&#x3d;T2.ORDER_RELEASE_GID    &#xd;&#xa;&#xd;&#xa;LEFT JOIN &#x28;&#xd;&#xa;    SELECT C.order_release_gid&#xd;&#xa;        ,MAX&#x28;B.RECEIPT_DATE&#x29; AS RECEIPT_DATE  --&#x56de;&#x5355;&#x65f6;&#x95f4;&#xd;&#xa;        ,max&#x28;B.INSERT_DATE&#x29; AS INSERT_DATE&#xd;&#xa;    FROM DW.DW_TP_ORDERMOVEMENT C&#xd;&#xa;    LEFT JOIN &#x28;&#xd;&#xa;        SELECT A.SHIPMENT_GID&#xd;&#xa;            ,MAX&#x28;A.receipt_date&#x29; AS RECEIPT_DATE  --&#x56de;&#x5355;&#x65f6;&#x95f4;&#xd;&#xa;            ,MAX&#x28;B.INSERT_DATE&#x29;     AS INSERT_DATE&#xd;&#xa;        FROM DW.DW_TP_SHIPMENT A&#xd;&#xa;        LEFT JOIN DW.DW_FIN_TPINVOICE B&#xd;&#xa;        ON A.INVOICE_GID &#x3d; B.INVOICE_GID&#xd;&#xa;        GROUP BY A.SHIPMENT_GID&#xd;&#xa;        &#x29; B &#xd;&#xa;    ON C.budget_shipment_gid &#x3d; B.SHIPMENT_GID&#xd;&#xa;    --WHERE C.perspective &#x3d; &#x27;S&#x27; --2221555&#xd;&#xa;    GROUP BY C.order_release_gid&#xd;&#xa;    &#x29; T3  --&#x7ed3;&#x7b97;&#x6570;&#x636e;&#xd;&#xa;ON T1.order_release_gid &#x3d; T3.order_release_gid&#xd;&#xa;&#xd;&#xa;LEFT JOIN  &#x28;SELECT  OBJID             AS OBJID&#xd;&#xa;                   ,MIN&#x28;INSERT_DATE&#x29;  AS INSERT_DATE&#xd;&#xa;&#xd;&#xa;            FROM   ODS_ZCOTM.XX_T_OPERATION_HISTORY  --&#x64cd;&#x4f5c;&#x5386;&#x53f2;&#xd;&#xa;            WHERE  LOG_DETAIL&#x3d;&#x27;OM LSP CONFIRMED&#x27;     --&#x627f;&#x8fd0;&#x5546;&#x786e;&#x8ba4;&#x65f6;&#x95f4;&#xd;&#xa;            GROUP BY OBJID &#x29; T8&#xd;&#xa;       ON  T1.ORDER_MOVEMENT_GID &#x3d; T8.OBJID&#xd;&#xa;LEFT JOIN &#x28;&#xd;&#xa;	SELECT servprov_id AS SUPPLIER_ID FROM dim.dim_servprov WHERE servprov_id LIKE &#x27;&#x25;&#x7279;&#x9510;&#x25;&#x27; &#xd;&#xa;		OR servprov_id LIKE &#x27;&#x25;&#x957f;&#x4e45;&#x25;&#x27; &#xd;&#xa;		OR servprov_id LIKE &#x27;&#x25;&#x5b89;&#x76db;&#x25;&#x27; &#xd;&#xa;		OR servprov_id LIKE &#x27;&#x25;&#x7f8e;&#x8054;&#x25;&#x27; &#xd;&#xa;		OR servprov_id LIKE &#x27;&#x25;&#x4f17;&#x5347;&#x25;&#x27;&#xd;&#xa;		&#x29; C&#xd;&#xa;ON T1.LSP &#x3d; C.SUPPLIER_ID&#xd;&#xa;--LEFT JOIN &#x28;SELECT OM_GID,MAX&#x28;INSERT_DATE&#x29; AS INSERT_DATE&#xd;&#xa;          --FROM ODS_ZCOTM.XX_T_ZC_CA_LSP_CHANGE_HIS&#xd;&#xa;	--GROUP BY OM_GID&#xd;&#xa;          --&#x29; T9 &#xd;&#xa;       --ON T9.OM_GID &#x3d; T1.ORDER_MOVEMENT_GID&#xd;&#xa;WHERE T1.PERSPECTIVE &#x3d; &#x27;B&#x27;&#xd;&#xa;	AND C.SUPPLIER_ID IS NULL&#x3b;</sql>
