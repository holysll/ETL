      <description/>
      <type>SQL</type>
      <sql>truncate table MD_BIZ_TBL_PHY_TBL_LIST_W&#x3b;&#xd;&#xa;insert into MD_BIZ_TBL_PHY_TBL_LIST_W&#x28;BIZ_TBL_ID,BIZ_TBL_NAME,FULL_NAME,BIZ_TBL_CNT&#x29;&#xd;&#xa;select a.BIZ_TBL_ID,a.BIZ_TBL_NAME,a.FULL_NAME,T.cnt from MD_BIZ_TBL_PHY_TBL_LIST a left join &#x28;&#xd;&#xa;SELECT FULL_NAME,count&#x28;1&#x29; as cnt FROM &#x60;MD_BIZ_TBL_PHY_TBL_LIST&#x60;&#xd;&#xa;GROUP BY FULL_NAME&#x29;T on a.FULL_NAME&#x3d;T.FULL_NAME&#x3b;&#xd;&#xa;&#xd;&#xa;delete from MD_BIZ_TBL_SAVE_COST where CRT_DATE &#x3d; curdate&#x28;&#x29;&#x3b;&#xd;&#xa;insert into MD_BIZ_TBL_SAVE_COST&#x28;BIZ_TBL_NAME,SUM_SIZE,SAVE_COST,CRT_DATE&#x29;&#xd;&#xa;select BIZ_TBL_NAME,sum&#x28;size_w&#x29; as size_sum,sum&#x28;cost_w&#x29; as cost_sum,curdate&#x28;&#x29; from&#x28;&#xd;&#xa;select w.BIZ_TBL_NAME,w.FULL_NAME,&#xd;&#xa;case when s.tbl_size is null then 0 else s.tbl_size&#x2f;w.BIZ_TBL_CNT end as size_w ,&#xd;&#xa;case when s.tbl_size is null then 0 else s.tbl_size&#x2f;w.BIZ_TBL_CNT&#x2f;1024&#x2f;1024&#x2f;1024 &#x2a; &#x24;&#x7b;SAVE_PRICE&#x7d; end as cost_w &#xd;&#xa;from MD_BIZ_TBL_PHY_TBL_LIST_W w &#xd;&#xa;left join MD_DB_TBL_SIZE s &#xd;&#xa;on w.full_name&#x3d;s.full_name&#xd;&#xa;&#x29; K GROUP BY BIZ_TBL_NAME&#x3b;</sql>
