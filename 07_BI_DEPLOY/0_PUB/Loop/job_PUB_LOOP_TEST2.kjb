ERE WORKFLOW_NAME &#x3d; &#x27;&#x24;V_JOB_NAME&#x27;&#xd;&#xa;        AND PARAMETER_NAME &#x3d; &#x27;&#x5c;&#x24;&#x5c;&#x24;START_EXTRACT_DATE&#x27;&#x3b;&#xd;&#xa;    &#xd;&#xa;     UPDATE &#x24;V_APD_DB.APD_FND_SCHEDULE_PARAMETER B&#xd;&#xa;        SET B.PARAMETER_VALUE &#x3d;&#xd;&#xa;            &#x28;SELECT U.UPDATE_VALUE FROM &#xd;&#xa;              &#x28;SELECT CASE&#xd;&#xa;                      WHEN A.FREQUENCY &#x3d; &#x27;MI&#x27; THEN&#xd;&#xa;                       DATE_FORMAT&#x28;DATE_ADD&#x28;A.PARAMETER_VALUE_INI, INTERVAL 2 MINUTE&#x29;,&#xd;&#xa;                                   REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;A.FORMAT_MASK,&#x27;YYYY&#x27;,&#x27;&#x25;Y&#x27;&#x29;,&#x27;MM&#x27;,&#x27;&#x25;m&#x27;&#x29;,&#x27;DD&#x27;,&#x27;&#x25;d&#x27;&#x29;,&#x27;MI&#x27;,&#x27;&#x25;i&#x27;&#x29;,&#x27;HH&#x27;,&#x27;&#x25;H&#x27;&#x29;&#x29;&#xd;&#xa;                      WHEN A.FREQUENCY &#x3d; &#x27;HH&#x27; THEN&#xd;&#xa;                       DATE_FORMAT&#x28;DATE_ADD&#x28;A.PARAMETER_VALUE_INI, INTERVAL 2 HOUR&#x29;,&#xd;&#xa;                                   REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;A.FORMAT_MASK,&#x27;YYYY&#x27;,&#x27;&#x25;Y&#x27;&#x29;,&#x27;MM&#x27;,&#x27;&#x25;m&#x27;&#x29;,&#x27;DD&#x27;,&#x27;&#x25;d&#x27;&#x29;,&#x27;MI&#x27;,&#x27;&#x25;i&#x27;&#x29;,&#x27;HH&#x27;,&#x27;&#x25;H&#x27;&#x29;&#x29;&#xd;&#xa;                      WHEN A.FREQUENCY &#x3d; &#x27;DD&#x27; THEN&#xd;&#xa;                       DATE_FORMAT&#x28;DATE_ADD&#x28;A.PARAMETER_VALUE_INI, INTERVAL 2 DAY&#x29;,&#xd;&#xa;                                   REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;A.FORMAT_MASK,&#x27;YYYY&#x27;,&#x27;&#x25;Y&#x27;&#x29;,&#x27;MM&#x27;,&#x27;&#x25;m&#x27;&#x29;,&#x27;DD&#x27;,&#x27;&#x25;d&#x27;&#x29;,&#x27;MI&#x27;,&#x27;&#x25;i&#x27;&#x29;,&#x27;HH&#x27;,&#x27;&#x25;H&#x27;&#x29;&#x29;&#xd;&#xa;                      WHEN A.FREQUENCY &#x3d; &#x27;MM&#x27; THEN&#xd;&#xa;                       DATE_FORMAT&#x28;DATE_ADD&#x28;CONCAT_WS&#x28;&#x27;-&#x27;,A.PARAMETER_VALUE_INI,&#x27;01&#x27;&#x29;,INTERVAL 2 MONTH&#x29;,&#xd;&#xa;                                   REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;A.FORMAT_MASK,&#x27;YYYY&#x27;,&#x27;&#x25;Y&#x27;&#x29;,&#x27;MM&#x27;,&#x27;&#x25;m&#x27;&#x29;,&#x27;DD&#x27;,&#x27;&#x25;d&#x27;&#x29;,&#x27;MI&#x27;,&#x27;&#x25;i&#x27;&#x29;,&#x27;HH&#x27;,&#x27;&#x25;H&#x27;&#x29;&#x29;&#xd;&#xa;                      WHEN A.FREQUENCY &#x3d; &#x27;YY&#x27; THEN&#xd;&#xa;                       DATE_FORMAT&#x28;DATE_ADD&#x28;CONCAT_WS&#x28;&#x27;-&#x27;,A.PARAMETER_VALUE_INI,&#x27;01-01&#x27;&#x29;,INTERVAL 2 YEAR&#x29;,&#xd;&#xa;                                   REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;A.FORMAT_MASK,&#x27;YYYY&#x27;,&#x27;&#x25;Y&#x27;&#x29;,&#x27;MM&#x27;,&#x27;&#x25;m&#x27;&#x29;,&#x27;DD&#x27;,&#x27;&#x25;d&#x27;&#x29;,&#x27;MI&#x27;,&#x27;&#x25;i&#x27;&#x29;,&#x27;HH&#x27;,&#x27;&#x25;H&#x27;&#x29;&#x29;&#xd;&#xa;                      WHEN A.FREQUENCY &#x3d; &#x27;LD&#x27; THEN&#xd;&#xa;                       DATE_FORMAT&#x28;LAST_DAY&#x28;DATE_ADD&#x28;CONCAT_WS&#x28;&#x27;-&#x27;,A.PARAMETER_VALUE_INI,&#x27;01&#x27;&#x29;,INTERVAL 2 MONTH&#x29;&#x29;,&#xd;&#xa;                                   REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;REPLACE&#x28;A.FORMAT_MASK,&#x27;YYYY&#x27;,&#x27;&#x25;Y&#x27;&#x29;,&#x27;MM&#x27;,&#x27;&#x25;m&#x27;&#x29;,&#x27;DD&#x27;,&#x27;&#x25;d&#x27;&#x29;,&#x27;MI&#x27;,&#x27;&#x25;i&#x27;&#x29;,&#x27;HH&#x27;,&#x27;&#x25;H&#x27;&#x29;&#x29;                                   &#xd;&#xa;                      ELSE&#xd;&#xa;                       DATE_FORMAT&#x28;A.PARAMETER_VALUE_INI, A.FORMAT_MASK&#x29;&#xd;&#xa;                      END AS UPDATE_VALUE&#xd;&#xa;                 FROM &#x24;V_APD_DB.APD_FND_SCHEDULE_PARAMETER A&#xd;&#xa;                WHERE A.WORKFL