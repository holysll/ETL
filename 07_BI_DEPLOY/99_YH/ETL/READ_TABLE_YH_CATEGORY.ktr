    <connection>yh_schedule</connection>
    <sql>SELECT CONCAT&#x28;t2.yhapi,t1.yhschedule&#x29; AS yhapi,userpwd,yhdelapi,lastday,nowday,lastmon,&#xd;&#xa;rel_id,&#xd;&#xa;dm_name,&#xd;&#xa;group_name,&#xd;&#xa;part,&#xa;job_type,&#xa;nowmon &#xd;&#xa;FROM&#xd;&#xa;&#x28;SELECT 1 AS num, &#xd;&#xa;	  rel_id,&#xd;&#xa;      dm_name,&#xd;&#xa;      group_name,&#xd;&#xa;      part,&#xa;job_type,&#xd;&#xa;      yh_schedule AS yhschedule&#xd;&#xa;FROM t_dm_yhschedule &#xd;&#xa;WHERE group_name &#x3d; &#x27;&#x24;&#x7b;JOB_GROUP_NAME&#x7d;&#x27;&#xd;&#xa;AND part &#x3d; &#x27;&#x24;&#x7b;RUN_PART&#x7d;&#x27;&#x29;t1&#xd;&#xa;LEFT JOIN&#xd;&#xa;&#x28;SELECT 1 AS num,&#xd;&#xa;       yh_api AS yhapi,&#xa;       yh_del_api AS yhdelapi,&#xa;       user_pwd AS userpwd,&#xa;       date_format&#x28;date_sub&#x28;curdate&#x28;&#x29;,interval 1 day&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29; as lastday,&#xa;       date_format&#x28;date_sub&#x28;curdate&#x28;&#x29;,interval 0 day&#x29;,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29; as nowday,&#xa;       date_format&#x28;date_sub&#x28;curdate&#x28;&#x29;,interval 1 day&#x29;,&#x27;&#x25;Y-&#x25;m&#x27;&#x29; as nowmon,&#xa;       date_format&#x28;date_sub&#x28;date_sub&#x28;curdate&#x28;&#x29;,interval 1 day&#x29;,interval 1 month&#x29;,&#x27;&#x25;Y-&#x25;m&#x27;&#x29; as lastmon&#xd;&#xa;FROM t_yh_api &#xd;&#xa;WHERE env &#x3d; &#x27;&#x24;&#x7b;YH_JOB_ENV&#x7d;&#x27;&#x29;t2&#xd;&#xa;ON&#x28;t1.num&#x3d;t2.num&#x29;&#xd;&#xa;&#xd;&#xa;-- &#x24;&#x7b;JOB_GROUP_NAME&#x7d;---&#x88ab;&#x4f9d;&#x8d56;&#x7684;ETL&#x7ec4;&#x540d;&#xd;&#xa;-- &#x24;&#x7b;YH_JOB_ENV&#x7d;-------&#x6c38;&#x6d2a;&#x8dd1;&#x6570;&#x73af;&#x5883;&#x7b;dev&#x28;&#x5f00;&#x53d1;&#x29;&#xff0c;uat&#x28;UAT&#x29;&#xff0c;cloud&#x28;&#x8d22;&#x52a1;&#x6b63;&#x5f0f;&#x29;&#xff0c;email&#x28;&#x90ae;&#x4ef6;&#x29;&#xff0c;GKEI_cloud&#x28;GKEI&#x29;&#x7d;&#xd;&#xa;-- &#x4e3a;&#x65b9;&#x4fbf;DQ&#x76d1;&#x63a7;&#x548c;&#x62a5;&#x8868;&#x67e5;&#x5386;&#x53f2;&#x8dd1;&#x6570;&#x72b6;&#x51b5;&#xff0c;&#x589e;&#x52a0;&#x4ee5;&#x4e0b;&#x5b57;&#x6bb5;--YZT--2016-11-16&#xd;&#xa;-- yhapi &#x7c7b;&#x4f3c;&#x4e8e;&#x2f;runJob&#x3f;job&#x3d;Midea_03_&#x8d22;&#x52a1;&#x4e3b;&#x9898;_&#x603b;&#x89c8;_&#x672c;&#x65e5;&#x6982;&#x51b5;_&#x672c;&#x65e5;&#x9500;&#x552e;&#x4e0e;&#x56de;&#x6b3e;&#x6982;&#x51b5;&#x4e91;&#x4efb;&#x52a1;&#xd;&#xa;-- rel_id &#x4f9d;&#x8d56;&#x5173;&#x7cfb;&#x7684;&#x552f;&#x4e00;ID&#xd;&#xa;-- dm_name &#x5bf9;&#x5e94;ETL&#x7a0b;&#x5e8f;&#xd;&#xa;-- group_name ETL&#x7a0b;&#x5e8f;&#x6240;&#x5728;&#x7684;&#x7ec4;&#x540d;&#xd;&#xa;-- part &#x88ab;&#x89e6;&#x53d1;&#x7684;&#x6c38;&#x6d2a;&#x4f5c;&#x4e1a;&#x6240;&#x5728;&#x7684;&#x5206;&#x7ec4;</sql>
