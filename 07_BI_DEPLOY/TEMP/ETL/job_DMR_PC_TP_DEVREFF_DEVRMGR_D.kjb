      <description/>
      <type>SQL</type>
      <sql>--hive conf settings&#xd;&#xa;--set hive.execution.engine&#x3d;spark&#x3b;&#xd;&#xa;set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--data insertion&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DMR_PC_TP_DEVREFF_DEVRMGR_D PARTITION &#x28;PART_SC,PART_DT&#x29;&#xd;&#xa;&#xd;&#xa;SELECT RGN_ID&#xd;&#xa;      ,BASE_ID&#xd;&#xa;      ,SUPPLIER_ID&#xd;&#xa;      ,TPMODE_ID&#xd;&#xa;      ,DATE_ADD&#x28;FACTORY_DAY, CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29; AS PLAN_DLVRY_DATE&#xd;&#xa;      ,ARRIVAL_DAY AS FACT_DLVRY_DATE&#xd;&#xa;  FROM &#x28;SELECT RGN_ID&#xd;&#xa;              ,BASE_ID&#xd;&#xa;              ,SUPPLIER_ID&#xd;&#xa;              ,TRANSPORTMODE_ID AS TPMODE_ID&#xd;&#xa;              ,CASE&#xd;&#xa;                   WHEN BRAND_ID &#x3d; &#x27;FT&#x27; THEN&#xd;&#xa;                    FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;FACTORY_DAY, &#x27;yyyy-MM-dd&#x27;&#x29;, &#x27;yyyy-MM-dd&#x27;&#x29;&#xd;&#xa;                   WHEN BRAND_ID &#x3d; &#x27;CA&#x27; THEN&#xd;&#xa;                    FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;UNLOAD_DAY, &#x27;yyyy-MM-dd&#x27;&#x29;, &#x27;yyyy-MM-dd&#x27;&#x29;&#xd;&#xa;                   ELSE&#xd;&#xa;                    FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;FACTORY_DAY, &#x27;yyyy-MM-dd&#x27;&#x29;, &#x27;yyyy-MM-dd&#x27;&#x29;&#xd;&#xa;               END AS FACTORY_DAY&#xd;&#xa;              ,ON_PASSAGE_DAYS&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;ARRIVAL_DAY, &#x27;yyyy-MM-dd&#x27;&#x29;, &#x27;yyyy-MM-dd&#x27;&#x29; AS ARRIVAL_DAY&#xd;&#xa;          FROM DMK.DMK_TP_DTD_PROG_TIME_D&#x29; T&#xd;&#xa;&#xd;&#xa;</sql>
