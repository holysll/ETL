    <entry>
      <name>TO_DMR</name>
      <description/>
      <type>SQL</type>
      <sql>set mapreduce.queue.name&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number&#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;--target table &#x3a; DMR_PC_TP_DEVREFF_DEVRMGR_D &#x4ea4;&#x4ed8;&#x4e0e;&#x6548;&#x7387;&#x7ba1;&#x7406;&#xff08;&#x65e5;&#xff09;-&#x4ea4;&#x4ed8;&#x7ba1;&#x7406;&#xd;&#xa;--need to display all of the regions, display top 10 suppliers and bases&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DMR.DMR_PC_TP_DEVREFF_DEVRMGR_D PARTITION&#x28;PART_SC, PART_DT&#x29;&#xd;&#xa;SELECT A.period_wid AS PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,COALESCE&#x28;B.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;      ,COALESCE&#x28;A.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,COALESCE&#x28;C.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME&#xd;&#xa;      ,COALESCE&#x28;A.BRAND_ID, &#x27;-1&#x27;&#x29; AS BRAND_ID&#xd;&#xa;      ,COALESCE&#x28;F.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BRAND_NAME&#xd;&#xa;      ,COALESCE&#x28;A.SUPPLIER_ID, &#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;      ,COALESCE&#x28;D.SERVPROV_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS SUPPLIER_NAME&#xd;&#xa;      ,COALESCE&#x28;A.tpmode_id, &#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;      ,COALESCE&#x28;E.TPMODE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS TPMODE_NAME&#xd;&#xa;      ,sum&#x28;A.dlvr_ontime_qty&#x29; AS FACT_DEVR_NUM&#xd;&#xa;      ,sum&#x28;A.dlvr_plan_qty&#x29; AS PLAN_DEVR_NUM&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;  FROM DMK.dmk_tp_shipqty_d A&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; B&#xd;&#xa;    ON A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; C&#xd;&#xa;    ON A.BASE_ID &#x3d; C.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT SERVPROV_ID&#xd;&#xa;                   ,SERVPROV_NAME&#xd;&#xa;               FROM DIM.DIM_SERVPROV&#xd;&#xa;              GROUP BY SERVPROV_ID&#xd;&#xa;                      ,SERVPROV_NAME&#x29; D&#xd;&#xa;    ON A.SUPPLIER_ID &#x3d; D.SERVPROV_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT TPMODE_ID&#xd;&#xa;                   ,TPMODE_NAME&#xd;&#xa;               FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;              GROUP BY TPMODE_ID&#xd;&#xa;                      ,TPMODE_NAME&#x29; E&#xd;&#xa;    ON A.TPMODE_ID &#x3d; E.TPMODE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BRAND_ID&#xd;&#xa;                   ,BRAND_NAME&#xd;&#xa;               FROM DIM.DIM_BRAND&#xd;&#xa;              GROUP BY BRAND_ID&#xd;&#xa;                      ,BRAND_NAME&#x29; F&#xd;&#xa;   ON  A.BRAND_ID &#x3d; F.BRAND_ID&#xd;&#xa; WHERE SUBSTR&#x28;A.period_wid,&#xd;&#xa;              1,&#xd;&#xa;              6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa;	AND SUBSTR&#x28;A.PERIOD_WID,1,8&#x29; &#x3c;&#x3d; FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;,&#x27;yyyyMMdd&#x27;&#x29;&#xd;&#xa;    AND A.tpmode_id NOT IN &#x28;&#x27;ZT&#x27;&#x29;&#xd;&#xa;    AND A.brand_id IN &#x28;&#x27;CA&#x27;,&#x27;FT&#x27;&#x29;&#xd;&#xa; GROUP BY A.period_wid&#xd;&#xa;         ,COALESCE&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;B.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;A.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;C.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;A.BRAND_ID, &#x27;-1&#x27;&#x29; &#xd;&#xa;         ,COALESCE&#x28;F.BRAND_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; &#xd;&#xa;         ,COALESCE&#x28;A.SUPPLIER_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;D.SERVPROV_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;A.tpmode_id, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;E.TPMODE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#x3b;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;SELECT SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;TO_DATE&#x28;FACTORY_DAY&#x29;,&#xd;&#xa;                                      CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                             &#x27;-&#x27;,&#xd;&#xa;                             &#x27;&#x27;&#x29;,&#xd;&#xa;              1,&#xd;&#xa;              8&#x29; AS PERIOD_WID&#xd;&#xa;      ,COALESCE&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29; AS RGN_ID&#xd;&#xa;      ,COALESCE&#x28;B.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS RGN_NAME&#xd;&#xa;      ,COALESCE&#x28;A.BASE_ID, &#x27;-1&#x27;&#x29; AS BASE_ID&#xd;&#xa;      ,COALESCE&#x28;C.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS BASE_NAME&#xd;&#xa;      ,COALESCE&#x28;A.SUPPLIER_ID, &#x27;-1&#x27;&#x29; AS SUPPLIER_ID&#xd;&#xa;      ,COALESCE&#x28;D.SERVPROV_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS SUPPLIER_NAME&#xd;&#xa;      ,COALESCE&#x28;A.TRANSPORTMODE_ID, &#x27;-1&#x27;&#x29; AS TPMODE_ID&#xd;&#xa;      ,COALESCE&#x28;E.TPMODE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29; AS TPMODE_NAME&#xd;&#xa;      ,COUNT&#x28;CASE&#xd;&#xa;               WHEN SUBSTR&#x28;REGEXP_REPLACE&#x28;DLVR_DAY, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 8&#x29; &#x3c;&#x3d;&#xd;&#xa;                    SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;TO_DATE&#x28;FACTORY_DAY&#x29;,&#xd;&#xa;                                                   CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                                          &#x27;-&#x27;,&#xd;&#xa;                                          &#x27;&#x27;&#x29;,&#xd;&#xa;                           1,&#xd;&#xa;                           8&#x29; THEN&#xd;&#xa;                VIN&#xd;&#xa;               ELSE&#xd;&#xa;                NULL&#xd;&#xa;             END&#x29; AS FACT_DEVR_NUM&#xd;&#xa;      ,COUNT&#x28;1&#x29; AS PLAN_DEVR_NUM&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS INSERT_TIME --&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS UPDATE_TIME --&#x66f4;&#x65b0;&#x65f6;&#x95f4;&#xd;&#xa;      ,&#x27;DMK&#x27; AS PART_SC&#xd;&#xa;      ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;  FROM DMK.DMK_TP_DTD_PROG_TIME_D A&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,RGN_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;R&#x27;&#xd;&#xa;              GROUP BY RGN_ID&#xd;&#xa;                      ,RGN_NAME&#x29; B&#xd;&#xa;    ON A.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT BASE_ID&#xd;&#xa;                   ,BASE_NAME&#xd;&#xa;               FROM DIM.DIM_ORG&#xd;&#xa;              WHERE ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;              GROUP BY BASE_ID&#xd;&#xa;                      ,BASE_NAME&#x29; C&#xd;&#xa;    ON A.BASE_ID &#x3d; C.BASE_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT SERVPROV_ID&#xd;&#xa;                   ,SERVPROV_NAME&#xd;&#xa;               FROM DIM.DIM_SERVPROV&#xd;&#xa;              GROUP BY SERVPROV_ID&#xd;&#xa;                      ,SERVPROV_NAME&#x29; D&#xd;&#xa;    ON A.SUPPLIER_ID &#x3d; D.SERVPROV_ID&#xd;&#xa;  LEFT JOIN &#x28;SELECT TPMODE_ID&#xd;&#xa;                   ,TPMODE_NAME&#xd;&#xa;               FROM DIM.DIM_TRANSPORT_MODE&#xd;&#xa;              GROUP BY TPMODE_ID&#xd;&#xa;                      ,TPMODE_NAME&#x29; E&#xd;&#xa;    ON A.TRANSPORTMODE_ID &#x3d; E.TPMODE_ID&#xd;&#xa; WHERE SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;TO_DATE&#x28;FACTORY_DAY&#x29;,&#xd;&#xa;                                      CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                             &#x27;-&#x27;,&#xd;&#xa;                             &#x27;&#x27;&#x29;,&#xd;&#xa;              1,&#xd;&#xa;              6&#x29; &#x3d; SUBSTR&#x28;REGEXP_REPLACE&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, &#x27;-&#x27;, &#x27;&#x27;&#x29;, 1, 6&#x29;&#xd;&#xa; GROUP BY SUBSTR&#x28;REGEXP_REPLACE&#x28;DATE_ADD&#x28;TO_DATE&#x28;FACTORY_DAY&#x29;,&#xd;&#xa;                                         CAST&#x28;ON_PASSAGE_DAYS AS INT&#x29;&#x29;,&#xd;&#xa;                                &#x27;-&#x27;,&#xd;&#xa;                                &#x27;&#x27;&#x29;,&#xd;&#xa;                 1,&#xd;&#xa;                 8&#x29;&#xd;&#xa;         ,COALESCE&#x28;A.RGN_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;B.RGN_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;A.BASE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;C.BASE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;A.SUPPLIER_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;D.SERVPROV_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;A.TRANSPORTMODE_ID, &#x27;-1&#x27;&#x29;&#xd;&#xa;         ,COALESCE&#x28;E.TPMODE_NAME, &#x27;&#x672a;&#x77e5;&#x27;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;SELECT T.PERIOD_WID--&#x671f;&#x95f4;ID&#xd;&#xa;      ,T.RGN_ID--&#x5927;&#x533a;ID&#xd;&#xa;      ,T.RGN_NAME--&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;      ,T.BASE_ID--&#x57fa;&#x5730;ID&#xd;&#xa;      ,T.BASE_NAME--&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;      ,B.SUPPLIER_ID--&#x627f;&#x8fd0;&#x5546;ID&#xd;&#xa;      ,SP.SERVPROV_NAME--&#x627f;&#x8fd0;&#x5546;&#x540d;&#x79f0;&#xd;&#xa;      ,B.TPMODE_ID--&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#xd;&#xa;      ,TP.TPMODE_NAME--&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;&#x540d;&#x79f0;&#xd;&#xa;      ,B.FACT_QTY AS FACT_DEVR_NUM  -- &#x4ea4;&#x4ed8;&#x6570; --OR	OTM&#x6570;&#x636e;	 &#xd;&#xa;      ,B.PLAN_QTY AS PLAN_DEVR_NUM--&#x8ba1;&#x5212;&#x4ea4;&#x4ed8;&#x6570; &#xd;&#xa;      ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29;   AS INSERT_TIME                     --&#x63d2;&#x5165;&#x65f6;&#x95f4; &#xd;&#xa;      ,&#x27;DMK&#x27;                                                    AS PART_SC&#xd;&#xa;      ,REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PART_DT&#xd;&#xa;  FROM &#x28;SELECT CAST&#x28;A.PERIOD_WID AS STRING&#x29; AS PERIOD_WID&#xd;&#xa;              ,B.RGN_ID&#xd;&#xa;              ,B.RGN_NAME&#xd;&#xa;              ,B.BASE_ID&#xd;&#xa;              ,B.BASE_NAME&#xd;&#xa;          FROM DIM.DIM_PERIOD_D A, DIM.DIM_ORG B&#xd;&#xa;         WHERE A.PERIOD_TYPE &#x3d; &#x27;D&#x27;&#xd;&#xa;           AND B.ORG_TYPE &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND B.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#xd;&#xa;           AND SUBSTRING&#x28;A.PERIOD_WID, 1, 6&#x29; &#x3d;&#xd;&#xa;               REGEXP_REPLACE&#x28;SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;, &#x27;-&#x27;, &#x27;&#x27;&#x29;&#x29; AS T&#xd;&#xa;  LEFT JOIN &#x28;SELECT RGN_ID&#xd;&#xa;                   ,BASE_ID&#xd;&#xa;                   ,SUPPLIER_ID&#xd;&#xa;                   ,TPMODE_ID&#xd;&#xa;                   ,PERIOD_WID&#xd;&#xa;                   ,SUM&#x28;FACT_QTY&#x29; AS FACT_QTY&#xd;&#xa;                   ,SUM&#x28;PLAN_QTY&#x29; AS PLAN_QTY&#xd;&#xa;               FROM &#x28;SELECT RGN_ID&#xd;&#xa;                           ,BASE_ID&#xd;&#xa;                           ,SUPPLIER_ID&#xd;&#xa;                           ,TPMODE_ID&#xd;&#xa;                           ,REGEXP_REPLACE&#x28;FACT_DLVRY_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                           ,COUNT&#x28;1&#x29; AS FACT_QTY&#xd;&#xa;                           ,0 AS PLAN_QTY&#xd;&#xa;                       FROM TEMP.TEMP_DMR_PC_TP_DEVREFF_DEVRMGR_D&#xd;&#xa;                      WHERE SUBSTRING&#x28;FACT_DLVRY_DATE, 1, 7&#x29; &#x3d;&#xd;&#xa;                            SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;&#xd;&#xa;                      GROUP BY RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,SUPPLIER_ID&#xd;&#xa;                              ,TPMODE_ID&#xd;&#xa;                              ,FACT_DLVRY_DATE&#xd;&#xa;                     UNION ALL&#xd;&#xa;                     SELECT RGN_ID&#xd;&#xa;                           ,BASE_ID&#xd;&#xa;                           ,SUPPLIER_ID&#xd;&#xa;                           ,TPMODE_ID&#xd;&#xa;                           ,REGEXP_REPLACE&#x28;PLAN_DLVRY_DATE, &#x27;-&#x27;, &#x27;&#x27;&#x29; AS PERIOD_WID&#xd;&#xa;                           ,0 AS FACT_QTY&#xd;&#xa;                           ,COUNT&#x28;1&#x29; AS PLAN_QTY&#xd;&#xa;                       FROM TEMP.TEMP_DMR_PC_TP_DEVREFF_DEVRMGR_D&#xd;&#xa;                      WHERE SUBSTRING&#x28;PLAN_DLVRY_DATE, 1, 7&#x29; &#x3d;&#xd;&#xa;                            SUBSTRING&#x28;&#x27;&#x24;&#x7b;START_DATE&#x7d;&#x27;, 1, 7&#x29;&#xd;&#xa;                      GROUP BY RGN_ID&#xd;&#xa;                              ,BASE_ID&#xd;&#xa;                              ,SUPPLIER_ID&#xd;&#xa;                              ,TPMODE_ID&#xd;&#xa;                              ,PLAN_DLVRY_DATE&#x29; A&#xd;&#xa;              GROUP BY RGN_ID, BASE_ID, SUPPLIER_ID, TPMODE_ID, PERIOD_WID&#x29; AS B&#xd;&#xa;    ON T.RGN_ID &#x3d; B.RGN_ID&#xd;&#xa;   AND T.BASE_ID &#x3d; B.BASE_ID&#xd;&#xa;   AND T.PERIOD_WID &#x3d; B.PERIOD_WID&#xd;&#xa;  LEFT JOIN DIM.DIM_TRANSPORT_MODE TP&#xd;&#xa;    ON B.TPMODE_ID &#x3d; TP.TPMODE_ID&#xd;&#xa;   AND TP.ENABLE_FLAG &#x3d; &#x27;G&#x27;&#xd;&#xa;  LEFT JOIN DIM.DIM_SERVPROV SP&#xd;&#xa;    ON B.SUPPLIER_ID &#x3d; SP.SERVPROV_CODE&#xd;&#xa;   AND SP.ENABLE_FLAG &#x3d; &#x27;T&#x27;&#x3b;&#xd;&#xa;&#x2a;&#x2f;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
