      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;CREATE_TEMP&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--current row count is 2.5 million&#xd;&#xa;--DROP TABLE IF EXISTS TEMP.TEMP_DMR_PC_TP_DEVREFF_DEVRMGR_D&#xd;&#xa;CREATE TABLE IF NOT EXISTS TEMP.TEMP_DMR_PC_TP_DEVREFF_DEVRMGR_D &#xd;&#xa;&#x28;&#xd;&#xa;    RGN_ID                         STRING                         COMMENT &#x27;&#x5927;&#x533a;ID&#x27;,&#xd;&#xa;    BASE_ID                        STRING                         COMMENT &#x27;&#x57fa;&#x5730;ID&#x27;,&#xd;&#xa;    SUPPLIER_ID                    STRING                         COMMENT &#x27;&#x4f9b;&#x5e94;&#x5546;ID&#x27;,&#xd;&#xa;    TPMODE_ID                      STRING                         COMMENT &#x27;&#x8fd0;&#x8f93;&#x65b9;&#x5f0f;ID&#x27;,&#xd;&#xa;    PLAN_DLVRY_DATE                STRING                         COMMENT &#x27;&#x8ba1;&#x5212;&#x4ea4;&#x4ed8;&#x65e5;&#x671f;&#x27;,&#xd;&#xa;    FACT_DLVRY_DATE                STRING                         COMMENT &#x27;&#x5b9e;&#x9645;&#x4ea4;&#x4ed8;&#x65e5;&#x671f;&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;COMMENT &#x27;temporary table for DMR_PC_TP_DEVREFF_DEVRMGR_D&#x27;&#xd;&#xa;ROW FORMAT DELIMITED FIELDS TERMINATED BY &#x27;&#x5c;036&#x27; STORED AS PARQUET&#x3b;&#xd;&#xa;</sql>
