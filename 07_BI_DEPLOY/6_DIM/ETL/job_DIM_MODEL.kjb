    <entry>
      <name>TO_DIM</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DIM&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE DIM.DIM_MODEL&#xd;&#xa;--------------------------------------&#xd;&#xa;SELECT T.ITEM_XID AS MODEL_ID&#xd;&#xa;      ,T.ITEM_NAME AS MODEL_NAME&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27; THEN&#xd;&#xa;          &#x27;FT&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x8f7b;&#x578b;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;CA&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x89c2;&#x81f4;&#x6c7d;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;GZ&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d; &#x27;&#x8f7f;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;CA&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d; &#x27;&#x5fae;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;CA&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          NULL&#xd;&#xa;       END AS CUST_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27; THEN&#xd;&#xa;          &#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x8f7b;&#x578b;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;&#x957f;&#x5b89;&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x89c2;&#x81f4;&#x6c7d;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;&#x89c2;&#x81f4;&#x6c7d;&#x8f66;&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d; &#x27;&#x8f7f;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;&#x957f;&#x5b89;&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d; &#x27;&#x5fae;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;&#x957f;&#x5b89;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          NULL&#xd;&#xa;       END AS CUST_NAME&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27; THEN&#xd;&#xa;          &#x27;FT&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x8f7b;&#x578b;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;CA&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x89c2;&#x81f4;&#x6c7d;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;GZ&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d; &#x27;&#x8f7f;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;CA&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d; &#x27;&#x5fae;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;CA&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          NULL&#xd;&#xa;       END AS BRAND_ID&#xd;&#xa;      ,CASE&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27; THEN&#xd;&#xa;          &#x27;&#x957f;&#x5b89;&#x798f;&#x7279;&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x8f7b;&#x578b;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;&#x957f;&#x5b89;&#x6c7d;&#x8f66;&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d;&#xd;&#xa;              &#x27;&#x89c2;&#x81f4;&#x6c7d;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;&#x89c2;&#x81f4;&#x6c7d;&#x8f66;&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d; &#x27;&#x8f7f;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;&#x957f;&#x5b89;&#x6c7d;&#x8f66;&#x27;&#xd;&#xa;         WHEN REGEXP_REPLACE&#x28;USER_DEFINED_COMMODITY_GID, &#x27;CMAL&#x2f;ZC.&#x27;, &#x27;&#x27;&#x29; &#x3d; &#x27;&#x5fae;&#x8f66;&#x27; THEN&#xd;&#xa;          &#x27;&#x957f;&#x5b89;&#x6c7d;&#x8f66;&#x27;&#xd;&#xa;         ELSE&#xd;&#xa;          NULL&#xd;&#xa;       END AS BRAND_NAME&#xd;&#xa;      ,&#x27;OTM&#x27; AS SOURCE_CODE -- &#x6e90;&#x7cfb;&#x7edf;&#xd;&#xa;       ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_INSERT_DT -- &#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8868;&#x63d2;&#x5165;&#x65f6;&#x95f4;&#xd;&#xa;       ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_UPDATE_DT -- &#x6570;&#x636e;&#x4ed3;&#x5