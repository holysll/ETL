      <note>&#x3e;&#x529f;&#x80fd;&#x8bf4;&#x660e;&#xff1a;&#x5730;&#x533a;&#x7ef4;&#xd;&#xa;&#x3e;&#x66f4;&#x65b0;&#x65b9;&#x5f0f;&#xff1a;APPEND&#xd;&#xa;&#x3e;&#x521b; &#x5efa; &#x4eba; &#xff1a;&#x6768;&#x632f;&#x9e4f; - &#x7f8e;&#x4e91;&#xd;&#xa;&#x3e;&#x521b;&#x5efa;&#x65e5;&#x671f;&#xff1a;2018&#x2f;5&#x2f;25&#xd;&#xa;&#x3e;&#x8fd0;&#x884c;&#x5468;&#x671f;&#xff1a;&#x8be5;&#x4f5c;&#x4e1a;&#x8fd0;&#x884c;&#x7684;&#x5468;&#x671f;,&#x5982;&#x65e5;&#x3001;&#x6708;&#x7b49;&#xd;&#xa;&#x3e;&#x4f8b;&#x3000;&#x3000;&#x7a0b;&#xff1a;&#x8c03;&#x7528;&#x8be5;&#x4f5c;&#x4e1a;&#x7684;&#x547d;&#x4ee4;&#x884c;&#x8bed;&#x53e5;,&#x542b;&#x8f93;&#x5165;&#x53c2;&#x6570;&#xd;&#xa;&#x3e;&#x5907;&#x3000;&#x3000;&#x6ce8;&#xff1a;&#xd;&#xa;SELECT &#x27;&#x5168;&#x56fd;00001&#x27; AS AREA_ID&#xd;&#xa;      ,&#x27;&#x5168;&#x56fd;&#x27; AS AREA_TYPE&#xd;&#xa;      ,&#x27;&#x4e2d;&#x56fd;&#x27; AS CTRY&#xd;&#xa;      ,&#x27;-1&#x27; AS PROVINCE&#xd;&#xa;      ,&#x27;-1&#x27; AS CITY&#xd;&#xa;      ,&#x27;-1&#x27; AS POSTAL_CODE&#xd;&#xa;      ,&#x27;-1&#x27; AS LOCATION_NAME&#xd;&#xa;  FROM DUAL&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT AREA_TYPE &#x7c;&#x7c;&#xd;&#xa;       LPAD&#x28;ROW_NUMBER&#x28;&#x29; OVER&#x28;ORDER BY AREA_TYPE, LOCATION_NAME&#x29;, 5, &#x27;0&#x27;&#x29; AS AREA_ID&#xd;&#xa;      ,AREA_TYPE&#xd;&#xa;      ,CTRY&#xd;&#xa;      ,PROVINCE&#xd;&#xa;      ,CITY&#xd;&#xa;      ,POSTAL_CODE&#xd;&#xa;      ,LOCATION_NAME&#xd;&#xa;  FROM &#x28;SELECT &#x27;&#x7701;&#x4efd;&#x27; AS AREA_TYPE&#xd;&#xa;              ,&#x27;&#x4e2d;&#x56fd;&#x27; AS CTRY&#xd;&#xa;              ,PROVINCE&#xd;&#xa;              ,&#x27;-1&#x27; AS CITY&#xd;&#xa;              ,&#x27;-1&#x27; AS POSTAL_CODE&#xd;&#xa;              ,&#x27;-1&#x27; AS LOCATION_NAME&#xd;&#xa;          FROM GLOGOWNER.LOCATION T&#xd;&#xa;         WHERE ERPC_GID &#x3d; &#x27;CMAL&#x2f;ZC.&#x57ce;&#x5e02;&#x27;&#xd;&#xa;         GROUP BY PROVINCE&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT AREA_TYPE &#x7c;&#x7c;&#xd;&#xa;       LPAD&#x28;ROW_NUMBER&#x28;&#x29; OVER&#x28;ORDER BY AREA_TYPE, LOCATION_NAME&#x29;, 5, &#x27;0&#x27;&#x29; AS AREA_ID&#xd;&#xa;      ,AREA_TYPE&#xd;&#xa;      ,CTRY&#xd;&#xa;      ,PROVINCE&#xd;&#xa;      ,CITY&#xd;&#xa;      ,POSTAL_CODE&#xd;&#xa;      ,LOCATION_NAME&#xd;&#xa;  FROM &#x28;SELECT &#x27;&#x57ce;&#x5e02;&#x27; AS AREA_TYPE&#xd;&#xa;              ,&#x27;&#x4e2d;&#x56fd;&#x27; AS CTRY&#xd;&#xa;              ,PROVINCE&#xd;&#xa;              ,NVL&#x28;CITY, PROVINCE&#x29; AS CITY&#xd;&#xa;              ,&#x27;-1&#x27; AS POSTAL_CODE&#xd;&#xa;              ,&#x27;-1&#x27; AS LOCATION_NAME&#xd;&#xa;          FROM GLOGOWNER.LOCATION T&#xd;&#xa;         WHERE ERPC_GID &#x3d; &#x27;CMAL&#x2f;ZC.&#x57ce;&#x5e02;&#x27;&#xd;&#xa;         GROUP BY PROVINCE, NVL&#x28;CITY, PROVINCE&#x29;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT AREA_TYPE &#x7c;&#x7c;&#xd;&#xa;       LPAD&#x28;ROW_NUMBER&#x28;&#x29; OVER&#x28;ORDER BY AREA_TYPE, LOCATION_NAME&#x29;, 5, &#x27;0&#x27;&#x29; AS AREA_ID&#xd;&#xa;      ,AREA_TYPE&#xd;&#xa;      ,CTRY&#xd;&#xa;      ,PROVINCE&#xd;&#xa;      ,CITY&#xd;&#xa;      ,POSTAL_CODE&#xd;&#xa;      ,LOCATION_NAME&#xd;&#xa;  FROM &#x28;SELECT &#x27;&#x533a;&#x53bf;&#x27; AS AREA_TYPE&#xd;&#xa;              ,&#x27;&#x4e2d;&#x56fd;&#x27; AS CTRY&#xd;&#xa;              ,PROVINCE&#xd;&#xa;              ,NVL&#x28;CITY, PROVINCE&#x29; AS CITY&#xd;&#xa;              ,NVL&#x28;POSTAL_CODE, &#x27;-1&#x27;&#x29; AS POSTAL_CODE&#xd;&#xa;              ,NVL&#x28;LOCATION_NAME, &#x27;&#x4e2d;&#x56fd;&#x27; &#x7c;&#x7c; PROVINCE&#x29; AS LOCATION_NAME&#xd;&#xa;          FROM GLOGOWNER.LOCATION T&#xd;&#xa;         WHERE ERPC_GID &#x3d; &#x27;CMAL&#x2f;ZC.&#x57ce;&#x5e02;&#x27;&#xd;&#xa;         GROUP BY PROVINCE&#xd;&#xa;                 ,NVL&#x28;CITY, PROVINCE&#x29;&#xd;&#xa;                 ,NVL&#x28;POSTAL_CODE, &#x27;-1&#x27;&#x29;&#xd;&#xa;                 ,NVL&#x28;LOCATION_NAME, &#x27;&#x4e2d;&#x56fd;&#x27; &#x7c;&#x7c; PROVINCE&#x29;&#x29;&#x3b;&#xd;&#xa;&#xd;&#x