    <entry>
      <name>TO_SRC</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;DIM_AREA_TO_SRC&#x3b;&#xd;&#xa;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DIM_AREA_SRC&#xd;&#xa;SELECT &#x27;&#x5168;&#x56fd;00001&#x27; AS AREA_ID&#xd;&#xa;      ,&#x27;&#x5168;&#x56fd;&#x27; AS AREA_TYPE&#xd;&#xa;      ,&#x27;&#x4e2d;&#x56fd;&#x27; AS CTRY&#xd;&#xa;      ,&#x27;-1&#x27; AS PROVINCE&#xd;&#xa;      ,&#x27;-1&#x27; AS CITY&#xd;&#xa;      ,&#x27;-1&#x27; AS POSTAL_CODE&#xd;&#xa;      ,&#x27;-1&#x27; AS LOCATION_NAME&#xd;&#xa;  FROM DUAL&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT AREA_TYPE &#x7c;&#x7c;&#xd;&#xa;       LPAD&#x28;ROW_NUMBER&#x28;&#x29; OVER&#x28;ORDER BY AREA_TYPE, LOCATION_NAME&#x29;, 5, &#x27;0&#x27;&#x29; AS AREA_ID&#xd;&#xa;      ,AREA_TYPE&#xd;&#xa;      ,CTRY&#xd;&#xa;      ,PROVINCE&#xd;&#xa;      ,CITY&#xd;&#xa;      ,POSTAL_CODE&#xd;&#xa;      ,LOCATION_NAME&#xd;&#xa;  FROM &#x28;SELECT &#x27;&#x7701;&#x4efd;&#x27; AS AREA_TYPE&#xd;&#xa;              ,&#x27;&#x4e2d;&#x56fd;&#x27; AS CTRY&#xd;&#xa;              ,PROVINCE&#xd;&#xa;              ,&#x27;-1&#x27; AS CITY&#xd;&#xa;              ,&#x27;-1&#x27; AS POSTAL_CODE&#xd;&#xa;              ,&#x27;-1&#x27; AS LOCATION_NAME&#xd;&#xa;          FROM GLOGOWNER.LOCATION T&#xd;&#xa;         WHERE ERPC_GID &#x3d; &#x27;CMAL&#x2f;ZC.&#x57ce;&#x5e02;&#x27;&#xd;&#xa;         GROUP BY PROVINCE&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT AREA_TYPE &#x7c;&#x7c;&#xd;&#xa;       LPAD&#x28;ROW_NUMBER&#x28;&#x29; OVER&#x28;ORDER BY AREA_TYPE, LOCATION_NAME&#x29;, 5, &#x27;0&#x27;&#x29; AS AREA_ID&#xd;&#xa;      ,AREA_TYPE&#xd;&#xa;      ,CTRY&#xd;&#xa;      ,PROVINCE&#xd;&#xa;      ,CITY&#xd;&#xa;      ,POSTAL_CODE&#xd;&#xa;      ,LOCATION_NAME&#xd;&#xa;  FROM &#x28;SELECT &#x27;&#x57ce;&#x5e02;&#x27; AS AREA_TYPE&#xd;&#xa;              ,&#x27;&#x4e2d;&#x56fd;&#x27; AS CTRY&#xd;&#xa;              ,PROVINCE&#xd;&#xa;              ,NVL&#x28;CITY, PROVINCE&#x29; AS CITY&#xd;&#xa;              ,&#x27;-1&#x27; AS POSTAL_CODE&#xd;&#xa;              ,&#x27;-1&#x27; AS LOCATION_NAME&#xd;&#xa;          FROM GLOGOWNER.LOCATION T&#xd;&#xa;         WHERE ERPC_GID &#x3d; &#x27;CMAL&#x2f;ZC.&#x57ce;&#x5e02;&#x27;&#xd;&#xa;         GROUP BY PROVINCE, NVL&#x28;CITY, PROVINCE&#x29;&#x29;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT AREA_TYPE &#x7c;&#x7c;&#xd;&#xa;       LPAD&#x28;ROW_NUMBER&#x28;&#x29; OVER&#x28;ORDER BY AREA_TYPE, LOCATION_NAME&#x29;, 5, &#x27;0&#x27;&#x29; AS AREA_ID&#xd;&#xa;      ,AREA_TYPE&#xd;&#xa;      ,CTRY&#xd;&#xa;      ,PROVINCE&#xd;&#xa;      ,CITY&#xd;&#xa;      ,POSTAL_CODE&#xd;&#xa;      ,LOCATION_NAME&#xd;&#xa;  FROM &#x28;SELECT &#x27;&#x533a;&#x53bf;&#x27; AS AREA_TYPE&#xd;&#xa;              ,&#x27;&#x4e2d;&#x56fd;&#x27; AS CTRY&#xd;&#xa;              ,PROVINCE&#xd;&#xa;              ,NVL&#x28;CITY, PROVINCE&#x29; AS CITY&#xd;&#xa;              ,NVL&#x28;POSTAL_CODE, &#x27;-1&#x27;&#x29; AS POSTAL_CODE&#xd;&#xa;              ,NVL&#x28;LOCATION_NAME, &#x27;&#x4e2d;&#x56fd;&#x27; &#x7c;&#x7c; PROVINCE&#x29; AS LOCATION_NAME&#xd;&#xa;          FROM GLOGOWNER.LOCATION T&#xd;&#xa;         WHERE ERPC_GID &#x3d; &#x27;CMAL&#x2f;ZC.&#x57ce;&#x5e02;&#x27;&#xd;&#xa;         GROUP BY PROVINCE&#xd;&#xa;                 ,NVL&#x28;CITY, PROVINCE&#x29;&#xd;&#xa;                 ,NVL&#x28;POSTAL_CODE, &#x27;-1&#x27;&#x29;&#xd;&#xa;                 ,NVL&#x28;LOCATION_NAME, &#x27;&#x4e2d;&#x56fd;&#x27; &#x7c;&#x7c; PROVINCE&#x29;&#x29;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
