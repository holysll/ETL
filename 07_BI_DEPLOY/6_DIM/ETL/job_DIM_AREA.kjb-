    <entry>
      <name>TO_DIM_I</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DIM_I&#x3b;&#xd;&#xa;&#xd;&#xa;--set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;&#xd;&#xa;create temporary function UniqueID as &#x22;com.midea.kepler.gyro.hive.udf.UniqueID&#x22; USING JAR &#x27;hdfs&#x3a;&#x2f;&#x2f;&#x2f;user&#x2f;hive&#x2f;udf&#x2f;hive-udf-genera_wid.jar&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x65b0;&#x589e;&#x7684;&#xd;&#xa;INSERT OVERWRITE TABLE TEMP.TEMP_DIM_AREA_ALL_I&#xd;&#xa;SELECT &#xd;&#xa;UNIQUEID&#x28;&#x29;  AS PRODUCT_WID        --&#x4ea7;&#x54c1;&#x7ef4;ID&#xd;&#xa;, N.AREA_ID         --&#x533a;&#x57df;ID&#xd;&#xa;, N.AREA_TYPE       --&#x533a;&#x57df;&#x7c7b;&#x578b;&#xd;&#xa;, N.CTRY_ID         --&#x56fd;&#x5bb6;ID&#xd;&#xa;, N.CTRY_NAME       --&#x56fd;&#x5bb6;&#x540d;&#x79f0;&#xd;&#xa;, N.PRVN_ID         --&#x7701;&#x4efd;ID&#xd;&#xa;, N.PRVN_NAME       --&#x7701;&#x4efd;&#x540d;&#x79f0;&#xd;&#xa;, N.CITY_ID         --&#x57ce;&#x5e02;ID&#xd;&#xa;, N.CITY_NAME       --&#x57ce;&#x5e02;&#x540d;&#x79f0;&#xd;&#xa;, N.POSTAL_ID       --&#x533a;&#x53bf;ID&#xd;&#xa;, N.POSTAL_NAME     --&#x533a;&#x53bf;&#x540d;&#x79f0;&#xd;&#xa;, N.LOCATION        --&#x8be6;&#x7ec6;&#x5730;&#x5740;&#xd;&#xa;, N.PRIOR_ID        --&#x4e0a;&#x7ea7;&#x7ec4;&#x7ec7;ID&#xd;&#xa;, N.SOURCE_CODE     --&#x6765;&#x6e90;&#xd;&#xa;, FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_INSERT_DT    --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x63d2;&#x5165;&#x65f6;&#x95f4; &#xd;&#xa;, FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS W_UPDATE_DT    --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x66f4;&#x65b0;&#x65f6;&#x95f4; &#xd;&#xa;, &#x27;1990-01-01&#x27;                    AS START_DATE_ACTIVE --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x751f;&#x6548;&#x65e5;&#x671f;&#xd;&#xa;, &#x27;2099-12-31&#x27;                    AS END_DATE_ACTIVE   --&#x6570;&#x636e;&#x4ed3;&#x5e93