    <entry>
      <name>TO_DIM</name>
      <description/>
      <type>SQL</type>
      <sql>set mapred.job.name&#x3d;&#x24;&#x7b;Internal.Job.Name&#x7d;&#x3a;TO_DIM&#x3b;&#xd;&#xa;set mapreduce.job.queuename&#x3d;&#x24;&#x7b;CMALDW_DEFAULT_QUEUE&#x7d;&#x3b;&#xd;&#xa;set hive.auto.convert.join&#x3d;false&#x3b;&#xd;&#xa;set hive.map.aggr&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel&#x3d;true&#x3b;&#xd;&#xa;set hive.exec.parallel.thread.number &#x3d;16&#x3b;&#xd;&#xa;set hive.exec.dynamic.partition.mode&#x3d;nonstrict&#x3b;&#xd;&#xa;&#xd;&#xa;--&#x7b2c;&#x4e00;&#x6b21;&#x5168;&#x91cf;&#x5e95;&#x5c42;&#x4ed3;&#x5e93;&#x63d2;&#x5165;&#xd;&#xa;insert  OVERWRITE TABLE DIM.DIM_PARKING&#xd;&#xa;SELECT DISTINCT&#xd;&#xa;       B.PARKING_ID&#xd;&#xa;      ,B.PARKING_NAME&#xd;&#xa;      ,B.PARKING_TYPE_ID&#xd;&#xa;      ,B.PARKING_TYPE_NAME&#xd;&#xa;      ,B.PARKING_BRIEF_ID&#xd;&#xa;      ,B.PARKING_PARENT_ID&#xd;&#xa;      ,B.PARKING_LEVEL&#xd;&#xa;      ,B.LASTLEVEL&#xd;&#xa;      ,NVL&#x28;B.STR_ID, R.STR_ID&#x29; AS STR_ID&#xd;&#xa;      ,NVL&#x28;B.STR_NAME, R.STR_NAME&#x29; AS STR_NAME&#xd;&#xa;      ,NVL&#x28;B.BASE_ID, R.BASE_ID&#x29; AS BASE_ID&#xd;&#xa;      ,NVL&#x28;B.BASE_NAME, R.BASE_NAME&#x29; AS BASE_NAME&#xd;&#xa;      ,NVL&#x28;B.RGN_ID, R.RGN_ID&#x29; AS RGN_ID&#xd;&#xa;      ,NVL&#x28;B.RGN_NAME, R.RGN_NAME&#x29; AS RGN_NAME&#xd;&#xa;      ,B.WHID&#xd;&#xa;      ,B.OGID&#xd;&#xa;      ,B.STATUS --&#x72b6;&#x6001;&#xd;&#xa;      ,B.LENGTH --&#x957f;&#xd;&#xa;      ,B.WIDTH --&#x5bbd;&#xd;&#xa;      ,B.HEIGHT --&#x9ad8;&#xd;&#xa;      ,B.VOLUME --&#x5bb9;&#x79ef;&#xd;&#xa;      ,B.MAXWEIGHT --&#x627f;&#x91cd;&#xd;&#xa;      ,B.MIXED --&#x662f;&#x5426;&#x6df7;&#x653e;&#xd;&#xa;      ,B.QTY --&#x5e93;&#x4f4d;&#x6807;&#x51c6;&#x505c;&#x653e;&#x6570;&#x91cf;&#xd;&#xa;      ,B.MAXNUM -- &#x6700;&#x5927;&#x5e93;&#x5b58;&#x91cf;&#xd;&#xa;      ,B.REMAK --&#x5907;&#x6ce8;&#xd;&#xa;      ,B.AUDITDATE --&#x5ba1;&#x6838;&#x65e5;&#x671f;&#xd;&#xa;      ,B.INPUTDATE --&#x5f55;&#x5165;&#x65e5;&#x671f;&#xd;&#xa;      ,B.LASTMODIFYDATE --&#x6700;&#x540e;&#x4e00;&#x6b21;&#x4fee;&#x6539;&#x65e5;&#x671f;&#xd;&#xa;      ,B.SOURCE_CODE --&#x6765;&#x6e90;&#xd;&#xa;      ,B.WD_INSERT_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x63d2;&#x5165;&#x65f6;&#x95f4; &#xd;&#xa;      ,B.WD_UPDATE_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x66f4;&#x65b0;&#x65f6;&#x95f4; &#xd;&#xa;      ,B.START_DATE_ACTIVE --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x751f;&#x6548;&#x65e5;&#x671f;&#xd;&#xa;      ,B.END_DATE_ACTIVE --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x5931;&#x6548;&#x65e5;&#x671f;&#xd;&#xa;      ,B.ENABLE_FLAG --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x662f;&#x5426;&#x6700;&#x65b0;&#x8bb0;&#x5f55; T&#x6700;&#x65b0;&#xff0c;&#x5176;&#x4ed6;&#x5386;&#x53f2;&#xd;&#xa;  FROM &#x28;SELECT A.LOCATIONID AS PARKING_ID --&#x5e93;&#x533a;&#x5e93;&#x4f4d;ID&#xd;&#xa;              ,A.LOCATIONNAME AS PARKING_NAME --&#x5e93;&#x533a;&#x5e93;&#x4f4d;&#x540d;&#x79f0;&#xd;&#xa;              ,A.LOCATIONTYPEID AS PARKING_TYPE_ID --&#x5e93;&#x533a;&#x5e93;&#x4f4d;&#x7c7b;&#x578b;&#xd;&#xa;              ,CASE&#xd;&#xa;                 WHEN A.LOCATIONTYPEID &#x3d; &#x27;01&#x27; THEN&#xd;&#xa;                  &#x27;B&#x26;H-B&#x26;H&#x533a;&#x57df;&#x27;&#xd;&#xa;                 WHEN A.LOCATIONTYPEID &#x3d; &#x27;11&#x27; THEN&#xd;&#xa;                  &#x27;RFD-&#x8f66;&#x8f86;&#x5b58;&#x653e;&#x533a;&#x57df;&#x27;&#xd;&#xa;                 WHEN A.LOCATIONTYPEID &#x3d; &#x27;12&#x27; THEN&#xd;&#xa;                  &#x27;&#x6837;&#x8f66;&#x533a;-&#x6837;&#x8f66;&#x5b58;&#x653e;&#x533a;&#x57df;&#x27;&#xd;&#xa;                 WHEN A.LOCATIONTYPEID &#x3d; &#x27;21&#x27; THEN&#xd;&#xa;                  &#x27;&#x5f85;&#x88c5;&#x8f66;&#x9053;-&#x8f66;&#x8f86;&#x5f85;&#x53d1;&#x533a;&#x57df;&#x27;&#xd;&#xa;                 WHEN A.LOCATIONTYPEID &#x3d; &#x27;31&#x27; THEN&#xd;&#xa;                  &#x27;&#x88c5;&#x8f66;&#x8f66;&#x9053;-&#x62d6;&#x8f66;&#x8f66;&#x9053;&#x27;&#xd;&#xa;                 WHEN A.LOCATIONTYPEID &#x3d; &#x27;41&#x27; THEN&#xd;&#xa;                  &#x27;&#x6536;&#x8f66;&#x79fb;&#x5e93;&#x533a;-&#x8f66;&#x8f86;&#x6536;&#x8f66;&#x540e;&#x76f4;&#x63a5;&#x8fdb;&#x5165;&#x7684;&#x5f85;&#x79fb;&#x5e93;&#x533a;&#x57df;&#x27;&#xd;&#xa;                 WHEN A.LOCATIONTYPEID &#x3d; &#x27;42&#x27; THEN&#xd;&#xa;                  &#x27;&#x53d1;&#x8fd0;&#x79fb;&#x5e93;&#x533a;-&#x8f66;&#x8f86;&#x53d1;&#x8fd0;&#x51fa;&#x5e93;&#x540e;&#x8fdb;&#x5165;&#x7684;&#x5f85;&#x79fb;&#x5e93;&#x533a;&#x57df;&#x27;&#xd;&#xa;                 WHEN A.LOCATIONTYPEID &#x3d; &#x27;43&#x27; THEN&#xd;&#xa;                  &#x27;&#x501f;&#x8f66;&#x79fb;&#x5e93;&#x533a;-&#x8f66;&#x8f86;&#x79fb;&#x5e93;&#x8c03;&#x6574;&#x65f6;&#x5165;&#x7684;&#x5f85;&#x79fb;&#x5e93;&#x533a;&#x57df;&#x27;&#xd;&#xa;                 WHEN A.LOCATIONTYPEID &#x3d; &#x27;44&#x27; THEN&#xd;&#xa;                  &#x27;&#x6df7;&#x5408;&#x79fb;&#x5e93;&#x533a;-&#x4e0d;&#x533a;&#x5206;&#x79fb;&#x5e93;&#x7c7b;&#x578b;&#xff0c;&#x53ef;&#x4ee5;&#x505c;&#x653e;&#x6240;&#x6709;&#x5f85;&#x79fb;&#x5e93;&#x8f66;&#x8f86;&#x7684;&#x533a;&#x57df;&#x27;&#xd;&#xa;                 WHEN A.LOCATIONTYPEID &#x3d; &#x27;51&#x27; THEN&#xd;&#xa;                  &#x27;&#x501f;&#x51fa;&#x533a;-&#x865a;&#x62df;&#x533a;&#x57df;&#xff0c;&#x8bb0;&#x5f55;&#x501f;&#x51fa;&#x8f66;&#x8f86;&#x27;&#xd;&#xa;                 ELSE&#xd;&#xa;                  NULL&#xd;&#xa;               END AS PARKING_TYPE_NAME --&#x5e93;&#x533a;&#x5e93;&#x4f4d;&#x7c7b;&#x578b;&#x540d;&#x79f0;&#xd;&#xa;              ,A.LOCATIONBRIEFID AS PARKING_BRIEF_ID --&#x672c;&#x7ea7;&#x5e93;&#x533a;&#x5e93;&#x4f4d;&#x540d;&#x79f0;&#xd;&#xa;              ,A.PARENTLOCATIONID AS PARKING_PARENT_ID --&#x4e0a;&#x7ea7;&#x5e93;&#x533a;&#x5e93;&#x4f4d;&#x4ee3;&#x7801;&#xd;&#xa;              ,A.LOCATIONLEVEL AS PARKING_LEVEL --&#x5e93;&#x533a;&#x5e93;&#x4f4d;&#x7ea7;&#x6b21;&#xd;&#xa;              ,A.LASTLEVEL LASTLEVEL --&#x672b;&#x7ea7;&#x5e93;&#x533a;&#x5e93;&#x4f4d;&#xd;&#xa;              ,NVL&#x28;S.STR_ID, &#x27;CMAL&#x2f;ZC.-1&#x27;&#x29; AS STR_ID --&#x4ed3;&#x5e93;ID&#xd;&#xa;              ,S.STR_NAME --&#x4ed3;&#x5e93;&#x540d;&#x79f0;&#xd;&#xa;              ,S.BASE_ID --&#x57fa;&#x5730;ID&#xd;&#xa;              ,S.BASE_NAME --&#x57fa;&#x5730;&#x540d;&#x79f0;&#xd;&#xa;              ,S.RGN_ID --&#x5927;&#x533a;ID&#xd;&#xa;              ,S.RGN_NAME --&#x5927;&#x533a;&#x540d;&#x79f0;&#xd;&#xa;              ,A.WHID --&#x4ed3;&#x5e93;&#x4ee3;&#x7801;&#xd;&#xa;              ,A.OGID --&#x7ec4;&#x7ec7;&#x4ee3;&#x7801;&#xd;&#xa;              ,A.STATUS --&#x72b6;&#x6001;&#xd;&#xa;              ,A.LENGTH --&#x957f;&#xd;&#xa;              ,A.WIDTH --&#x5bbd;&#xd;&#xa;              ,A.HEIGHT --&#x9ad8;&#xd;&#xa;              ,A.VOLUME --&#x5bb9;&#x79ef;&#xd;&#xa;              ,A.MAXWEIGHT --&#x627f;&#x91cd;&#xd;&#xa;              ,A.MIXED --&#x662f;&#x5426;&#x6df7;&#x653e;&#xd;&#xa;              ,A.QTY --&#x5e93;&#x4f4d;&#x6807;&#x51c6;&#x505c;&#x653e;&#x6570;&#x91cf;&#xd;&#xa;              ,ALV.MAXNUM -- &#x6700;&#x5927;&#x5e93;&#x5b58;&#x91cf;&#xd;&#xa;              ,A.LOCATIONREMAK AS REMAK --&#x5907;&#x6ce8;&#xd;&#xa;              ,A.AUDITDATE --&#x5ba1;&#x6838;&#x65e5;&#x671f;&#xd;&#xa;              ,A.INPUTDATE --&#x5f55;&#x5165;&#x65e5;&#x671f;&#xd;&#xa;              ,A.LASTMODIFYDATE --&#x6700;&#x540e;&#x4e00;&#x6b21;&#x4fee;&#x6539;&#x65e5;&#x671f;&#xd;&#xa;              ,&#x27;VDC&#x27; AS SOURCE_CODE --&#x6765;&#x6e90;&#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS WD_INSERT_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x63d2;&#x5165;&#x65f6;&#x95f4; &#xd;&#xa;              ,FROM_UNIXTIME&#x28;UNIX_TIMESTAMP&#x28;&#x29;, &#x27;yyyy-MM-dd HH&#x3a;mm&#x3a;ss&#x27;&#x29; AS WD_UPDATE_DT --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x66f4;&#x65b0;&#x65f6;&#x95f4; &#xd;&#xa;              ,&#x27;19900101&#x27; AS START_DATE_ACTIVE --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x751f;&#x6548;&#x65e5;&#x671f;&#xd;&#xa;              ,&#x27;20991231&#x27; AS END_DATE_ACTIVE --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x5931;&#x6548;&#x65e5;&#x671f;&#xd;&#xa;              ,&#x27;T&#x27; AS ENABLE_FLAG --&#x6570;&#x636e;&#x4ed3;&#x5e93;&#x8bb0;&#x5f55;&#x662f;&#x5426;&#x6700;&#x65b0;&#x8bb0;&#x5f55; T&#x6700;&#x65b0;&#xff0c;&#x5176;&#x4ed6;&#x5386;&#x53f2;&#xd;&#xa;          FROM ODS_ZCVDC.ARC_LOCATION A&#xd;&#xa;          LEFT JOIN &#x28;SELECT T.ATTRIBUTE3&#xd;&#xa;                          ,O.RGN_ID&#xd;&#xa;                          ,O.RGN_NAME&#xd;&#xa;                          ,O.BASE_TYPE&#xd;&#xa;                          ,O.BASE_ID&#xd;&#xa;                          ,O.BASE_NAME&#xd;&#xa;                          ,O.STR_TYPE&#xd;&#xa;                          ,O.STR_ID&#xd;&#xa;                          ,O.STR_NAME&#xd;&#xa;                      FROM DIM.DIM_ORG O&#xd;&#xa;                      LEFT JOIN ODS_ZCOTM.LOCATION T&#xd;&#xa;                        ON O.ORG_ID &#x3d; T.LOCATION_GID&#xd;&#xa;                     WHERE 1 &#x3d; 1&#xd;&#xa;                       AND O.ORG_TYPE &#x3d; &#x27;S&#x27;&#xd;&#xa;                       AND O.STR_ID NOT IN &#x28;&#x27;CMAL&#x2f;ZC.CQ2VDC2&#x27;&#x29;&#xd;&#xa;                     GROUP BY T.ATTRIBUTE3&#xd;&#xa;                             ,O.RGN_ID&#xd;&#xa;                             ,O.RGN_NAME&#xd;&#xa;                             ,O.BASE_TYPE&#xd;&#xa;                             ,O.BASE_ID&#xd;&#xa;                             ,O.BASE_NAME&#xd;&#xa;                             ,O.STR_TYPE&#xd;&#xa;                             ,O.STR_ID&#xd;&#xa;                             ,O.STR_NAME&#x29; AS S&#xd;&#xa;            ON A.WHID &#x3d; S.ATTRIBUTE3&#xd;&#xa;          LEFT JOIN ODS_ZCVDC.ARC_LOCATION_VEHICLE ALV&#xd;&#xa;            ON A.OGID &#x3d; ALV.OGID&#xd;&#xa;           AND A.WHID &#x3d; ALV.WHID&#xd;&#xa;           AND A.LOCATIONID &#x3d; ALV.LOCATIONID&#x29; AS B&#xd;&#xa;  LEFT JOIN DIM.DIM_ORG R&#xd;&#xa;    ON B.STR_ID &#x3d; R.STR_ID&#xd;&#xa;   AND R.ORG_TYPE &#x3d; &#x27;S&#x27;&#x3b;</sql>
      <useVariableSubstitution>T</useVariableSubstitution>
